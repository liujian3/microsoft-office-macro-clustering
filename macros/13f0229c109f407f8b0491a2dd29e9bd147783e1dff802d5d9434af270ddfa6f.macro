Sub Makr|fffd|1()

'H|fffd|ny soros a t|fffd|bl|fffd|zat
utolsosor = 1
Do While Cells(utolsosor, 2) <> ""
    utolsosor = utolsosor + 1
Loop
    utolsosor = utolsosor - 1



'H|fffd|ny oszlopos a t|fffd|bl|fffd|zat
utolsooszlop = 2
Do While Cells(1, utolsooszlop) <> ""
    utolsooszlop = utolsooszlop + 1
Loop
utolsooszlop = utolsooszlop - 2

MsgBox "A t|fffd|bl|fffd|zat " & utolsosor & "soros |fffd|s " & utolsooszlop & "oszlopos"

'adatok megsz|fffd|mol|fffd|sa t|fffd|pus szerint
Dim szam As Long
Dim szoveg As Long
Dim datum As Long


For i = 1 To utolsosor
    For j = 2 To utolsooszlop
        Select Case Cells(sor, oszlop)
        Case Cells(sor, oszlop) = IsNumeric
        szam = szam + 1
        Case Cell(sor, oszlop) = IsDate
        datum = datum + 1
        End Select
    Next
Next
szam = sor * (oszlop - 1) - datum - szoveg

Cells(utolsosor + 1, oszlop) = "szam"
Cells(utolsosor + 1, oszlop) = "datum"
Cells(utolsosor + 1, oszlop) = "szoveg"
Cells(utolsosor + 1, oszlop) = szam
Cells(utolsosor + 1, oszlop) = datum
Cells(utolsosor + 1, oszlop) = szoveg
  
End Sub

