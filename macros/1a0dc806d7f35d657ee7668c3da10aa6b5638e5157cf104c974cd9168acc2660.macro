Private Sub txtValorEnSangre_KeyPress(ByVal KeyAscii As MSForms.ReturnInteger)
    Dim TeclaPulsada As Long
    TeclaPulsada = KeyAscii

    If TeclaPulsada >= 48 And TeclaPulsada <= 57 Then
        KeyAscii = TeclaPulsada
    Else
        KeyAscii = 0
    End If
End Sub
