Sub InicializarUserFormCRUDbd(Tipo As Byte)
    Dim i As Long

    Select Case Tipo
       'REGISTRAR GLUCOMETR|fffd|A
        Case 1
            Load frmCRUDbasedatos
            With frmCRUDbasedatos
                .Caption = "Registrar glucometr|fffd|a"
                .cmdAccion.Caption = "Registrar"
                
                For i = 3 To 9
                    .cboControlAprox.AddItem i
                    .cboControlAprox.List(i - 3, 1) = LCase(Hoja1.Cells(1, i).Value)
                Next i
            End With
            frmCRUDbasedatos.Show
       ''''''''''''''''''''''
    End Select
End Sub

Sub InicializarCalendario()
    Select Case Accion
        Case 1, 2
            Load frmCalendario
            frmCalendario.StartUpPosition = 0
            frmCalendario.Left = frmCRUDbasedatos.Left + frmCRUDbasedatos.txtFechaId.Left
            frmCalendario.Top = frmCRUDbasedatos.Top + frmCRUDbasedatos.txtFechaId.Top + frmCRUDbasedatos.txtFechaId.Height + 24
            frmCalendario.Show
    End Select
End Sub

Sub BotonAccion()
    Dim oTabla As ListObject
    Dim oFila As ListRow
    Dim IdFila As Long
    
    Select Case Accion
       'REGISTRAR GLUCOMETR|fffd|A
        Case 1
            With frmCRUDbasedatos
                If .cboControlAprox.ListIndex = -1 Then
                    MsgBox "Debe seleccionar un tipo de control aproximado.", vbOKOnly + vbInformation, "Alerta"
                    .cboControlAprox.SetFocus
                    Exit Sub
                End If
                
                If .txtFechaId.Text = "" Then
                    MsgBox "Debe especificar una fecha para el control de la glucometr|fffd|a.", vbOKOnly + vbInformation, "Alerta"
                    Exit Sub
                End If
                
                If .txtValorEnSangre.Text = "" Or Val(.txtValorEnSangre.Text) = 0 Then
                    MsgBox "Valor en sangre incorrecto.", vbOKOnly + vbInformation, "Alerta"
                    .txtValorEnSangre.SetFocus
                    Exit Sub
                End If
                
                Application.ScreenUpdating = False
                Set oTabla = Hoja1.ListObjects("BDDiabetes")
                Hoja1.Unprotect "diabetes123456"
                If Hoja1.Range("A2").Value = "" Then
                    IdFila = 1
                    Hoja1.Range("A2").Value = IdFila
                    Hoja1.Range("B2").Value = DateSerial(CInt(Mid(.txtFechaId.Text, 7, 4)), CInt(Mid(.txtFechaId.Text, 4, 2)), CInt(Mid(.txtFechaId.Text, 1, 2)))
                    Hoja1.Cells(2, CLng(.cboControlAprox.Value)).Value = Val(.txtValorEnSangre.Value)
                    Hoja1.Range("J2").Value = Val(.txtValorEnSangre.Value)
                Else
                    IdFila = WorksheetFunction.Max(oTabla.ListColumns(1).Range) + 1
                    Set oFila = oTabla.ListRows.Add
                    oFila.Range.Cells(1, 1).Value = IdFila
                    oFila.Range.Cells(1, 2).Value = DateSerial(CInt(Mid(.txtFechaId.Text, 7, 4)), CInt(Mid(.txtFechaId.Text, 4, 2)), CInt(Mid(.txtFechaId.Text, 1, 2)))
                    oFila.Range.Cells(1, CLng(.cboControlAprox.Value)).Value = Val(.txtValorEnSangre.Value)
                    oFila.Range.Cells(1, 10).Value = Val(.txtValorEnSangre.Value)
                End If
                Hoja1.Protect "diabetes123456"
                Application.ScreenUpdating = True
                MsgBox "El valor de la medici|fffd|n se guard|fffd| en el registro: " & IdFila, vbOKOnly + vbInformation, "Registro exitoso"
            End With
       ''''''''''''''''''''''
       
       'EDITAR GLUCOMETR|fffd|A
        Case 2
            With frmCRUDbasedatos
                If .txtValorEnSangre.Text = "" Or Val(.txtValorEnSangre.Text) = 0 Then
                    MsgBox "Valor en sangre incorrecto.", vbOKOnly + vbInformation, "Alerta"
                    .txtValorEnSangre.SetFocus
                    Exit Sub
                End If
                
                Application.ScreenUpdating = False
                
                Set oTabla = Hoja1.ListObjects("BDDiabetes")
                Hoja1.Unprotect "diabetes123456"
                Set oFila = oTabla.ListRows(RegistroEditar)
                oFila.Range.ClearContents
                oFila.Range.Cells(1, 1).Value = RegistroEditar
                oFila.Range.Cells(1, 2).Value = DateSerial(CInt(Mid(.txtFechaId.Text, 7, 4)), CInt(Mid(.txtFechaId.Text, 4, 2)), CInt(Mid(.txtFechaId.Text, 1, 2)))
                oFila.Range.Cells(1, CLng(.cboControlAprox.Value)).Value = Val(.txtValorEnSangre.Value)
                oFila.Range.Cells(1, 10).Value = Val(.txtValorEnSangre.Value)
                
                Hoja1.Protect "diabetes123456"
                Application.ScreenUpdating = True
                MsgBox "El valor de la medici|fffd|n se edit|fffd| correctamente.", vbOKOnly + vbInformation, "Edici|fffd|n exitosa sobre el registro n|fffd|m: " & RegistroEditar
            End With
       '''''''''''''''''''
        
    End Select
    Set oTabla = Nothing
    Set oFila = Nothing
    Unload frmCRUDbasedatos
End Sub

Sub BotonAceptarFrmNumEditarRegistro()
    Dim oTabla1 As ListObject
    Dim oFila1 As ListRow
    Dim RegistroMaximo As Long
    Dim Indice1 As Long
    Dim Indice2 As Long
    
    With frmNumEditarRegistro
        If .txtNumRegistro.Text = "" Then
            MsgBox "Debe especificar un n|fffd|mero de registro valido.", vbOKOnly + vbInformation, "Alerta"
            .txtNumRegistro.Text = ""
            .txtNumRegistro.SetFocus
            Exit Sub
        End If
                
        If Val(.txtNumRegistro.Text) = 0 Then
            MsgBox "Debe especificar un n|fffd|mero de registro valido.", vbOKOnly + vbInformation, "Alerta"
            .txtNumRegistro.Text = ""
            .txtNumRegistro.SetFocus
            Exit Sub
        End If
        
        If CLng(.txtNumRegistro.Text) >= 1048576 Then
            MsgBox "Debe especificar un n|fffd|mero de registro valido.", vbOKOnly + vbInformation, "Alerta"
            .txtNumRegistro.Text = ""
            .txtNumRegistro.SetFocus
            Exit Sub
        End If
        
        Set oTabla1 = Hoja1.ListObjects("BDDiabetes")
        RegistroMaximo = WorksheetFunction.Max(oTabla1.ListColumns(1).Range)
        RegistroEditar = CLng(.txtNumRegistro.Text)
        
        If RegistroEditar <= RegistroMaximo Then
            Accion = 2
            Set oFila1 = oTabla1.ListRows(RegistroEditar)
            For Indice1 = 3 To 9
                If oFila1.Range.Cells(1, Indice1) <> "" Then
                    Indice2 = Indice1
                    Exit For
                End If
            Next Indice1
            
            Load frmCRUDbasedatos
            For Indice1 = 3 To 9
                frmCRUDbasedatos.cboControlAprox.AddItem Indice1
                frmCRUDbasedatos.cboControlAprox.List(Indice1 - 3, 1) = LCase(Hoja1.Cells(1, Indice1).Value)
            Next Indice1
            frmCRUDbasedatos.Caption = "Editar glucometr|fffd|a"
            frmCRUDbasedatos.cmdAccion.Caption = "Editar"
            
            frmCRUDbasedatos.cboControlAprox.ListIndex = Indice2 - 3
            frmCRUDbasedatos.txtFechaId.Text = Format(oFila1.Range.Cells(1, 2).Value, "dd/mm/yyyy")
            frmCRUDbasedatos.txtValorEnSangre.Text = oFila1.Range.Cells(1, 10).Value
            
            Unload frmNumEditarRegistro
            frmCRUDbasedatos.Show
        Else
            MsgBox "Debe especificar un n|fffd|mero de registro valido.", vbOKOnly + vbInformation, "Alerta"
            .txtNumRegistro.Text = ""
            .txtNumRegistro.SetFocus
        End If
    End With
    Set oTabla1 = Nothing
    Set oFila1 = Nothing
End Sub
