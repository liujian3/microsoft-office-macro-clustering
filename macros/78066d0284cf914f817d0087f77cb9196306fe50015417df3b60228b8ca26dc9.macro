Sub tabrevue()
Dim lig As Integer
Dim col As Integer
Dim ligne As Integer
Dim colonne As Integer
Dim membre As Integer
Dim supp As Integer
ActiveSheet.Unprotect Password:="#dprep#"
lig = 8
col = 8

For supp = 8 To 27
      Sheets("revues").Cells(supp, 8).Value = ""
      Sheets("revues").Cells(supp, 9).Value = ""
Next supp
ActiveSheet.Protect Password:="#dprep#"
colonne = Sheets("revues").Cells(32, 16).Value + 1 'ligne colonne
ligne = Sheets("revues").Cells(32, 15).Value + 1
membre = Sheets("revues").Cells(33, 15).Value
ActiveSheet.Protect Password:="#dprep#"
For compteur = 1 To membre
      Sheets("revues").Cells(lig, col).Value = Sheets("revues").Cells(ligne, colonne)
      Sheets("revues").Cells(lig, col + 1).Value = Sheets("revues").Cells(ligne, colonne + 1)
      lig = lig + 1
      ligne = ligne + 1
Next compteur
Sheets("revues").Select
ActiveSheet.Protect Password:="#dprep#", _
                    AllowFormattingCells:=True, _
                    AllowFormattingColumns:=False, _
                    AllowFormattingRows:=True, _
                    AllowInsertingRows:=False, _
                    AllowDeletingRows:=False, _
                    AllowSorting:=True, _
                    AllowFiltering:=True, _
                    DrawingObjects:=True, _
                    Contents:=True, _
                    Scenarios:=True
End Sub
