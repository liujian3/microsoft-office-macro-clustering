Attribute VB_Name = "Module2"
Option Explicit
 Function traitementcomptermotrange(range As range) ' calcule le nombre du mots
 Dim NombreMots As Integer
 Dim departrecherche As Integer
 Dim newdepartrecherche As Integer
 Dim cellule As Variant
 Dim x As Integer
 Dim totalmots As Integer
 totalmots = 0
 For Each cellule In range
 NombreMots = 1
 departrecherche = 1
 newdepartrecherche = 1
    For x = 1 To Len(cellule)
      If Not InStr(departrecherche, cellule, " ", 1) = 0 Then
      NombreMots = NombreMots + 1
      newdepartrecherche = InStr(departrecherche, cellule, " ", 1) + 1
      departrecherche = newdepartrecherche
      Else
      totalmots = totalmots + NombreMots
      Exit For
      End If
      Next
      Next
      traitementcomptermotrange = totalmots
End Function
