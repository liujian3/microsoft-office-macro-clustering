  Next
  
  For Each cell In Range("NewZadostPodpis")
    If cell.Value <> "TRUE" Then nevyplnena_pole6 = nevyplnena_pole6 & vbNewLine & cell.Value
  Next
  
  If nevyplnena_pole0 & nevyplnena_pole1 & nevyplnena_pole2 & nevyplnena_pole5 & nevyplnena_pole3 & nevyplnena_pole4 <> "" Then
    If nevyplnena_pole0 <> "" Then nevyplnena_pole0 = vbNewLine & "Z|fffd|kladn|fffd| |fffd|daje " & nevyplnena_pole0 & vbNewLine
