Sub MaximalniVyseUveru()
  ' v12a MiBre: (oprava) k adminDisponibilniZdroje p|fffd|i|fffd||fffd|t|fffd|m admiOstatniSplatky, proto|fffd|e v|fffd|po|fffd|et adminDisponibilniZdroje
  '             chybn|fffd| ovliv|fffd|uje adminOstatniSplatky maj|fffd| b|fffd|t ode|fffd|teny a|fffd| v diskontovan|fffd| podob|fffd|j
  'PETRA |fffd|prava - max v|fffd||fffd|e |fffd|v|fffd|ru se sazbou ni|fffd||fffd||fffd| ne|fffd| standard
  Dim siMaxVyse As Single
  
  With Sheets("adminBonita")

      siMaxVyse = 12 * ((.Range("adminDisponibilniZdroje").Value - Sheet4.Range("VyseMesPoj").Value + .Range("adminOstatniSplatky").Value) * .Range("MAXDR").Value * _
                  (1 - (1 / (1 + Sheets("adminSplatka").Range("UrSazba").Value) ^ Sheets("Splatka").Range("DobaSplatnosti").Value)) - _
                  .Range("adminOstatniSplatky").Value * _
                  (1 - (1 / (1 + Sheets("adminSplatka").Range("UrSazba").Value) ^ Sheets("Splatka").Range("DobaSplatnosti").Value))) _
                  / Sheets("adminSplatka").Range("UrSazba").Value
      siMaxVyse = Application.WorksheetFunction.RoundDown(siMaxVyse, -4)
      If siMaxVyse < 200000 Then: .Range("adminMaxVyse").Value = 0: Else: .Range("adminMaxVyse").Value = siMaxVyse
    
 
  End With

  Call ModNabidka.NastavitMaxVysi
End Sub

