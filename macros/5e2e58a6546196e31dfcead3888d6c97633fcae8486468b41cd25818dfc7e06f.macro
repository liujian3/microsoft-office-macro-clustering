Sub PridejVyslKontroly(Result As String)
  ' p|fffd|vodn|fffd| |fffd||fffd|st aplikace
  If UCase(Mid(Result, 1, 2)) <> "OK" Or UCase(Range("VypisovatOK")) = "ANO" Then
    Cells(VyslKontroly, 2) = Result
    VyslKontroly = VyslKontroly + 1
  End If
  If UCase(Mid(Result, 1, 3)) = "CHY" Then
    PocetERR = PocetERR + 1
  ElseIf UCase(Mid(Result, 1, 3)) = "UPO" Then
    PocetWarn = PocetWarn + 1
  End If
End Sub

