Sub Rozvedeny()
  With Sheets("Dokumenty")
    .Unprotect cnsHeslo
    If .Range("zenaty").Value = 1 Then
      With .Shapes("opbuMaSjmAno").OLEFormat.Object
        .Enabled = 1
        .Value = 0
      End With
      With .Shapes("opbuMaSjmNe").OLEFormat.Object
        .Enabled = 1
        .Value = 0
      End With
      With .Shapes("opbuRozvodAno").OLEFormat.Object
        .Enabled = 0
        .Value = 0
      End With
      With .Shapes("opbuRozvodNe").OLEFormat.Object
        .Enabled = 0
        .Value = 0
      End With

    ElseIf .Range("zenaty").Value = 2 Then
      With .Shapes("opbuMaSjmAno").OLEFormat.Object
        .Enabled = 1
        .Value = 0
      End With
      With .Shapes("opbuMaSjmNe").OLEFormat.Object
        .Enabled = 1
        .Value = 0
      End With
      With .Shapes("opbuRozvodAno").OLEFormat.Object
        .Enabled = 1
        .Value = 0
      End With
      With .Shapes("opbuRozvodNe").OLEFormat.Object
        .Enabled = 1
        .Value = 0
      End With

    Else
      With .Shapes("opbuMaSjmAno").OLEFormat.Object
        .Enabled = 0
        .Value = 0
      End With
      With .Shapes("opbuMaSjmNe").OLEFormat.Object
        .Enabled = 0
        .Value = 0
      End With
      With .Shapes("opbuRozvodAno").OLEFormat.Object
        .Enabled = 0
        .Value = 0
      End With
      With .Shapes("opbuRozvodNe").OLEFormat.Object
        .Enabled = 0
        .Value = 0
      End With
    End If
    .Protect cnsHeslo
  End With
End Sub
Attribute VB_Name = "ModDoplneni"
Option Explicit

Sub PridatZadateleDoplneni()
  ' v12 MiBre: p|fffd|id|fffd| dal|fffd||fffd|ho |fffd|adatele a zobraz|fffd| p|fffd||fffd|p. dal|fffd||fffd| |fffd||fffd|st formul|fffd||fffd|e k vypln|fffd|n|fffd|/tisku
  With Sheets("Doplneni")
    .Unprotect cnsHeslo
    If .Range("DopPocetZadatelu").Value < 4 Then .Range("DopPocetZadatelu").Value = .Range("DopPocetZadatelu").Value + 1
    .Protect cnsHeslo
  End With
  
  Call ModDoplneni.ZobrazitDoplneni
End Sub

Sub OdebratZadateleDoplneni()
  ' v12 MiBre: odebere |fffd|adatele a skryje p|fffd||fffd|p. |fffd||fffd|st formul|fffd||fffd|e k vypln|fffd|n|fffd|/tisku
  With Sheets("Doplneni")
    .Unprotect cnsHeslo
    If .Range("DopPocetZadatelu").Value > 1 Then .Range("DopPocetZadatelu").Value = .Range("DopPocetZadatelu").Value - 1
    .Protect cnsHeslo
  End With
  
  Call ModDoplneni.ZobrazitDoplneni
End Sub

Sub ZobrazitDoplneni()
  ' v12 MiBre: zobraz|fffd|/skryje |fffd||fffd|sti formul|fffd||fffd|e podle po|fffd|tu zvolen|fffd|ch |fffd|adatel|fffd|
  With Sheets("Doplneni")
    .Unprotect cnsHeslo
    Select Case .Range("DopPocetZadatelu").Value
      Case 1, 2
        .Range("DopDoplneni1").EntireRow.Hidden = False
        .Range("DopDoplneni2").EntireRow.Hidden = True
      Case 3, 4
        .Range("DopDoplneni1").EntireRow.Hidden = False
