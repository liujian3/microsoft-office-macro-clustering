Private Function Velikonoce(rok As Integer) As Date ' vr|fffd|t|fffd| velikono|fffd|n|fffd| ned|fffd|li, zdroj WWW
 'P|fffd|id|fffd|no MPL 19.5.2011 (vytvo|fffd|eno MVA)
  Dim a, b, c, m, n, d, e, U, V
  
  a = rok Mod 19
  b = rok Mod 4
  c = rok Mod 7
  m = 24
  n = 5
  d = (19 * a + m) Mod 30
  e = (n + 2 * b + 4 * c + 6 * d) Mod 7
  U = d + e - 9
  
  If U = 25 And d = 28 And e = 6 And a > 10 Then
    U = 18
    V = 4
  ElseIf U >= 1 And U <= 25 Then
    V = 4
  ElseIf U > 25 Then
    U = U - 7
    V = 4
  Else
    U = 22 + d + e
    V = 3
  End If
  Velikonoce = DateSerial(rok, V, U)
End Function

