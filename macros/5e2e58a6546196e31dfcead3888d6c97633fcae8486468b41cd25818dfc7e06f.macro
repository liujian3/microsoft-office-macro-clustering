Function VypoctiSplatku(UrSazba As Double, VyseUveru As Double, Splatnost As Double) As Double
  ' p|fffd|vodn|fffd| |fffd||fffd|st aplikace
  VypoctiSplatku = Round(VyseUveru * Application.Power(1 + UrSazba / 12, Splatnost) * UrSazba / 12 / (Application.Power(1 + UrSazba / 12, Splatnost) - 1), 2)
End Function

Function VratProductDomain(ProdTyp As String, domain As Integer) As String
  ' p|fffd|vodn|fffd| |fffd||fffd|st aplikace
  With Sheets("DefProd")
    n = 2
    Do While Not IsEmpty(.Cells(n, 1))
      If .Cells(n, 1) = ProdTyp Then
        VratProductDomain = .Cells(n, domain)
        Exit Function
      End If
      n = n + 1
    Loop
  End With
  VratProductDomain = ""
End Function

Function VratSazbu() As Boolean
  ' p|fffd|vodn|fffd| |fffd||fffd|st aplikace
  Dim MesicniPojisteni, JednorazovePojisteni As Single
  
  Fixace = Range("Fixace")
  Splatnost = Range("Splatnost")
  VyseUveru = (Sheet3.Range("VyseUveruZaklad").Value + Sheet3.Range("vyseUveru2").Value)
  Product_sub = Range("Product_sub")
  Range("SlevaNaTvrdo").Value = Round(Range("SlevaNaTvrdo").Value, 4)
  LTV = Range("C_D_CardIncl")
  MesicniPojisteni = 0
  JednorazovePojisteni = 0

  n = 2
  
  With Sheets("DefSazby")
    If Sheets("Splatka").Range("Pojisteni") = "Ano" Then MesicniPojisteni = .Cells(n, 17) / 100
  
    Do While Not IsEmpty(.Cells(n, 1))
      If Fixace = .Cells(n, 4) _
      And LTV * 100 > .Cells(n, 5) And LTV * 100 <= .Cells(n, 6) _
      And Splatnost / 12 > .Cells(n, 7) And Splatnost / 12 <= .Cells(n, 8) _
      And VyseUveru > .Cells(n, 9) And VyseUveru <= .Cells(n, 10) _
      And Product_sub = .Cells(n, 1) _
      Then
'        sazba = .Cells(n, 11) / 100
        If Range("Sleva_CF") = "Ano" Then Range("Sleva_CF_V") = .Cells(n, 12) / 100
        If Range("Sleva_CP") = "Ano" Then Range("Sleva_CP_V") = .Cells(n, 13) / 100
        If Range("Sleva_AB") = "Ano" Then Range("Sleva_AB_V") = .Cells(n, 16) / 100
        If Range("Sleva_KK") = "Ano" Then Range("Sleva_KK_V") = .Cells(n, 15) / 100
        If Range("Sleva_AB_FIX") = "Ano" Then Range("Sleva_AB_FIX_V") = .Cells(n, 14) / 100
        
        'x-sell slevy pro poji|fffd|t|fffd|n|fffd|
        If Sheet3.Range("Pojisteni") = "m|fffd|s|fffd||fffd|n|fffd| poji|fffd|t|fffd|n|fffd|" Then MesicniPojisteni = Sheet4.Range("slevaMP").Value
        If Sheet3.Range("Pojisteni") = "jednor|fffd|zov|fffd| poji|fffd|t|fffd|n|fffd|" Then JednorazovePojisteni = Sheet4.Range("slevaJP").Value
        
        '|fffd|v|fffd|r nad 20 MIO, cizom|fffd|nn|fffd|, ru|fffd|n|fffd| |fffd|prava sazeb
        If Range("VypocitatPodle").Value = 2 Then
          Range("UrSazba") = (Sheet3.Range("SplCsZmena2").Value + Sheet3.Range("SplOsZmena2").Value + Sheet3.Range("SplSrsZmena2") + Sheet3.Range("SplRmZmena2") + _
                             Sheet3.Range("SplOmZmena2") + Sheet3.Range("SplCoSpZmena2")) - Range("Sleva_CF_V") - Range("Sleva_CP_V") - Range("Sleva_AB_V") - _
                             Range("Sleva_KK_V") - Range("Sleva_AB_FIX_V") - Range("SlevaNaTvrdo") - Range("SplMarketingSleva") - MesicniPojisteni
          Sheet3.Range("Zmena2").Value = "POZOR! ZM|fffd|NA FUNDINGU!"
        
        Else
         '|fffd|v|fffd|r nad 20 MIO, cizom|fffd|nn|fffd|, ru|fffd|n|fffd| |fffd|prava sazeb PRIVAT
        If Range("VypocitatPodle").Value = 22 Then
          Range("UrSazba") = (Sheet3.Range("SplCsZmena2").Value + Sheet3.Range("SplOsZmena2").Value + Sheet3.Range("SplSrsZmena2") + Sheet3.Range("SplRmZmena2") + _
                             Sheet3.Range("SplOmZmena2") + Sheet3.Range("SplCoSpZmena2")) - Range("Sleva_CF_V") - Range("Sleva_CP_V") - Range("Sleva_AB_V") - _
                             Range("Sleva_KK_V") - Range("Sleva_AB_FIX_V") - Range("SlevaNaTvrdo") - MesicniPojisteni '- Range("SplMarketingSleva")
          Sheet3.Range("Zmena2").Value = "POZOR! ZM|fffd|NA FUNDINGU!"
        Else
