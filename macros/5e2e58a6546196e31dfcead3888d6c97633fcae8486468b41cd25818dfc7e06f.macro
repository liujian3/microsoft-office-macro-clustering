   Application.Calculation = xlCalculationManual ' Nyn|fffd| p|fffd|epnu na manu|fffd|ln|fffd| - jinak by to bylo stra|fffd|n|fffd| pomal|fffd|
   
   ' Vymazan|fffd| spl|fffd|tkov|fffd|ho Kalendare
   Range("SplKalendar").ClearContents
   Range("SplKalBezPrvniho").Clear
      
   'Definov|fffd|n|fffd| do kter|fffd|ch sloupc|fffd| se maj|fffd| d|fffd|t vysledky
   Const Vysesplatka = 9
   Const VysesplUrok = 10
   Const VysesplJist = 11
   Const VyseDluhu = 12
   Const CSplatky = 7
   Const DatumSplatky = 8
   
   ' Nacteni promennych
   Dim VyseUveru As Double, sazba As Double, sazbaAfterFix As Double, anuita As Double, anuitaAfterFix As Double, PocetSpl As Double
   Dim PocetDnivPrvnimMesici As Integer, SKStartR As Integer, CisloSplatky As Integer
   Dim SplatkaDatum As Date
   
   SKStartR = 21
   CisloSplatky = 1
   
   'v|fffd|po|fffd|et spl|fffd|tky pro |fffd|v|fffd|r bez poji|fffd|t|fffd|n|fffd|
'   Sheet8.Range("Anuita") = Application.RoundUp(VypoctiSplatku(Sheet4.Range("UrSazba"), Sheet9.Range("VyseUveruBezPojisteni"), Sheet4.Range("Splatnost")), 0)

'   With Sheets("adminKalendar")
'    sazbaAfterFix = .Range("sazbaAfterFix").Value
'    PocetDnivPrvnimMesici = .Range("PocetDnivPrvnimMesici").Value
'   End With
    
'  With Sheets("Kalendar")
'    VyseUveru = Sheet9.Range("VyseUveruBezPojisteni") 'v17 KHO: spl|fffd|tkov|fffd| kalend|fffd||fffd|e pro |fffd|v|fffd|r a poji|fffd|t|fffd|n|fffd| jsou odd|fffd|len|fffd| - na konci se s|fffd||fffd|taj|fffd| dohromady
'    sazba = .Range("Sazba").Value
'    anuita = .Range("Anuita").Value
'    PocetSpl = .Range("PocetSpl").Value
'    SplatkaDatum = .Range("PrvniSplatka").Value
'  End With
   
   ' smazani - ve finale bude udelano pomoci range.delete
   Range("G21", "L400").ClearContents
   
   ' prvni Splatka
   Cells(SKStartR, VyseDluhu) = VyseUveru
   Cells(SKStartR, VysesplUrok) = Round(VyseUveru * sazba / 360 * PocetDnivPrvnimMesici, 2)
   Cells(SKStartR, VysesplJist) = 0
   Cells(SKStartR, Vysesplatka) = Cells(SKStartR, VysesplJist) + Cells(SKStartR, VysesplUrok)
   Cells(SKStartR, DatumSplatky) = SplatkaDatum
   Cells(SKStartR, CSplatky) = CisloSplatky
   
   ' Od druhe splatky do fixu + poji|fffd|t|fffd|n|fffd|
   SKStartR = SKStartR + 1
   For splatka = 2 To Range("FixAfter") + 1
      Cells(SKStartR, Vysesplatka) = anuita
      Cells(SKStartR, VysesplUrok) = Round(VyseUveru * sazba / 12, 2)
      Cells(SKStartR, VysesplJist) = anuita - Cells(SKStartR, VysesplUrok)
      VyseUveru = VyseUveru - Cells(SKStartR, VysesplJist)
      Cells(SKStartR, VyseDluhu) = VyseUveru
      SplatkaDatum = DateAdd("m", 1, SplatkaDatum)
      Cells(SKStartR, DatumSplatky) = SplatkaDatum
      CisloSplatky = CisloSplatky + 1
      Cells(SKStartR, CSplatky) = CisloSplatky
      
      SKStartR = SKStartR + 1
   Next splatka
   
   If PocetSpl > 60 Then
   Calculate ' Aby se dob|fffd|e spo|fffd||fffd|tala AnuitaAfterFix
   anuitaAfterFix = Range("anuitaAfterFix")
   End If
   
   'Od fixu od p|fffd|edposledn|fffd| spl|fffd|tky
   For splatka = Range("FixAfter") + 1 To Range("PocetSpl") - 1
      Cells(SKStartR, Vysesplatka) = anuitaAfterFix
      Cells(SKStartR, VysesplUrok) = Round(VyseUveru * sazbaAfterFix / 12, 2)
      Cells(SKStartR, VysesplJist) = anuitaAfterFix - Cells(SKStartR, VysesplUrok)
      VyseUveru = VyseUveru - Cells(SKStartR, VysesplJist)
      Cells(SKStartR, VyseDluhu) = VyseUveru
      SplatkaDatum = DateAdd("m", 1, SplatkaDatum)
      Cells(SKStartR, DatumSplatky) = SplatkaDatum
      CisloSplatky = CisloSplatky + 1
      Cells(SKStartR, CSplatky) = CisloSplatky
      SKStartR = SKStartR + 1
   Next splatka
   
   'Posledni splatka
   Cells(SKStartR, VysesplUrok) = Round(VyseUveru * sazbaAfterFix / 12, 2)
   Cells(SKStartR, VysesplJist) = VyseUveru
   Cells(SKStartR, VyseDluhu) = 0
   Cells(SKStartR, Vysesplatka) = Cells(SKStartR, VysesplJist) + Cells(SKStartR, VysesplUrok)
   SplatkaDatum = DateAdd("m", 1, SplatkaDatum)
   Cells(SKStartR, DatumSplatky) = SplatkaDatum
   CisloSplatky = CisloSplatky + 1
   Cells(SKStartR, CSplatky) = CisloSplatky
        
   Rows("21:21").Select
