Sub NastavitSluzby()
  'v12c MiBre: zobraz|fffd|/skryje sekci slu|fffd|by p|fffd|i v|fffd|b|fffd|ru typu hypot|fffd|ky
  With Sheets("Splatka")
    .Unprotect cnsHeslo
    Select Case .Range("Fix_obdobi").Value
      Case "1 rok", "2 roky", "3 roky", "5 let", "7 let", "10 let"
        If .Range("ProduktList").Value = "U hypot|fffd|ka |fffd||fffd|elov|fffd|" Then
          .Range("nazevSluzby").Value = "Slu|fffd|by"
          Call modFunction.OhranicenaOblastCervena(.Range("oblastSluzby"))
          Call modFunction.OhranicenaBunka(.Range("N29:P29"))
          .Range("Flexi").Value = "a|fffd| 25% jistiny"
        Else
          .Range("nazevSluzby").Value = ""
          Call modFunction.BunkaBezOhraniceniBila(.Range("oblastSluzby"))
          Call modFunction.BunkaBezOhraniceniPrazdna(.Range("N29:P29"))
        End If
      Case Else
        .Range("nazevSluzby").Value = ""
        Call modFunction.BunkaBezOhraniceniBila(.Range("oblastSluzby"))
        Call modFunction.BunkaBezOhraniceniPrazdna(.Range("N29:P29"))
    End Select
    .Protect cnsHeslo
  End With
End Sub

