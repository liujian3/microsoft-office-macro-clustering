Function SpoctiSazbuProBonitu() As Boolean
  ' p|fffd|vodn|fffd| |fffd||fffd|st aplikace
  
  Splatnost = Range("Splatnost")
  VyseUveru = (Sheet3.Range("VyseUveruZaklad").Value + Sheet3.Range("vyseUveru2").Value)
  Product_sub = Range("Product_subBon")  ' 01D se p|fffd|ipadn|fffd| nahrazuje 01A
  Fixace = Range("FixaceBon")   ' vzdy se bere 3 lety
  LTV = Range("C_D_CardIncl")

  n = 2
  With Sheets("DefSazby")
  
  Dim MesicniPojisteni As Single
  MesicniPojisteni = 0
  If Sheets("Splatka").Range("Pojisteni") = "Ano" Then MesicniPojisteni = .Cells(n, 17) / 100
    
    Do While Not IsEmpty(.Cells(n, 1))
      If Fixace = .Cells(n, 4) _
         And LTV * 100 > .Cells(n, 5).Value * 1 And LTV * 100 <= .Cells(n, 6).Value * 1 _
         And Splatnost / 12 > .Cells(n, 7).Value * 1 And Splatnost / 12 <= .Cells(n, 8).Value * 1 _
         And VyseUveru > .Cells(n, 9) And VyseUveru <= .Cells(n, 10) _
         And Product_sub = .Cells(n, 1) _
      Then
        If Sheets("adminSplatka").Range("FIXACE") = "Fl" Or Sheets("adminSplatka").Range("FIXACE") = "CA" Then
          ' pro float
           Sheets("adminSplatka").Range("Sleva_AB_VBON") = .Cells(n, 14) / 100   ' Sleva za balicek pokud byla i p|fffd|edt|fffd|m
           Sheets("adminSplatka").Range("Sleva_KK_VBON") = .Cells(n, 15) / 100   ' Sleva za KK pokud byla i p|fffd|edt|fffd|m
           ' Pro Cardif slevu bereme v|fffd|dy 3-lety fix, ale jen pokud byla vybrana
           'If Sheets("adminSplatka").Range("Sleva_CF") = "Ano" Then Sheets("adminSplatka").Range("Sleva_CF_VBON") = .Cells(n, 12) / 100
        Else
          ' pro fix
           Sheets("adminSplatka").Range("Sleva_AB_VBON") = .Cells(n, 14) / 100  ' Sleva za balicek pokud byla i p|fffd|edt|fffd|m
           Sheets("adminSplatka").Range("Sleva_KK_VBON") = .Cells(n, 15) / 100 ' Sleva za KK pokud byla i p|fffd|edt|fffd|m
           ' Pro Cardif slevu bereme v|fffd|dy 3-lety fix, ale jen pokud byla vybrana
           'If Sheets("adminSplatka").Range("Sleva_CP") = "Ano" Then Sheets("adminSplatka").Range("Sleva_CF_VBON") = .Cells(n, 12) / 100
        End If
        
        Sheets("adminSplatka").Range("UrSazbaBON") = .Cells(n, 11) / 100 - Sheets("adminSplatka").Range("Sleva_CF_VBON") - Sheets("adminSplatka").Range("Sleva_AB_VBON") - Sheets("adminSplatka").Range("Sleva_KK_VBON") - MesicniPojisteni + Sheets("adminsplatka").Range("prirazka_objem_hypo") + Sheets("adminsplatka").Range("prirazka_pevna_splatka")
        SpoctiSazbuProBonitu = True
        Exit Function
      End If
      n = n + 1
    Loop
  End With
  SpoctiSazbuProBonitu = False
End Function

