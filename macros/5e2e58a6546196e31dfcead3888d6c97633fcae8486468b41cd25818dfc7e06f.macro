Sub zafixovaniVstupnichDat()
  ' p|fffd|vodn|fffd| |fffd||fffd|st aplikace
   r = Range("ZmenaDatKontr").Row + 1
   s = Range("ZmenaDatKontr").Column
   
   With Sheets("Splatka")
     Do While Not IsEmpty(.Cells(r, s))
       .Cells(r, s + 1) = .Cells(r, s)
       r = r + 1
     Loop
   End With
End Sub


