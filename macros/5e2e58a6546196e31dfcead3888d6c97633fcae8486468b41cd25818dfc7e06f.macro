Sub NastavitRodneCislo()
  ' v12 MiBre: zobraz|fffd|/skryje rodn|fffd| |fffd||fffd|slo p|fffd|i v|fffd|b|fffd|ru ob|fffd|astv|fffd| |fffd|adatele
  With Sheets("Splatka")
    .Unprotect cnsHeslo
    Select Case .Range("SplZadatel").Value
      Case "Ob|fffd|an |fffd|R", "Ob|fffd|an SR bez trval|fffd|ho pobytu"
        With .Range("SplRodneCislo")
          .Value = "M|fffd| |fffd|esk|fffd| nebo slovensk|fffd| R|fffd|"
          .Font.Color = -16776961
          .Interior.Color = 255
        End With
        .Range("SplNazevRodneCislo").Value = ""
        Call modFunction.BunkaBezOhraniceni(.Range("N22:P22"))
      Case Else
        .Range("SplNazevRodneCislo").Value = "Rodn|fffd| |fffd||fffd|slo"
        Call modFunction.OhranicenaBunka(.Range("N22:P22"))
    End Select
    .Protect cnsHeslo
  End With
