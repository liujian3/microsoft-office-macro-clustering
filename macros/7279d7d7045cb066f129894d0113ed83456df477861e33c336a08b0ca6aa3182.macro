Sub ResetScoreResults()

Dim ws As Worksheet

    Application.ScreenUpdating = False
    
    For Each ws In Worksheets
        If ws.Name = "Touchpoint" Then
            ws.Activate
            'clear the answers placeholder text
            ActiveSheet.ScoreResultsViewer.Value = ""
                
            'Click the Reset Highlighting button
            ''Note: to call the code located in another sheet, it must be in a Public Sub
            Call ActiveSheet.Button_ResetHighlight_Click
        End If
    Next ws
    
    Sheets("Scores").Field_ClaimScore1.Value = ""
'    Sheets("Scores").Field_ClaimScore2.Value = ""
'    Sheets("Scores").Field_ClaimScore3.Value = ""
'    Sheets("Scores").Field_ClaimScore4.Value = ""
