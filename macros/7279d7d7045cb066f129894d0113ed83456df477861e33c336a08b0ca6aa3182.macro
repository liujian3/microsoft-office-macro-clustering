Private Sub ScoreClaim()

   

    Dim Info As String
    
    Dim RngIndicative As Range: Set RngIndicative = Application.Range("E335 - E403")
    Dim HeadingIndicative As String
    Dim MissedIndicative As String
    
'    Dim RngLineItems As Range: Set RngLineItems = Application.Range("E330:E371")
'    Dim HeadingLineItems As String
'    Dim MissedLineItems As String
'
'    Dim RngRemainingItems As Range: Set RngRemainingItems = Application.Range("E394:E2307")
'    Dim HeadingRemainingItems As String
'    Dim MissedRemainingItems As String
    
    ''''''''''''''''
    Dim cel As Range


'==================================================================
'==================================================================
'==================================================================

    
    'User Info Section
    Info = Application.Range("E324").Value & vbLf & _
                Application.Range("E325").Value & vbLf & _
                Application.Range("E326").Value & vbLf & _
                Application.Range("E327").Value & vbLf & _
                Application.Range("E328").Value & vbLf & _
                Application.Range("E329").Value & " " & Application.Range("F329").Value & vbLf & _
                " - " & Application.Range("E329").Value & " " & Application.Range("F329").Value & vbLf & _
                " - " & Application.Range("E330").Value & " " & Application.Range("F330").Value & vbLf & _
                " - " & Application.Range("E331").Value & " " & Application.Range("F331").Value & vbLf & _
                " - " & Application.Range("E332").Value & " " & Application.Range("F332").Text
                       'note: vbLf is same as Chr(13) - it is a vb Line Feed

'===========================
    'Indicative Heading
    HeadingIndicative = "   ------------------------------" & vbLf & _
                        "      Indicative Section" & vbLf & _
                        "   ------------------------------"
    'Indicative Section
    For Each cel In RngIndicative.Cells
        With cel
            cel.Select
            If Not cel.Text = "" Then
                MissedIndicative = MissedIndicative & Chr(149) & " " & cel.Text & vbLf  ' Chr(13)
            End If
        End With
    Next cel
'===========================
    'Line Items Heading
    HeadingLineItems = "   ------------------------------" & vbLf & _
                       "      Line Items Section" & vbLf & _
                       "   ------------------------------"

    'Line Items Section
    For Each cel In RngLineItems.Cells
        With cel
            cel.Select
            If Not cel.Text = "" Then
                MissedLineItems = MissedLineItems & Chr(149) & " " & cel.Text & vbLf  ' Chr(13)
            End If
        End With
    Next cel
    
'===========================
    'Remaining Items Heading
    HeadingRemainingItems = "   ----------------------------------------" & vbLf & _
                            "      Remaining Items Section" & vbLf & _
                            "   ----------------------------------------"

    'Line Items Section
    For Each cel In RngRemainingItems.Cells
        With cel
            cel.Select
            If Not cel.Text = "" Then
                MissedRemainingItems = RemainingItems & Chr(149) & " " & cel.Text & vbLf  ' Chr(13)
            End If
        End With
    Next cel


'========================================================================
    
    ActiveSheet.ScoreResultsViewer.Value = HeadingIndicative & vbLf & _
                                        MissedIndicative & vbLf & _
                                        HeadingLineItems & vbLf & _
                                        MissedLineItems & vbLf & _
                                        HeadingRemainingItems & vbLf & _
                                        MissedRemainingItems
    
    ''''''''''''''''''''''''''
