Private Sub UserForm_Initialize()
    
    If Sheets("Data").Range("B16").Value = "Locked" Then
        Button_LockUnlock.Caption = "Unlock"
        Button_LockUnlock.BackColor = &H8080FF
    Else
        Button_LockUnlock.Caption = "Lock"
        Button_LockUnlock.BackColor = &H80FF80
    End If
    
    Field_AditorName.Value = Sheets("Scores").Range("B13").Value
    
End Sub

Private Sub Button_LockUnlock_Click()
    If Sheets("Data").Range("B16").Value = "Locked" Then
        Button_LockUnlock.Caption = "Lock"
        Button_LockUnlock.BackColor = &H80FF80
        Call Module1.Simulation_Unlock
        Sheets("Data").Range("B16").Value = "Unlocked"
        
    Else
        Button_LockUnlock.Caption = "Unlock"
        Button_LockUnlock.BackColor = &H8080FF
        Sheets("Data").Range("B16").Value = "Locked"
        Call Module1.Simulation_Lock
        
    End If
End Sub

Private Sub Button_Score_Click()
    Sheets("Scores").Range("B13").Value = Field_AditorName.Value
    Call Module1.ScoreAll
End Sub
Private Sub Button_ClearScores_Click()

Dim ClearScoreAnswer As Integer
ClearScoreAnswer = MsgBox("This will clear out all scores and reset the Scores page to the default" & vbLf & vbLf & _
    "Do you want to continue?", vbYesNo + vbQuestion, "Clear Scores?  Are You Really Sure?")

    If ClearScoreAnswer = vbYes Then
        Module1.ResetScoreResults
        Call ActiveSheet.Button_ResetHighlight_Click
    Else
        'do nothing
    End If
    
End Sub


