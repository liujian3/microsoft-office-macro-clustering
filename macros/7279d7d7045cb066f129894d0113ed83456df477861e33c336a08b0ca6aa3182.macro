Attribute VB_Name = "Module1"
Sub Simulation_Lock()
    'ActiveSheet.Protect , UserInterfaceOnly:=True

Dim ws As Worksheet

    Application.ScreenUpdating = False
    
    For Each ws In Worksheets
'        If ws.Name = "Completion" Then
'            GoTo SkipToHere
'        End If
'
        ws.Activate
        ws.Range("A1").Select
        ActiveWindow.DisplayHeadings = False
        ActiveWindow.DisplayGridlines = False
        
        If ws.Name = "Touchpoint" Then
            ws.Activate
            ActiveSheet.Button_NavAdmin.Visible = False
            ActiveSheet.Button_Administration.Visible = False
'            ActiveSheet.AdministrationTableOfContents.Visible = False
'            ActiveSheet.Button_ADMINGoToTOCSelection.Visible = False
'            move admin cover to the right
            ActiveSheet.Shapes.Range(Array("Shape_AdminCoverSheet")).Left = 0
            ws.Range("A1").Select
        End If

SkipToHere:
        ws.Protect password:="Pa$$word", UserInterFaceOnly:=True
        
    Next ws

    ActiveWorkbook.Sheets("Admin").Visible = False
    ActiveWorkbook.Sheets("Data").Visible = False
    ActiveWorkbook.Sheets("Scores").Visible = True
'    ActiveWorkbook.Sheets("To Do").Visible = False
'    ActiveWorkbook.Sheets("ScoringTesting").Visible = False
'    ActiveWorkbook.Sheets("Scratch").Visible = False
    
    Call Simulation_HideRibbon
    
    Sheets("Welcome").Activate
    
    Application.ScreenUpdating = True
    
    MsgBox "Touchpoint is LOCKED", vbInformation, "Touchpoint"
    
TheEnd:
   
End Sub


Sub Simulation_Unlock()

Dim ws As Worksheet

    Application.ScreenUpdating = False
      
    For Each ws In Worksheets
        'unprotect all but the scores page
        If Not ws.Name = "Scoreszzzz" Then
            ws.Unprotect password:="Pa$$word"
            ActiveWindow.DisplayHeadings = True
            ActiveWindow.DisplayGridlines = True
        End If
        
        If ws.Name = "Touchpoint" Then
            ws.Activate
            ActiveSheet.Button_NavAdmin.Visible = True
            ActiveSheet.Button_Administration.Visible = True
''            ActiveSheet.AdministrationTableOfContents.Visible = True
'            ActiveSheet.Button_ADMINGoToTOCSelection.Visible = True
            'move admin cover to the right
            ActiveSheet.Shapes.Range(Array("Shape_AdminCoverSheet")).Left = 9000
        End If
    Next ws
        
    ActiveWorkbook.Sheets("Admin").Visible = True
    ActiveWorkbook.Sheets("Data").Visible = True
    ActiveWorkbook.Sheets("Scores").Visible = True
'    ActiveWorkbook.Sheets("To Do").Visible = True
'    ActiveWorkbook.Sheets("ScoringTesting").Visible = True
'    ActiveWorkbook.Sheets("Scratch").Visible = True
    
    Call Simulation_ShowRibbon
   
    Application.ScreenUpdating = True

    Sheets("Welcome").Activate
   

    MsgBox "Touchpoint is UNLOCKED", vbInformation, "Touchpoint"

End Sub
Sub Simulation_HideRibbon()
    'collapsed ribbon default height is 73
    If CommandBars("Ribbon").Height > 100 Then
        CommandBars.ExecuteMso "MinimizeRibbon" 'code to toggle
    End If
    
    'hide other stuff too
    Application.DisplayFormulaBar = False
    
End Sub
Sub Simulation_ShowRibbon()
    'expanded ribbon default height is 159
    If CommandBars("Ribbon").Height < 100 Then
        CommandBars.ExecuteMso "MinimizeRibbon" 'code to toggle
    End If
    
    'hide other stuff too
    Application.DisplayFormulaBar = True
End Sub
