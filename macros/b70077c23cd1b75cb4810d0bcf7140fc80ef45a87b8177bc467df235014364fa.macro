Private Sub Worksheet_Change(ByVal Target As Range)

Dim Oldvalue As String
Dim Newvalue As String



If Not Intersect(Target, Range("Program_Type")) Is Nothing Then
    

    Application.EnableEvents = False
    Newvalue = Target.Value
    Application.Undo
    Oldvalue = Target.Value
    Target.Value = Newvalue

    If Oldvalue = "" Then
    Else
        If Newvalue = "" Then
            'do nothing
        Else
            Used = InStr(1, Oldvalue, Newvalue)
            If Used > 0 Then
                If Right(Oldvalue, Len(Newvalue)) = Newvalue Then
                    If InStr(1, Oldvalue, ",") Then           'added
                        Target.Value = Left(Oldvalue, Len(Oldvalue) - Len(Newvalue) - 2)
                    Else
                        Target.Value = ""               'added
                    End If
                Else
                    Target.Value = Replace(Oldvalue, Newvalue & ", ", "")
                End If
            Else
                Target.Value = Oldvalue & ", " & Newvalue
            End If
exitHandler:
        'Application.EnableEvents = True
        End If
    End If
End If
Application.EnableEvents = True
End Sub
