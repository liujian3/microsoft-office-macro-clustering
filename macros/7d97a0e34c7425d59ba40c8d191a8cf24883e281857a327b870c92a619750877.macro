Sub h|fffd|heneinheiten_eintragen()
    Application.ScreenUpdating = False
    Set blatt_ma|fffd|e = ActiveWorkbook.Sheets("Ma|fffd|e")
    
    h|fffd|heneinheit_aktuell = ActiveSheet.DrawingObjects(Application.Caller).Text
    
    Set finden = blatt_ma|fffd|e.Rows(Range("H|fffd|heneinheiten_rubrik").Row).Find(h|fffd|heneinheit_aktuell, LookAt:=xlWhole)
    If finden Is Nothing Then
    MsgBox "Racktyp '" & h|fffd|heneinheit_aktuell & "' ist nicht definiert!", vbExclamation
    Exit Sub
    End If
    he_spalte = blatt_ma|fffd|e.Rows(Range("H|fffd|heneinheiten_rubrik").Row).Find(h|fffd|heneinheit_aktuell, LookAt:=xlWhole).Column
    
    For i = Range("H|fffd|heneinheiten_rubrik").Row + 1 To Range("H|fffd|heneinheiten_rubrik").Row + Application.CountIf(blatt_ma|fffd|e.Columns(Range("H|fffd|heneinheiten_ma|fffd|e").Column), "*") - 1
    On Error GoTo fehler
    he_rubrik = blatt_ma|fffd|e.Cells(i, Range("H|fffd|heneinheiten_ma|fffd|e").Column)
    he_ma|fffd| = blatt_ma|fffd|e.Cells(i, he_spalte)
    Range(he_rubrik) = he_ma|fffd|
    Next i
    
    On Error GoTo 0
    ActiveSheet.Unprotect
    Range("Racktyp") = h|fffd|heneinheit_aktuell
    ActiveSheet.Protect
    Exit Sub
fehler:
    MsgBox "'" & he_rubrik & "' nicht gefunden!", vbExclamation

End Sub


