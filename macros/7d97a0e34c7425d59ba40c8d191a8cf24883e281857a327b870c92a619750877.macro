Private Sub Worksheet_Change(ByVal Target As Range)
    Exit Sub
    Application.ScreenUpdating = False
    Set blatt_ma|fffd|e = ActiveWorkbook.Sheets("Ma|fffd|e")
    h|fffd|heneinheit_zeile = 1
    h|fffd|heneinheit_spalte = 1
    If Target.Address <> Range("weitere_racks1").Address And Target.Address <> Range("weitere_racks2").Address Then Exit Sub
    'MsgBox "test"
    
    If IsEmpty(Target) Then
    Range(Cells(Target.Row, Target.Column + 1), Cells(Target.Row, Target.Column + 4)).ClearContents
    Exit Sub
    End If
    
    h|fffd|heneinheit_aktuell = Target.Value
    
    
    Set finden = blatt_ma|fffd|e.Rows(h|fffd|heneinheit_zeile).Find(h|fffd|heneinheit_aktuell, LookAt:=xlWhole)
    If finden Is Nothing Then
    Range(Cells(Target.Row, Target.Column + 1), Cells(Target.Row, Target.Column + 4)).ClearContents
    Range(Target.Address).Select
    Exit Sub
    End If
    he_spalte = blatt_ma|fffd|e.Rows(h|fffd|heneinheit_zeile).Find(h|fffd|heneinheit_aktuell, LookAt:=xlWhole).Column
    
    For i = h|fffd|heneinheit_zeile + 1 To h|fffd|heneinheit_zeile + Application.CountIf(blatt_ma|fffd|e.Columns(h|fffd|heneinheit_spalte), "*") - 1
    On Error GoTo fehler
    he_rubrik = blatt_ma|fffd|e.Cells(i, h|fffd|heneinheit_spalte)
    he_ma|fffd| = blatt_ma|fffd|e.Cells(i, he_spalte)
    Cells(Target).Offset(0, 1).Select
    Cells(Target.Row, Range(he_rubrik).Column) = he_ma|fffd|
    Next i
    
    Range(Target.Address).Select
    Exit Sub
fehler:
    Range(Cells(Target.Row, Target.Column + 1), Cells(Target.Row, Target.Column + 4)).ClearContents
    MsgBox "'" & he_rubrik & "' nicht gefunden!", vbExclamation
    Range(Target.Address).Select
    
End Sub
