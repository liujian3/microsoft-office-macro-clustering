Sub |fffd|bersetzung_durchf|fffd|hren()
Attribute |fffd|bersetzung_durchf|fffd|hren.VB_ProcData.VB_Invoke_Func = " \n14"
    Application.ScreenUpdating = False
    
    Set ole_objekte = ActiveSheet.OLEObjects
    blatt_formular = ActiveSheet.Name
    blatt_language = "language"
    spalte_feldnamen = 2
    weitere_Erkl|fffd|rungen = "weitere_Erkl|fffd|rungen"
    weitere_Erkl|fffd|rungen1 = "weitere_Erkl|fffd|rungen1"
    
    ActiveSheet.Unprotect
    aktiv_objekt = Application.Caller
    If Right(aktiv_objekt, 2) = "69" Then
        spalte_sprache = 3
        text1 = "Frei Haus"
        text2 = "Frei Aufstellraum"
    ElseIf Right(aktiv_objekt, 2) = "71" Then
        spalte_sprache = 4
        text1 = "Delivery onto loading dock / first easily accessible door of the building"
        text2 = "Delivery into server room "
    ElseIf Right(aktiv_objekt, 2) = "70" Then
        spalte_sprache = 5
        text1 = "Livraison sur la quai de d|fffd|chargement"
        text2 = "Livraison dans la salle informatique"
    ElseIf Right(aktiv_objekt, 3) = "117" Then
        spalte_sprache = 6
        text1 = "Entrega en el muelle de carga"
        text2 = "Entrega en alguna sala/habitaci|fffd|n interior"
    Else
        spalte_sprache = 7
        text1 = "Consegna a magazzino (dotato di autonomi mezzi di scarico)"
        text2 = "Consegna al piano (CED ecc.)"
    End If
    
    Set blatt_input = ActiveWorkbook.Sheets(blatt_language)
    anzahl_felder = Application.CountIf(blatt_input.Columns(spalte_feldnamen), "*")
    
    For i = 2 To anzahl_felder
        If blatt_input.Cells(i, spalte_feldnamen) = weitere_Erkl|fffd|rungen Or blatt_input.Cells(i, spalte_feldnamen) = weitere_Erkl|fffd|rungen1 Then
        Else
        Range("" & blatt_input.Cells(i, spalte_feldnamen) & "") = blatt_input.Cells(i, spalte_sprache)
        End If
    Next i
    
    ole_objekte("OptionButton1").Object.Caption = text1
    ole_objekte("OptionButton2").Object.Caption = text2
    
    If ole_objekte("OptionButton1").Object.Value = True Then
    Range("Anlieferbedingungen_wahl") = text1
    ElseIf ole_objekte("OptionButton2").Object.Value = True Then
    Range("Anlieferbedingungen_wahl") = text2
    Else
    'Range("Anlieferbedingungen_wahl") = ""
    End If
    
    Range("sprache_wahl") = spalte_sprache
    Sheets(blatt_language).Select
    Set finden = ActiveSheet.Columns(spalte_feldnamen).Find(weitere_Erkl|fffd|rungen, LookAt:=xlWhole)
    If finden Is Nothing Then
    MsgBox "|fffd|bersetzungstext nicht gefunden!", vbExclamation
    Exit Sub
    End If
    text_erkl|fffd|rung = ActiveSheet.Columns(spalte_feldnamen).Find(weitere_Erkl|fffd|rungen, LookAt:=xlWhole).Offset(0, spalte_sprache - spalte_feldnamen)
    Sheets(blatt_formular).Select
    If Range("zusatz1") <> "" Then
    Range("zusatz1") = text_erkl|fffd|rung
    End If
    If Range("zusatz2") <> "" Then
    Range("zusatz2") = text_erkl|fffd|rung
    End If
    If Range("zusatz3") <> "" Then
    Range("zusatz3") = text_erkl|fffd|rung
    End If
    If Range("zusatz4") <> "" Then
    Range("zusatz4") = text_erkl|fffd|rung
    End If
    If Range("zusatz5") <> "" Then
    Range("zusatz5") = text_erkl|fffd|rung
    End If
    If Range("zusatz6") <> "" Then
    Range("zusatz6") = text_erkl|fffd|rung
    End If
    
    ActiveSheet.Protect
    ActiveSheet.EnableSelection = xlUnlockedCells
End Sub
