Private Sub Worksheet_Change_test(ByVal Target As Range)

Exit Sub
' Incoterm auf G|fffd|ltigkeit pr|fffd|fen
    If Target.Address = "$C$33" Or Target.Address = "$C$34" Then
                If Range("C9").Value = Range("J10").Value Or Range("C9").Value = "" Then
                    Range("C9").Value = Range("J11").Value
                    MsgBox Range("J12").Value
                Else
                End If
     End If

' Rackangaben bei manueller Eingabe pr|fffd|fen
    If Target.Address = "$C$18" Then
        If Range("C18").Value = "" Then
            Range("C16").Value = ""
            Range("D16").Value = ""
            Range("E16").Value = ""
            Range("E17").Value = ""
        Else
            var1 = Range("C18").Value
            Range("C16").Value = "ca." & var1 * 45 + 260 & "mm"
            Range("D16").Value = "775mm"
            Range("E16").Value = "1180mm"
            Range("E17").Value = var1 * 2.5 + 55 & "kg empty"
        End If
     End If

End Sub
