                Range("weitere_racks2").Offset(0, -1) = 1
                Range("weitere_racks2").Offset(0, 1) = h|fffd|he
                Range("weitere_racks2").Offset(0, 2) = breite
                Range("weitere_racks2").Offset(0, 3) = tiefe
            Else
                MsgBox "Keine weitere Auswahl m|fffd|glich!", vbExclamation
            End If
        End If
    Next i
    Rack.Hide
    abbruch = 0
