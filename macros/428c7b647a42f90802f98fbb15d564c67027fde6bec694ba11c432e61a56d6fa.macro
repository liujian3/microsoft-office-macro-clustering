Attribute VB_Name = "SubsidiaryLedger"
Attribute VB_Base = "0{00020820-0000-0000-C000-000000000046}"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Attribute VB_TemplateDerived = False
Attribute VB_Customizable = True

Private Sub Worksheet_Activate()
If ThisWorkbook.bIsSaving Then Exit Sub
    Dim fsdate
    fsdate = InputSheet.Range("fsdate").Value
    Application.ScreenUpdating = False
    If fsdate <> "" Then
    Range("filterrange").AdvancedFilter Action:=xlFilterInPlace, CriteriaRange:= _
        Range("Criteria"), Unique:=False
    Range("bstart").Select
    Application.StatusBar = "Ready"
    Application.ScreenUpdating = True
    End If
End Sub

Private Sub Worksheet_Deactivate()
If ThisWorkbook.bIsSaving Then Exit Sub
    Application.StatusBar = False
    If FilterMode = True Then
    SubsidiaryLedger.ShowAllData
    End If
End Sub

