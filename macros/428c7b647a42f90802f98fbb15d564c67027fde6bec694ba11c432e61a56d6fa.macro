Dim year_end As String
Dim Linkname As String

Public Sub Set_Link()
  Dim nErr As Integer
  Dim sErrDesc As String
  Dim sSourcePathFileName As String
  Dim vntSPFN As Variant
  
  '
  ' Don Judd's substitute method that silently set's the link (no activation). This is done
  '  to ensure that security works.
  '
  
  On Error GoTo SLErr
  
  vntSPFN = Application.GetOpenFilename("Excel Files (*.xls*), *.xls*", 1, "Select the promises to give workbook to which this row should be linked.")
  If vntSPFN = False Then
    MsgBox "No workbook is selected to link to.", vbOKOnly + vbInformation, "PPC's e-Workpapers"
    Exit Sub
  End If
  sSourcePathFileName = vntSPFN
  
  If AddInMisc.vntSilentGetXLValue(sSourcePathFileName, "Input", "$b$11", False) <> "Face amount of promise" Then
    Application.ScreenUpdating = True
    MsgBox AddInMisc.sGetBaseFileName(sSourcePathFileName, False) & " is not a valid promise to give file. Make sure the selected file is a Promise to Give workbook that contains worksheets titled 'Input,' and 'Subsidiary Ledger.'", vbOKOnly + vbCritical, "PPC's e-Workpapers"
    Exit Sub
  End If

  Application.ScreenUpdating = False
  Application.EnableEvents = False
  If ActiveSheet.FilterMode = True Then
    ActiveSheet.ShowAllData
  End If
  
  '
  ' BIG NOTE! The third option to AddInMisc.vntSilentGetXLValue (sSourceCellRef) CANNOT be
  '  a defined name or this will not work. Any questions, see Don Judd 9/11/2003.
  '
  
'  SubsidiaryLedger.Range(Linkname).Value = sSourcePathFileName
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Input", "c9", "Subsidiary Ledger", Donor
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "c13", "Subsidiary Ledger", Terms
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "d13", "Subsidiary Ledger", Face
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "e13", "Subsidiary Ledger", AdjtoFace
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "f13", "Subsidiary Ledger", NetFace
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "h13", "Subsidiary Ledger", BegAr
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "i13", "Subsidiary Ledger", ArAdd
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "j13", "Subsidiary Ledger", ArRed
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "m13", "Subsidiary Ledger", BegDisc
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "n13", "Subsidiary Ledger", NewDisc
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "o13", "Subsidiary Ledger", CyAmort
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "r13", "Subsidiary Ledger", BegAllow
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "s13", "Subsidiary Ledger", BdExp
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "t13", "Subsidiary Ledger", Recovery
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "u13", "Subsidiary Ledger", WriteOffs
'  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Input", "g5", "Subsidiary Ledger", year_end
  
' Added new cell locations for the linked data to accommodate changes to the format of the Promises to Give template.
  
  SubsidiaryLedger.Range(Linkname).Value = sSourcePathFileName
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Input", "d9", "Subsidiary Ledger", Donor
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "d11", "Subsidiary Ledger", Terms
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "b20", "Subsidiary Ledger", Face
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "c20", "Subsidiary Ledger", AdjtoFace
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "c21", "Subsidiary Ledger", NetFace
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "b27", "Subsidiary Ledger", BegAr
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "c27", "Subsidiary Ledger", ArAdd
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "d27", "Subsidiary Ledger", ArRed
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "b34", "Subsidiary Ledger", BegDisc
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "c34", "Subsidiary Ledger", NewDisc
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "d34", "Subsidiary Ledger", CyAmort
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "b41", "Subsidiary Ledger", BegAllow
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "c41", "Subsidiary Ledger", BdExp
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "d41", "Subsidiary Ledger", Recovery
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Subsidiary Ledger", "e41", "Subsidiary Ledger", WriteOffs
  AddInMisc.SilentLinkToExternalWorkbook sSourcePathFileName, "Input", "h5", "Subsidiary Ledger", year_end
  
  Application.EnableEvents = True
  Application.ScreenUpdating = True
  If SubsidiaryLedger.Range(year_end).Value <> InputSheet.Range("fsdate").Value Then

    '
    ' Warn user if balance sheet dates don't agree.
    '
  
    MsgBox "The statement of financial position date used to determine this promise to give's balance and payment information differs from the statement of financial position date entered above.", vbOKOnly + vbInformation, "PPC's e-Workpapers Invalid Date"
  End If
  
  On Error GoTo 0                               ' cancel error tracking
  Exit Sub

  '
  ' Error handler.
  '
  
SLErr:
  nErr = Err.Number                             ' save error and description because any code to follow will change these values
  sErrDesc = Err.Description
  
  On Error GoTo 0                               ' cancel error tracking
  InputSheet.Select
  Application.EnableEvents = True
  Application.ScreenUpdating = True
  
  Select Case nErr                              ' Evaluate error number
  Case 1004
     MsgBox "A link could not be added because " & AddInMisc.sGetBaseFileName(sSourcePathFileName, False) & " is already open or could not be read by Excel " & Application.Version & ".", vbOKOnly + vbInformation, "PPC's e-Workpapers"
  Case Else
     MsgBox "A link to " & AddInMisc.sGetBaseFileName(sSourcePathFileName, False) & " could not be added. The error is [" & nErr & "] " & sErrDesc & ".", vbOKOnly + vbCritical, "PPC's e-Workpapers"
  End Select
End Sub

