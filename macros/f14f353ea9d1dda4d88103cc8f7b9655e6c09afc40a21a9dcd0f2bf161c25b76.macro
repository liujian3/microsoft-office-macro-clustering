Public Function DateConverter(dateU)
If InStr(dateU, "/") > 0 Then

DateConverter = dateU
Else
DateConverter = Right(dateU, 2) & "/" & Mid(dateU, 5, 2) & "/" & Left(dateU, 4)

 End If

End Function


Public Function TimeConverter(TimeU)
  TimeConverter = Left(TimeU, 2) & ":" & Mid(TimeU, 3, 2) & ":" & Mid(TimeU, 5, 2)
  
End Function






