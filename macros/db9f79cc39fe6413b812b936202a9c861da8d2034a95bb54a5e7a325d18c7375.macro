Sub NaMinimum(ByRef wkst As Worksheet)
  Dim sp As Excel.Spinner
  Dim ii As Integer
  Dim jj As Integer
  Dim rg As Range
  
'  On Error GoTo ErrorHandler
'  wkst.Range("$A$1").Select
  Application.ScreenUpdating = False
  Call OtevriList(wkst)
  For Each sp In wkst.Spinners
    Set rg = wkst.Range(sp.LinkedCell)
    jj = rg.Row
    For ii = rg.Offset(0, 3) To rg.Offset(0, 2) + 1 Step -1
      If wkst.Cells(jj + ii - 1, 1).EntireRow.Hidden = False Then
        wkst.Cells(jj + ii - 1, 1).EntireRow.Hidden = True
      End If
    Next
    rg.Value = rg.Offset(0, 2).Value
    rg.Offset(0, 1).Value = rg.Offset(0, 2).Value
  Next
  Call ZavriList(wkst)
  Application.ScreenUpdating = True
'ErrorHandler:
'  Call CommonErrorHandler(Err.Number, Err.Description)
End Sub

