Function Vypis()
   Dim WB As Workbook
   Dim VBC As Variant
   Dim modCode, modName As String
   Dim fileName As String
   Set WB = ThisWorkbook
   fileName = "C:\A\VypisMaker.txt"
   Open fileName For Output As #1
   For Each VBC In WB.VBProject.VBComponents
     If VBC.CodeModule.CountOfLines > 0 Then
         modName = VBC.CodeModule.Name
         modCode = VBC.CodeModule.Lines(1, VBC.CodeModule.CountOfLines)
         Write #1, modName
         Write #1, modCode
     End If
   Next VBC
   Close #1
End Function

