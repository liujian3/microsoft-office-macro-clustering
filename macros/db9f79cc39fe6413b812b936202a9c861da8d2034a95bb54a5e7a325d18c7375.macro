Private Sub Worksheet_Change(ByVal Target As Range)
  Dim ii As Long

  Application.ScreenUpdating = False
  Application.EnableEvents = False
  With ThisWorkbook.Worksheets(osmyList)
   For ii = 26 To 12 Step -1
    If Not Intersect(Target, .Cells(ii, 5)) Is Nothing Then
      If Len(.Cells(ii, 5).Value) > 0 Then
        posledniNeprazdnyRadekESSO = ii - 18
        Exit For
      End If
    End If
   Next ii
  End With
  If posledniNeprazdnyRadekESSO > 0 Then
     RozsireniMatice
  End If
  Application.EnableEvents = True
  Application.ScreenUpdating = True
End Sub

Private Sub RozsireniMatice()
  Dim ii, jj As Long

  Application.ScreenUpdating = False
  posledniNeprazdnyRadekESSO = NajdiPosledniNeprazdnyRadekESSO
  jj = pocetPridanychRadekESSO - KolikJePridanychSloupcuVMatici
  If jj > 0 Then
    Call OtevriList(ThisWorkbook.Worksheets(patyList))
    For ii = 1 To jj
    ' zobraz skryte sloupce
      With ThisWorkbook.Worksheets(patyList)
        .Cells(12, 23 + (ii - 1) * 2).EntireColumn.Hidden = False   ' 23 ~ W
        .Cells(12, 23 + (ii - 1) * 2 + 1).EntireColumn.Hidden = False
'        .Cells(12, 23 + (ii - 1) * 2).ColumnWidth = 10
      End With
    Next ii
    ' zobraz skryte radky
    For ii = 1 To jj
      With ThisWorkbook.Worksheets(patyList)
        .Cells(18 + ii, 1).EntireRow.Hidden = False   ' 18 je posledni zobrazeny radek v odst. 23
        .Cells(39 + ii, 1).EntireRow.Hidden = False   ' 18 je posledni zobrazeny radek v odst. 23
        .Cells(60 + ii, 1).EntireRow.Hidden = False   ' 18 je posledni zobrazeny radek v odst. 23
        .Cells(82 + ii, 1).EntireRow.Hidden = False   ' 18 je posledni zobrazeny radek v odst. 23
        .Cells(103 + ii, 1).EntireRow.Hidden = False   ' 18 je posledni zobrazeny radek v odst. 23
        .Cells(124 + ii, 1).EntireRow.Hidden = False   ' 18 je posledni zobrazeny radek v odst. 23
        .Cells(146 + ii, 1).EntireRow.Hidden = False   ' 18 je posledni zobrazeny radek v odst. 23
        .Cells(167 + ii, 1).EntireRow.Hidden = False   ' 18 je posledni zobrazeny radek v odst. 23
        .Cells(188 + ii, 1).EntireRow.Hidden = False   ' 18 je posledni zobrazeny radek v odst. 23
      End With
    Next ii
    Call ZavriList(ThisWorkbook.Worksheets(patyList))
  End If
  Application.ScreenUpdating = True
End Sub


