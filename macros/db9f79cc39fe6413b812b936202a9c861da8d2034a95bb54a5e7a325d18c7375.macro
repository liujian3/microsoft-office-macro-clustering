Public Sub SpinButton_Obsluha(es As Excel.Spinner, min As Integer, max As Integer)
 Dim rg As Range
 Dim ah As Integer        ' aktu|fffd|ln|fffd| hodnota
 Dim mh As Integer        ' minul|fffd| hodnota
 Dim dif As Integer
 Dim owkst As Worksheet

' On Error GoTo ErrorHandler
 Application.EnableEvents = False
 Application.ScreenUpdating = False
 Set owkst = ThisWorkbook.Worksheets(es.Application.ActiveSheet.Name)
' owkst.Activate
 Call OtevriList(owkst)
 Set rg = owkst.Range(es.LinkedCell)
 ah = rg.Value
 mh = rg.Offset(0, 1).Value
 dif = ah - mh   ' -1,0 = dolu, +1 = nahoru
 If (dif < 0 Or (dif = 0 And ah = min)) Then
   rg.Offset(ah, -20).Select
   If (ah = min And dif = 0) Then
     rg.Value = rg.Value + 1
     ah = rg.Value
   Else
    If (ah >= min And ah < max) Then
      rg.Value = rg.Value + 2
      ah = rg.Value
    End If
   End If
   If ah <= max Then
     rg.Offset(ah - 1, 0).Select
     If Application.Selection.EntireRow.Hidden Then
       Application.Selection.EntireRow.Hidden = False
       rozdil = ColumnNumber("AG") - ColumnNumber(ProhlizeneSloupce(1))
       ActiveCell.Offset(0, -rozdil).Select
     End If
   Else
     rg.Value = max
     ah = rg.Value
   End If
 Else
   If dif = 1 And mh = min Then
     rg.Value = rg.Value - 1
     ah = rg.Value
   Else
    If ((ah < max) Or (ah = max) And (dif = 1)) Then
     rg.Value = rg.Value - 2
    Else
     rg.Value = rg.Value - 1
    End If
    ah = rg.Value
    rg.Offset(ah, 0).Select
    If Not Application.Selection.EntireRow.Hidden Then
      If Not SloupceVyplneny(rg) Then   ' !!! Len(link.Value) > 0
        Application.Selection.EntireRow.Hidden = True
        rozdil = ColumnNumber("AG") - ColumnNumber(ProhlizeneSloupce(1))
        ActiveCell.Offset(-1, -rozdil).Select
        ' kdyz je to paty list, zuzit matici
        If owkst.Name = osmyList Then       ' nahrazen activesheet.name
           On Error Resume Next
           ZuzeniMatice
        End If
      Else
        rg.Value = mh
        ah = mh
      End If
    End If
   End If
 End If
 rg.Offset(0, 1).Value = ah
 Call ZavriList(ThisWorkbook.Worksheets(es.Application.ActiveSheet.Name))
 Application.EnableEvents = True
 Application.ScreenUpdating = True
' Exit Sub
'ErrorHandler:
' Call CommonErrorHandler(Err.Number, Err.Description)
End Sub

Function SloupceVyplneny(rg As Range) As Boolean
  Dim i As Integer
  
  Application.ScreenUpdating = False
  SloupceVyplneny = False
  If Len(ProhlizeneSloupce(1)) < 1 Then
    Return
  End If
  For i = LBound(ProhlizeneSloupce) To UBound(ProhlizeneSloupce)
    If Len(ProhlizeneSloupce(i)) >= 1 Then
      If Len(Cells(rg.Row + rg.Value, ColumnNumber(ProhlizeneSloupce(i)))) > 0 Then
        SloupceVyplneny = True
        Exit For
      End If
    End If
  Next i
  Application.ScreenUpdating = True
End Function

Public Function UcetniObdobiMaSpravnyFormat() As Boolean
  Dim re As New VBScript_RegExp_55.RegExp
  Dim hd As String
  Dim vysledek As Boolean
  vysledek = False
  re.Global = True
  re.Pattern = "\d{1,2}\.\d{1,2}\."
  hd = Trim(ThisWorkbook.Worksheets(prvniList).Range("U7").Value)
  On Error Resume Next
  If Len(hd) > 0 Then
    vysledek = re.Test(hd)
  Else
    vysledek = True
  End If
  Err.Clear
  On Error GoTo 0
  UcetniObdobiMaSpravnyFormat = vysledek
End Function

Public Function KolikJePridanychSloupcuVMatici() As Long
  Dim colIndex As Long
  Dim posledni As Long
  posledni = 21    ' 21 ~ U
  For colIndex = 8 To 15
    With ThisWorkbook.Worksheets(patyList).Cells(21 + (colIndex - 7) * 2, 1).EntireColumn   ' 21 ~ U
      If .Hidden = True Then
        posledni = 21 + (colIndex - 7) * 2
        Exit For
      End If
    End With
  Next colIndex
  KolikJePridanychSloupcuVMatici = posledni - 21
End Function

Public Function ColumnNumber(Col As String) As Integer
    Dim C1, C2 As String
    Dim ColRef1 As Integer
    Col = UCase(Col)
    If Len(Col) = 1 Then
        ColumnNumber = Asc(Col) - 64
    ElseIf Len(Col) = 2 Then
        C1 = Left$(Col, 1)
        ColRef1 = (Asc(C1) - 64) * 26
        C2 = Right$(Col, 1)
        ColumnNumber = ColRef1 + (Asc(C2) - 64)
    End If
    
    If (ColumnNumber > 256) Then    ' ponech|fffd|no z Excelu 2003
        MsgBox "Wrong Column number", vbExclamation
        ColumnNumber = -1
        Exit Function
    End If
End Function

Public Function PocetSloupcuMatice() As Long
  Dim ii As Integer
  Dim vysledek As Long
  For ii = ColumnNumber("W") To ColumnNumber("AK") Step 2
    With ThisWorkbook.Worksheets(patyList)
      If (.Cells(12, ii).EntireColumn.Hidden = True) Then
        vysledek = 7 + (ii - ColumnNumber("W")) / 2
        Exit For
      End If
    End With
  Next ii
  PocetSloupcuMatice = vysledek
End Function

Public Function NajdiPosledniNeprazdnyRadekESSO() As Integer
  Dim vysledek As Integer
  Dim ii As Integer
  For ii = 26 To 12 Step -1
     If Len(ThisWorkbook.Worksheets(osmyList).Cells(ii, 5).Value) > 0 Then
        vysledek = ii - 18 + 7
        Exit For
     End If
  Next ii
  If vysledek > SM Then
    pocetPridanychRadekESSO = vysledek - SM
  Else
    pocetPridanychRadekESSO = 0
  End If
  NajdiPosledniNeprazdnyRadekESSO = vysledek
End Function

Public Sub ZuzeniMatice()
  Dim ii As Integer
  Dim soucet As Integer
    
'  On Error GoTo ErrorHandler
  Application.ScreenUpdating = False
  posledniNeprazdnyRadekESSO = NajdiPosledniNeprazdnyRadekESSO
  soucet = ColumnNumber(sloupecOd) + pocetPridanychRadekESSO * 2
  Call OtevriList(ThisWorkbook.Worksheets(patyList))
  For ii = ColumnNumber(sloupecDo) To soucet Step -2
      With ThisWorkbook.Worksheets(patyList)
        If .Cells(12, ii).EntireColumn.Hidden = False Then
          .Cells(12, ii + 1).EntireColumn.Hidden = True
          .Cells(12, ii).EntireColumn.Hidden = True
'          .Cells(12, ii).ColumnWidth = 10
          .Cells(18 + ((ii - ColumnNumber(sloupecOd)) / 2) + 1, 1).EntireRow.Hidden = True
          .Cells(39 + ((ii - ColumnNumber(sloupecOd)) / 2) + 1, 1).EntireRow.Hidden = True
          .Cells(60 + ((ii - ColumnNumber(sloupecOd)) / 2) + 1, 1).EntireRow.Hidden = True
          .Cells(82 + ((ii - ColumnNumber(sloupecOd)) / 2) + 1, 1).EntireRow.Hidden = True
          .Cells(103 + ((ii - ColumnNumber(sloupecOd)) / 2) + 1, 1).EntireRow.Hidden = True
          .Cells(124 + ((ii - ColumnNumber(sloupecOd)) / 2) + 1, 1).EntireRow.Hidden = True
          .Cells(146 + ((ii - ColumnNumber(sloupecOd)) / 2) + 1, 1).EntireRow.Hidden = True
          .Cells(167 + ((ii - ColumnNumber(sloupecOd)) / 2) + 1, 1).EntireRow.Hidden = True
          .Cells(188 + ((ii - ColumnNumber(sloupecOd)) / 2) + 1, 1).EntireRow.Hidden = True
        End If
      End With
  Next ii
  Call ZavriList(ThisWorkbook.Worksheets(patyList))
  Application.ScreenUpdating = True
'  Exit Sub
