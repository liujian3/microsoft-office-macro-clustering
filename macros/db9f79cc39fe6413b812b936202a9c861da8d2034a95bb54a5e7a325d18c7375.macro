Private Sub CheckBox1_Click()                             ' volba "m|fffd|"
 Dim Cell As Range
  Call OtevriList(ThisWorkbook.Worksheets(desatyList))
  Application.ScreenUpdating = False
'  If ActiveSheet.Shapes("CheckBox1").ControlFormat.Value = 1 Then
  If ThisWorkbook.Worksheets(desatyList).Shapes("CheckBox1").ControlFormat.Value = 1 Then
    For Each Cell In Range("$A$19:$A$35")
      Cell.EntireRow.Hidden = False ''Not Cell.EntireRow.Hidden
    Next
    If Worksheets(prvniList).Range("$D$24") = "English" Then
       Application.Worksheets(desatyList).Range("$C$14") = Worksheets(prvniList).Range("$AF$37").Value
    Else
       Application.Worksheets(desatyList).Range("$C$14") = Worksheets(prvniList).Range("$AF$16").Value
    End If
    ThisWorkbook.Worksheets(desatyList).Shapes("CheckBox2").ControlFormat.Value = 0
  Else
    Application.Worksheets(desatyList).Range("$C$14") = ""
    For Each Cell In Range("$A$19:$A$35")
      Cell.EntireRow.Hidden = True ''Not Cell.EntireRow.Hidden
    Next
  End If
  Application.ScreenUpdating = True
  ThisWorkbook.Worksheets(desatyList).Range("$AF$13") = False
  Call ZavriList(ThisWorkbook.Worksheets(desatyList))
End Sub

Private Sub CheckBox2_Click()                               ' volba "nem|fffd|"
 Dim Cell As Range
  Call OtevriList(ThisWorkbook.Worksheets(desatyList))
  Application.ScreenUpdating = False
  If ThisWorkbook.Worksheets(desatyList).Shapes("CheckBox2").ControlFormat.Value = 1 Then
    For Each Cell In Range("$A$19:$A$35")
      Cell.EntireRow.Hidden = True ''Not Cell.EntireRow.Hidden
    Next
    Application.Worksheets(desatyList).Range("$C$14") = ""
    ThisWorkbook.Worksheets(desatyList).Shapes("CheckBox1").ControlFormat.Value = 0
  End If
  Application.ScreenUpdating = True
  ThisWorkbook.Worksheets(desatyList).Range("$AE$13") = False
  Call ZavriList(ThisWorkbook.Worksheets(desatyList))
End Sub

