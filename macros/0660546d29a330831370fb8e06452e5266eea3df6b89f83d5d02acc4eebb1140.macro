Private Sub Worksheet_Change(ByVal Target As Range)   ' IFRIC 5P  Rights to Interests
                                                      ' arising from decommissioning,
                                                      ' restoration and environmental
                                                      ' rehabilitation funds
Dim thecurrentcell

thecurrentcell = ActiveCell.AddressLocal

' Check if Additional Guidance was requested (Show/Hide button)

If thecurrentcell = "$E$5" Then
   Call ShowHide(thecurrentcell)
   GoTo EndofSub
End If

' Check if TQ IFRIC 5A was answered Yes/No/NA

If thecurrentcell = "$E$14" Then
   Call TQIFRIC5A(thecurrentcell)
   GoTo EndofSub
End If

' Check if sub TQ IFRIC5A-1 was answered Yes/No/NA

If thecurrentcell = "$E$17" And ActiveCell.Value = "Yes" Then
   Worksheets("IFRIC5P").Rows("19:19").Select
   selection.EntireRow.Hidden = False
   GoTo EndofSub
ElseIf thecurrentcell = "$E$17" Then
   Worksheets("IFRIC5P").Rows("18:19").Select
   selection.EntireRow.Hidden = True
   Worksheets("IFRIC5P").Range("E19").Value = ""
   GoTo EndofSub
End If

' Check if sub TQ IFRIC5A-2 was answered Yes/No/NA

If thecurrentcell = "$E$20" And ActiveCell.Value = "Yes" Then
   Worksheets("IFRIC5P").Rows("22:22").Select
   selection.EntireRow.Hidden = False
   GoTo EndofSub
ElseIf thecurrentcell = "$E$20" Then
   Worksheets("IFRIC5P").Rows("21:23").Select
   selection.EntireRow.Hidden = True
   Worksheets("IFRIC5P").Range("E22").Value = ""
   GoTo EndofSub
End If

' Check if Detail question IFRIC5A-2 IFRIC 5.13 needs guidance

If thecurrentcell = "$E$22" And ActiveCell.Value = "Guidance" Then
        Worksheets("IFRIC5P").Rows("23:23").Select
        selection.EntireRow.Hidden = False
        GoTo EndofSub
'    ElseIf thecurrentcell = "$E$20" Then
'        GoTo EndofSub
'    End If
ElseIf thecurrentcell = "$E$22" And ActiveCell.Value = "No" Then
   Worksheets("IFRIC5P").Rows("23:23").Select
   selection.EntireRow.Hidden = True
   'Worksheets("IFRIC5P").Range("E31").Value = ""
   GoTo EndofSub
End If


EndofSub:

End Sub

Private Sub ShowHide(thecurrentcell)

' Check if Additional Guidance was requested (Show/Hide button)

If thecurrentcell = "$E$5" And Worksheets("IFRIC5P").Range("E5").Value = "Show" Then  ' Show was selected
   If Worksheets("IFRIC5P").Range("E22").Value = "Guidance" Then
      Worksheets("IFRIC5P").Rows("23:23").Select
      selection.EntireRow.Hidden = False
   End If
   Worksheets("IFRIC5P").Rows("6:12").Select
   selection.EntireRow.Hidden = False
ElseIf thecurrentcell = "$E$5" Then                            ' Hide was selected
   Worksheets("IFRIC5P").Rows("23:23").Select
   selection.EntireRow.Hidden = True
   Worksheets("IFRIC5P").Rows("6:12").Select
   selection.EntireRow.Hidden = True
 End If
 
