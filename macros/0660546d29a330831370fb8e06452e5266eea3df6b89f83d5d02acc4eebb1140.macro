Private Sub Worksheet_Change(ByVal Target As Range)
'confirm that the choice is on a selection that is actually a parent object


Dim A_answer As String
Dim parent As Integer

' Dim level, parent_tq As Integer

sheet_rows = 110          'set the number of rows to scan in the worksheet
level = 5                'Column number where the level is defined
parent = level - 1       'Column number where the parent label is defined
parent_tq = level + 5    'Column number where the drop down list is located

If ActiveCell.Column <> parent_tq Then
   GoTo Done
End If

If ActiveCell.Value = "Yes" Or ActiveCell.Value = "No" Or ActiveCell.Value = "N/A" Or ActiveCell.Value = "Guidance" Or ActiveCell.Value = "Show" _
Or ActiveCell.Value = "Hide" Or ActiveCell.Value = "" Then
    If Cells(ActiveCell.Row, level).Value = "" Then
        'Exit Sub   'commented out - delete
        show_hide_remaining_content
    Else
        show_hide_activations 'call the show/hide activations for items that activate others
    End If
End If

Done:

End Sub

Sub show_hide_activations()


    Dim guidance_rows, i, j, k, parent, loopcheck As Integer
    
 '   Dim parent, parent_tq, level As Integer

    Dim selection, cvalue, search_result, search_string, parent_str As String
    
    'set the columns the following variables search - allows expansion
    
    parent = level - 1
    
    selection = ActiveCell.Value
    cvalue = Cells(ActiveCell.Row, parent).Value
    
 '   If Cells(ActiveCell.Row, level) = 1 Then j = 1
 '   If Cells(ActiveCell.Row, level) = 2 Then j = 2
 '   If Cells(ActiveCell.Row, level) = 3 Then j = 3
 '   If Cells(ActiveCell.Row, level) = 4 Then j = 4
    
    j = Cells(ActiveCell.Row, level).Value
    
Application.ScreenUpdating = False


If selection = "No" Then
    For i = 1 To sheet_rows
    
        'hide the "Yes" answers and guidance
        If InStr(Cells(i, j).Value, cvalue & "-Y") > 0 Or InStr(Cells(i, j).Value, cvalue & "-G") > 0 Then
            
            Application.EnableEvents = False 'switch off events trapping
            
            
            'if instr contains or search for parent item after comma, scroll though text using two variables. if found yes, then good
            'where a row is activated by multiple questions, the code searches for the parent and detects if yes/no
            'and if any parent is yes, the row will not hide. Uses search_result string initially set to false
            search_result = False
            'populate search string with the string being searched for the parent
            search_string = Cells(i, j).Value
            
            'search the string for parents
            If Left(search_string, 2) = "OR" Then
                'trim the "OR," text
                search_string = Right(search_string, Len(search_string) - 3)
                loopcheck = 1
                Do
                    'strips out the "-Y" to find the parent question
                    parent_str = Left(search_string, InStr(search_string, "-Y") - 1)
                    
                    'searches for all of the parent questions and if it finds it, checks to see if it is a Yes and makes
                    'search result = true and then does not hide the row
                    For k = 1 To sheet_rows
                        If Cells(k, parent).Value = parent_str Then
                            If Cells(k, parent_tq).Value = "Yes" Then
                                search_result = True
                            End If
                        
                        End If
                        
                    Next k
                    
                    'trim off the previous search parent
                    If InStr(search_string, ",") > 0 Then
                        search_string = Right(search_string, Len(search_string) - InStr(search_string, ","))
                    Else
                        loopcheck = loopcheck + 1 'allows the loop to loop twice
                    End If
                
                Loop Until loopcheck = 2
                
            
            End If
            
            
            'if the parents are all "no", then the row is hidden
            If search_result = False Then
                'reset the questions to "" if the rows are hidden
                Cells(i, parent_tq).Value = ""
                'hide the rows
                Rows(i).Hidden = True
            Else ' If the row contains guidance and it was hidden, leave the row as hidden
                If InStr(Cells(i, j + 1).Value, "-G") > 0 And Rows(i).Hidden = True Then
                  Rows(i).Hidden = True
                Else
                  Rows(i).Hidden = False
                End If
            End If
            
            Application.EnableEvents = True
            
            
            
            
        End If
        
        'show the "No" Answers
        If (InStr(Cells(i, j).Value, cvalue & "-N") > 0 And Cells(i, j + 1).Value = "") Or _
           (InStr(Cells(i, j).Value, cvalue & "-N&G") > 0) Then
    
            Rows(i).Hidden = False
            
        End If
        
    Next i
    
End If

If selection = "Yes" Then
    For i = 1 To sheet_rows
        'show the "yes" answers
        If (InStr(Cells(i, j).Value, cvalue & "-Y") > 0 And Cells(i, j + 1).Value = "") Or _
           (InStr(Cells(i, j).Value, cvalue & "-Y&G") > 0) Then 'only unhides the rows with no content in the next column - e.g. activation.

            Rows(i).Hidden = False

        End If
        
        'hide the "no" answers
        If InStr(Cells(i, j).Value, cvalue & "-N") > 0 Then
    
            Application.EnableEvents = False 'switch off events trapping
            Cells(i, parent_tq).Value = ""
            Rows(i).Hidden = True
            Application.EnableEvents = True
            
        End If
        

        
        
    Next i
    
End If

If selection = "N/A" Or selection = "" Then
    For i = 1 To sheet_rows
    
        'hide the "yes" answers
        'If InStr(Cells(i, j).Value, cvalue & "-Y") > 0 And Cells(i, j + 1).Value = "" Then 'only unhides the rows with no content in the next column - e.g. activation.
        If InStr(Cells(i, j).Value, cvalue & "-Y") > 0 Then
            Application.EnableEvents = False 'switch off events trapping
            
            
            'if instr contains or search for parent item after comma, scroll though text using two variables. if found yes, then good
            'where a row is activated by multiple questions, the code searches for the parent and detects if yes/no
            'and if any parent is yes, the row will not hide. Uses search_result string initially set to false
            search_result = False
            'populate search string with the string being searched for the parent
            search_string = Cells(i, j).Value
            
            'search the string for parents
            If Left(search_string, 2) = "OR" Then
                'trim the "OR," text
                search_string = Right(search_string, Len(search_string) - 3)
                loopcheck = 1
                Do
                    'strips out the "-Y" to find the parent question
                    parent_str = Left(search_string, InStr(search_string, "-Y") - 1)
                    
                    'searches for all of the parent questions and if it finds it, checks to see if it is a Yes and makes
                    'search result = true and then does not hide the row
                    For k = 1 To sheet_rows
                        If Cells(k, parent).Value = parent_str Then
                            If Cells(k, parent_tq).Value = "Yes" Then
                                search_result = True
                            End If
                        
                        End If
                        
                    Next k
                    
                    'trim off the previous search parent
                    If InStr(search_string, ",") > 0 Then
                        search_string = Right(search_string, Len(search_string) - InStr(search_string, ","))
                    Else
                        loopcheck = loopcheck + 1 'allows the loop to loop twice
                    End If
                
                Loop Until loopcheck = 2
                
            
            End If
            
            
            'if the parents are all "no", then the row is hidden
            If search_result = False Then
                'reset the questions to "" if the rows are hidden
                Cells(i, parent_tq).Value = ""
                'hide the rows
                Rows(i).Hidden = True
            End If
            
            Application.EnableEvents = True

        End If
        
        'hide the "no" answers
       ' If InStr(Cells(i, j).Value, cvalue & "-N") > 0 And Cells(i, j + 1).Value = "" Then
        If InStr(Cells(i, j).Value, cvalue & "-N") > 0 Then
            Application.EnableEvents = False 'switch off events trapping
            Cells(i, parent_tq).Value = ""
            Rows(i).Hidden = True
            Application.EnableEvents = True
            
        End If
        'hide the "Guidance" answers
       ' If InStr(Cells(i, j).Value, cvalue & "-N") > 0 And Cells(i, j + 1).Value = "" Then
        If InStr(Cells(i, j).Value, cvalue & "-G") > 0 Then
            Application.EnableEvents = False 'switch off events trapping
            Cells(i, parent_tq).Value = ""
            Rows(i).Hidden = True
            Application.EnableEvents = True
            
        End If
        
    Next i
    
End If

If selection = "Guidance" Then
      
    'search for rows that follow the selected item that have a "-G" tag and display them.
    'stop after the last guidance item
    guidance_rows = ActiveCell.Row + 1
    Do While (guidance_rows <= sheet_rows)
    
      If InStr(Cells(guidance_rows, j).Value, cvalue & "-G") > 0 Then
        Rows(guidance_rows).Hidden = False
      End If
      guidance_rows = guidance_rows + 1
    Loop
    
End If

Application.ScreenUpdating = True

End Sub

