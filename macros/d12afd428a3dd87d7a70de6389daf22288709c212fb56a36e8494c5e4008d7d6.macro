Private Sub Worksheet_Change(ByVal Target As Range)
    If Target.Address = "$B$5" Then
        'Stop any possible runtime errors and halting code
        On Error Resume Next

        'Turn off ALL events so the action does not put the code into a loop.
        'Application.EnableEvents = False

        Range("B6") = Range("C6")
        Range("B24") = Range("C24")
        Range("B25") = Range("C25")
        Range("B26") = Range("C26")
        Range("B27") = Range("C27")
        Range("B28") = Range("C28")
        Range("B29") = Range("C29")
        Range("B30") = Range("C30")
        
        'Turn events back on
        'Application.EnableEvents = True

        'Allow run time errors again
        On Error GoTo 0
    
    ElseIf Target.Address = "$B$10" Then
        Range("B24") = Range("C24")
        Range("B25") = Range("C25")
        
    End If
End Sub

