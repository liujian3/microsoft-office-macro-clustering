Function AtivarPlanilha(ByVal p As String)
    Dim p1 As Worksheet
    Dim p2 As Worksheet
    
    Set p1 = ActiveSheet
    Set p2 = Worksheets(p)
    
    p2.Visible = xlSheetVisible
    p1.Visible = xlSheetVeryHidden
    p2.Activate
    
End Function


Sub UnhideAllSheets()
    Dim ws As Worksheet
 
    For Each ws In ActiveWorkbook.Worksheets
        ws.Visible = xlSheetVisible
    Next ws
 
End Sub

Sub HideAllSheets()
    Dim ws As Worksheet
 
    For Each ws In ActiveWorkbook.Worksheets
        'MsgBox ws.Name
        If ws.Name <> "Painel" Then
            ws.Visible = xlSheetVeryHidden
        End If
    Next ws
 
End Sub

