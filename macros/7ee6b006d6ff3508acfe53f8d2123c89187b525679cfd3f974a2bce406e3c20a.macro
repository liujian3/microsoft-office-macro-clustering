Sub Formel()
Dim c As Range
    For Each c In Selection
        bezug = c.FormulaLocal
        bezug = Replace(bezug, "=", "")
        FormelNeu = "=wenn(istleer(" & bezug & ");""--"";" & bezug & ")"
        c.FormulaLocal = FormelNeu
    Next c
End Sub
