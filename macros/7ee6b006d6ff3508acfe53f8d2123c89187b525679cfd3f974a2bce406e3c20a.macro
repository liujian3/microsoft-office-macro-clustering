Sub ImportCSV()

    CSVDat = Application.GetOpenFilename("*.csv, *.csv", 1, "Please select CSV-File")
    If CSVDat = False Then Exit Sub
    
    Workbooks.Open Filename:=CSVDat, local:=False
    CSVName = ActiveWorkbook.Name
    
    Listenende = Range("A100000").End(xlUp).Row
    
    Application.ScreenUpdating = False
    Application.EnableEvents = False
    On Error Resume Next
    For i = 2 To Listenende
        Tabelle = Cells(i, 1).Value
        zelle = Cells(i, 2).Value
        wert = Cells(i, 3).Value
    
        ThisWorkbook.Worksheets(Tabelle).Range(zelle).Value = wert
    Next i
    Workbooks(CSVName).Close (False)
    Application.EnableEvents = True
    Fragen.Rows("13:185").Hidden = False
    Call ProzessschritteEinAusblenden
End Sub
    
