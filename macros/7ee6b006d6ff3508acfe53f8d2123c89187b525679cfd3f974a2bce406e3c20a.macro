Private Sub Worksheet_Activate()
    Application.ScreenUpdating = False
    Application.Calculation = xlCalculationManual
    NB = Tabelle7.Range("K7").Value
    Rows("11:320").Hidden = False
    Rows("11:320").AutoFit
    For i = 11 To 320
        If Cells(i, 8).Value = 10 Or Cells(i, 8).Value = NB Then
            ausblenden = ausblenden & i & ":" & i & ","
            z|fffd|hler = z|fffd|hler + 1
        End If
        If z|fffd|hler = 30 Then
            ausblenden = Left(ausblenden, Len(ausblenden) - 1)
            Range(ausblenden).Select
            Selection.EntireRow.Hidden = True
