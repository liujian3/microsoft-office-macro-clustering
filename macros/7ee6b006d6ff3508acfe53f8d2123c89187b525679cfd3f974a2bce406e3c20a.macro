Sub AllesNB()
Application.Calculation = xlCalculationManual
On Error GoTo Fehler
For i = 13 To 185
    v = 0
    On Error Resume Next
    v = Cells(i, 4).SpecialCells(xlCellTypeSameValidation).Count
    On Error GoTo Fehler
    If v > 0 Then
        Cells(i, 4).Value = Range("_nb").Value
        Cells(i, 5).NumberFormat = "General"
        Cells(i, 5).FormulaR1C1 = _
        "=IF(ISBLANK(RC[-1]),R13C12,IF(RC[-1]=10,R14C12,R15C12))"
        Cells(i, 5).NumberFormat = "@"
        Cells(i, 6).ClearContents
    End If
Fehler:
Next i
Application.Calculation = xlCalculationAutomatic
End Sub

