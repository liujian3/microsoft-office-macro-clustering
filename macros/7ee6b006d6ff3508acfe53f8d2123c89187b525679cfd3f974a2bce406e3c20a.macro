Private Sub UserForm_Initialize()
Dim i As Integer
  
  For i = 1 To maxSheets
    Me.Controls("Checkbox" & i).Value = Range("Ausdruck").Rows(i).Value
  Next i
  
  Me.Controls("ChB_Vorab").Value = Range("Ausdruck").Rows(maxSheets + 1).Value
  Me.Controls("ChB_Original").Value = Range("Ausdruck").Rows(maxSheets + 2).Value
End Sub

Private Sub UserForm_Terminate()
Dim i As Integer
  
  For i = 1 To maxSheets
    Range("Ausdruck").Rows(i).Value = Me.Controls("Checkbox" & i).Value
  Next i
  
  Range("Ausdruck").Rows(maxSheets + 1).Value = Me.Controls("ChB_Vorab").Value
  Range("Ausdruck").Rows(maxSheets + 2).Value = Me.Controls("ChB_Original").Value
End Sub
