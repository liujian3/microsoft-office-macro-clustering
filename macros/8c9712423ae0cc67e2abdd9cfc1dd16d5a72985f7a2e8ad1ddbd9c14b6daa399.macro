Attribute VB_Name = "Module5"
Option Explicit
Function KeyDistribution(CodeKey As Variant) As String
Dim Digit10 As Variant
Dim i As Integer
Dim Key(9) As Variant
    Key(1) = Chr(50)
    Key(2) = Chr(87)
    Key(3) = Chr(55)
    Key(4) = Chr(82)
    Key(5) = Chr(53)
    Key(6) = Chr(65)
    Key(7) = Chr(56)
    Key(8) = Chr(76)
    Key(9) = Chr(52)
    Key(0) = Chr(85)
    
    Digit10 = Right("12345" & CodeKey, 5)
    Do
        If Len(Digit10) >= 10 Then Exit Do
        Digit10 = Digit10 * CodeKey
    Loop
    For i = 1 To 10
        KeyDistribution = KeyDistribution & Key(Mid(Digit10, i, 1))
    Next i
End Function


Attribute VB_Name = "Module6"
Function checkdigit(ByVal Code39 As String) As String
Dim i As Integer
Dim subtotal As Integer
Dim temp As String
temp = "ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"
For i = 1 To Len(Code39)
    subtotal = InStr(temp, Mid(Code39, i, 1)) - 1 + subtotal
Next i
checkdigit = Mid(temp, (subtotal Mod 43 + 1), 1)
End Function

