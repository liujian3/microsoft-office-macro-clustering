Sub Cetakcover()
Dim Awal, Akhir As Integer
Awal = Range("w2").Value
Akhir = Range("w5").Value

If Awal <= Akhir And Awal >= 1 Then
Application.ScreenUpdating = False
    For i = Awal To Akhir
        With Sheets("cover")
            .Range("d1").Value = i
            .PrintOut
        End With
    Next i
Else
    MsgBox "Cek lagi Nomor yang akan dicetak...!!!!", vbCritical, "Error Cetak"
End If
End Sub
Sub Gantilogo1()
    Dim vFilePic
    ChDir ActiveWorkbook.Path
    vFilePic = Application.GetOpenFilename _
    ("Pictures (*.gif; *.jpg; *.bmp; *.tif; *.png),*.gif; *.jpg; *.bmp; *.tif; *.png", , "Pilih Gambar")
