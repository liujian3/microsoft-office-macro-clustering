Public Function check_control_number_new(i As Integer, ival As String, kol As Integer, num As Integer, icheck_val)

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd|-|fffd||fffd|
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Dim symb_kol    As Integer
Dim sum_1       As Integer
Dim sum_2       As Integer
Dim sum_all     As Integer
Dim str         As String
Dim num_1       As Integer
Dim num_2       As Integer
Dim num_3       As Integer
Dim val_new     As String

If (icheck_val = "WN" Or icheck_val = "PL") And (kol >= 4 And kol <= 6) Then
  Exit Function
End If

'|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd|
num_3 = Mid(ival, kol, 1)

'|fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd|
val_new = Mid(ival, 1, (kol - 1))

'|fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|(|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd| |fffd| |fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd|)
For ii = 1 To (kol - 1) Step 2
  sum_1 = sum_1 + Mid(val_new, ii, 1)
Next ii

'|fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd|(|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd| |fffd| |fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd|)
For jj = 2 To (kol - 1) Step 2
  sum_2 = sum_2 + Mid(val_new, jj, 1)
Next jj

If (kol - 1) Mod 2 = 0 Then
 sum_2 = sum_2 * 3
Else
 sum_1 = sum_1 * 3
End If

sum_all = sum_1 + sum_2
str = sum_all
symb_kol = Len(str)

'|fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd|
If symb_kol > 1 Then
 num_1 = Mid(str, symb_kol, 1)
Else
 num_1 = sum_all
End If

If num_1 = 0 Then
 num_2 = 0
Else
 num_2 = 10 - num_1
End If

If num_3 <> num_2 Then
   fail_control = True
End If

End Function

Public Function check_control_number(i As Integer, ival As String, kol As Integer, num As Integer, icheck_val)

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd|-|fffd||fffd|
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
Dim symb_kol    As Integer
Dim sum_1       As Integer
Dim sum_2       As Integer
Dim sum_all     As Integer
Dim str         As String
Dim num_1       As Integer
Dim num_2       As Integer
Dim num_3       As Integer


If icheck_val = "WN" Or icheck_val = "PL" Then
  Exit Function
End If

num_3 = Mid(ival, kol, 1)

For ii = 1 To kol Step 2
 If ii <> kol Then
   sum_1 = sum_1 + Mid(ival, ii, 1)
 End If
Next ii

For jj = 2 To kol Step 2
 If jj <> kol Then
   sum_2 = sum_2 + Mid(ival, jj, 1)
 End If
Next jj

sum_2 = sum_2 * 3

sum_all = sum_1 + sum_2
str = sum_all
symb_kol = Len(str)

'|fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd|
If symb_kol > 1 Then
 num_1 = Mid(str, symb_kol, 1)
Else
 num_1 = sum_all
End If

num_2 = 10 - num_1

If num_3 <> num_2 Then
   fail_control = True
End If

End Function

