Option Explicit
Private Sub Workbook_SheetChange(ByVal Sh As Object, ByVal Target As Range)
If TypeName(Sh) <> "Worksheet" Then Exit Sub
Dim Bereich As Range, Zelle As Range
Dim flag As Integer
 
'On Error Resume Next
'    Set Bereich = Sh.UsedRange.SpecialCells(xlCellTypeAllValidation)
'On Error GoTo 0
'If Bereich Is Nothing Then Exit Sub
 
'Set Bereich = Intersect(Target, Sh.UsedRange.SpecialCells(xlCellTypeAllValidation))
Set Bereich = Target
If Bereich Is Nothing Then Exit Sub
flag = 0
ActiveSheet.Unprotect Password:="SH_3_01"
For Each Zelle In Bereich
    If Not Zelle.Validation.Value Then
        Zelle.ClearContents
        'Zelle.Font.Name = "Arial"
        'Zelle.Font.ColorIndex = 1
        'Zelle.Interior.ColorIndex = 15
        flag = 1
    End If
Next Zelle
If flag = 1 Then
    MsgBox "Der von ihnen eingef|fffd|gte Inhalt enspricht nicht vollst|fffd|ndig den G|fffd|ltigkeitsregeln. Es wurden daher nur die g|fffd|ltigen Werte eingef|fffd|gt!", vbInformation, "G|fffd|ltigkeitspr|fffd|fung fehlgeschlagen"
    flag = 0
End If
ActiveSheet.Protect Password:="SH_3_01"
Set Bereich = Nothing: Set Zelle = Nothing
End Sub

