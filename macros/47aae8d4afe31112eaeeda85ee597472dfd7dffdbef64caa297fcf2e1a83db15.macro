Private Sub Worksheet_BeforeDoubleClick(ByVal Target As Range, Cancel As Boolean)
Dim addr As String
    Dim x As Variant
    Dim rng, c, r, cll As String
    
    If NoEvents Then Exit Sub
    If Target.Cells.Count > 1 Then Exit Sub
        addr = ActiveCell.Address()
    x = Split(addr, "$")
    
    c = x(1)
    r = x(2)
    rng = c & ":" & c & "," & r & ":" & r
    Range(rng).Select
    cll = c & r
    Range(cll).Activate
End Sub

