Attribute VB_Control = "CommandButton2, 99, 0, MSForms, CommandButton"
Private Sub CommandButton2_Click()
PrjNr = Worksheets("Eingabe").Cells(3, 3)

If PrjNr < 1 Then MsgBox ("Bitte Projektnummer eingeben! Dann noch einmal versuchen!")
If PrjNr < 1 Then GoTo 1000

Dim objOL As Object
Dim objMail As Object
Set objOL = CreateObject("Outlook.Application")
Set objMail = objOL.CreateItem(0)
Application.ScreenUpdating = False
PrjNr = Worksheets("Eingabe").Cells(3, 3)
Monat = Worksheets("Eingabe").Cells(6, 4)
Jahr = Worksheets("Eingabe").Cells(6, 5)
With objMail
'.To = "S.mahnert-lueg@evb.net ;D.Lewandowski@EVB.net"
.To = "praemien@evb.net; taetigkeitsbericht@EVB.net"
.Subject = "T|fffd|tigkeitsbericht P " & PrjNr & "_" & Jahr & "_" & Monat
.Attachments.Add ActiveWorkbook.FullName
.Send
End With
Application.ScreenUpdating = True

1000

End Sub
