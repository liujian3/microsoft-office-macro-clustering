Sub Insert_records(nDays As Integer, nRecords As Integer)
'
' Insert_records Macro
'
'
    If nDays > 2 Then
        Range("C5:C7").Select
        Selection.Copy
        Range(Cells(5, 4), Cells(7, nDays + 1)).Select
        ActiveSheet.Paste
    End If
    If nRecords > 1 Then
        Rows("7:7").Select
        Selection.Copy
        Rows("7:" & Trim(Str(5 + nRecords))).Select
        Selection.Insert Shift:=xlDown
    End If
End Sub
