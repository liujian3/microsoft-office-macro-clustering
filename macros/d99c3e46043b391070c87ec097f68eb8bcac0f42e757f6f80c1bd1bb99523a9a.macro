Private Function spolecnaPodtabulka(typ As String, r As Range) As Table
    Dim t As Table
    Dim r2 As Range
    Dim sl As Integer, ra As Integer
    
    r.Collapse 'Nastav|fffd| range na za|fffd||fffd|tek bu|fffd|ky, ale u|fffd| v paragrafu
    
    ra = 2
    sl = 3
    If typ = "produkty" Then
        sl = 2
    End If
    
    ' Spole|fffd|n|fffd|
    Set t = ThisDocument.Tables.Add(r, ra, sl)
    t.Columns.Width = CentimetersToPoints(2.3)
    t.Rows.Height = CentimetersToPoints(0.7)
    
    Set r2 = t.Cell(1, 1).Range
    r2.End = t.Cell(1, sl).Range.End
    r2.Cells.Merge
    
    Call spolecneFormatovaniTabulky(t)
    
    t.PreferredWidthType = wdPreferredWidthPoints
    
    ' Doform|fffd|tov|fffd|n|fffd| a popisky, pro ka|fffd|dou tabulku jin|fffd|
    If typ = "produkty" Then
        t.PreferredWidth = CentimetersToPoints(4.6)
        t.Cell(1, 1).Range = "Tr|fffd|by"
        t.Cell(1, 1).Borders(wdBorderBottom).LineStyle = wdLineStyleDot
        t.Cell(2, 1).Borders(wdBorderRight).LineStyle = wdLineStyleDot
        t.Cell(2, 1).Range = "tis. CZK"
        t.Cell(2, 2).Range = "%"
    Else
        t.PreferredWidth = CentimetersToPoints(6.9)
        t.Rows(1).Range.Shading.BackgroundPatternColor = RGB(255, 255, 255)
        t.Cell(1, 1).Range.ParagraphFormat.Alignment = wdAlignParagraphLeft
        t.Cell(1, 1).Range.ParagraphFormat.LeftIndent = CentimetersToPoints(0.2)
        
        t.Cell(1, 1).Borders(wdBorderBottom).LineStyle = wdLineStyleDot
        t.Cell(2, 1).Borders(wdBorderRight).LineStyle = wdLineStyleDot
        t.Cell(2, 2).Borders(wdBorderRight).LineStyle = wdLineStyleDot
        
        If typ = "zvirata" Then
            t.Cell(2, 1).Range = "Po|fffd|et (ks) k 31.12"
            t.Cell(2, 2).Range = "U|fffd|itkovost"
            t.Cell(2, 3).Range = "Realiz. cena (K|fffd|/kg)"
        Else
            t.Cell(2, 1).Range = "V|fffd|m|fffd|ra (ha) k 31.12"
            t.Cell(2, 2).Range = "V|fffd|nos (t/ha)"
            t.Cell(2, 3).Range = "Realiz. cena (K|fffd|/t)"
        End If
    End If
    
    Set spolecnaPodtabulka = t
End Function

Private Function formatovaniRadku(typ As String, tabulka As Table, i As Integer)
    With tabulka.Rows(i)
        If typ = "standard" Then
            .Range.Bold = True
            .Range.Shading.BackgroundPatternColor = RGB(242, 242, 242)
        ElseIf typ = "dvojite" Then
            .Range.Bold = True
            .Range.Shading.BackgroundPatternColor = RGB(242, 242, 242)
            .Height = CentimetersToPoints(1.4)
            .Range.ParagraphFormat.WordWrap = True
        End If
    End With
End Function

Private Function vlozTabulku(rng As Range, pocetRadku As Integer, pocetSloupcu As Integer) As Table
    Dim t As Table
        
    Set t = ThisDocument.Tables.Add(rng, pocetRadku, pocetSloupcu)
    t.Rows.Height = CentimetersToPoints(0.7)
    t.Rows(1).HeadingFormat = True 'znamen|fffd|, |fffd|e se to bude opakovat p|fffd|i p|fffd|ete|fffd|en|fffd| na dal|fffd||fffd| str|fffd|nku jako z|fffd|hlav|fffd|
    
    'Sv|fffd|zat s dal|fffd||fffd|m
    t.Rows(1).Range.ParagraphFormat.KeepWithNext = True
    If pocetRadku > 2 Then t.Rows(2).Range.ParagraphFormat.KeepWithNext = True
    If pocetRadku > 5 Then
        t.Rows(pocetRadku - 2).Range.ParagraphFormat.KeepWithNext = True
        t.Rows(pocetRadku - 1).Range.ParagraphFormat.KeepWithNext = True
    End If
    
    Call spolecneFormatovaniTabulky(t)
    
    Call t.Rows.SetLeftIndent(CentimetersToPoints(0.18), wdAdjustFirstColumn)
    
    t.PreferredWidthType = wdPreferredWidthPoints
    t.PreferredWidth = CentimetersToPoints(18.49)
    
    t.Borders.InsideLineStyle = wdLineStyleDot
