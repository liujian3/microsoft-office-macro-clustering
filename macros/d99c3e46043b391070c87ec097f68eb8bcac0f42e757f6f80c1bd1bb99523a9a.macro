Public Function importujDataDoTabulky(t As Table, sloupec As Integer, xpath As String, zacatek As Integer, nodes As IXMLDOMNodeList, Optional format As String = "")
    Dim node As IXMLDOMNode
    Dim n As Integer, m As Integer
    Dim txt As String
    
    n = 1
    m = 2
    For Each node In nodes
        If n > zacatek Then
            txt = node.SelectSingleNode(xpath).text
            If format = "" Then
                
            ElseIf format = "tisice" Then
                txt = formatovani.oddeleneTisice(txt)
            ElseIf format = "procenta" And txt <> "" Then
                txt = txt + "%"
            ElseIf format = "banka" And txt = "" Then
                txt = node.SelectSingleNode("KODBANKY").text
            End If
            If m <= t.Rows.Count Then t.Cell(m, sloupec).Range = txt
            m = m + 1
        End If
        n = n + 1
    Next node
End Function

Public Function importujDataDoTabulkyHor(t As Table, radek As Integer, xpath As String, zacatek As Integer, pocet As Integer, nodes As IXMLDOMNodeList, Optional format As String = "")
    Dim node As IXMLDOMNode
    Dim n As Integer, m As Integer
    Dim txt As String
    
    n = 1
    m = 2
    For Each node In nodes
        If n > zacatek And m - 1 <= pocet Then
            txt = node.SelectSingleNode(xpath).text
            If format = "" Then
                
            ElseIf format = "tisice" Then
                txt = formatovani.oddeleneTisice(txt)
            ElseIf format = "typProduktu" Then
                txt = typProduktu(txt)
            ElseIf format = "procenta" And txt <> "" Then
                txt = txt + "%"
            ElseIf format = "periodaSplatek" Then
                txt = periodaSplatek(txt)
            End If
            If m <= t.Rows.Count Then t.Cell(radek, m).Range = txt
            m = m + 1
        End If
        n = n + 1
    Next node
End Function

