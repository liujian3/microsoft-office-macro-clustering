    End If
    
    If ThisDocument.ChkPrZajem.Value Then
        str(n) = "Potvrzen|fffd| z|fffd|jmu |fffd|adatele o zpracov|fffd|n|fffd| nab|fffd|dky financov|fffd|n|fffd|, zalo|fffd|en|fffd| na dotazov|fffd|n|fffd| do CR|fffd|"
        zadost = False
        n = n + 1
    End If
    
    Dim txt As String
    If zadost Then txt = "|fffd||fffd|dost "
    For i = 1 To n - 1
        If i = n - 1 And i > 1 Then
            txt = txt & " a "
        ElseIf i > 1 Then
            txt = txt & ", "
        End If
        txt = txt + str(i)
    Next i
    
    If n > 1 Then
