Public Function jsouStejne(str1 As String, str2 As String, vzdalenost As Integer) As Boolean
    str1 = odstranDiakritiku(str1)
    str1 = odstranCisla(str1)
    str1 = odstranZnaky(str1)
    str1 = odstranMezery(str1)
    
    str2 = odstranDiakritiku(str2)
    str2 = odstranCisla(str2)
    str2 = odstranZnaky(str2)
    str2 = odstranMezery(str2)
    
    jsouStejne = damerauLevenshtein(str1, str2) <= vzdalenost
End Function

Private Function damerauLevenshtein(a As String, b As String)
    Dim da(0 To eta) As Integer
    Dim d() As Integer
    Dim i As Integer, j As Integer, k As Integer, l As Integer
    Dim db As Integer, maxDist As Integer
    
    ReDim d(-1 To Len(a), -1 To Len(b)) As Integer
    
    For i = 1 To eta
        da(i) = 0
    Next i
    
    maxDist = Len(a) + Len(b)
    For i = 0 To Len(a)
        d(i, -1) = maxDist
        d(i, 0) = i
    Next i
    For j = 0 To Len(b)
        d(-1, j) = maxDist
        d(0, j) = j
    Next j
    
    For i = 1 To Len(a)
        db = 0
        For j = 1 To Len(b)
            k = da(oneChr(Mid(b, j, 1)))
            l = db
            If Mid(a, i, 1) = Mid(b, j, 1) Then
                cost = 0
                db = j
            Else
                cost = 1
            End If
            d(i, j) = min(min(d(i - 1, j - 1) + cost, _
            d(i, j - 1) + 1), _
            min(d(i - 1, j) + 1, _
            d(k - 1, l - 1) + (i - k - 1) + 1 + (j - l - 1)))
        Next j
        da(oneChr(Mid(a, i, 1))) = i
    Next i
    damerauLevenshtein = d(Len(a), Len(b))
End Function

Private Function oneChr(s As String) As Integer
    If (s = " ") Then
        oneChr = 0
    Else
        oneChr = Asc(s) - Asc("a") + 1
    End If
End Function

Private Function min(a As Integer, b As Integer) As Integer
    If a < b Then
        min = a
    Else
        min = b
    End If
End Function

Private Function odstranDiakritiku(source As String) As String
    Const cz As String = "|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||61a||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||75e||fffd|"
    Const en As String = "aAcCdDeEeEiInNoOrRsStTuUuUyYzZ"
    Dim TmpS As String
    Dim OutS As String
    Dim i As Integer
    
    OutS = ""
    If IsNull(source) Or source = "" Then
        odstranDiakritiku = ""
    Else
        For i = 1 To Len(source)
            TmpS = Mid(source, i, 1)
            If InStr(1, cz, TmpS, vbBinaryCompare) > 0 Then TmpS = Mid(en, InStr(1, cz, TmpS, vbBinaryCompare), 1)
            OutS = OutS & TmpS
        Next i
        odstranDiakritiku = LCase(OutS)
    End If
End Function

Private Function odstranZnaky(text As String) As String
    text = Replace(text, ".", " ")
    text = Replace(text, ",", " ")
    text = Replace(text, ";", " ")
    text = Replace(text, ":", " ")
    text = Replace(text, "+", " ")
    text = Replace(text, "-", " ")
    text = Replace(text, "/", " ")
    text = Replace(text, "-", " ")
    text = Replace(text, "\\", " ")
    
    odstranZnaky = text
End Function

Private Function odstranMezery(text As String) As String
    For i = 1 To 5
        text = Replace(text, "  ", " ")
    Next i
    If Right(text, 1) = " " Then text = left(text, Len(text) - 1) ' mezera na konci
    If left(text, 1) = " " Then text = Right(text, Len(text) - 1)  ' mezera na za|fffd||fffd|tku
    odstranMezery = text
End Function

Private Function odstranCisla(text As String) As String
    For i = 0 To 9
        text = Replace(text, CStr(i), "")
    Next i
    odstranCisla = text
End Function
