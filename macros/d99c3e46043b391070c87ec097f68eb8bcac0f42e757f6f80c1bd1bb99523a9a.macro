Sub skryjTlacitka()
    Dim o As InlineShape
    For Each o In ThisDocument.InlineShapes
        If left(o.OLEFormat.Object.name, 3) = "Btn" Then
            o.Width = 1 '40.25
            o.Height = 1 ' 23.85
        End If
    Next
End Sub

Sub zobrazTlacitka()
    Dim o As InlineShape
    For Each o In ThisDocument.InlineShapes
        Call velikostiTlacitek(o)
    Next
End Sub

Public Function velikostiTlacitek(ByRef o As InlineShape)
    If o.OLEFormat.Object.name = "BtnPrevodDokumentu1" Or o.OLEFormat.Object.name = "BtnPrevodDokumentu2" Then
        o.Height = 23.85
        o.Width = 140.25
    ElseIf left(o.OLEFormat.Object.name, 3) = "Btn" Then
        'V|fffd|echny p|fffd|id|fffd|vac|fffd| tla|fffd||fffd|tka
        o.Height = 20
        o.Width = 114.8
    End If
End Function

Public Function zobrazeniTlacitka(nazev As String, zobraz As Boolean)
    Dim o As InlineShape
    For Each o In ThisDocument.InlineShapes
        If o.OLEFormat.Object.name = nazev Then
            If zobraz Then
                Call velikostiTlacitek(o)
            Else
                o.Height = 1
                o.Width = 1
            End If
        End If
    Next
End Function

Public Function smazTlacitka()
    If ThisDocument.bLive Then
        Dim o As InlineShape
        For Each o In ThisDocument.InlineShapes
            If left(o.OLEFormat.Object.name, 3) = "Btn" Then o.Delete
        Next
    Else
    MsgBox "Dokument je v Debug m|fffd|du, upravte v Modulu ThisDocument hodnoty bDebug a/nebo bLive"
    End If
End Function
Attribute VB_Name = "XMLImport"
Private Function zk()
    Dim str As String, rng As Range
    reset
    data = Texty.DatovaVeta2
    Set XML = CreateObject("Microsoft.XMLDOM")
    
    XML.validateOnParse = True
    XML.async = False
    XML.LoadXML (data)
    Call dodavatele(XML.SelectNodes("/INFO/DODAVATELE/DODAVATEL"))
    Call odberatele(XML.SelectNodes("/INFO/ODBERATELE/ODBERATEL"))
    Call produktyTrzby(XML.SelectNodes("/INFO/TRZBY/PRODUKT"))
    Call management(XML.SelectNodes("/INFO/MANAGEMENT/MANAZER"))
    Call uctyMimo(XML.SelectNodes("/INFO/UCTYMIMOGE/UCET"))
    'Call ciziZdroje(XML.SelectNodes("/INFO/CIZIZDROJE/UVER"))
    Call zvirata(XML.SelectNodes("/INFO/ZVIRATA/ROK1/ZVIRE"), XML.SelectNodes("/INFO/ZVIRATA/ROK2/ZVIRE"), _
        XML.SelectSingleNode("/INFO/ROKY/ROK1").text, XML.SelectSingleNode("/INFO/ROKY/ROK2").text)
    Call plodiny(XML.SelectNodes("/INFO/PLODINY/ROK1/PLODINA"), XML.SelectNodes("/INFO/PLODINY/ROK2/PLODINA"), _
        XML.SelectSingleNode("/INFO/ROKY/ROK1").text, XML.SelectSingleNode("/INFO/ROKY/ROK2").text)
        
    Set rng = zalozky.rangeMeziZalozkami("FinNefinEnd", "FinNefinAddendum1End")
    str = CStr(rng.Tables.Count) + ", "
    Set rng = zalozky.rangeMeziZalozkami("FinNefinAddendum1End", "FinNefinAddendum2End")
    str = str + CStr(rng.Tables.Count)
    'MsgBox (str)
End Function

