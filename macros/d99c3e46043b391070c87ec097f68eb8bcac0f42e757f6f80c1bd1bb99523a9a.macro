Private Function vyberAdresu(nodes As IXMLDOMNodeList, typ As String) As IXMLDOMNode
    Dim r As IXMLDOMNode
    
    Set r = nodes.item(0)
    For Each node In nodes
        If (adresaMaVyssiPrioritu(CInt(r.SelectSingleNode("TYP").text), CInt(node.SelectSingleNode("TYP").text), typ)) Then
            Set r = node
        End If
    Next
    Set vyberAdresu = r
End Function

Private Function adresaMaVyssiPrioritu(typPuvodni As Integer, typNovy As Integer, typ As String) As Boolean
    Dim priorita(4) As Integer
    Dim n As Integer
    
    If (typ = "s|fffd|dlo") Then 'Ni|fffd||fffd||fffd| index odpov|fffd|d|fffd| vy|fffd||fffd||fffd| priorit|fffd|
        priorita(1) = 10 'Smlouvy: s|fffd|dlo
        priorita(2) = 1  'S|fffd|dlo
        priorita(3) = 9  'Smlouvy: m|fffd|sto podnik|fffd|n|fffd|
        priorita(4) = 4  'provozovna
    Else 'trval|fffd| pobyt pro FO
        priorita(1) = 8  'Smlouvy: bytem
        priorita(2) = 7  'Trval|fffd| pobyt
        priorita(3) = 10 'Smlouvy: s|fffd|dlo
        priorita(4) = 1  'S|fffd|dlo
    End If
    
    adresaMaVyssiPrioritu = najdiIndex(priorita, typNovy, 1000) < najdiIndex(priorita, typPuvodni, 1000)
End Function

Private Function najdiIndex(ByRef ar() As Integer, item As Integer, default As Integer) As Integer
    najdiIndex = default
    For i = LBound(ar) To UBound(ar)
        If CInt(ar(i)) = item Then
            najdiIndex = i
            Exit For
        End If
    Next i
End Function

Private Function nahradTextPole(nazev As String, xpath As String, max As Integer, nodes As IXMLDOMNodeList, Optional formatovat As Boolean = False)
    Dim node As IXMLDOMNode
    Dim n As Integer
    Dim txt As String
    
    n = 1
    For Each node In nodes
        If n <= max Then
            txt = node.SelectSingleNode(xpath).text
            If formatovat Then
                txt = formatovani.oddeleneTisice(txt)
            End If
            Call zalozky.nahradTextZalozky(nazev + "_" + CStr(n), txt)
        End If
        n = n + 1
    Next node
End Function

Public Function nactiSchranku() As String
    Dim dataObj As MSForms.DataObject
    Set dataObj = New MSForms.DataObject
    
    On Error GoTo Chyba
    dataObj.GetFromClipboard
    
    text = dataObj.GetText(1)
    nactiSchranku = text
Chyba:
    If Err <> 0 Then
        'MsgBox "Chyba 001: Schr|fffd|nka neobsahuje textov|fffd| data", Title:="Chyba"
        nactiSchranku = ""
        End
    End If
End Function

