Public Function import()
    Dim data As String
    Dim typOsoby As String
    Dim pravniForma As String
    Dim rng As Range
    
    Call reset
    'data = Texty.DatovaVeta2
    data = nactiSchranku()
    
    If data = "" Then
        End
    End If
    
    If ThisDocument.bLive Then
        On Error GoTo chybaImport
    End If
    
    Set XML = CreateObject("Microsoft.XMLDOM")
    
    XML.validateOnParse = True
    XML.async = False
    XML.LoadXML (data)
    
    If XML.SelectSingleNode("/INFO/PO/NAZEV") Is Nothing Then
        End
    End If
    
    'MsgBox data
    
    'Mus|fffd|m zobrazit, aby to fungovalo
    Set rng = zalozky.rangeMeziZalozkami("FinNefinEnd", "FinNefinAddendum2End")
    rng.Font.Hidden = False
    
    Call po(XML.SelectSingleNode("/INFO/PO"))
    typOsoby = XML.SelectSingleNode("/INFO/PO/TYP").text
    If typOsoby = "2" Or typOsoby = "1" Then 'FOP anebo i FO
        Call fo(XML.SelectSingleNode("/INFO/FO"))
    End If
    
    'Pro fin/nefin ukazatele
    Call dodavatele(XML.SelectNodes("/INFO/DODAVATELE/DODAVATEL"))
    Call odberatele(XML.SelectNodes("/INFO/ODBERATELE/ODBERATEL"))
    Call produktyTrzby(XML.SelectNodes("/INFO/TRZBY/PRODUKT"))
    Call management(XML.SelectNodes("/INFO/MANAGEMENT/MANAZER"))
    If Not XML.SelectSingleNode("/INFO/JAN/ROK1") Is Nothing Then
        Call udajeJAN("1", XML.SelectSingleNode("/INFO/JAN/ROK1"))
    End If
    If Not XML.SelectSingleNode("/INFO/JAN/ROK2") Is Nothing Then
        Call udajeJAN("2", XML.SelectSingleNode("/INFO/JAN/ROK2"))
    End If
    Call nace(XML.SelectNodes("/INFO/NACES/NACE"))
    Call uctyMimo(XML.SelectNodes("/INFO/UCTYMIMOGE/UCET"))
    Call ciziZdroje(XML.SelectNodes("/INFO/CIZIZDROJE/UVER"))
    Call zvirata(XML.SelectNodes("/INFO/ZVIRATA/ROK1/ZVIRE"), XML.SelectNodes("/INFO/ZVIRATA/ROK2/ZVIRE"), _
        XML.SelectSingleNode("/INFO/ROKY/ROK1").text, XML.SelectSingleNode("/INFO/ROKY/ROK2").text)
    Call plodiny(XML.SelectNodes("/INFO/PLODINY/ROK1/PLODINA"), XML.SelectNodes("/INFO/PLODINY/ROK2/PLODINA"), _
        XML.SelectSingleNode("/INFO/ROKY/ROK1").text, XML.SelectSingleNode("/INFO/ROKY/ROK2").text)
    If Not XML.SelectSingleNode("/INFO/DOTACE/ROK1/DOTACE") Is Nothing Then
        Call zalozky.nahradTextZalozky("iProvozniDotace_Rok1", formatovani.oddeleneTisice(XML.SelectSingleNode("/INFO/DOTACE/ROK1/DOTACE").text))
    End If
    If Not XML.SelectSingleNode("/INFO/DOTACE/ROK2/DOTACE") Is Nothing Then
        Call zalozky.nahradTextZalozky("iProvozniDotace_Rok2", formatovani.oddeleneTisice(XML.SelectSingleNode("/INFO/DOTACE/ROK2/DOTACE").text))
    End If
    If Not XML.SelectSingleNode("INFO/PUDA/VLASTNI") Is Nothing Then
        Call puda(XML.SelectSingleNode("/INFO/PUDA"))
    End If
    
    Call roky("1", XML.SelectSingleNode("/INFO/ROKY/ROK1").text)
    Call roky("2", XML.SelectSingleNode("/INFO/ROKY/ROK2").text)
    
    'Skryj nevypln|fffd|n|fffd| dodatky
    Set rng = zalozky.rangeMeziZalozkami("FinNefinEnd", "FinNefinAddendum1End")
    If rng.Tables.Count = 0 Then
        rng.Font.Hidden = True
    End If
    
    Set rng = zalozky.rangeMeziZalozkami("FinNefinAddendum1End", "FinNefinAddendum2End")
    If rng.Tables.Count = 0 Then
        rng.Font.Hidden = True
    End If
    
chybaImport:
    If Err <> 0 Then
        MsgBox "Chybn|fffd| form|fffd|t datov|fffd| v|fffd|ty. Nebyla naimportov|fffd|na v|fffd|echna data"
        End
    End If
End Function

Private Function po(node As IXMLDOMNode)
    Call zalozky.nahradTextZalozkyADuplicit("iNazev", node.SelectSingleNode("NAZEV").text)
    Call zalozky.nahradTextZalozkyADuplicit("iICO", node.SelectSingleNode("ICO").text)
    
    Call zalozky.nahradTextZalozkyBunku("iHistorie", node.SelectSingleNode("HISTORIE").text)
    Call zalozky.nahradTextZalozkyBunku("iSoucasnyStav", node.SelectSingleNode("SOUCASNYSTAV").text)
    If node.SelectSingleNode("DANOVYPORADCE").text = "1" Then ThisDocument.ChkPoradce1 = True Else ThisDocument.ChkPoradce2 = True
    Call zalozky.nahradTextZalozky("iPocetProdejen", node.SelectSingleNode("POCETPRODEJEN").text)
    Call zalozky.nahradTextZalozky("iPocetProvozoven", node.SelectSingleNode("POCETPROVOZOVEN").text)
    Call zalozky.nahradTextZalozky("iStrukturaRizeni", Ciselniky.strukturaRizeni(node.SelectSingleNode("STRUKTURARIZENI").text))
    Call zalozky.nahradTextZalozky("iPocetLiniiRizeni", node.SelectSingleNode("POCETLINIIRIZENI").text)
End Function

Private Function fo(node As IXMLDOMNode)
    Call zalozky.nahradTextEnclZalozky("encVzdelani", Ciselniky.vzdelani(node.SelectSingleNode("VZDELANI").text))
End Function

Private Function poAdresy(nodes As IXMLDOMNodeList)
    Dim ulice As String
    'Provozovny do finan|fffd|n|fffd|ch a nefinan|fffd|n|fffd|ch ukazatel|fffd|
    Dim n As Integer
    Dim max As Integer
    
    max = 5
    n = 1
    For Each node2 In nodes
        If node2.SelectSingleNode("TYP").text = "4" Then 'provozovna
            If n <= max Then
                ulice = node2.SelectSingleNode("ULICE").text + " " + node2.SelectSingleNode("CP").text
                If node2.SelectSingleNode("CO").text <> "" Then ulice = ulice + "/" + node2.SelectSingleNode("CO").text
                ulice = ulice + ", " + node2.SelectSingleNode("PSC").text + " " + node2.SelectSingleNode("MESTO").text
                Call zalozky.nahradTextZalozky("itProvozovnaNazev_" + CStr(n), node2.SelectSingleNode("ADRESAT").text)
                Call zalozky.nahradTextZalozky("itProvozovnaAdresa_" + CStr(n), ulice)
            End If
            n = n + 1
        End If
    Next node2
End Function

Private Function vlastnici(nodes As IXMLDOMNodeList)
    Dim node As IXMLDOMNode
    Dim tmp As String
    Dim n, max As Integer
    
    max = 5
    n = 1
    For Each node In nodes
        If n <= max Then
            'R|fffd| / I|fffd|
            tmp = node.SelectSingleNode("RC").text
            If tmp = "" Then tmp = tmp + node.SelectSingleNode("DATUMNAROZENI").text
            If node.SelectSingleNode("ICO").text <> "" Then
                If tmp <> "" Then tmp = tmp + "/"
                tmp = tmp + node.SelectSingleNode("ICO").text
            End If
            Call zalozky.nahradTextZalozky("itVRCIC" + "_" + CStr(n), tmp)
            
            'Jm|fffd|no/N|fffd|zev vlastn|fffd|ka
            tmp = node.SelectSingleNode("JMENO").text + " " + node.SelectSingleNode("PRIJMENI").text
            If tmp = " " Then tmp = node.SelectSingleNode("NAZEV").text
            Call zalozky.nahradTextZalozky("itVNazev" + "_" + CStr(n), tmp)
        End If
        n = n + 1
    Next node
    'Call nahradTextPole("itVNazev", "NAZEV", 5, nodes)
    Call nahradTextPole("itVPodil", "PODIL", 5, nodes)
End Function

Private Function koncoviVlastnici(koncoviVl As IXMLDOMNodeList, vlastnici As IXMLDOMNodeList)
    Dim tmp As String
    Dim n As Integer, m As Integer, max As Integer, maxVlastnici As Integer
    Dim node As IXMLDOMNode, vlastnik As IXMLDOMNode
    Dim vlozit As Boolean
    
    max = 5
    maxVlastnici = 5 ' kdyby jednotliv|fffd| tabulky nem|fffd|ly stejn|fffd| po|fffd|ty |fffd||fffd|dk|fffd|, je pot|fffd|eba to zohlednit
    n = 1
    For Each node In koncoviVl
        If n <= max Then
            'zjisti, jestli je ve vlastn|fffd|c|fffd|ch, pomoc|fffd| shody jm|fffd|na, p|fffd||fffd|jmen|fffd| a data narozen|fffd|
            vlozit = True
            m = 1
            For Each vlastnik In vlastnici
                If m <= maxVlastnici Then
                    vlozit = vlozit And node.SelectSingleNode("PRIJMENI").text <> ""
                    vlozit = vlozit _
                        And Not (node.SelectSingleNode("JMENO").text = vlastnik.SelectSingleNode("JMENO").text _
                                And node.SelectSingleNode("PRIJMENI").text = vlastnik.SelectSingleNode("PRIJMENI").text _
                                And node.SelectSingleNode("DATUMNAROZENI").text = vlastnik.SelectSingleNode("DATUMNAROZENI").text)
                    m = m + 1
               '     MsgBox (node.SelectSingleNode("JMENO").text + " x " + vlastnik.SelectSingleNode("JMENO").text + ", " + _
                        node.SelectSingleNode("PRIJMENI").text + " x " + vlastnik.SelectSingleNode("PRIJMENI").text + ", " _
                        + node.SelectSingleNode("DATUMNAROZENI").text + " x " + vlastnik.SelectSingleNode("DATUMNAROZENI").text + "->" + CStr(vlozit))
                End If
            Next vlastnik
            
            If vlozit Then
                tmp = node.SelectSingleNode("JMENO").text + " " + node.SelectSingleNode("PRIJMENI").text
                Call zalozky.nahradTextZalozky("itKVJmeno" + "_" + CStr(n), tmp)
                If node.SelectSingleNode("STATNIPRISLUSNOST").text <> "N/A" Then Call zalozky.nahradTextZalozky("itKVStat" + "_" + CStr(n), node.SelectSingleNode("STATNIPRISLUSNOST").text)
                Call zalozky.nahradTextZalozky("itKVDatumNarozeni" + "_" + CStr(n), node.SelectSingleNode("DATUMNAROZENI").text)
                Call zalozky.nahradTextZalozky("itKVPodil" + "_" + CStr(n), node.SelectSingleNode("PODIL").text)
                n = n + 1
            End If
        End If

    Next node
End Function

Private Function vlastnenoVlastniky(nodes As IXMLDOMNodeList, ico As String)
    Dim nodes2 As IXMLDOMNodeList
    Dim node, node2 As IXMLDOMNode
    Dim max, n As Integer
    Dim tmp As String
    ' Slo|fffd|it|fffd|j|fffd||fffd| ne|fffd| ostatn|fffd| tabulky, nem|fffd||fffd|u pou|fffd||fffd|t nahradTextPole
    max = 4
    n = 1
    For Each node In nodes
        Set nodes2 = node.SelectNodes("PODILY/SUBJEKT")
        tmp = node.SelectSingleNode("NAZEV").text
        If tmp = "" Then tmp = node.SelectSingleNode("JMENO").text + " " + node.SelectSingleNode("PRIJMENI").text
        For Each node2 In nodes2
            If node2.SelectSingleNode("ICO").text <> ico Then
                If n <= max Then
                    Call zalozky.nahradTextZalozky("itVVVlastnik" + "_" + CStr(n), tmp)
                    Call zalozky.nahradTextZalozky("itVVNazev" + "_" + CStr(n), node2.SelectSingleNode("NAZEV").text)
                    Call zalozky.nahradTextZalozky("itVVIC" + "_" + CStr(n), node2.SelectSingleNode("ICO").text)
                    Call zalozky.nahradTextZalozky("itVVVztah" + "_" + CStr(n), Ciselniky.vlastniciODS(node2.SelectSingleNode("TYP").text))
                    Call zalozky.nahradTextZalozky("itVVPodil" + "_" + CStr(n), node2.SelectSingleNode("PODIL").text)
                End If ' vejde se do tabulky
                n = n + 1
            End If 'Jin|fffd| i|fffd|o
        Next node2
    Next node
End Function

Private Function ovladaneSpolecnosti(nodes As IXMLDOMNodeList)
    Call nahradTextPole("itOIC", "ICO", 4, nodes) 'lze pou|fffd||fffd|t, pokud nem|fffd|m dal|fffd||fffd| n|fffd|roky na logiku (nen|fffd| to to sam|fffd| I|fffd| a podobn|fffd|)
    Call nahradTextPole("itONazev", "NAZEV", 4, nodes)
    Call nahradTextPole("itOPodil", "PODIL", 4, nodes)
End Function

Private Function pridejTextPredZalozku(zalozka As String, txt As String, Optional before As Integer = 15)
    Dim rng As Range
    
    Set rng = zalozky.rangePredZalozkou(zalozka)
    rng.InsertParagraph
    rng.InsertBefore (txt)
    rng.Bold = True
    rng.Paragraphs(1).spaceBefore = before
    rng.Paragraphs(1).SpaceAfter = 5
    rng.Paragraphs(1).KeepWithNext = True
End Function

Private Function dodavatele(nodes As IXMLDOMNodeList)
    Dim max As Integer, rng As Range, t As Table
    
    max = 5
    Call nahradTextPole("itDodNazev", "NAZEV", max, nodes)
    Call nahradTextPole("itDodICO", "ICO", max, nodes)
    Call nahradTextPole("itDodPodil", "PODIL", max, nodes)
    Call nahradTextPole("itDodKomentar", "KOMENTAR", max, nodes)
    Call nahradTextPole("itDodSplatnost", "SPLATNOST", max, nodes)
    
    ' Dopln|fffd|n|fffd| do p|fffd||fffd|lohy
    If nodes.Length > max Then
        Call pridejTextPredZalozku("FinNefinAddendum1End", "Ad II. Dodavatelsko-odb|fffd|ratelsk|fffd| vztahy, Dodavatel|fffd|")
        Set rng = zalozky.rangePredZalozkou("FinNefinAddendum1End")
        Set t = Tabulky.vlozTabulkuOdberatele(rng, nodes.Length - max)
        Call Tabulky.importujDataDoTabulky(t, 1, "NAZEV", max, nodes)
        Call Tabulky.importujDataDoTabulky(t, 2, "ICO", max, nodes)
        Call Tabulky.importujDataDoTabulky(t, 3, "PODIL", max, nodes)
        Call Tabulky.importujDataDoTabulky(t, 4, "KOMENTAR", max, nodes)
        Call Tabulky.importujDataDoTabulky(t, 5, "SPLATNOST", max, nodes)
    End If
End Function

Private Function odberatele(nodes As IXMLDOMNodeList)
    Dim max As Integer, rng As Range, t As Table
    
    max = 5
    Call nahradTextPole("itOdbNazev", "NAZEV", max, nodes)
    Call nahradTextPole("itOdbICO", "ICO", max, nodes)
    Call nahradTextPole("itOdbPodil", "PODIL", max, nodes)
    Call nahradTextPole("itOdbKomentar", "KOMENTAR", max, nodes)
    Call nahradTextPole("itOdbSplatnost", "SPLATNOST", max, nodes)
    
    ' Dopln|fffd|n|fffd| do p|fffd||fffd|lohy
