Private Function zvirataPlodiny(nazev As String, maxVzdalenost As Integer, polozkyRok1 As IXMLDOMNodeList, polozkyRok2 As IXMLDOMNodeList, rok1 As String, rok2 As String)
    Dim zpracovano2() As Boolean
    Dim radek As Integer, maxRadku As Integer
    Dim t As Table, rng As Range, radekAdd As Integer
    Dim ar(50, 3) As String 'dostate|fffd|n|fffd| velk|fffd| pole, a|fffd| se mi to tam v|fffd|e vejde, nechci pou|fffd||fffd|vat redim
    Dim lzePridat As Boolean
    
    
    If polozkyRok2.Length > 0 Then
        ReDim zpracovano(1 To polozkyRok2.Length) As Boolean
    End If
    
    maxRadku = 5
    radek = 1
    radekAdd = 0
    For i = 1 To polozkyRok1.Length
        lzePridat = False
        If radek <= maxRadku Then
            Call zalozky.nahradTextZalozky(nazev + "Nazev_" + CStr(radek), polozkyRok1.item(i - 1).SelectSingleNode("NAZEV").text)
            Call zalozky.nahradTextZalozky(nazev + "Pocet_rok1_" + CStr(radek), formatovani.oddeleneTisice(polozkyRok1.item(i - 1).SelectSingleNode("POCET").text))
        ElseIf radekAdd < UBound(ar, 1) Then
            radekAdd = radekAdd + 1
            ar(radekAdd, 1) = polozkyRok1.item(i - 1).SelectSingleNode("NAZEV").text
            ar(radekAdd, 2) = formatovani.oddeleneTisice(polozkyRok1.item(i - 1).SelectSingleNode("POCET").text)
            lzePridat = True
        End If
        
        For j = 1 To polozkyRok2.Length
            If Podobnost.jsouStejne(polozkyRok1.item(i - 1).SelectSingleNode("NAZEV").text, polozkyRok2.item(j - 1).SelectSingleNode("NAZEV").text, maxVzdalenost) _
            And Not zpracovano(j) Then
                zpracovano(j) = True
                If radek <= maxRadku Then
                    Call zalozky.nahradTextZalozky(nazev + "Pocet_rok2_" + CStr(radek), formatovani.oddeleneTisice(polozkyRok2.item(j - 1).SelectSingleNode("POCET").text))
                ElseIf lzePridat Then 'tady nem|fffd||fffd|u pou|fffd||fffd|t ubound a radekAdd, proto|fffd|e v nich nen|fffd| informace o tom, jestli jsem p|fffd|idal v t|fffd|to iteraci do ar
                    ar(radekAdd, 3) = formatovani.oddeleneTisice(polozkyRok2.item(j - 1).SelectSingleNode("POCET").text)
                End If
            End If
        Next j
        radek = radek + 1
    Next i
    
    For i = 1 To polozkyRok2.Length
        If radek <= maxRadku Then
            If Not zpracovano(i) Then
                Call zalozky.nahradTextZalozky(nazev + "Nazev_" + CStr(radek), polozkyRok2.item(i - 1).SelectSingleNode("NAZEV").text)
                Call zalozky.nahradTextZalozky(nazev + "Pocet_rok2_" + CStr(radek), formatovani.oddeleneTisice(polozkyRok2.item(i - 1).SelectSingleNode("POCET").text))
                radek = radek + 1
            End If
        ElseIf radekAdd < UBound(ar, 1) Then
            If Not zpracovano(i) Then
                radekAdd = radekAdd + 1
                ar(radekAdd, 1) = polozkyRok2.item(i - 1).SelectSingleNode("NAZEV").text
                ar(radekAdd, 3) = formatovani.oddeleneTisice(polozkyRok2.item(i - 1).SelectSingleNode("POCET").text)
            End If
        End If
    Next i
    
    If radekAdd > 0 Then
        If nazev = "itZvirata" Then
            Call pridejTextPredZalozku("FinNefinAddendum1End", "Ad VII. Dopl|fffd|uj|fffd|c|fffd| |fffd|daje - zem|fffd|d|fffd|lci, |fffd|ivo|fffd|i|fffd|n|fffd| v|fffd|roba")
            Set rng = zalozky.rangePredZalozkou("FinNefinAddendum1End")
            Set t = Tabulky.vlozTabulkuZivocisna(rng, radekAdd)
        Else 'plodiny
            Call pridejTextPredZalozku("FinNefinAddendum1End", "Ad VII. Dopl|fffd|uj|fffd|c|fffd| |fffd|daje - zem|fffd|d|fffd|lci, Rostlinn|fffd| v|fffd|roba")
            Set rng = zalozky.rangePredZalozkou("FinNefinAddendum1End")
            Set t = Tabulky.vlozTabulkuRostlinna(rng, radekAdd)
        End If
        
        t.Cell(1, 2).Range = "rok: " + rok1
        t.Cell(1, 3).Range = "rok: " + rok2
        
        For i = 1 To radekAdd
            t.Cell(i + 1, 1).Range = ar(i, 1)
            t.Cell(i + 1, 2).Range = ar(i, 2)
            t.Cell(i + 1, 5).Range = ar(i, 3)
        Next i
    End If
    
End Function

