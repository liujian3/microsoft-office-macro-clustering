Public Function nastavPopisProduktu()
    'Nadpis |fffd||fffd|dosti
    Dim str(10) As String
    Dim n As Integer
    Dim zadost As Boolean
    
    zadost = True
    n = 1
    If ThisDocument.ChkPrOstatni.Value Then
