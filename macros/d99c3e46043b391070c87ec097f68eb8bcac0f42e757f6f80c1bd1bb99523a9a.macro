Public Function promaz(nazevZalozek As String)
    Dim r As Range
    Set r = rangeZalozek(Chovani.vratZalozky, nazevZalozek)
    r.Delete
End Function


Public Function nahradTextZalozkyBunku(zalozka As String, text As String)
    Dim r As Range
    Set r = najdiZalozku(zalozka)
    r.Expand (wdCell)
    r.text = text
End Function

Public Function nahradTextZalozky(zalozka As String, text As String)
    Dim r As Range
    Set r = najdiZalozku(zalozka)
    r.Expand (wdSentence) 'ozna|fffd||fffd| cel|fffd| obsah bu|fffd|ky
    r.text = text
End Function

Public Function nahradTextZalozkyADuplicit(zalozka As String, text As String)
    Dim n As Integer
    Call nahradTextZalozky(zalozka & "_1", text)
    n = 2
    Do While ThisDocument.Bookmarks.Exists(zalozka & "_" & n)
        Call nahradTextZalozky(zalozka & "_" & n, text)
        n = n + 1
    Loop
End Function

Public Function nahradTextEnclZalozky(zalozka As String, text As String)
    'nelze jen nahradit, mus|fffd| se znovu vytvorit
    Dim rng As Range
    Set rng = ThisDocument.Bookmarks(zalozka).Range
    rng.text = text
    ThisDocument.Bookmarks.Add name:=zalozka, Range:=rng
End Function

Public Function nahradTextEnclZalozkyADuplicit(zalozka As String, text As String)
    Dim n As Integer
    Dim rng  As Range
    n = 0
    Do While ThisDocument.Bookmarks.Exists(zalozka & n)
        Call nahradTextEnclZalozky(zalozka & n, text)
        n = n + 1
    Loop
End Function

Public Function najdiZalozku(zalozka As String) As Range
    If Not ThisDocument.Bookmarks.Exists(zalozka) Then
        MsgBox Prompt:="Chyba 007: Z|fffd|lo|fffd|ka " & zalozka & " nen|fffd| definov|fffd|na", Title:="Chyba"
        End
    Else
        Set najdiZalozku = ThisDocument.Bookmarks(zalozka).Range
    End If
End Function

Public Function rangeMeziZalozkami(zalozkaZacatek, zalozkaKonec) As Range
    Dim msg As String
    Dim Chyba As Boolean
    Chyba = False
    If Not ThisDocument.Bookmarks.Exists(zalozkaZacatek) Then
        msg = Chr(10) & "Lev|fffd| z|fffd|lo|fffd|ka " & zalozkaZacatek & " neexistuje"
        Chyba = True
    End If
    If Not ThisDocument.Bookmarks.Exists(zalozkaKonec) Then
        msg = Chr(10) & "Prav|fffd| z|fffd|lo|fffd|ka " & zalozkaKonec & " neexistuje"
        Chyba = True
    End If
    If Chyba Then
        MsgBox "Chyba 101: " & msg, Title:="Chyba v z|fffd|lo|fffd|k|fffd|ch"
        End
    End If

    Set rangeMeziZalozkami = ThisDocument.Range( _
        ThisDocument.Bookmarks(zalozkaZacatek).Range.Start, _
        ThisDocument.Bookmarks(zalozkaKonec).Range.End _
        )
End Function


Public Function pridejZalozku(ByRef zalozky() As String, ByVal jmeno, leva, prava As String)
    ReDim Preserve zalozky(2, UBound(zalozky, 2) + 1) ' roz|fffd||fffd||fffd|it se zachov|fffd|n|fffd|m dat lze pouze posledn|fffd| dimenze multidim. pole
    radek = UBound(zalozky, 2) - 1
    zalozky(0, radek) = jmeno
    zalozky(1, radek) = leva
    zalozky(2, radek) = prava
End Function

Public Function rangeZalozek(ByRef zalozky() As String, ByVal jmeno As String) As Range
    Dim n, index As Integer
    Dim Chyba As Boolean
    Chyba = True
    For n = LBound(zalozky, 2) To UBound(zalozky, 2)
        If jmeno = zalozky(0, n) Then
            index = n
            Chyba = False
        End If
    Next n
    If Chyba Then
        MsgBox "Chyba 020: Z|fffd|lo|fffd|ka se jm|fffd|nem " & jmeno & " neexistuje", Title:=Chyba
        End
    Else
      Set rangeZalozek = rangeMeziZalozkami(zalozky(1, index), zalozky(2, index))
    End If
End Function

Public Function nastavHodnotuSeznamu(n As Integer, rng As Range, lt As ListTemplate)
    With lt.ListLevels(1)
