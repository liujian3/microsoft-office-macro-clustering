    Else
        Call zalozky.nahradTextEnclZalozky("encNadpis", "|fffd||fffd|dost o produkt")
    End If
    
    '|fffd|adatel o ...
    n = 1
    With ThisDocument
        If .ChkPrOstatni.Value Or .ChkPrZT Or .ChkPrOtocka Then
            str(n) = "o |fffd|v|fffd|r"
            n = n + 1
        End If
        If .ChkPrZmena.Value Then
            str(n) = "o zm|fffd|nu |fffd|v|fffd|ru"
            n = n + 1
        End If
        If .ChkPrBZ.Value Then
            str(n) = "o bankovn|fffd| z|fffd|ruku/r|fffd|mec"
            n = n + 1
        End If
        If .ChkPrFX.Value Then
            str(n) = "o forward/spot"
            n = n + 1
        End If
        If .ChkPrZajem.Value Then
            str(n) = "o zpracov|fffd|n|fffd| nab|fffd|dky financov|fffd|n|fffd|"
            n = n + 1
        End If
        If .ChkPrNUZ.Value Then
            str(n) = "o ne|fffd||fffd|elov|fffd| zaji|fffd|t|fffd|n|fffd| |fffd|v|fffd|r"
            n = n + 1
        End If
    End With
    
    txt = ""
    For i = 1 To n - 1
        If i = n - 1 And i > 1 Then
            txt = txt & " a "
        ElseIf i > 1 Then
            txt = txt & ", "
        End If
        txt = txt + str(i)
    Next i
    
    If n > 1 Then
