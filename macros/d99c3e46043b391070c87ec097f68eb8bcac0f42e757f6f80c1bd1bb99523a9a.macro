Private Function ciziZdroje(nodes As IXMLDOMNodeList)
    Dim max As Integer, n As Integer, rng As Range, zbyvajiciSl As Integer, pocetSl As Integer, t As Table, cast As Integer, maxSl As Integer
    
    max = 3
    Call nahradTextPole("itCiziNazev", "POSKYTOVATEL", max, nodes)
    Call nahradTextPole("itCiziVyse", "VYSE", max, nodes, True)
    Call nahradTextPole("itCiziZustatek", "ZUSTATEK", max, nodes, True)
    Call nahradTextPole("itCiziZjisteno", "ZJISTENOKDATU", max, nodes)
    n = 1
    For Each node In nodes
        If n <= max Then
            Call zalozky.nahradTextZalozky("itCiziProdukt_" + CStr(n), typProduktu(node.SelectSingleNode("PRODUKT").text))
            If node.SelectSingleNode("UROK").text <> "" Then
                Call zalozky.nahradTextZalozky("itCiziUrok_" + CStr(n), node.SelectSingleNode("UROK").text + "%")
            End If
            Call zalozky.nahradTextZalozky("itCiziPeriodaSplatek_" + CStr(n), periodaSplatek(node.SelectSingleNode("PERIODA").text))
        End If
        n = n + 1
    Next node
    Call nahradTextPole("itCiziPodpis", "DATUMPODPISUSMLOUVY", max, nodes)
    Call nahradTextPole("itCiziSplatnost", "DATUMSPLATNOSTI", max, nodes)
    Call nahradTextPole("itCiziUcel", "UCEL", max, nodes)
    Call nahradTextPole("itCiziSplatka", "SPLATKA", max, nodes, True)
    Call nahradTextPole("itCiziZajisteni", "ZAJISTENI", max, nodes)
    
    zbyvajiciSl = nodes.Length - max
    maxSl = 5
    cast = 1
    n = max
    
    While zbyvajiciSl > 0
        If cast > 1 Then
            Set rng = zalozky.rangePredZalozkou("FinNefinAddendum2End")
            rng.InsertBreak (wdPageBreak)
        End If
    
        If cast > 1 Or zbyvajiciSl > maxSl Then
            Call pridejTextPredZalozku("FinNefinAddendum2End", "Ad V. Ciz|fffd| zdroje, |fffd||fffd|st " + CStr(cast))
        Else
            Call pridejTextPredZalozku("FinNefinAddendum2End", "Ad V. Ciz|fffd| zdroje")
        End If
            
        If zbyvajiciSl > maxSl Then
            pocetSl = maxSl
        Else
            pocetSl = zbyvajiciSl
        End If
        
        Set rng = zalozky.rangePredZalozkou("FinNefinAddendum2End")
        
        Set t = Tabulky.vlozTabulkuCiziZdroje(rng, pocetSl)
        Call Tabulky.importujDataDoTabulkyHor(t, 2, "POSKYTOVATEL", n, pocetSl, nodes)
        Call Tabulky.importujDataDoTabulkyHor(t, 3, "VYSE", n, pocetSl, nodes, "tisice")
        Call Tabulky.importujDataDoTabulkyHor(t, 4, "ZUSTATEK", n, pocetSl, nodes, "tisice")
        Call Tabulky.importujDataDoTabulkyHor(t, 5, "ZJISTENOKDATU", n, pocetSl, nodes)
        Call Tabulky.importujDataDoTabulkyHor(t, 6, "PRODUKT", n, pocetSl, nodes, "typProduktu")
        Call Tabulky.importujDataDoTabulkyHor(t, 7, "DATUMPODPISUSMLOUVY", n, pocetSl, nodes)
        Call Tabulky.importujDataDoTabulkyHor(t, 8, "DATUMSPLATNOSTI", n, pocetSl, nodes)
        Call Tabulky.importujDataDoTabulkyHor(t, 9, "UROK", n, pocetSl, nodes, "procenta")
        Call Tabulky.importujDataDoTabulkyHor(t, 10, "UCEL", n, pocetSl, nodes)
        Call Tabulky.importujDataDoTabulkyHor(t, 11, "SPLATKA", n, pocetSl, nodes)
        Call Tabulky.importujDataDoTabulkyHor(t, 12, "PERIODA", n, pocetSl, nodes, "periodaSplatek")
        Call Tabulky.importujDataDoTabulkyHor(t, 13, "ZAJISTENI", n, pocetSl, nodes)
        
        For i = 1 To pocetSl
            n = n + 1 ' spr|fffd|vn|fffd| nadep|fffd|u z|fffd|hlav|fffd| a z|fffd|rove|fffd| mi to i posouv|fffd| index pro pou|fffd|it|fffd| v dal|fffd||fffd| "|fffd||fffd|sti" tabulky
            t.Cell(1, i + 1).Range = CStr(n) + "."
        Next i
        
        cast = cast + 1
        zbyvajiciSl = zbyvajiciSl - pocetSl
    Wend
End Function

