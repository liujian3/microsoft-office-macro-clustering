Public Function oddeleneTisice(cislo As String, Optional oddelovac As String = " ", Optional desOddelovac As String = ",") As String
    Dim pole() As String
    Dim predCarkou As String, zaCarkou As String
    
    'rozd|fffd|l na dv|fffd| |fffd||fffd|sti, pokud m|fffd| desetinn|fffd| odd|fffd|lova|fffd|
    If InStr(1, cislo, desOddelovac) <> 0 Then
        pole = Split(cislo, desOddelovac)
        predCarkou = pole(0)
        zaCarkou = pole(1)
    Else
        predCarkou = cislo
        zaCarkou = ""
    End If
    
    predCarkou = rozdelNaSkupiny(predCarkou, 3, oddelovac, False)
    zaCarkou = rozdelNaSkupiny(zaCarkou, 3, oddelovac, True)
    
    If zaCarkou <> "" Then
        oddeleneTisice = predCarkou + desOddelovac + zaCarkou
    Else
        oddeleneTisice = predCarkou
    End If
End Function

Private Function rozdelNaSkupiny(txt As String, delkaSkupiny As Integer, oddelovac As String, zleva As Boolean)
    If delkaSkupiny <= 0 Then
        delkaSkupiny = 3
    End If

    Dim pocetSkupin As Integer
    Dim i As Integer
    Dim tmp As String
    Dim lft As Integer
    
    If zleva Then
        lft = 1
    Else
        lft = Len(txt) Mod delkaSkupiny
        tmp = left(txt, lft)
        lft = lft + 1
    End If
    
    While lft < Len(txt)
        If lft = 1 Then
            tmp = Mid(txt, lft, delkaSkupiny)
        Else
            tmp = tmp + oddelovac + Mid(txt, lft, delkaSkupiny)
        End If
        lft = lft + delkaSkupiny
    Wend
    
    rozdelNaSkupiny = tmp
End Function
Attribute VB_Name = "NahravaniDat"
Sub VyplnFormular(schranka As String)

    'Dim tbl As Table
    'Set tbl = ThisDocument.Tables(1)
    'tbl.Select
    
    Dim bDebug As Boolean
        
    bDebug = False
        
    Dim polozky() As String
    
    Dim info() As String
    Dim delka As Integer
    Dim zprava As String
    
    Dim rng As Range
    Dim rng2 As Range
    
    'Na|fffd|ten|fffd| dat ze schr|fffd|nky
    
    'Ostran|fffd|n|fffd| od|fffd||fffd|dkov|fffd|n|fffd| - koliduje s operac|fffd| split
    schranka = Replace(schranka, vbCr, "")
    schranka = Replace(schranka, vbLf, "")
    schranka = Replace(schranka, vbCrLf, "")
     
    'MsgBox schranka, Title:="Obsah schr|fffd|nky"
    polozky = Split(schranka, ";")
    
    'Info o osob|fffd|
    info = Split(polozky(0), ":")
    If UBound(info) < 1 Then
        MsgBox "Chyba 008: Datov|fffd| v|fffd|ta neobsahuje informace o PO, FOP", Title:="Chyba"
        End
    End If
        
    'Skryt|fffd| neplatn|fffd|ch osob
    If info(1) = "PO" Then
        Chovani.zobrazPO
    ElseIf info(1) = "FOP" Then
        Chovani.zobrazFOP
    ElseIf info(1) = "Mun" Then
        Chovani.zobrazMun
    Else
        MsgBox "Chyba 005: Nespr|fffd|vn|fffd| data: " & polozky(0)
        End
    End If
    
    'Prvn|fffd| polo|fffd|ka mi jen |fffd|ekne jestli je to PO nebo FOP
    'Posledn|fffd| polo|fffd|ka tak|fffd| kon|fffd||fffd| st|fffd|edn|fffd|kem, za n|fffd| u|fffd| nic nen|fffd|
    For i = LBound(polozky) + 1 To UBound(polozky) - 1
        info = Split(polozky(i), ":")
        delka = UBound(info) - LBound(info) + 1
        If delka = 5 Then
            'Pole
            If info(0) = "POLE" Then
                'Funkce zap|fffd|e prvky v poli do tabulky, vr|fffd|t|fffd| zpr|fffd|vu o p|fffd|ete|fffd|en|fffd|, pokud k n|fffd|mu do|fffd|lo
                zprava = nahradTextPole(polozky, i, info, zprava)
                'P|fffd|esko|fffd| prvky, kter|fffd| nastavilo pole: i += pocetPrvku*rozmerPrvku
                i = i + info(4) * info(2)
            Else
                MsgBox "Chyba 003: Nespr|fffd|vn|fffd| data: " & i & ". " & polozky(i), Title:="Chyba"
                End
            End If
        ElseIf delka = 2 Then
            'Zaps|fffd|n|fffd| dat
            Call nahradTextZalozky(info(0), info(1))
        Else '|fffd|patn|fffd| zadan|fffd| data, form|fffd|t m|fffd| b|fffd|t Z|fffd|lo|fffd|ka:Hodnota
            MsgBox "Chyba 002: Nespr|fffd|vn|fffd| data: " & i & ". " & polozky(i), Title:="Chyba"
            End
        End If
        
        strg = strg & vbNewLine & "|fffd||fffd|st |fffd|. " & i & ": " & polozky(i) & " -> " & LBound(info) & " - " & UBound(info) & ", " & delka
    Next i
    
    If bDebug Then MsgBox strg
    'Pokud se data neve|fffd|la do tabulky, zobraz zpr|fffd|vu o p|fffd|ete|fffd|en|fffd|
    If Len(zprava) > 0 Then _
        MsgBox "N|fffd|kter|fffd| data se neve|fffd|la do p|fffd|eddefinovan|fffd|ch tabulek. Zkontrolujte, pros|fffd|m, spr|fffd|vnost zad|fffd|n|fffd|: " & Chr(10) & zprava, Title:="Data se nevejdou do tabulky"
End Sub


Private Function nahradTextPole(ByRef data() As String, ByVal offset As Integer, ByVal poleInfo, pretecenaData As String) As String
    Dim zacatekZpravy As Boolean
    Dim info() As String
    
    Dim nazev As String
    Dim pocetPrvku, rozmerPrvku, kapacita As Integer
    
    'Na|fffd|ti data o poli
    info = Split(poleInfo, ":")
    nazev = info(1) 'N|fffd|zev tabulky, pou|fffd||fffd|v|fffd| se jen pro hl|fffd|en|fffd| chyb
    rozmerPrvku = info(2) 'Kolik textov|fffd|ch pol|fffd| je v jednom prvku, kolik sloupc|fffd| v jednom |fffd||fffd|dku (nebo obr|fffd|cen|fffd|)
    kapacita = info(3) 'Rozm|fffd|r tabulky v dokumentu, maxim|fffd|ln|fffd| po|fffd|et prvk|fffd|, kter|fffd| se tam vejdou
    pocetPrvku = info(4) 'po|fffd|et |fffd||fffd|dk|fffd|/sloupc|fffd|
    
    zacatekZpravy = True
    For j = 1 To pocetPrvku 'po|fffd|et |fffd||fffd|dk|fffd|/sloupc|fffd|
        For k = 1 To rozmerPrvku 'po|fffd|et element|fffd| na |fffd||fffd|dku
            i = offset + (j - 1) * rozmerPrvku + k
            info = Split(polozky(i), ":")
            delka = UBound(info) - LBound(info) + 1
            If delka = 2 Then
                If j > kapacita Then 'nevejde se do tabulky
                    'Zapi|fffd| do zpr|fffd|vy pro u|fffd|ivatele
                    If zacatekZpravy Then
                        zprava = zprava & Chr(10) & "Tabulka " & nazev & Chr(10)
                        zacatekZpravy = False
                    End If
                    zprava = zprava & info(1)
                Else 'Vejde se do tabulky
                    Call nahradTextZalozky(info(0) & j, info(1))
                End If
            Else ' |fffd|patn|fffd| zadan|fffd| data, form|fffd|t m|fffd| b|fffd|t Z|fffd|lo|fffd|ka:hodnota
                MsgBox "Chyba 004: Nespr|fffd|vn|fffd| data pro pole " & nazev & ": " & i & ". " & polozky(i)
            End If
            'P|fffd|idej odd|fffd|lova|fffd| do zpr|fffd|vy o p|fffd|ete|fffd|en|fffd| dat mimo tabulku
            If (Not zacatekZpravy) And k < rozmerPrvku Then zprava = zprava & ", "
        Next k
        If Not zacatekZpravy Then zprava = zprava & Chr(10)
    Next j
    nahradTextPole = pretecenaData
End Function


