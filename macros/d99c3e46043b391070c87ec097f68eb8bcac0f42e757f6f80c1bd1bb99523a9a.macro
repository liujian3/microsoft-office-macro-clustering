Public Function vbaHash()
    Dim vbComp As Object
    Dim str As String
    Dim hash As Long
    
    str = "Hashe pro jednotliv|fffd| moduly:" & vbNewLine
    For Each vbComp In ThisDocument.VBProject.VBComponents
        str = str & vbNewLine
        With vbComp
            If .Type = 100 Then 'Vlastn|fffd| dokument
                str = str & .name & ": "
                hash = smallHash(CStr(.codemodule.Lines(1, .codemodule.countoflines)))
                str = str & CStr(hash)
            Else   'Vlo|fffd|en|fffd| moduly
                str = str & .name & ": "
                hash = smallHash(CStr(.codemodule.Lines(1, .codemodule.countoflines)))
                str = str & CStr(hash) 'Hashuje se v|fffd|etn|fffd| tohoto modulu
            End If
        End With
    Next vbComp
    MsgBox (str)
End Function

Public Function smallHash(ByRef x As String) As Long
    Dim h As Long
    Dim a As Long
    Dim p As Long
    Dim tmp As Long
    Dim ar() As String

    Dim t As Long
    't = 2147483647 'maxim|fffd|ln|fffd| hodnota pro long, jen pro orientaci
    a = 32
    ReDim ar(Len(x) - 1)
    ar = rozdelRetezec(x)
    p = 49979693 'Prvo|fffd||fffd|slo, mus|fffd| platit a*p+256 < t
    h = Asc(ar(1))
    For i = 2 To Len(x) - 1
        tmp = Asc(ar(i)) 'P|fffd|eve|fffd| ascii na |fffd||fffd|slo
        h = (h * a + tmp) Mod p
    Next i
    smallHash = h
End Function

Public Function rozdelRetezec(retezec As String) As String()
    Dim buff() As String
    ReDim buff(Len(retezec) - 1)
    For i = 1 To Len(retezec)
        buff(i - 1) = Mid$(retezec, i, 1)
    Next
    rozdelRetezec = buff
End Function
Attribute VB_Name = "Podobnost"
Private Const eta As Integer = 26

