Attribute VB_Name = "Checkboxy"
Private Function nastavVysku(name As String, vyska As Double)
    Dim chk As InlineShape
    Dim delka As Integer
    delka = Len(name)
    For Each chk In ThisDocument.InlineShapes
        If chk.OLEFormat.ProgID = "Forms.CheckBox.1" Then 'Ur|fffd|uje typ prvku
            If left(chk.OLEFormat.Object.name, delka) = name Then 'Fragment n|fffd|zvu souhlas|fffd|
                chk.Height = vyska
            End If
        End If
    Next
End Function

