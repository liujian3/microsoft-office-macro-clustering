        str(n) = "o |fffd|v|fffd|r na zem|fffd|d|fffd|lskou techniku/r|fffd|mec pro zem|fffd|d|fffd|lskou techniku"
        n = n + 1
    End If
    
    If ThisDocument.ChkPrOtocka.Value Then
