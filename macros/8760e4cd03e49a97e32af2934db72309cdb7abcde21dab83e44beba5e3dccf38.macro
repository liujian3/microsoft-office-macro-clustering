Private Sub Workbook_BeforeSave(ByVal SaveAsUI As Boolean, Cancel As Boolean)

'If the six specified cells do not contain data, then display a message box with an error
   'and cancel the attempt to save.
   If WorksheetFunction.Sum(Worksheets("DATA").Range("AJ1")) = 0 Then
      MsgBox "ERROR: The changes made to this Workbook have NOT been Saved." & vbCrLf & _
      "Data is required for all Cells with red formatting and this is currently incomplete." & vbCrLf & _
      "Please recify the issue and then try again."
      Cancel = True
   End If
End Sub



