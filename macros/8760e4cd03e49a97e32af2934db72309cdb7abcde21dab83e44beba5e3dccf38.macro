Attribute VB_Name = "Module5"
Sub ChooseSchool()
Attribute ChooseSchool.VB_Description = "Macro recorded 30/01/2007 by Dave Austin"
Attribute ChooseSchool.VB_ProcData.VB_Invoke_Func = " \n14"
'
' ChooseSchool Macro
' Macro recorded 30/01/2007 by Dave Austin
'
Dim SH As String
Dim USERVALUE As String
start:
USERVALUE = InputBox("Please enter your Schools 4 Digit DFE Number" & vbCrLf & _
vbCrLf & "(If you do not know the DFE Number, please type CHECK and you will be directed to a School List)", "Business Support")
If USERVALUE = "" Then Exit Sub

If USERVALUE = "CHECK" Then GoTo check:
If USERVALUE = "check" Then GoTo check:
ActiveWorkbook.Unprotect "ANGEL"
Sheets("DFES").Visible = True
Sheets("DFES").Select
Range("E1").Value = USERVALUE
Sheets("DFES").Visible = False
Sheets("Start").Select
ActiveWorkbook.Protect "ANGEL"
If Range("D1") = "0" Then GoTo error
If Range("D1") <> "0" Then GoTo DONE:
check:
ActiveWorkbook.Unprotect "ANGEL"
Sheets("School List").Visible = True
Sheets("Start").Select
Sheets("Start").Visible = False
Sheets("School List").Select
SH = MsgBox("This is a List of Birmingham Schools sorted into alphabetical order." & vbCrLf & _
vbCrLf & "Click OK and then familiarise yourself with the appropriate DFE Number." & vbCrLf & _
vbCrLf & "Then Left Click on the Choose button in the top left hand corner of this Page.", vbInformation + vbOKOnly, "Business Support")
ActiveWorkbook.Protect "ANGEL"
Exit Sub
error:
SH = MsgBox("The DFE Number has not been recognised." & vbCrLf & _
"Please try again.", vbCritical + vbOKOnly, "Business Support")

GoTo start:

Exit Sub


DONE:
If Range("G1") = "Y" Then GoTo FINAL:
If Range("G1") <> "Y" Then GoTo ERROR2:
Exit Sub

ERROR2:
SH = MsgBox("According to our records this School does not have an Open Credit Arrangement.  Therefore no action is required." & vbCrLf & _
"However if you believe this judgement to be incorrect, then please email the Mailbox above with the appropriate information.", vbCritical + vbOKOnly, "Business Support")

Exit Sub

FINAL:
ActiveWorkbook.Unprotect "ANGEL"
Sheets("Data").Visible = True
Sheets("Start").Visible = False
Sheets("Data").Select
ActiveWorkbook.Protect "ANGEL"
End Sub

