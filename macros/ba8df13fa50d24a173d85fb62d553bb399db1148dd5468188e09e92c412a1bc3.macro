            With FormCheesV_stolikLE
               Rem .Checkbox_CheesV_0500.Value = False
               Rem .Checkbox_CheesV_0500.Enabled = False
            End With
        End If
        Checkbox_CheesV_0050.Font.Bold = True
    Else
        checkbox_nazov.Font.Bold = False
        Call odpocitaj_cenu(checkbox_nazov, textcena_nazov)
        
