Sub ChangeLanguage(meno_formulara)
    Dim ctl As Control
    Dim Cap As String
    
    For Each ctl In meno_formulara.Controls
        
        If HasCaption(ctl) Then
            Cap = Translate(ctl.Name, UserLanguage)
            If Cap <> "" Then ctl.Caption = Cap
        End If
        ctl.BackColor = RGB(240, 240, 240)
        
        '-------------------------------
        'Funkcia "MID" hladaj |fffd|as|fffd| textu zo za|fffd|iatku, konca alebo uprostredtextu (1- zaciatok z lava, 13 pocet znakov)
        'MyString = "Uk|fffd|zka funkce Mid", Vytvo|fffd|en|fffd| textov|fffd|ho |fffd|et|fffd|zce: FirstWord = Mid(MyString, 1, 3)
        ' Vr|fffd|t|fffd| |fffd|et|fffd|zec "Uk|fffd|zka": 'LastWord = Mid(MyString, 14, 4),  Vr|fffd|t|fffd| |fffd|et|fffd|zec "Mid": MidWords = Mid(MyString, 5)
        'zabarvenie textovych vstupnych poli vo formulari na bielo
        If Mid(ctl.Name, 1, 14) = "Input_pozicia_" Then ctl.BackColor = RGB(255, 255, 255)
        If ctl.Name = "Text_Pocks" Or _
           ctl.Name = "text_zlava_supravy" Or _
           ctl.Name = "text_zlava_naradie" Or _
           ctl.Name = "FormPoznamka_TextBox1" Or _
           ctl.Name = "Text_koeficient_prepoctu" Then
            ctl.BackColor = RGB(255, 255, 255)
        End If
        
        If ctl.Name = "CmdCheesV_Poznamka" Then ctl.BackColor = &HC0E0FF
        
       Rem If Mid(ctl.Name, 1, 10) = "Text_cena_" Then MsgBox ctl.Value
        
      
        '------------------------------------------------
    Next ctl
    
    
    'Update the caption
    Rem Cap = APPNAME & " - Step " _
      & MultiPage1.Value + 1 & " of " _
      & MultiPage1.Pages.Count
    Rem FormSupravy.Caption = Translate(Cap, UserLanguage)
    
    
 End Sub
