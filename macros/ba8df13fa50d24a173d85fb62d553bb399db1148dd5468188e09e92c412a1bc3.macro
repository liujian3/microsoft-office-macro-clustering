    If Typ_supravy_ImpExp = "CModul" Then FormSupravy.CmdCModul = True
    If Typ_supravy_ImpExp = "CMini04" Then FormSupravy.CmdCMini04 = True
    If kreslo_z_hlavneho_panela = True Then FormSupravy.CmdKreslo = True
    If dennar_z_hlavneho_panela = True Then FormSupravy.CmdDennar = True
    
    FormVyberEcennik.Hide
    
    '------SPUST CMD CENNIK-------------------------------------
    FormSupravy.CmdCennik = True
    '-------------------------------------------
   If a > 0 Then zamok = False
   If a = 0 Then zamok = True
    
    If Nastav = False Then
        Citac = 90 'Call Module5.Nastav_externy_cennik
        FormPleaseWait.Show
        Nastav = True
    End If
    
    Citac = 91 'Call Module5.spracuj_externy_cennik
    FormVyberEcennik.Hide
    FormPleaseWait.Show
    
    If kreslo_z_hlavneho_panela = False And dennar_z_hlavneho_panela = False Then
        FormCheesV_zaklad.CmdCheesV_cancel.Value = True
        Unload FormCheesV_zaklad
        If a > 0 Then GoTo dalsiaSuprava
        Rem FormCheesV_zaklad.CmdCheesV_cancel.Tag = "cancel"
    End If
    
    
    If kreslo_z_hlavneho_panela = True Then
        kreslo_koniec = True
        If a > 0 Then GoTo dalsiaSuprava
        Rem FormKreslo.CmdKreslo_cancel = True
    End If
    
    If dennar_z_hlavneho_panela = True Then
        FormDennar.CmdDennar_cancel.Tag = "cancel"
        If a > 0 Then GoTo dalsiaSuprava
        Rem FormDennar.CmdDennar_cancel = True
    End If
    
    Call Module5.procedure_export_ecennik
    Call Module5.Procedure_vycisti_ecennik
    

