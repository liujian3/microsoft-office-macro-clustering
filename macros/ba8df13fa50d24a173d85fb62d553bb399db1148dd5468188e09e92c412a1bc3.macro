        typ_supravy = "CMini04"
        Typ_supravy_ImpExp = "CMini04"
        Checkbox_CMINI4.Value = False
        GoTo preskoc
    End If
    
    
    If Checkbox_CModul.Value = True Then
        typ_supravy = "CModul"
        Typ_supravy_ImpExp = "CModul"
        Checkbox_CModul.Value = False
        GoTo preskoc
    End If
    
    If Checkbox_Kreslo.Value = True Then
        typ_kresla = "Sk101"
        kreslo_z_hlavneho_panela = True
        Checkbox_Kreslo.Value = False
        GoTo preskoc
    End If
