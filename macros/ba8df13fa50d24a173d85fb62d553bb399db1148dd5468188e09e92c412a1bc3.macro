Function WorkbookIsActive()
'   Vr|fffd|ti hodnotu true ak je aktivny workbook
    Dim X As String
    On Error Resume Next
    X = ActiveWorkbook.Name
    If Err = 0 Then WorkbookIsActive = True Else WorkbookIsActive = False
End Function

