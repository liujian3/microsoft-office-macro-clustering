Public Function Box(Left As Single, Top As Single, Width As Single, Height As Single) As Shape
'
' Pridajte do v|fffd|kresu r|fffd|m|fffd|ek
'
    On Error GoTo ErrBox
    Set Box = m_shtHolder.Shapes.AddShape(msoShapeRectangle, Left, Top, Width, Height)
    Exit Function
ErrBox:
    Set Box = Nothing
    Exit Function
End Function
Public Sub CleanCanvas()
'
' Vyma|fffd|te u|fffd||fffd|vate|fffd|sk|fffd| aj do|fffd|asn|fffd| pracovn|fffd| h|fffd|rok
'
    On Error Resume Next
    m_ClearHolder
    WipeCanvas
End Sub

Public Function Line(X1 As Single, Y1 As Single, X2 As Single, Y2 As Single) As Shape
'
' Pridajte do v|fffd|kresu riadok
' vzdy kreslite |fffd|cary s X2 a Y2 najv|fffd||fffd||fffd|mi
'
    Dim sngX1 As Single
    Dim sngY1 As Single
    Dim sngX2 As Single
    Dim sngY2 As Single
    
    On Error GoTo ErrLine
    sngX1 = X1
    If X2 < sngX1 Then
        sngX1 = X2
        sngX2 = X1
    Else
        sngX2 = X2
    End If
    sngY1 = Y1
    If Y2 < sngY1 Then
        sngY1 = Y2
        sngY2 = Y1
    Else
        sngY2 = Y2
    End If
    
    Set Line = m_shtHolder.Shapes.AddLine(X1, Y1, X2, Y2)
    Exit Function
ErrLine:
    Set Line = Nothing
    Exit Function
End Function


Public Property Set CanvasGeneric(MyCanvas As Object)
'
' Vytvorte ukazovate|fffd| na objekt
' to umo|fffd||fffd|uje pou|fffd|i|fffd| ak|fffd|ko|fffd|vek kontrolu, ktor|fffd| m|fffd| vlastnos|fffd| obr|fffd|zka
' nastavi|fffd| ve|fffd|kos|fffd| a zarovnanie obr|fffd|zka
'
    On Error GoTo ErrCanvas
    m_blnUseUF = False
    Set m_objCanvas = MyCanvas
    m_objCanvas.PictureSizeMode = fmPictureSizeModeClip
    m_objCanvas.PictureAlignment = fmPictureAlignmentTopLeft
ErrCanvas:
    Exit Property
End Property
Public Property Set CanvasUserform(MyUserform As MSForms.UserForm)
'
' Nastav kam sa ma skopirovat obrazok
' nastavi|fffd| ve|fffd|kos|fffd| a zarovnanie obr|fffd|zka
'
    
    On Error GoTo ErrUFCanvas
    m_blnUseUF = True
    
    If nazov_formu = "FormFarby" Then Set m_frmUFCanvas = FormFarbyZS
    If nazov_formu = "FormFarbyST" Then Set m_frmUFCanvas = FormFarbyST
    m_frmUFCanvas.PictureSizeMode = fmPictureSizeModeZoom
    m_frmUFCanvas.PictureAlignment = fmPictureAlignmentCenter
    Exit Property
ErrUFCanvas:
    Set CanvasUserform = Nothing
    Exit Property
End Property
Public Sub WipeCanvas()
' ciste pl|fffd|tno
    On Error Resume Next
    If m_blnUseUF Then
        m_frmUFCanvas.Picture = LoadPicture("")
    Else
        m_objCanvas.Picture = LoadPicture("")
    End If
End Sub
Private Sub m_ClearHolder()
'
'   Odstrante vsetky tvary z pracovn|fffd|ho harku
'
    On Error GoTo ErrClearHolder
    Do While m_shtHolder.Shapes.Count > 0
        m_shtHolder.Shapes(1).Delete
    Loop
ErrClearHolder:
    Exit Sub
End Sub

Public Sub Paintx()
'
' Zoskupuje v|fffd|etky tvary a kop|fffd|rujte ich do jedneho obr|fffd|zka
'
    Dim shpFrame As Shape
    Dim intIndex As Integer
    
    On Error Resume Next
    Set shpFrame = m_CreateFrame
    
    ReDim vntItems(m_shtHolder.Shapes.Count - 1) As Variant
    For Each shpFrame In m_shtHolder.Shapes
        vntItems(intIndex) = shpFrame.Name
        intIndex = intIndex + 1
    Next
    m_shtHolder.Shapes.Range(vntItems).Group
    
    Set shpFrame = m_shtHolder.Shapes(1)
    Set obrazok_shape_ZS = m_shtHolder.Shapes(1)
Rem shpFrame.CopyPicture xlScreen, xlBitmap
    shpFrame.CopyPicture xlScreen, xlPicture
'-----------------------------------------------------------------------
'------------------------Urci kam sa prilepi obrazok --------------------
    'Prilep obr|fffd|zok zo schr|fffd|nky do n|fffd|ho konkretneho prvku obrazu
    
    
    If m_blnUseUF Then
    
        Rem If nazov_formu = "FormFarby" Then Set m_frmUFCanvas.FormFarbyZS_Image1.Picture = PastePicture(xlBitmap)
        Rem 21_01_2019 If nazov_formu = "FormFarby" Then Set m_frmUFCanvas.FormFarbyZS_Image1.Picture = PastePicture(xlPicture)
    Else
        Rem 21_01_2019 Set m_objCanvas.FormFarbyZS_Image1.Picture = PastePicture(xlBitmap)
    End If
    
    With FormFarbyZS
        .FormFarbyZS_Image1.AutoSize = False
        .FormFarbyZS_Image1.PictureAlignment = fmPictureAlignmentCenter
        .FormFarbyZS_Image1.PictureSizeMode = fmPictureSizeModeZoom
    End With
'-----------------------------------------------------------------------
'-----------------------------------------------------------------------
     
End Sub

