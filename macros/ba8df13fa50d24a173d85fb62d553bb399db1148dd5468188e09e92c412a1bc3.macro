    If FormNoznyOvladac.Checkbox_CheesV_5700.Value = True Then
        Call pripocitaj_cenu(checkbox_nazov, textcena_nazov)
    Else
        checkbox_nazov.Font.Bold = False
        Call odpocitaj_cenu(checkbox_nazov, textcena_nazov)
    End If
    FormNoznyOvladac.Repaint
End Sub

Private Sub pripocitaj_cenu(checkbox_nazov, textcena_nazov)
    fcena = ThisWorkbook.Sheets("Data").Range("cena_vysledna_NoznyOvladac").Value
    fcena = textcena_nazov.Text + fcena
     ThisWorkbook.Sheets("Data").Range("cena_vysledna_NoznyOvladac").Value = fcena
    FormNoznyOvladac.Form_cena_NoznyOvladac.Text = FormatNumber(fcena, , , , vbTrue)
    checkbox_nazov.Font.Bold = True
End Sub
Private Sub odpocitaj_cenu(checkbox_nazov, textcena_nazov)
    fcena = ThisWorkbook.Sheets("Data").Range("cena_vysledna_NoznyOvladac").Value
    fcena = fcena - textcena_nazov.Text
     ThisWorkbook.Sheets("Data").Range("cena_vysledna_NoznyOvladac").Value = fcena
    FormNoznyOvladac.Form_cena_NoznyOvladac.Text = FormatNumber(fcena, , , , vbTrue)
    
End Sub
Private Sub pripocitaj_cenucombo(textcena_nazov, combobox_nazov)
    
    Call Module3.zisti_kod_zvolenej_polozky(combobox_nazov)
    
    'Vyhladaj cenu podla kodu polozky
    fcena1 = Application.WorksheetFunction.VLookup(kodpolozky, ThisWorkbook.Sheets("data").Range("a2:F2000"), 4, False)
    If textcena_nazov.Value = "" Then ' rataj cenu
        fcena2 = 0
    Else
        fcena2 = textcena_nazov
    End If
    textcena_nazov.Value = FormatNumber(fcena1, , , , vbTrue)
    fcena = ThisWorkbook.Sheets("Data").Range("Cena_vysledna_NoznyOvladac").Value
    fcena = textcena_nazov + fcena - fcena2
     ThisWorkbook.Sheets("Data").Range("cena_vysledna_NoznyOvladac").Value = fcena
    FormNoznyOvladac.Form_cena_NoznyOvladac.Text = FormatNumber(fcena, , , , vbTrue)
        
End Sub
Private Sub odpocitaj_cenucombo(textcena_nazov)
    If textcena_nazov.Value = "" Then
        fcena2 = 0
    Else
        fcena2 = textcena_nazov.Value
    End If
    fcena = ThisWorkbook.Sheets("Data").Range("cena_vysledna_NoznyOvladac").Value
    fcena = fcena - fcena2
     ThisWorkbook.Sheets("Data").Range("cena_vysledna_NoznyOvladac").Value = fcena
    FormNoznyOvladac.Form_cena_NoznyOvladac.Text = FormatNumber(fcena, , , , vbTrue)
    textcena_nazov.Value = ""
End Sub

