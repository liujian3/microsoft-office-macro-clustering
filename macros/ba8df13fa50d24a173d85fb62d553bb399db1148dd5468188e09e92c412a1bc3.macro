For Each ctl In meno_formulara.Controls

    If Left(ctl.Name, 5) = "Check" Or _
        Left(ctl.Name, 5) = "check" Or _
        Left(ctl.Name, 5) = "combo" Or _
        Left(ctl.Name, 5) = "combo" Then
       
        If zamkni_frame = True Then ctl.Object.Locked = True
        If zamkni_frame = False Then ctl.Object.Locked = False
        
    End If
  
  Next ctl
End Sub

