        zdrojovy_zosit.Close savechanges:=False
        exit_sub = True
        Exit Sub
    End If
    
    
    
    '---------------Otvorenie zositu---------------------------
    Workbooks.OpenText Filename:=subor
    '----------------------------------------------------------
    
    Rem Application.ScreenUpdating = False
    
    Set zdrojovy_zosit = Application.Workbooks.Open(subor)
    
    sheetname = zdrojovy_zosit.Name
    
    sheetexist = False
    For i = 1 To zdrojovy_zosit.Sheets.Count 'Prehlada nazvy listov otvaraneho zosita
    If Worksheets(i).Name = "Tlac" Then
        sheetexist = True
        
        'copiruj data z importovaneho suboru
        Dim targetSheet As Worksheet
        Set targetSheet = cielovy_zosit.Worksheets("Tlac")
        Dim sourceSheet As Worksheet
        Set sourceSheet = zdrojovy_zosit.Worksheets("Tlac")

        sourceSheet.Range("A1", "E300").Copy
        targetSheet.Range("A1", "E300").PasteSpecial Paste:=xlPasteAll, Operation:=xlNone, SkipBlanks:= _
            False, Transpose:=False
        
        ' Close customer workbook
        zdrojovy_zosit.Close savechanges:=False
        Exit Sub
    End If
    
    Next i
    
End Sub

