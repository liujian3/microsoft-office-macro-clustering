            Dim sourceSheet As Worksheet
            Set sourceSheet = zdrojovy_zosit.Worksheets("UVN")
            
            Dim posledna_bunka1 As Variant
            posledna_bunka = "L" & pocetriadkov_ceny
            posledna_bunka1 = "A" & pocetriadkov_data
            
            Set oblast = ThisWorkbook.Sheets("Ceny").Range("L2:L" & pocetriadkov_ceny)
            Set oblast1 = zdrojovy_zosit.Sheets("UVN").Range("A1:X" & pocetriadkov_data)
            
            
            'Vlozenie vzorca
            For Each bunka1 In oblast
            
                bunka1.Formula = "=VLOOKUP(RC[-11],[ZdrojovaDB_VN.xlsx]UVN!A1:H1000,8,FALSE)"
                bunka1.Formula = Replace(bunka1.Formula, "'", "") 'Odstranenie znaku apostrofu
 
            Next
          
            oblast.Copy
