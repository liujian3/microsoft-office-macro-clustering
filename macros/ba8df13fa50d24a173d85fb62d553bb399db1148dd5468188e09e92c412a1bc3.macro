Private Sub pripocitaj_cenu_stolicka(checkbox_nazov, textcena_nazov, textpock_nazov, labelpocks_nazov)
    
    Call Module3.PocetKusov
    textpock_nazov.Text = pocet_kusov
    
   
    fcena = ThisWorkbook.Sheets("Data").Range("cena_vysledna_kreslo").Value
    fcena = (textcena_nazov.Text * textpock_nazov.Text) + fcena
    
    
     ThisWorkbook.Sheets("Data").Range("cena_vysledna_kreslo").Value = fcena
    FormKreslo.Form_cena_kreslo.Text = FormatNumber(fcena, , , , vbTrue)
    checkbox_nazov.Font.Bold = True
    textpock_nazov.Visible = True
    labelpocks_nazov.Visible = True
    pocet_kusov = Val(0)
    
    If sprava = "exit" Then checkbox_nazov.Value = False
    FormKreslo.Repaint
End Sub
Private Sub odpocitaj_cenu_stolicka(checkbox_nazov, textcena_nazov, textpock_nazov, labelpocks_nazov)
    fcena = ThisWorkbook.Sheets("Data").Range("cena_vysledna_kreslo").Value
    fcena = fcena - (textcena_nazov.Text * textpock_nazov.Text)
   
    '---------------------------------
     ThisWorkbook.Sheets("Data").Range("cena_vysledna_kreslo").Value = fcena
    FormKreslo.Form_cena_kreslo.Text = FormatNumber(fcena, , , , vbTrue)
    textpock_nazov.Visible = False
    labelpocks_nazov.Visible = False
    checkbox_nazov.Font.Bold = False
    pocet_kusov = Val(0)
    FormKreslo.Repaint
End Sub
Private Sub pripocitaj_cenu(checkbox_nazov, textcena_nazov)
    fcena = ThisWorkbook.Sheets("Data").Range("cena_vysledna_kreslo").Value
     fcena = textcena_nazov.Text + fcena
     ThisWorkbook.Sheets("Data").Range("cena_vysledna_kreslo").Value = fcena
    FormKreslo.Form_cena_kreslo.Text = FormatNumber(fcena, , , , vbTrue)
    checkbox_nazov.Font.Bold = True
    FormKreslo.Repaint
End Sub
Private Sub odpocitaj_cenu(checkbox_nazov, textcena_nazov)
    fcena = ThisWorkbook.Sheets("Data").Range("cena_vysledna_kreslo").Value
    fcena = fcena - textcena_nazov.Text
     ThisWorkbook.Sheets("Data").Range("cena_vysledna_kreslo").Value = fcena
    FormKreslo.Form_cena_kreslo.Text = FormatNumber(fcena, , , , vbTrue)
    FormKreslo.Repaint
