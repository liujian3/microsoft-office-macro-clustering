Private Sub vyber_nastroje(combobox_nazov_turnas, textcena_nazov_turnas, cmdinfo_nazov_turnas, _
                label_nazov_turnas, combobox_nazov_prisl, textcena_nazov_prisl, _
                cmdinfo_nazov_prisl, label_nazov_prisl)
    
    If combobox_nazov_turnas.Tag = "HadicaTGL637" Then 'TGL 637A, TG637 informacia z comboboxu Hadica1
         
         Rem Select Case combobox_nazov_turnas.ListIndex
            
            Select Case kod_polozky_naradie
            
            Case Is = ""
                Call odpocitaj_cenucombo_turnas(textcena_nazov_turnas)
                        
                Call combobox_prisl_skry(combobox_nazov_prisl, textcena_nazov_prisl, cmdinfo_nazov_prisl, _
                    label_nazov_prisl, combobox_nazov_turnas, textcena_nazov_turnas)
