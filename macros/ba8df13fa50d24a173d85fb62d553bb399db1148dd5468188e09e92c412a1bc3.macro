Sub procedure_tlac_cena_vzorec(dalsiriadok)
Dim medzisucet_od As Variant
Dim medzisucet_do As Variant
Dim medzisucet_od1 As Variant
Dim medzisucet_do1 As Variant
Dim medzisucet_od2 As Variant
Dim medzisucet_do2 As Variant
Dim zaciatok_vzorca As Variant
Dim koniec_vzorca As Variant

    If prvy_medzisucet = True Then
        zaciatok_vzorca = 2
    Else
        zaciatok_vzorca = dalsiriadok1 + 1
    End If
    koniec_vzorca = dalsiriadok - 1


    medzisucet_od = "C" & zaciatok_vzorca
    medzisucet_do = "C" & koniec_vzorca
    
    
    ' Ak ide cyklus prvy krat
    If prvy_medzisucet = True Then
        sucet = "C" & dalsiriadok
    Else
        sucet = sucet & "," & "C" & dalsiriadok
    End If
    
        
    ThisWorkbook.Sheets("Tlac").Cells(dalsiriadok, 3).Formula = "=SUM(" & medzisucet_od & ":" & medzisucet_do & ")"
    
    '----------------------------cennik dealer---------------------------
    If ThisWorkbook.Sheets("Parametre").Range("Konecny_cennik").Value = 1 Then
          
        medzisucet_od1 = "E" & zaciatok_vzorca
        medzisucet_do1 = "E" & koniec_vzorca
        ' Ak ide cyklus prvy krat
        If prvy_medzisucet = True Then
            sucet1 = "E" & dalsiriadok
        Else
            sucet1 = sucet1 & "," & "E" & dalsiriadok
        End If
        ThisWorkbook.Sheets("Tlac").Cells(dalsiriadok, 5).Formula = "=SUM(" & medzisucet_od1 & ":" & medzisucet_do1 & ")"
    End If
    '--------------------------END cennik dealer---------------------------
    If UVN = True Then
        medzisucet_od2 = "F" & zaciatok_vzorca
        medzisucet_do2 = "F" & koniec_vzorca
        ' Ak ide cyklus prvy krat
        If prvy_medzisucet = True Then
            sucet2 = "F" & dalsiriadok
        Else
            sucet2 = sucet2 & "," & "F" & dalsiriadok
        End If
        ThisWorkbook.Sheets("Tlac").Cells(dalsiriadok, 6).Formula = "=SUM(" & medzisucet_od2 & ":" & medzisucet_do2 & ")"
        ThisWorkbook.Sheets("Tlac").Cells(dalsiriadok, 6).Font.ColorIndex = 3
    End If
    
    
    'FARBA medzisuctov
    If ThisWorkbook.Sheets("Parametre").Range("Konecny_cennik").Value = 1 Then
        ThisWorkbook.Sheets("Tlac").Cells(dalsiriadok, 3).Font.ColorIndex = 3
    Else
        ThisWorkbook.Sheets("Tlac").Cells(dalsiriadok, 3).Font.ColorIndex = 1
    End If
    
    
    dalsiriadok1 = dalsiriadok
    prvy_medzisucet = False
    
End Sub
