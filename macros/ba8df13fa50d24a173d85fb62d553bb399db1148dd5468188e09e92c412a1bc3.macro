    End If
        
    Set zdrojovy_zosit = Application.Workbooks.Open(subor)
    For i = 1 To zdrojovy_zosit.Sheets.Count 'Prehlada nazvy listov otvaraneho zosita
        If Worksheets(i).Name = "Tlac" Then
            sheetexist = True
        End If
    Next
    If sheetexist = False Then
