Private Sub pripocitaj_cenu(checkbox_nazov, textcena_nazov, textpock_nazov, labelpocks_nazov)
    
    Call Module3.PocetKusov
    textpock_nazov.Text = pocet_kusov
    
    fcena = ThisWorkbook.Sheets("Data").Range("Cena_vysledna_dennar2").Value
    fcena = (textcena_nazov.Text * textpock_nazov.Text) + fcena
    
    '---------------------------------
    
     ThisWorkbook.Sheets("Data").Range("Cena_vysledna_dennar2").Value = fcena
    FormDennar3.Form_cena_dennar3.Text = FormatNumber(fcena, , , , vbTrue)
    
    checkbox_nazov.Font.Bold = True
    textpock_nazov.Visible = True
    labelpocks_nazov.Visible = True
    pocet_kusov = Val(0)
    FormDennar3.Repaint
    
    If sprava = "exit" Then checkbox_nazov.Value = False
    
End Sub

Private Sub odpocitaj_cenu(checkbox_nazov, textcena_nazov, textpock_nazov, labelpocks_nazov)
    fcena = ThisWorkbook.Sheets("Data").Range("Cena_vysledna_dennar2").Value
    fcena = fcena - (textcena_nazov.Text * textpock_nazov.Text)
   
    '---------------------------------
     ThisWorkbook.Sheets("Data").Range("Cena_vysledna_dennar2").Value = fcena
    FormDennar3.Form_cena_dennar3.Text = FormatNumber(fcena, , , , vbTrue)
    textpock_nazov.Visible = False
    labelpocks_nazov.Visible = False
    pocet_kusov = Val(0)
    FormDennar3.Repaint
End Sub

