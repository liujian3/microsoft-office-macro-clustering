Function Funkcia_last(vyber As Long, zoznam As Range)
'Hlubocky, 13 Jun 2011
' 1 = Posledny riadok
' 2 = Posledny stlpec
' 3 = Posledna bunka

    Dim poslr As Long
    Dim posls As Long

    Select Case vyber

    Case 1:
        On Error Resume Next
        Funkcia_last = zoznam.Find(what:="*", _
                        after:=zoznam.Cells(1), _
                        Lookat:=xlPart, _
                        LookIn:=xlFormulas, _
                        searchorder:=xlByRows, _
                        searchdirection:=xlPrevious, _
                        MatchCase:=False).Row
        On Error GoTo 0

    Case 2:
        On Error Resume Next
        Funkcia_last = zoznam.Find(what:="*", _
                        after:=zoznam.Cells(1), _
                        Lookat:=xlPart, _
                        LookIn:=xlFormulas, _
                        searchorder:=xlByColumns, _
                        searchdirection:=xlPrevious, _
                        MatchCase:=False).Column
        On Error GoTo 0

    Case 3:
        On Error Resume Next
        poslr = zoznam.Find(what:="*", _
                       after:=zoznam.Cells(1), _
                       Lookat:=xlPart, _
                       LookIn:=xlFormulas, _
                       searchorder:=xlByRows, _
                       searchdirection:=xlPrevious, _
                       MatchCase:=False).Row
        On Error GoTo 0

        On Error Resume Next
        posls = zoznam.Find(what:="*", _
                        after:=zoznam.Cells(1), _
                        Lookat:=xlPart, _
                        LookIn:=xlFormulas, _
                        searchorder:=xlByColumns, _
                        searchdirection:=xlPrevious, _
                        MatchCase:=False).Column
        On Error GoTo 0

        If PoslednyStlpec > 0 Then posls = PoslednyStlpec
        Poslednyriadok = poslr

        On Error Resume Next
        Funkcia_last = zoznam.Parent.Cells(poslr, posls).Address(False, False)
        If Err.Number > 0 Then
            Funkcia_last = zoznam.Cells(1).Address(False, False)
            Err.Clear
        End If
        On Error GoTo 0

    End Select
End Function

