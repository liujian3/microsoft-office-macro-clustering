Private Sub Cmd_zmencennik_dealer1_Click()
    
    pocetriadkov_ceny = ThisWorkbook.Sheets("Ceny").Cells(Rows.Count, 1).End(xlUp).Row
    'Zrataj pocet riadkov v liste Data
    pocetriadkov_data = ThisWorkbook.Sheets("Data").Cells(Rows.Count, 1).End(xlUp).Row
    cenaceny = Application.WorksheetFunction.Subtotal(9, ThisWorkbook.Sheets("Ceny").Range("C2:C2000"))
    cenadata = Application.WorksheetFunction.Subtotal(9, ThisWorkbook.Sheets("Data").Range("D:D"))
    
    If (pocetriadkov_data <> pocetriadkov_ceny) Then
        kodmsgbox = "msgbox8003" 'Pozor, nes|fffd|lad cien ! Cennn|fffd|k nie je mo|fffd|n|fffd| zmeni|fffd| !
        kodbuttons = 48
        Call Module3.Initialize_MsgBox(kodmsgbox, kodbuttons)
        Exit Sub
    End If
    
    If FormZmenaCeny.text_zlava_supravy.Text = "" And FormZmenaCeny.text_zlava_naradie.Text = "" Then Exit Sub
    
    If znovu_prepocitat_dealer1 = False Then
        kodbuttons = 4
        kodmsgbox = "msgbox8002"  'Prepo|fffd||fffd|ta|fffd| cenn|fffd|k so zadan|fffd|mi z|fffd|avami ?
        Call Module3.Initialize_MsgBox(kodmsgbox, kodbuttons)
        If hodnotamsgbox = 7 Then Exit Sub
    End If
    '-----------------------------------------------------------------------------------------------------------------
    ThisWorkbook.Sheets("Parametre").Range("Posledne_tlacitko").Value = "Dealer1"
    '-----------------------------------------------------------------------------------------------------------------
    'Zisti koeficient zadany uzivatelom
    koeficient = FormZmenaCeny.text_zlava_supravy.Text
    If koeficient = "" Then koeficient = 0
    koeficient = koeficient + ThisWorkbook.Sheets("Parametre").Range("dealer1_zlava_supravy").Value
    If koeficient >= 0 Then
        ThisWorkbook.Sheets("Parametre").Range("dealer1_zlava_supravy").Value = Val(koeficient)
    End If
    
    'Zisti koeficient zadany uzivatelom
    koeficient = FormZmenaCeny.text_zlava_naradie.Text
    If koeficient = "" Then koeficient = 0
    koeficient = koeficient + ThisWorkbook.Sheets("Parametre").Range("dealer1_zlava_naradie").Value
    ThisWorkbook.Sheets("Parametre").Range("dealer1_zlava_naradie").Value = Val(koeficient)
    '-----------------------------------------------------------------------------------------------------------------
    '2-cennik dealersky zmena cien celeho cennika, 3 obe zmeny dohromady, 5 konecny nemenitelny cennik
    'Nastavenie parametra: 0-cennik povodny, 1-cennik dealersky zlava na naradie a supravy s moznostou vratit naspet,
    Call Module3.nastav_typ_cennika
    '-----------------------------------------------------------------------------------------------------------------
    
    If pocetriadkov_data = pocetriadkov_ceny Then
        With FormZmenaCeny
            zmena_ceny = "dealer1"
            Call Module3.nastav_list_Ceny
            '------------------
            ' save cennika
            Citac = 100
            FormPleaseWait.Show
            '---------------------
                
        End With
    End If
    
    
