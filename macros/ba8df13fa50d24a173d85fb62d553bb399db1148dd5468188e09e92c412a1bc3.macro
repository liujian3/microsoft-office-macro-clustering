Sub rataj_zlavy_dealer(posledna_bunka)
 Dim i As Integer
'Zrataj pocet riadkov v liste Ceny
pocetriadkov_ceny = ThisWorkbook.Sheets("Ceny").Cells(Rows.Count, 1).End(xlUp).Row

'Zrataj pocet riadkov v liste Data
pocetriadkov_data = ThisWorkbook.Sheets("Data").Cells(Rows.Count, 1).End(xlUp).Row


Set oblast1 = ThisWorkbook.Sheets("Ceny").Range("A2", posledna_bunka)

For Each bunka1 In oblast1
    
    Dim dealer1_supravy As Integer
    i = bunka1.Row
    Rem MsgBox bunka1.Value
    
    Rem MsgBox Mid(bunka1, 1, 6)
    
    '-------------------------------------------------
    'Urceneie priznaku ZLAVY na supravy a naradie z dovodu zlavy
    bunka1.Offset(0, 10).Value = 0
    If Mid(bunka1, 1, 6) = "TurNas" Then bunka1.Offset(0, 10).Value = 1
    If Mid(bunka1, 1, 6) = "Dennar" Then bunka1.Offset(0, 10).Value = 1
    
    '-------------------------------------------------------
    dealer1_supravy = ThisWorkbook.Sheets("Parametre").Range("Dealer1_zlava_supravy").Value
    
    With bunka1.Offset(0, 2)
            
        'bunka 10 je parameter zlavy na supravy a naradie
        '--------------------ZLAVA NA SUPRAVY value 0------------------------------------------------
        If bunka1.Offset(0, 10).Value = 0 Then
            
            'Nastavenie parametra: 'Nastavenie parametra: 0-cennik povodny, 1-cennik dealersky zlava na naradie a supravy s moznostou vratit naspet,
            '2-cennik dealersky zmena cien celeho cennika, 3 obe zmeny dohromady, 5 konecny nemenitelny cennik
           
            '------Ak je typ cennika 0 ziadna zlava-------------------------
            If ThisWorkbook.Sheets("Parametre").Range("Konecny_cennik").Value = 0 Then
              
                .FormulaR1C1 = "=ROUND((RC2),0)"
            
            End If
            
           
            '------Ak je typ cennika 1 a je len jeden typ zlavy ----
            If ThisWorkbook.Sheets("Parametre").Range("Konecny_cennik").Value = 1 Then
                
                .FormulaR1C1 = "=ROUND(RC2 -((RC2/100)*Parametre!Dealer1_zlava_supravy),0)"
            
            End If
            
             '------Ak je typ cennika 2 a je len jeden typ zlavy ----
            If ThisWorkbook.Sheets("Parametre").Range("Konecny_cennik").Value = 2 Then
                
                .FormulaR1C1 = "=ROUND(RC2 +((RC2/100)*(Parametre!Dealer2_prepocet_cennika)),0)"
            
            End If
            '----------
            '------Ak je typ cennika 3 ------------------------------
            If ThisWorkbook.Sheets("Parametre").Range("Konecny_cennik").Value = 3 And _
              ThisWorkbook.Sheets("Parametre").Range("Posledne_tlacitko").Value = "Dealer1" Then
              
                .FormulaR1C1 = "=ROUND((RC2 +((RC2/100)*Parametre!Dealer2_prepocet_cennika))-((RC2 +((RC2/100)*Parametre!Dealer2_prepocet_cennika))/100 * dealer1_zlava_supravy),0)"
            
            End If
            
            If ThisWorkbook.Sheets("Parametre").Range("Konecny_cennik").Value = 3 And _
               ThisWorkbook.Sheets("Parametre").Range("Posledne_tlacitko").Value = "Dealer2" Then
                
                .FormulaR1C1 = "=ROUND((RC2 +((RC2/100)*Parametre!Dealer2_prepocet_cennika))-((RC2 +((RC2/100)*Parametre!Dealer2_prepocet_cennika))/100 * Dealer1_zlava_supravy),0)"
            
            End If
        End If
        
        '--------------------------------------------------------------------------------------------
        '--------------------ZLAVA NA NARADIE value 1------------------------------------------------
        'bunka 10 je parameter zlavy na supravy a naradie
        If bunka1.Offset(0, 10).Value = 1 Then
     
            '------Ak je typ cennika 0 ziadna zlava-------------------------
            If ThisWorkbook.Sheets("Parametre").Range("Konecny_cennik").Value = 0 Then
                .FormulaR1C1 = "=ROUND((RC2),0)"
                
            End If
            
            '------Ak je typ cennika 1 a je len jeden typ zlavy ----
            If ThisWorkbook.Sheets("Parametre").Range("Konecny_cennik").Value = 1 Then
                
                .FormulaR1C1 = "=ROUND(RC2 -((RC2/100)*Parametre!dealer1_zlava_naradie),0)"
                
            End If
             '-------Ak je typ cennika 2 a je len jeden typ zlavy ----
            If ThisWorkbook.Sheets("Parametre").Range("Konecny_cennik").Value = 2 Then
                
                .FormulaR1C1 = "=ROUND(RC2 +((RC2/100)*(Parametre!Dealer2_prepocet_cennika)),0)"
                
            End If
            
            
            '------Ak je typ cennika 3 ------------------------------
            If ThisWorkbook.Sheets("Parametre").Range("Konecny_cennik").Value = 3 And _
              ThisWorkbook.Sheets("Parametre").Range("Posledne_tlacitko").Value = "Dealer1" Then
                
                .FormulaR1C1 = "=ROUND((RC2 +((RC2/100)*Parametre!Dealer2_prepocet_cennika))-((RC2 +((RC2/100)*Parametre!Dealer2_prepocet_cennika))/100 * dealer1_zlava_naradie),0)"
                
            End If
            
            If ThisWorkbook.Sheets("Parametre").Range("Konecny_cennik").Value = 3 And _
               ThisWorkbook.Sheets("Parametre").Range("Posledne_tlacitko").Value = "Dealer2" Then
                
                .FormulaR1C1 = "=ROUND((RC2 +((RC2/100)*Parametre!Dealer2_prepocet_cennika))-((RC2 +((RC2/100)*Parametre!Dealer2_prepocet_cennika))/100 * Dealer1_zlava_naradie),0)"
            
            End If '------Ak je typ cennika 3 ------------------------------
            
            
        End If
        
        '--------------------------------------------------------------------------------------------
        '--------------------ZLAVA NA NARADIE value 2------------------------------------------------
        'bunka 10 je parameter zlavy na supravy a naradie
        
    
    End With
    
    With bunka1.Offset(0, 3)
        
        'Vzorec, ktory zrata najprv zlavu na supravy a na naradie z bunky B2 v Ceny a potom uplatni zlavu (percento) na celkovy cennik
        '---------------------------Polozky v stlpci "D"---------------------------
        If bunka1.Offset(0, 10).Value = 0 Then
            
            .FormulaR1C1 = "=ROUND(RC2 -((RC2/100)*Parametre!Dealer1_zlava_supravy),0)"
           
        End If
        If bunka1.Offset(0, 10).Value = 1 Then
            
            .FormulaR1C1 = "=ROUND(RC2 -((RC2/100)*Parametre!dealer1_zlava_naradie),0)"
          
        End If
        
        
        
        '--------------------------------------------------------------------------
        '--------------------------------------------------------------------------
        '---------------------------Polozky v stlpci "E"---------------------------
        With bunka1.Offset(0, 4)
            
            .FormulaR1C1 = "=ROUND(RC2 +((RC2/100)*(Parametre!Dealer2_prepocet_cennika)),0)"
            
        End With
        '--------------------------------------------------------------------------
        
        
        '---------------------------Kurz USD---stlpec F-------------------
        With bunka1.Offset(0, 5)
            .FormulaR1C1 = "=ROUND(RC3*Parametre!kurz_USD,0)"
        End With
        '---------------------------Kurz CZK---G-------------------
        With bunka1.Offset(0, 6)
            .FormulaR1C1 = "=ROUND(RC3*Parametre!kurz_CZK,0)"
        End With
        '---------------------------Kurz PLN----H------------------
        With bunka1.Offset(0, 7)
            .FormulaR1C1 = "=ROUND(RC3*Parametre!kurz_PLN,0)"
        End With
        '---------------------------Kurz RUB---I------------------
        With bunka1.Offset(0, 8)
