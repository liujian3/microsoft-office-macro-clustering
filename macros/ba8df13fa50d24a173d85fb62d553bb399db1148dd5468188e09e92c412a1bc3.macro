Private Sub CmdHadiceV_ok_Click()
    Application.ScreenUpdating = False
    
    If Export_cennik = True Then GoTo preskoc
    
    '---- Cheese E------------------------------
    If typ_supravy = "CheesE" Then
        ' Ak je zvoleny 4 - nastrojovy stolik
        
        If FormCheesV_stolikLE.Checkbox_CheesV_0050.Value = True Or _
           FormCheesV_stolikLE.Checkbox_CheesV_0060.Value = True Or _
           FormCheesV_stolikLE.Checkbox_CheesV_0063.Value = True Then
   
            ' Ak nie je vybrata 4ta hadica - treba zaslepku
            
            If kod_vybratej_polozky1 <> "" And _
               kod_vybratej_polozky2 <> "" And _
               kod_vybratej_polozky3 <> "" And _
               kod_vybratej_polozky4 = "" Then
            
                kodmsgbox = "msgbox4950"
                kodbuttons = 16
                Call Module3.Initialize_MsgBox(kodmsgbox, kodbuttons)
                Exit Sub
             End If
             
            ' Ak nie je vybrata 4ta hadica - treba zaslepku
            
            If kod_vybratej_polozky5 <> "" Or _
               kod_vybratej_polozky6 <> "" Then
               
                kodmsgbox = "msgbox4970"
                kodbuttons = 16
                Call Module3.Initialize_MsgBox(kodmsgbox, kodbuttons)
                Exit Sub
             End If
             
        End If
        
        ' Ak je zvoleny 6 - nastrojovy stolik
        If FormCheesV_stolikLE.Checkbox_CheesV_0070.Value = True Or _
           FormCheesV_stolikLE.Checkbox_CheesV_0100.Value = True Then
           

            ' Ak nie je vybrata 6ta hadica - treba zaslepku
            
            If kod_vybratej_polozky1 <> "" And _
               kod_vybratej_polozky2 <> "" And _
               kod_vybratej_polozky3 <> "" And _
               kod_vybratej_polozky4 <> "" And _
              (kod_vybratej_polozky5 = "" Or _
               kod_vybratej_polozky6 = "") Then
               
            
                kodmsgbox = "msgbox4950"
                kodbuttons = 16
                Call Module3.Initialize_MsgBox(kodmsgbox, kodbuttons)
                Exit Sub
            End If
        End If
    End If
    
    '---- CheesM ---- CheesL ------------------------------
    If typ_supravy = "CheesM" Or typ_supravy = "CheesL" Then
        
        If kod_vybratej_polozky1 = "" Or _
           kod_vybratej_polozky2 = "" Or _
           kod_vybratej_polozky3 = "" Or _
           kod_vybratej_polozky4 = "" Or _
           kod_vybratej_polozky5 = "" Then
               
            kodmsgbox = "msgbox4550"
            kodbuttons = 16
            Call Module3.Initialize_MsgBox(kodmsgbox, kodbuttons)
            Exit Sub
        End If
    End If
    
    '---- CheesU------------------------------
    If typ_supravy = "CheesU" Then
        If kod_vybratej_polozky1 = "" Or _
           kod_vybratej_polozky2 = "" Or _
           kod_vybratej_polozky3 = "" Or _
           kod_vybratej_polozky4 = "" Or _
           kod_vybratej_polozky5 = "" Then
               
            kodmsgbox = "msgbox4550"
            kodbuttons = 16
            Call Module3.Initialize_MsgBox(kodmsgbox, kodbuttons)
            Exit Sub
        End If
        
        'Test na DCI podla stolika lekara
        With FormCheesV_stolikLE
            If .Checkbox_CheesV_0500 = True Then
                If .Checkbox_CheesV_0520.Value = True Or .Checkbox_CheesV_0600.Value = True Then DCI = False
                If .Checkbox_CheesV_0550.Value = True Or .Checkbox_CheesV_0650.Value = True Then DCI = True
            End If
            If .Checkbox_CheesV_0500 = False Then
                If dci_N > 0 Then DCI = True
                If bezdci_N > 0 Then DCI = False
            End If
        
        End With
        
        If pluvblok = True Then Call Module3.test_DCI_PluvBlok
    
    End If
    '---- CMini04 ------------------------------
    If typ_supravy = "CMini04" Then
        
        '4 nastrojovy stolik
        If FormCheesV_stolikLE.Checkbox_CheesV_0050.Value = True Then
        
            If kod_vybratej_polozky1 = "" Or _
               kod_vybratej_polozky2 = "" Or _
               kod_vybratej_polozky3 = "" Or _
               kod_vybratej_polozky4 = "" Then
               
                kodmsgbox = "msgbox4550"
                kodbuttons = 16
                Call Module3.Initialize_MsgBox(kodmsgbox, kodbuttons)
                Exit Sub
            End If
        End If
        '5 nastrojovy stolik
         If FormCheesV_stolikLE.Checkbox_CheesV_0060.Value = True Then
        
            If kod_vybratej_polozky1 = "" Or _
               kod_vybratej_polozky2 = "" Or _
               kod_vybratej_polozky3 = "" Or _
               kod_vybratej_polozky4 = "" Or _
               kod_vybratej_polozky5 = "" Then
               
                kodmsgbox = "msgbox4550"
                kodbuttons = 16
                Call Module3.Initialize_MsgBox(kodmsgbox, kodbuttons)
                Exit Sub
            End If
        End If
            
    End If
    '-------End CMini04------------------------------------------
     
preskoc:
    
    With FormCheesV_zaklad
        .Text_Vysledna_cena = FormatNumber(ThisWorkbook.Sheets("Data").Range("cena_vysledna_hadice").Value + _
             ThisWorkbook.Sheets("Data").Range("cena_zaslepka").Value + _
             ThisWorkbook.Sheets("Data").Range("cena_vysledna").Value, , , , vbTrue)
        Rem.Text_cena_nastrojeSL.Caption = FormHadiceV.Form_cena_hadiceV.text + FormHadiceV.Form_cena_zaslepka.text
            
        .Text_cena_nastrojeSL.Caption = FormatNumber(ThisWorkbook.Sheets("Data").Range("cena_vysledna_hadice").Value + _
             ThisWorkbook.Sheets("Data").Range("cena_zaslepka").Value, , , , vbTrue)
        .CmdCheesV_zaklad_hadice.BackColor = &HC0FFC0
    End With
        
     ThisWorkbook.Sheets("Data").Range("cena_vysledna").Value = ThisWorkbook.Sheets("Data").Range("cena_vysledna_hadice").Value + _
         ThisWorkbook.Sheets("Data").Range("cena_zaslepka").Value + ThisWorkbook.Sheets("Data").Range("cena_vysledna").Value
   
    If automat = False And import_tlace = False Then FormHadiceV.Hide
    hadice = True
End Sub

