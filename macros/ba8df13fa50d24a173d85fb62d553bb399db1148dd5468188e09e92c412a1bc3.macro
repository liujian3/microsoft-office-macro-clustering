Private Sub vyber_prisl(combobox_nazov_prisl, textcena_nazov_prisl, _
                cmdinfo_nazov_prisl, label_nazov_prisl)
                
    Rem Call najdi_kod_prisl(combobox_nazov)
     
    If combobox_nazov_prisl.Tag = "TurNas_22" Then ' 637A, 637AHF, R|fffd|chlospojka 420 Svet. nerezov|fffd|
    
        Rem Select Case combobox_nazov_prisl.ListIndex
        Select Case kod_polozky_prisl
        
            Case Is = ""
                Call odpocitaj_cenucombo_prisl(textcena_nazov_prisl)
            Case Is = "TurNas_2200" 'Rychlospojka 420
                Call pripocitaj_cenucombo_prisl(textcena_nazov_prisl, combobox_nazov_prisl)
        End Select
    End If
     '--------------------------------------------------------------------------------------
   
    If combobox_nazov_prisl.Tag = "TurNas_24" Then ' Mikromotor elektrick|fffd| CH 660,Bal|fffd||fffd|ek: N|fffd|s. 110 D + sp. diel 120D + hlav. 168 D
        
        Rem Select Case combobox_nazov_prisl.ListIndex
        Select Case kod_polozky_prisl
        
            Case Is = ""
            Call odpocitaj_cenucombo_prisl(textcena_nazov_prisl)
            
            Case Is = "TurNas_2400" '110D
                Call pripocitaj_cenucombo_prisl(textcena_nazov_prisl, combobox_nazov_prisl)
            Case Is = "TurNas_2500" '168D
                Call pripocitaj_cenucombo_prisl(textcena_nazov_prisl, combobox_nazov_prisl)
            Case Is = "TurNas_2550" '168D
                Call pripocitaj_cenucombo_prisl(textcena_nazov_prisl, combobox_nazov_prisl)
            Case Is = "TurNas_2650" '168D
                Call pripocitaj_cenucombo_prisl(textcena_nazov_prisl, combobox_nazov_prisl)
        End Select
        
     End If
     '----------------------------------------------------------------------------------------
    If combobox_nazov_prisl.Tag = "TurNas_27" Then ' Spodn|fffd| diel 120L + Hlavi|fffd|ka 168 D/T
    
        Rem Select Case combobox_nazov_prisl.ListIndex
        Select Case kod_polozky_prisl
        
            Case Is = ""
                Call odpocitaj_cenucombo_prisl(textcena_nazov_prisl)
            
            Case Is = "TurNas_2700" 'Bal|fffd||fffd|ek: Spodn|fffd| diel 120L + Hlavi|fffd|ka 168 D/T
                Call pripocitaj_cenucombo_prisl(textcena_nazov_prisl, combobox_nazov_prisl)
            
             Case Is = "TurNas_2710" '120LB
                Call pripocitaj_cenucombo_prisl(textcena_nazov_prisl, combobox_nazov_prisl)
             
             Case Is = "TurNas_2720" '120LR
                Call pripocitaj_cenucombo_prisl(textcena_nazov_prisl, combobox_nazov_prisl)
                
            
            Case Is = "TurNas_2750" 'Bez prislusenstva
                Call pripocitaj_cenucombo_prisl(textcena_nazov_prisl, combobox_nazov_prisl)
            
        End Select
    End If
     
End Sub
