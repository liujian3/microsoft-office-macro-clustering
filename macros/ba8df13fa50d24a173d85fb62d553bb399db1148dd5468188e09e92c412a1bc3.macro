Sub KopirujPolozky(kodpolozky)

    hladajkodpolozky = Application.WorksheetFunction.VLookup(kodpolozky, ThisWorkbook.Sheets("data").Range("a2:F2000"), 1, False)
    If FormVyberEcennik.Formvyberecennik_RozsirenyPopis.Value = True Then
        hladajpopis = Application.WorksheetFunction.VLookup(kodpolozky, ThisWorkbook.Sheets("data").Range("a2:F2000"), 2, False)
    Else
        hladajpopis = Application.WorksheetFunction.VLookup(kodpolozky, ThisWorkbook.Sheets("data").Range("a2:F2000"), 3, False)
    End If
    '--------------------------------
    'Cena v liste "Data" stlpec "D"
    hladajcena = Application.WorksheetFunction.VLookup(kodpolozky, ThisWorkbook.Sheets("data").Range("a2:F2000"), 4, False)
    '----------------------------------
    
   Rem If hladajcena = "" Then hladajcena = "0"
  
    
    dalsiriadok = ThisWorkbook.Sheets("Tlac_ecennik").Cells(Rows.Count, 1).End(xlUp).Row + 1
   
    
    ThisWorkbook.Sheets("Tlac_ecennik").Cells(dalsiriadok, 1) = hladajkodpolozky
    ThisWorkbook.Sheets("Tlac_ecennik").Cells(dalsiriadok, 2) = hladajpopis
    ThisWorkbook.Sheets("Tlac_ecennik").Cells(dalsiriadok, 3) = hladajcena
    
    If hladajcena = "0" And InStr(1, hladajpopis, "---") Then _
        ThisWorkbook.Sheets("Tlac_ecennik").Cells(dalsiriadok, 3) = ""
    
    '-------Ak je Checkbox ---------------
    If combo = False Then
        With ThisWorkbook.Sheets("Tlac_ecennik").Cells(dalsiriadok, 2)
            Rem .Font.Bold = True
            Rem .Font.Size = 10
        End With
        With ThisWorkbook.Sheets("Tlac_ecennik").Cells(dalsiriadok, 3)
            .Interior.color = RGB(255, 255, 0)
            .Font.Bold = True
            .Font.Size = 9
        End With
    Else
    '-------Ak je Combobox ---------------
        With ThisWorkbook.Sheets("Tlac_ecennik").Cells(dalsiriadok, 2)
            If InStr(1, hladajpopis, "---") And MenoFormuExport.Name = "FormHadiceV" Then
                .Font.Bold = True
                .Font.Size = 10
            End If
        End With
        With ThisWorkbook.Sheets("Tlac_ecennik").Cells(dalsiriadok, 3)
            .Interior.color = RGB(255, 255, 0)
            If InStr(1, hladajpopis, "---") Then .Interior.color = xlNone
            
            .Font.Bold = True
            .Font.Size = 9
        End With
