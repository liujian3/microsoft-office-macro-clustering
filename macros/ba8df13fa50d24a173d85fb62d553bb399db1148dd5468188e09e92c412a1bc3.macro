Sub ZapisLenCislo(ByVal KeyAscii As MSForms.ReturnInteger, Ct As Control, Optional desCis As Boolean = True)
'Povoluje zapisat do textboxu len cislo
'Datum: 25.09.2011
'-------------------------------------------
'Pokial nie je stisknuta kl|fffd|vesa 0-9, bodka, alebo ciarka - nevkladat
If Not ((KeyAscii >= Asc("0") And KeyAscii <= Asc("9")) _
      Or KeyAscii = Asc(".") _
      Or KeyAscii = Asc(",") _
      Or KeyAscii = Asc(Application.International(xlDecimalSeparator)) _
      Or (KeyAscii = Asc("-") And Len(Ct) = 0)) Then
           KeyAscii = 0
End If
If desCis Then 'Parameter descis nastavujeme priamo v odkaze na sub zapislencislo call...(KeyAscii, Me.Input_Text, True) - True je povolenie des.cisla
     'Zmenit bodku alebo ciarku podla narodneho nastavenia a oddelovac desetinnych miest
     If KeyAscii = Asc(".") Then KeyAscii = Asc(Application.International(xlDecimalSeparator))
     If KeyAscii = Asc(",") Then KeyAscii = Asc(Application.International(xlDecimalSeparator))
     'Kontrola na zadanie dvoch oddelovacov des. miest
     If KeyAscii = Asc(Application.International(xlDecimalSeparator)) Then
          If Len(Ct.Text) - Len(Replace(Ct.Text, Application.International(xlDecimalSeparator), "")) > 0 Then KeyAscii = 0
     End If
Else
     'Pokial nie je povelene vkladanie desetinnych cisiel - nevkladat
     If KeyAscii = Asc(".") Then KeyAscii = 0
     If KeyAscii = Asc(",") Then KeyAscii = 0
     If KeyAscii = Asc("-") Then KeyAscii = 0
End If
End Sub

Function JeCislo(Vstup, Optional JenCele As Boolean = False) As Boolean
'Vracia PRAVDA, pokial je vstup cislo
'pokial nastavime druhy nepovinny parametr na True, vrati PRAVDA len v pripade celeho cisla
'Datum: 25.9.2011
'-------------------------------------------
JeCislo = False
If IsEmpty(Vstup) Then Exit Function
If IsNumeric(Vstup) Then
     If JenCele Then
          If CLng(Vstup) <> Vstup Then Exit Function
          JeCislo = True
     End If
JeCislo = True
End If
End Function

