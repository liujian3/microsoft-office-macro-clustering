        Rem druh_vyrobku = "naradie"
        dennar_z_hlavneho_panela = True
        Checkbox_Dennar.Value = False
        GoTo preskoc
    End If
      
     
preskoc:
    
    If Typ_supravy_ImpExp = "CheesE" Then FormSupravy.CmdCheesE = True
    If Typ_supravy_ImpExp = "CheesM" Or Typ_supravy_ImpExp = "CheesMtla7" Or _
      Typ_supravy_ImpExp = "CheesMtla8" Or Typ_supravy_ImpExp = "CheesMtla9" Then FormSupravy.CmdCheesM = True
