Sub BlattschutzAUS()
'Entfernt den Blattschutz aus allen Bl|fffd|ttern, falls anderes Passwort als benz33 muss dies unten angepasst werden
Dim Passwort

Set dat = Application.FileDialog(msoFileDialogFilePicker)
Passwort = InputBox("Bitte Passwort eingeben" & Chr(13) & _
     "", "Passwort")

  Dim i As Integer
    For i = 1 To Sheets.Count
    Worksheets(i).Activate
    ActiveSheet.Unprotect Password:=Passwort
  
  Next i
  
 

End Sub

  Sub BlattschutzEIN()
' Aktiviert den Blattschutz in jedem Arbeitsblatt und sch|fffd|tzt auch das komplette Workbook
'Sollte der Blattschutz in Verbindung mit dem Makro Reiterfarbe |fffd|ndern verwendet, muss hier der Schutz des gesamten Workbooks entfernt werden.
Dim Passwort

Set dat = Application.FileDialog(msoFileDialogFilePicker)
Passwort = InputBox("Bitte Passwort eingeben" & Chr(13) & _
     "", "Passwort")

  Dim i As Integer
    For i = 1 To Sheets.Count
    Worksheets(i).Activate
    ActiveSheet.EnableSelection = xlNoRestrictions
    ActiveSheet.Protect Password:=Passwort, AllowFiltering:=True
    
      
  Next i
  


End Sub




