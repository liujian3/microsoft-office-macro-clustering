Attribute VB_Name = "group"


Sub Row_Group_UnGroup()
Dim vRowSt, vRowEnd, vRowGroup1, vRowGroup2, vColumnT, vColumnYear1, vColumnYear2, vFl, vFl1 As Integer
Dim vSumm As Double
Dim vT, vT1 As Variant
Dim vShapesName As String

vShapesName = ThisWorkbook.ActiveSheet.Name
ThisWorkbook.ActiveSheet.Shapes(3).Name = vShapesName
    For i = 2 To 19
        If ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(i, 1).Value = ThisWorkbook.ActiveSheet.Name Then
            vRowSt = Int(ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(i, 10).Value)
            vRowEnd = Int(ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(i, 11).Value)
            vColumnYear1 = Int(ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(i, 6).Value)
            vColumnT = Int(ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(i, 3).Value) - 1
            vFl = Int(ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(i, 7).Value)
            vColumnYear2 = vColumnYear1 + 1
            vColumnYear3 = vColumnYear1 + 2
            vColumnYear4 = vColumnYear1 + 3
             vFl1 = 0
            
           If vFl = 0 Then
           
               ThisWorkbook.ActiveSheet.Shapes(vShapesName).Select
               Selection.Characters.text = "|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd|"
               ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(i, 7).Value = 1
               ThisWorkbook.ActiveSheet.Cells(vRowSt, 2).Select
            For j = vRowSt To vRowEnd
'If j = 73 Then Stop
                vSumm = Abs(ThisWorkbook.ActiveSheet.Cells(j, vColumnYear1).Value + ThisWorkbook.ActiveSheet.Cells(j, vColumnYear2).Value + ThisWorkbook.ActiveSheet.Cells(j, vColumnYear3).Value + ThisWorkbook.ActiveSheet.Cells(j, vColumnYear4).Value)
                vT = ThisWorkbook.ActiveSheet.Cells(j, vColumnT).Value
                vT1 = ThisWorkbook.ActiveSheet.Cells(j, vColumnT - 1).Value
               
                If IsEmpty(vT) Or IsNull(vT) Or IsNull(vT1) Or IsEmpty(vT1) Then vSumm = 1
                If vSumm = 0 Then
                    If vFl1 = 0 Then
                        vRowGroup1 = j
                        vFl1 = 1
                    Else
                        vRowGroup2 = j
                        If vRowGroup2 = vRowEnd Then GoTo metGr1
                    End If
                
                Else
metGr1:
                  If IsNull(vRowGroup1) = False And IsEmpty(vRowGroup2) = False Then
                    If IsNull(vRowGroup2) Then vRowGroup2 = vRowGroup1
                    Rows(vRowGroup1 & ":" & vRowGroup2).Select
                    Selection.Rows.group
                    
                    vFl1 = 0
                    
                  'Else
                  
                  End If
                  vRowGroup1 = Null
                  vRowGroup2 = Null
                End If
            Next j
            ActiveSheet.Outline.ShowLevels RowLevels:=1
           Else
             ActiveSheet.Outline.ShowLevels RowLevels:=2
             Rows(vRowSt & ":" & vRowEnd).Select
             Selection.Rows.Ungroup
             ThisWorkbook.ActiveSheet.Shapes(vShapesName).Select
             Selection.Characters.text = "C|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| 0-|fffd| |fffd||fffd||fffd||fffd||fffd||fffd|"
             ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(i, 7).Value = 0
             ThisWorkbook.ActiveSheet.Cells(vRowSt, 2).Select
           End If
      End If
    Next i
    

End Sub

