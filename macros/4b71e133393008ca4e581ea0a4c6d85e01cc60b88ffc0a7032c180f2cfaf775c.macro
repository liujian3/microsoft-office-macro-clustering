Sub HideColumnsOnList(pTitleList As String, pRow As Integer, pBeginColumn As Integer, pEndColumn As Integer, pSizeColumn As Integer)
    i = pBeginColumn
    While (i <= pEndColumn)
        ThisWorkbook.Worksheets(pTitleList).Columns(i).ColumnWidth = pSizeColumn
        If ThisWorkbook.Worksheets(pTitleList).Cells(pRow, i).Value = 0 Then
            ThisWorkbook.Worksheets(pTitleList).Columns(i).EntireColumn.Hidden = True
        End If
        If ThisWorkbook.Worksheets(pTitleList).Cells(pRow, i).Value = 1 Then
            ThisWorkbook.Worksheets(pTitleList).Columns(i).EntireColumn.Hidden = False
        End If
        i = i + 1
    Wend
End Sub

Sub HideColumnsOnAllList()
    Dim vTitleList As String
    Dim vRow, vBeginColumn, vEndColumn, vBP As Integer
    i = 2
    While (i <= 25)
        vTitleList = ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(i, 1).Value
        vBP = ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(34, 10).Value
        '|fffd||fffd||fffd| |fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd|, |fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd| |fffd||fffd| 3 |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd| |fffd||fffd| 3 |fffd||fffd|.
        If vTitleList <> "" Then
            If vTitleList = "|fffd||fffd||fffd| |fffd||fffd|" Then
               nrst = 5
               nrend = 50
               nr = nrst
               While (nr <= nrend)
                  If vBP <> 5 And vBP <> 6 Then
                     ThisWorkbook.Worksheets(vTitleList).Rows(nr).EntireRow.Hidden = True
                  Else
                     If nr = 8 Or nr = 9 Or nr = 10 Or nr = 11 Or nr = 12 Then
                        ThisWorkbook.Worksheets(vTitleList).Rows(nr).EntireRow.Hidden = True
                     Else
                        ThisWorkbook.Worksheets(vTitleList).Rows(nr).EntireRow.Hidden = False
                     End If
                  End If
                nr = nr + 1
               Wend
            End If
           '|fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|
            
            vRow = Int(ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(i, 2).Value)
            vBeginColumn = Int(ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(i, 3).Value)
            vEndColumn = Int(ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(i, 4).Value)
            vSizeColumn = Int(ThisWorkbook.Worksheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Cells(i, 5).Value)
            HideColumnsOnList vTitleList, Int(vRow), Int(vBeginColumn), Int(vEndColumn), Int(vSizeColumn)
        End If
        i = i + 1
    Wend
End Sub


Sub ShowColumnsOnList(pTitleList As String, pRow As Integer, pBeginColumn As Integer, pEndColumn As Integer)
    i = pBeginColumn
    While (i <= pEndColumn)
        ThisWorkbook.Worksheets(pTitleList).Columns(i).EntireColumn.Hidden = False
        
        i = i + 1
    Wend
