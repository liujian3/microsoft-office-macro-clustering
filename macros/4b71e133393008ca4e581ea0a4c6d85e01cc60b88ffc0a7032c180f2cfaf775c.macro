Dim linkname As String

Dim linkpos As Integer

For i = 1 To PPPres.Slides.Count
  
   With PPPres.Slides(i)
      
        For k = 1 To .Shapes.Count
            ' Use only shapes on the slide.
            With .Shapes(k)
                  ' If the shape's type is an OLE object then...
                  If .Type = msoLinkedOLEObject Then
                    
                    With .LinkFormat
                            '|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| linkpos |fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd| |fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd| ! 'ppUpdateOptionAutomatic
                            linkposTemp = InStrRev(.SourceFullName, "!", -1, vbTextCompare)
                            SourceFullNameRep = Left(.SourceFullName, linkposTemp - 1) & Right(.SourceFullName, Len(.SourceFullName) - linkposTemp)
                            linkpos = InStrRev(SourceFullNameRep, "!", -1, vbTextCompare)
                            
                            linknameold = Right(.SourceFullName, Len(.SourceFullName) - linkpos)
                              If WsName = Left(linknameold, Len(WsName)) And Selection.Rows.Count > 1 Then
                                linkname = WsName & "!R" & Selection.Row & "C" & Selection.Column & ":R" & Selection.Row + Selection.Rows.Count - 1 & "C" & Selection.Column + Selection.Columns.Count - 1
                                .SourceFullName = ThisFile & "!" & linkname
                                .AutoUpdate = 2
                                Exit For
                              End If
                    End With
                 End If
           End With
      Next k
   End With
Next i
