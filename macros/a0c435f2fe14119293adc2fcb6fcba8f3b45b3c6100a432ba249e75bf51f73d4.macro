Attribute VB_Name = "Module3"
Dim myscreen As Object

Function Wait()
    Do Until myscreen.OIA.XStatus <> 5
    Loop
End Function

Function System()
'Assumes an open extra session

    Set Sys = CreateObject("EXTRA.System")
    Set Sess = Sys.Sessions.Item(A)
    Sess.Activate
    Set myscreen = Sess.Screen
    Worksheets("Sheet1").Select
End Function
Sub FIMS()
'
' FIMS Macro
' Macro recorded 3/18/04 by Thatch
'
'Clear Prior Day
    
    Range("AE178:AE191").Select
    Selection.ClearContents
    Range("AE178").Select

'FIMS Macro Starts Here

    Dim EMonth As String
    Dim EDate As String
    Dim EYear As String
    Dim Fund As String
    Dim FIMS As String
   
    Range("AA178").Select
    Fund = Range("AA178")
    Range("AB178").Select
    EMonth = Range("AB178")
    Range("AC178").Select
    EDate = Range("AC178")
    Range("AD178").Select
    EYear = Range("AD178")
   
    System
    
    Do While IsEmpty(ActiveCell) = False
    
    With myscreen
        .SendKeys ("<home>FIMS<enter>")
        Wait
        .moveto 4, 15
        .SendKeys (Fund)
        .SendKeys ("<EraseEOF>")
        .moveto 5, 9
        .SendKeys (EMonth)
        .SendKeys (EDate)
        .SendKeys (EYear)
        .moveto 5, 34
        .SendKeys (" ")
        .SendKeys ("<enter>")
        Wait
        
    End With
        
        
        If myscreen.getstring(23, 2, 6) = "ACMCS3" Then
            ActiveCell.Offset(0, 1).Select
            ActiveCell = "Please Sweep The Fund"
            
            ActiveCell.Offset(1, -4).Select
            Fund = ActiveCell
            ActiveCell.Offset(0, 1).Select
            EMonth = ActiveCell
            ActiveCell.Offset(0, 1).Select
            EDate = ActiveCell
            ActiveCell.Offset(0, 1).Select
            EYear = ActiveCell
            
                              
        Else: With myscreen
        
      
        FIMS = myscreen.getstring(21, 52, 6)
        
        ActiveCell.Offset(0, 1).Select
        ActiveCell = FIMS
    
        ActiveCell.Offset(1, -4).Select
        Fund = ActiveCell
        ActiveCell.Offset(0, 1).Select
        EMonth = ActiveCell
        ActiveCell.Offset(0, 1).Select
        EDate = ActiveCell
        ActiveCell.Offset(0, 1).Select
        EYear = ActiveCell
        
        
    End With
    
   
   
    End If
    Loop
        
    Range("M3").Select
            
End Sub

