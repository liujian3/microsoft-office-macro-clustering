Attribute VB_Name = "Module5"
Sub Navroll()
Attribute Navroll.VB_Description = "Macro recorded 7/13/04 by RichardB"
Attribute Navroll.VB_ProcData.VB_Invoke_Func = " \n14"
'
' Navroll Macro
' Macro recorded 1/28/05 by Prim
'

'
    Columns("I:L").Select
    Selection.EntireColumn.Hidden = False
    
    Range("K12:K25").Select
    Selection.Copy

    Range("L12").Select
    Selection.PasteSpecial Paste:=xlValues, Operation:=xlNone, SkipBlanks:= _
        False, Transpose:=False
    Application.CutCopyMode = False
    
    Columns("J:J").Select
    Selection.EntireColumn.Hidden = True
    
    Range("K7").Select

'Roll Prior Day's Exchange Rates
'EUR
    
    Range("E4").Select
    Selection.Copy
    Range("E3").Select
    Selection.PasteSpecial Paste:=xlValues, Operation:=xlNone, SkipBlanks:= _
        False, Transpose:=False
    
   
'GBP
    Range("E6").Select
    Application.CutCopyMode = False
    Selection.Copy
    Range("E5").Select
    Selection.PasteSpecial Paste:=xlValues, Operation:=xlNone, SkipBlanks:= _
        False, Transpose:=False

'EURGBP
    Range("E8").Select
    Application.CutCopyMode = False
    Selection.Copy
    Range("E7").Select
    Selection.PasteSpecial Paste:=xlValues, Operation:=xlNone, SkipBlanks:= _
        False, Transpose:=False

'Clear Current Day's Date

    Range("E4").Select
    Application.CutCopyMode = False
    ActiveCell.FormulaR1C1 = "0"
    Range("E6").Select
    ActiveCell.FormulaR1C1 = "0"
    Range("E8").Select
    ActiveCell.FormulaR1C1 = "0"
    Range("E9").Select
    
End Sub
