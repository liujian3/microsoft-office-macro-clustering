Attribute VB_Name = "Module1"
Private Sub cmdProtection_Click()
    Call ToggleProtectionAllSheets
End Sub
Public Sub ToggleProtectionAllSheets()
    Dim oSheet As Worksheet
    
    On Error GoTo ErrHandler
    
    'Enable protection
    If cmdProtection.Caption = "Protect" Then
        Call ProtectAllSheets
    'Disable protection
    Else
        Call UnProtectAllSheets
    End If

    Exit Sub

ErrHandler:
    Resume Next
End Sub
Public Sub ProtectAllSheets()
    Dim oSheet As Worksheet
    
    On Error GoTo ErrHandler
    
    For Each oSheet In ThisWorkbook.Worksheets
        'Enable protection
        With Worksheets(oSheet.Name)
            .Protect , UserInterfaceOnly:=True
            .EnableOutlining = True
            'Password:="Password123"
        End With
    Next oSheet

    'cmdProtection.Caption = "Unprotect"
    Exit Sub

ErrHandler:
    Resume Next
End Sub
Public Sub UnProtectAllSheets()
    Dim oSheet As Worksheet
    
    On Error GoTo ErrHandler
    
    For Each oSheet In ThisWorkbook.Worksheets
        'Disable protection
        With Worksheets(oSheet.Name)
            .Unprotect
            'Password:="Password123"
        End With
    Next oSheet

    cmdProtection.Caption = "Protect"
    Exit Sub

ErrHandler:
    Resume Next
End Sub

