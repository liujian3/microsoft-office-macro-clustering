                ElseIf cell.Text = "U" Then
                    cell.EntireColumn.Hidden = False
                End If
            End If
       Next
    Sheets("Pricing Worksheet").Select
    ActiveSheet.Protect
    ActiveSheet.EnableSelection = xlUnlockedCells
End Sub
