Private Sub Worksheet_Change(ByVal Target As Range)
    Set KeyCells = Range("H17:H2883")
    Set OtherCells = Range("W17:X2883")
    Dim iRow As Integer
    Dim iColumn As Integer
    Dim iChoice As String
    
    iRow = ActiveCell.Row
    iColumn = ActiveCell.Column
    iChoice = ActiveCell.Value
    ActiveSheet.Unprotect
    If Not Application.Intersect(KeyCells, Range(Target.Address)) _
           Is Nothing Then

       If iChoice = "Effort" Then
        'Put Formula in Cell
       ' MsgBox "Cell " & Target.Address & " has changed to Effort"
        ActiveSheet.Cells(iRow, iColumn + 15).Select
        ActiveCell.FormulaR1C1 = _
        "=(RC[-13]*R8C10)+(RC[-12]*R8C11)+(RC[-11]*R8C12)+(RC[-10]*R8C13)+(RC[-9]*R8C14)+(RC[-8]*R8C15)+(RC[-7]*R8C16)+(RC[-6]*R8C17)+(RC[-5]*R8C18)+(RC[-4]*R8C19)+(RC[-3]*R8C20)+(RC[-2]*R8C21)"
        Selection.Locked = True
       'Change formatting of Cell
         With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = -0.249977111117893
        .PatternTintAndShade = 0
         End With
        Selection.Font.Bold = False
       'Change Formatting of Range
        ActiveSheet.Range(Cells(iRow, iColumn + 2), Cells(iRow, iColumn + 13)).Select
         Selection.Locked = False
        With Selection.Interior
        .Pattern = xlNone
        .TintAndShade = 0
        .PatternTintAndShade = 0
        End With
        Selection.Font.Bold = False
        ActiveSheet.Cells(iRow, iColumn + 2).Select
    ElseIf iChoice = "Value" Then
        'MsgBox "Cell " & Target.Address & " has changed to Value"
       'Clear Formula
        ActiveSheet.Cells(iRow, iColumn + 15).Select
        Selection.Locked = False
        Selection.ClearContents
        'Change Formatting of Cell
         With Selection.Interior
        .Pattern = xlNone
        .TintAndShade = 0
        .PatternTintAndShade = 0
         End With
         Selection.Font.Bold = False
         'Change Formatting of Range
        ActiveSheet.Range(Cells(iRow, iColumn + 2), Cells(iRow, iColumn + 13)).Select
        Selection.Locked = True
        Selection.ClearContents
        With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = -0.249977111117893
        .PatternTintAndShade = 0
         End With
        ActiveSheet.Cells(iRow, iColumn + 15).Select
        ElseIf iChoice = "Please Select" Then
       Selection.Font.Bold = False
       'MsgBox "Cell " & Target.Address & " has changed to Value"
       'Clear Formula
        ActiveSheet.Cells(iRow, iColumn + 15).Select
        Selection.ClearContents
        Selection.Locked = True
       'Change Formatting of Cell
         With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = -0.249977111117893
        .PatternTintAndShade = 0
         End With
         'Change Formatting of Range
         ActiveSheet.Range(Cells(iRow, iColumn + 2), Cells(iRow, iColumn + 13)).Select
         Selection.ClearContents
         Selection.Locked = True
        With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = -0.249977111117893
        .PatternTintAndShade = 0
         End With
        Selection.Font.Bold = False
        ActiveSheet.Cells(iRow, iColumn).Select
        
        End If
        If Not Application.Intersect(OtherCells, Range(Target.Address)) _
           Is Nothing Then
        
        End If
    ActiveSheet.Protect
    ActiveSheet.EnableSelection = xlUnlockedCells
    End If
