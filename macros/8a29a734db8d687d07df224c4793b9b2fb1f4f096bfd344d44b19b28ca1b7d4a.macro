Dim DevM As DEVMODE

Sub ChangeRes(iWidth As Single, iHeight As Single)
    Dim A As Boolean
    Dim i As Integer
    i = 0
    Do
        A = EnumDisplaySettings(0&, i, DevM)
        i = i + 1
    Loop Until (A = False)

    Dim B&
    DevM.dmFields = DM_PELSWIDTH Or DM_PELSHEIGHT

    DevM.dmPelsWidth = iWidth
    DevM.dmPelsHeight = iHeight

    ChangeDisplaySettings DevM, 0
End Sub



