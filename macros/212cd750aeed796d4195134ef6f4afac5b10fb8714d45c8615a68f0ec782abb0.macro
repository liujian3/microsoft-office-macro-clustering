Sub Preise()
    
    Inaktiv
    BlattschutzAus
    
    Dim i As Integer
    
    Cells.Select
    Selection.EntireRow.Hidden = False
   
    For i = 7 To 65 Step 2
        If Range("B" & i).Value = "Kinder" Then
        Else
            Rows(i & ":" & i).Select
            Selection.EntireRow.Hidden = True
        End If
    Next i
    
    For i = 6 To 64 Step 2
        If Range("B" & i).Value = "" Then
            Rows(i & ":" & i).Select
            Selection.EntireRow.Hidden = True
        End If
    Next i

    Range("A1").Select
    BlattschutzAn

    
    If Sheets("Preisaushang").Range("A1").Value = 1 Then
        Sheets("Preisaushang Kinotag").Visible = True
        Sheets("Preisaushang Kinotag").Select
        BlattschutzAus
    
        Cells.Select
        Selection.EntireRow.Hidden = False
   
        For i = 7 To 65 Step 2
            If Range("B" & i).Value = "Kinder" Then
            Else
                Rows(i & ":" & i).Select
                Selection.EntireRow.Hidden = True
            End If
        Next i
    
        For i = 6 To 64 Step 2
            If Range("B" & i).Value = "" Then
                Rows(i & ":" & i).Select
                Selection.EntireRow.Hidden = True
            End If
        Next i

        Range("A1").Select
    
        BlattschutzAn
        Sheets("Preisaushang").Select
    
    Else
    
        Sheets("Preisaushang Kinotag").Visible = False
    
    End If
    
    
    Aktiv
    
    
End Sub

