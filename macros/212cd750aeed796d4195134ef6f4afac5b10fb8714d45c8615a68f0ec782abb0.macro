Sub Einblenden()
    Dim Anzahl As Integer
    Dim Leinwandzahl As Integer
    Dim Zaehler As Integer
    Anzahl = Range("C1").FormulaR1C1 - 1
    Leinwandzahl = Range("D1").Value
    Inaktiv
    BlattschutzAus
    
    For Zaehler = 1 To Leinwandzahl
        Range(4 + (Zaehler - 1) * 21 & ":" & Anzahl + 4 + ((Zaehler - 1) * 21)).Select
        'Range("4:" & Anzahl + 4 & ",25:" & Anzahl + 25 & ",46:" & Anzahl + 46 & ",67:" & Anzahl + 67 & ",88:" & Anzahl + 88 & ",109:" & Anzahl + 109 & ",130:" & Anzahl + 130 & ",151:" & Anzahl + 151 & ",172:" & Anzahl + 172).Select
        Selection.EntireRow.Hidden = False
    Next Zaehler
    
    BlattschutzAn
    Aktiv
    Range("A2").Select
End Sub

