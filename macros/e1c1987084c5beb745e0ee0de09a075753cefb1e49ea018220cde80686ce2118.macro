Attribute VB_Name = "Module1"

Sub worksheet_change(ByVal target As Range)
Set target = Range("AF5")
If target.Value = "Yes" Then
 Call Hide_Row
End If
If target.Value <> "Yes" Then
 Call Hide_Row
End Sub


Sub Hide_Row()
Attribute Hide_Row.VB_ProcData.VB_Invoke_Func = " \n14"
'
' Hide_Row Macro
'

'
If Range(AF5).Value = "Yes" Then
        Range("D5").Activate
    Selection.EntireRow.Hidden = False
Else: Range("D5").Activate
 Selection.EntireRow.Hidden = True
End Sub
