Private Sub txtQt_Change()
If txtQt.Text <> "" Then
    If (Asc(Mid(txtQt.Text, Len(txtQt.Text))) >= Asc("0") And Asc(Mid(txtQt.Text, Len(txtQt.Text))) <= Asc("9")) Or Asc(Mid(txtQt.Text, Len(txtQt.Text))) = Asc(",") Then
          'la valeur est un chiffre
    Else
          ' la valeur n'est pas un chiffre je la retire
        txtQt.Text = Mid(txtQt.Text, 1, Len(txtQt.Text) - 1)
    End If
End If
End Sub

