Sub Disclaimer()
Attribute Disclaimer.VB_Description = "Macro recorded 7/08/2006 by dorizzis"
Attribute Disclaimer.VB_ProcData.VB_Invoke_Func = "Project.NewMacros.test2"
'
' Disclaimer Macro edited 20-Nov-08 by Ruud.Smit
'

    Selection.GoTo What:=wdGoToBookmark, Name:="TableDisc"
    Selection.Tables(1).Select
    Selection.Delete Unit:=wdCharacter, Count:=1

    Selection.GoTo What:=wdGoToBookmark, Name:="TableDisc"
    Selection.Find.ClearFormatting
    With Selection.Find
        .Text = ""
        .Replacement.Text = ""
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = False
        .MatchKashida = False
        .MatchDiacritics = False
        .MatchAlefHamza = False
        .MatchControl = False
        .MatchWildcards = False
        .MatchSoundsLike = False
        .MatchAllWordForms = False
    End With
    Selection.TypeText Text:="Disclaimer"
    Selection.TypeParagraph
    Selection.MoveUp Unit:=wdLine, Count:=1
    Selection.EndKey Unit:=wdLine, Extend:=wdExtend
    Selection.Style = ActiveDocument.Styles("DiscHead")
    Selection.MoveDown Unit:=wdLine, Count:=1
    Selection.TypeText Text:="This report has been prepared on behalf of and for the exclusive use of "
    Selection.Fields.Add Range:=Selection.Range, Type:=wdFieldEmpty, Text:= _
        "DOCPROPERTY  Client ", PreserveFormatting:=True
    Selection.TypeText Text:=", and is subject to and issued in accordance with the agreement between "
    Selection.Fields.Add Range:=Selection.Range, Type:=wdFieldEmpty, Text:= _
        "DOCPROPERTY  Client ", PreserveFormatting:=True
    Selection.TypeText Text:=" and INTECSEA B.V.  INTECSEA B.V. accepts no liability or responsibility whatsoever for it in respect of any use of or reliance upon this report by any third party."
    Selection.TypeParagraph
    Selection.TypeText Text:="Copying this report without the permission of "
        Selection.Fields.Add Range:=Selection.Range, Type:=wdFieldEmpty, Text:= _
        "DOCPROPERTY  Client ", PreserveFormatting:=True
    Selection.TypeText Text:=" or INTECSEA B.V. is not permitted."
End Sub
