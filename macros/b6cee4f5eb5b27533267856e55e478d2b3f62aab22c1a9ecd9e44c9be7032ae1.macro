Private Sub CommandOK_Click()
    Frm_Update.Hide
    ActiveDocument.Fields.Update
    ActiveDocument.PrintPreview
    ActiveDocument.ClosePrintPreview

    MsgBox "If you wish to update the field information, first click the Add-Ins tab then the INTECSEA Update button." _
    & vbCrLf & vbCrLf & "Fields in headers and footers will update on printing.", vbInformation, "Information"
        
    MsgBox "Please, do not save this document as a .DOCX file." _
    & vbCrLf & vbCrLf & "Save this document as a .DOCM file (or .DOC for Office 2003 compatibility) to retain macro functionality." _
    , vbInformation, "Warning for MS Office 2007/2010 Users"
    
    If ActiveDocument.Bookmarks.Exists("Synopsis") = True Then
        ActiveDocument.Bookmarks("Synopsis").Select
    
    End If
    
End Sub

