Attribute VB_Name = "Module1"
Function nxLibTwoColumnData(heading1, heading2, data1 As Range, data2 As Range, someID As String) As String
    If data1.Cells.Count <> data2.Cells.Count Then
        nxLibTwoColumnData = "Error: data sizes are different"
        Exit Function
    End If
    Dim heads(): ReDim heads(1 To 2)
    heads(1) = heading1
    heads(2) = heading2
    Dim values(): ReDim values(1 To data1.Cells.Count, 1 To 2)
    For i = 1 To data1.Cells.Count
        values(i, 1) = data1.Cells(i).Value
        values(i, 2) = CDbl(data2.Cells(i).Value)
    Next
    nxLibTwoColumnData = Application.Run("NxLibData", heads, values, someID)(1)
End Function


Function nxLibTwoColumnData2(heading1, heading2, data1 As Range, data2 As Range, someID As String) As String
    nxLibTwoColumnData2 = "here"
End Function


