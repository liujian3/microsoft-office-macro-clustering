Attribute VB_Name = "Module1"
Sub reset_filters()
Attribute reset_filters.VB_ProcData.VB_Invoke_Func = " \n14"
'
' reset_filters Macro
'

'
Application.ScreenUpdating = False

ActiveSheet.Unprotect
ActiveWorkbook.Unprotect "ldod1988"

ActiveSheet.ChartObjects("Chart 3").Activate
    ActiveChart.PlotArea.Select
    ActiveChart.FullSeriesCollection(1).Select
    ActiveChart.FullSeriesCollection(2).Select
    Application.CommandBars("Format Object").Visible = False
    With Selection.Format.Line
        .Visible = msoFalse
        .ForeColor.RGB = RGB(192, 0, 0)
    End With
    ActiveChart.FullSeriesCollection(3).Select
    Application.CommandBars("Format Object").Visible = False
    With Selection.Format.Line
        .Visible = msoFalse
        .ForeColor.RGB = RGB(192, 0, 0)
    End With
     ActiveChart.FullSeriesCollection(4).Select
    Application.CommandBars("Format Object").Visible = False
    With Selection.Format.Line
        .Visible = msoFalse
        .ForeColor.RGB = RGB(192, 0, 0)
    End With
     ActiveChart.FullSeriesCollection(5).Select
    Application.CommandBars("Format Object").Visible = False
    With Selection.Format.Line
        .Visible = msoFalse
        .ForeColor.RGB = RGB(192, 0, 0)
    End With
        Range("N5").Select
    Selection.ClearContents
    ActiveWorkbook.SlicerCaches("Slicer_Departments").ClearManualFilter
    ActiveWorkbook.SlicerCaches("NativeTimeline_Month").ClearDateFilter
    Sheets("Pivot Tables").Visible = True
    Sheets("Pivot Tables").Select
    ActiveWindow.SmallScroll Down:=-48
    With ActiveSheet.PivotTables("Trigger Points Salary").PivotFields("Month")
        .PivotItems("(blank)").Visible = False
    End With
    With ActiveSheet.PivotTables("Proportion Pivot").PivotFields("Month")
        .PivotItems("(blank)").Visible = False
    End With
    With ActiveSheet.PivotTables("Proportion Pivot").PivotFields("Departments")
        .PivotItems("(blank)").Visible = False
    End With
    ActiveWindow.SmallScroll Down:=18
    With ActiveSheet.PivotTables("Cost of Sickness Pivot").PivotFields("Month")
        .PivotItems("(blank)").Visible = False
    End With
    With ActiveSheet.PivotTables("Cost of Sickness Pivot").PivotFields( _
        "Departments")
        .PivotItems("(blank)").Visible = True
    End With
    With ActiveSheet.PivotTables("Cost of Sickness Pivot").PivotFields( _
        "Departments")
        .PivotItems("(blank)").Visible = False
    End With
    Sheets("Pivot Tables").Visible = False
    Sheets("Sickness").Select
    Range("A3").Select
     ActiveSheet.Protect DrawingObjects:=True, Contents:=True, Scenarios:=True _
        , AllowUsingPivotTables:=True
        ActiveWorkbook.Protect "ldod1988"

End Sub
