Private Sub Worksheet_Change(ByVal Target As Range)
    Dim KeyCells As Range
    ActiveWorkbook.Unprotect "ldod1988"

    ' The variable KeyCells contains the cells that will
    ' cause an alert when they are changed.
    Set KeyCells = Range("V15")
    
    If Not Application.Intersect(KeyCells, Range(Target.Address)) _
           Is Nothing Then

        ' Display a message when one of the designated cells has been
        ' changed.
        Sheets("Pivot Tables").Visible = True
         Sheets("Pivot Tables").Select
         ActiveSheet.PivotTables("HR Cases No Open Cases").PivotFields("Month Notified"). _
        ClearAllFilters
    ActiveSheet.PivotTables("HR Cases No Open Cases").PivotFields("Month Notified"). _
        PivotFilters.Add2 Type:=xlBefore, Value1:=Format(Cells(15, 22), "Long Date")
        
        ActiveSheet.PivotTables("HR Cases No Closed Cases").PivotFields("Month Notified"). _
        ClearAllFilters
    ActiveSheet.PivotTables("HR Cases No Closed Cases").PivotFields("Month Notified"). _
        PivotFilters.Add2 Type:=xlBefore, Value1:=Format(Cells(15, 22), "Long Date")
        
            Sheets("Pivot Tables").Select
    ActiveWindow.SelectedSheets.Visible = False
    Sheets("HR Activity").Select
       ActiveWorkbook.Protect "ldod1988"
        
       
    End If
End Sub

