'******************************************************************************
'*Procedure   : cmdOK_Click
'*Description : Best|fffd|tigung der Abfrage durch User
'*              Call bCostCenterOK
'*              Call bDateOK
'*              Call bPersonalNoOK
'*              Call bSheetNewDouble
'*              Call FirstRun
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Private Sub cmdOK_Click()
Dim i As Integer
Dim bFirstRun As Boolean            'Index kennzeichnet Erstaufruf
Dim bInputFault As Boolean          'Index f|fffd|r Eingabefehler
Dim dtBegin As Date                 'Montagsdatum laut Dialogfeldeingabe
Dim dtEnd As Date                   'Berechnetes Sonntagsdatum
Dim nCalWeek As Integer             'Kalenderwoche
Dim nYear As Integer                'Jahreszahl
Dim oCells As Range                 'Bereichsfelder f|fffd|r Namenszuordnung
Dim strPeriod As String             'Zeitraum der Kalenderwoche im Datumsformat

With Application
    .ScreenUpdating = False
    .EnableEvents = False
End With

'------------------------------------------------------------------------------
'Pr|fffd|fung auf Eingabefehler/Blattkontrolle ob Woche (KW-Blatt) schon vorhanden
If frmNewWeekReport.bDateOK = True And _
  frmNewWeekReport.bPersonalNoOK = True Then
    nYear = Year(txtDate.Value)
    nCalWeek = modConvert.nISOWeek(txtDate.Value)
    If frmNewWeekReport.bSheetNewDouble(nYear, nCalWeek) = True Then
        bInputFault = True
    Else
        bInputFault = False
    End If
Else
    bInputFault = True
End If

If bInputFault = False Then
    If ActiveSheet.Name = "xx" Then
        '----------------------------------------------------------------------
        'Erstaufruf der Datei - Neues Blatt schon vorhanden
        bFirstRun = True
        Sheets("xx").Name = gstrSheetAct
    Else
        '----------------------------------------------------------------------
        'Neues Blatt einf|fffd|gen
        Sheets("NEU").Visible = True
        Sheets("NEU").Copy After:=Sheets(Sheets.Count)
        Sheets("NEU").Visible = xlVeryHidden
        Sheets(Sheets.Count).Name = gstrSheetAct
    End If

    '--------------------------------------------------------------------------
    'Inhalte |fffd|bertragen
    ActiveSheet.Unprotect "DUERR_APT"
    Cells(3, 5).Value = nCalWeek
    Cells(4, 5).Value = nYear
    dtBegin = txtDate.Value
    dtEnd = dtBegin + 6
    strPeriod = dtBegin & " - " & dtEnd
    Cells(3, 15).Value = txtDescription.Value
    Cells(4, 15).Value = strPeriod
    Cells(1, 30).Value = txtShortMark.Value
    Cells(2, 30).Value = txtDepartment.Value
    Cells(3, 30).Value = txtPersonalNo.Value
    Cells(4, 30).Value = txtCostCenter.Value
    Cells(7, 20).Value = txtCostCenter.Value
    Cells(10, 2).Value = dtBegin
    Unload Me

    '--------------------------------------------------------------------------
    'Namenszuordnung im aktuellen Blatt mit Bereichsver|fffd|nderngen
    ActiveWorkbook.Names.Add Name:="AA_" & gstrSheetAct, RefersToR1C1:="=" & _
    gstrSheetAct & "!R7C2:R8C2"
    Set oCells = Range("B12,B16,B20,B24,B28,B32,B36")
    With oCells.Validation
        .Delete
        .Add Type:=xlValidateList, AlertStyle:=xlValidAlertStop, Operator:= _
         xlBetween, Formula1:="=AA_" & gstrSheetAct
        .IgnoreBlank = True
        .InCellDropdown = True
        .ShowInput = True
        .ShowError = True
    End With
    ActiveSheet.Protect "DUERR_APT", DrawingObjects:=False, contents:=True, _
                                     Scenarios:=True, AllowFormattingRows:=True
    If bFirstRun = True Then
        frmNewWeekReport.FirstRun strPeriod
    End If
    
    '--------------------------------------------------------------------------
    'Projektexplorer initialisieren
    modOverview.AllProjects
   
    '--------------------------------------------------------------------------
    'Kontrolle ob Erstaufruf
    If bFirstRun = False Then
        '----------------------------------------------------------------------
        'Kontrolle der Anzahl der Daten
        Select Case gnDataCount
        Case Is <= 1
            GoTo ErrorHandler
        Case Is > 1
            frmProjectSelect.Show
        End Select
    End If
End If
bFirstRun = False

'------------------------------------------------------------------------------
'Namenszuweisungen ohne Bezug l|fffd|schen
Application.ScreenUpdating = False
modDelete.DeleteNames
With Application
    .ScreenUpdating = True
    .EnableEvents = True
End With
Sheets(gstrSheetAct).Activate
Cells(7, 2).Select
Exit Sub

'************
ErrorHandler:
MsgBox "Keine Auftr|fffd|ge zur Auswahl vorhanden!!!!", vbOKOnly + vbInformation, "Info!"
bFirstRun = False
Application.ScreenUpdating = True

End Sub         'End of cmdOK_Click


'******************************************************************************
'*Procedure   : cmdOKCopy_Click
'*Description : Best|fffd|tigung der Abfrage durch User mit Copy-Funktion f|fffd|r Vorwoche
'*              Call bDateOK
'*              Call bPersonalNoOK
'*              Call bSheetNewDouble
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Private Sub cmdOKCopy_Click()
Dim i As Integer
Dim bInputFault As Boolean          'Index f|fffd|r Eingabefehler
Dim dtBegin As Date                 'Montagsdatum laut Dialogfeldeingabe
Dim dtEnd As Date                   'Berechnetes Sonntagsdatum
Dim nCalWeek As Integer             'Kalenderwoche
Dim nYear As Integer                'Jahreszahl
Dim oCells As Range                 'Zellen mit G|fffd|ltigkeitszuordnung
Dim oNames As Object                'Angelegte Namen der Arbeitsmappe
Dim strPeriod As String             'Zeitraum der Kalenderwoche im Datumsformat
Dim strRange As String              'Bereichsadressen der Namen
Dim strValidName As String          'G|fffd|ltigkeitbezeichnungen Zelle
Dim strSheetOld As String           'Zwischenspeicherung von gstrSheetOld

If ActiveSheet.Index = 5 Then
    GoTo ErrorHandler
End If

With Application
    .ScreenUpdating = False
    .EnableEvents = False
End With

'------------------------------------------------------------------------------
'Pr|fffd|fung auf Eingabefehler/Blattkontrolle ob Woche (KW-Blatt) schon vorhanden
If frmNewWeekReport.bDateOK = True And _
  frmNewWeekReport.bPersonalNoOK = True Then
    nYear = Year(txtDate.Value)
    nCalWeek = modConvert.nISOWeek(txtDate.Value)
    If frmNewWeekReport.bSheetNewDouble(nYear, nCalWeek) = True Then
        bInputFault = True
    Else
        bInputFault = False
    End If
Else
    bInputFault = True
End If

If bInputFault = False Then

    '--------------------------------------------------------------------------
    'Blatt der Vorwoche einf|fffd|gen
    ActiveSheet.Copy After:=Sheets(Sheets.Count)
    Sheets(Sheets.Count).Name = gstrSheetAct

    '--------------------------------------------------------------------------
    'Neuen Namen f|fffd|r Projektbereich anlegen
    For Each oNames In ActiveWorkbook.Names
        If oNames.Name = "AA_" & gstrSheetOld Then
            strRange = oNames.RefersToR1C1
            strRange = "=" & gstrSheetAct & Right(strRange, _
                       Len(strRange) - InStr(strRange, "!") + 1)
            oNames.Delete
            ActiveWorkbook.Names.Add Name:="AA_" & gstrSheetAct, _
                                     RefersToR1C1:=strRange
        End If
    Next oNames
    
    '--------------------------------------------------------------------------
    'Inhalte |fffd|bertragen
    ActiveSheet.Unprotect "DUERR_APT"
    Cells(3, 5).Value = nCalWeek
    Cells(4, 5).Value = nYear
    dtBegin = txtDate.Value
    dtEnd = dtBegin + 6
    strPeriod = dtBegin & " - " & dtEnd
    Cells(3, 15).Value = txtDescription.Value
    Cells(4, 15).Value = strPeriod
    Cells(1, 30).Value = txtShortMark.Value
    Cells(2, 30).Value = txtDepartment.Value
    Cells(3, 30).Value = txtPersonalNo.Value
    Cells(4, 30).Value = txtCostCenter.Value
    Cells(7, 20).Value = txtCostCenter.Value
    i = 0
    Do
        i = i + 1
    Loop Until Cells(7 + i, 2).Interior.ColorIndex = -4142
    Cells(8 + i, 2).Value = dtBegin
    Unload Me
    
    '--------------------------------------------------------------------------
    'Stundenwerte der Tage auf Nullsetzen
    For Each oCells In ActiveSheet.UsedRange
        If oCells.Column = 3 And oCells.Interior.ColorIndex = -4142 _
          And oCells.Value > 0 Then
          oCells.Value = 0
        End If
    Next oCells
    ActiveSheet.Cells(7, 32).Value = ""

    '--------------------------------------------------------------------------
    'Namenszuordnungen im aktuell kopierten Blatt anpassen
    For Each oCells In ActiveSheet.UsedRange
        With oCells.Validation
        On Error Resume Next
        strValidName = .Formula1
        If Left(strValidName, 4) = "=AA_" Then
            .Delete
            .Add Type:=xlValidateList, AlertStyle:=xlValidAlertStop, Operator:= _
             xlBetween, Formula1:="=AA_" & gstrSheetAct
            .IgnoreBlank = True
            .InCellDropdown = True
            .ShowInput = True
            .ShowError = True
        End If
        strValidName = ""
        End With
    Next oCells
    ActiveSheet.Protect "DUERR_APT", DrawingObjects:=False, contents:=True, _
                                     Scenarios:=True, AllowFormattingRows:=True
    '--------------------------------------------------------------------------
    'Alte Projekteintr|fffd|ge l|fffd|schen
    
    strSheetOld = gstrSheetOld
    Application.EnableEvents = True
    ActiveSheet.Cells(8, 3).Select
    Application.EnableEvents = False
    
    Do While ActiveSheet.Cells(8, 3).Interior.ColorIndex = 34
        modDelete.DelData2
    Loop
    
    '--------------------------------------------------------------------------
    'Projektexplorer initialisieren
    modOverview.AllProjects
    
    '--------------------------------------------------------------------------
    'Projektauswahl starten
    gstrSheetOld = strSheetOld
    frmProjectSelect.Show
End If

'------------------------------------------------------------------------------
'Namenszuweisungen ohne Bezug l|fffd|schen
Application.ScreenUpdating = False
modDelete.DeleteNames
With Application
    .ScreenUpdating = True
    .EnableEvents = True
End With
Sheets(gstrSheetAct).Activate
Cells(7, 2).Select
Exit Sub

'************
ErrorHandler:
MsgBox "Transaktion in erster Woche nicht m|fffd|glich!", _
       vbOKOnly + vbInformation, "Info!"

End Sub         'End of cmdOKCopy_Click


