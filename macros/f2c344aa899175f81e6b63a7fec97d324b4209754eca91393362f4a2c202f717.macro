'******************************************************************************
'*Procedure   : Workbook_Open
'*Description : Prozedur zum Start der Datei; Initialisierung von Variablen
'*              Call frmNewWeekReport
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Private Sub Workbook_Open()

gnX = ActiveCell.Column
gnY = ActiveCell.Row
gnOldX = gnX
gnOldY = gnY
gstrActFile = ActiveWorkbook.Name
gstrSheetOld = ActiveSheet.Name

If ActiveSheet.Name = "xx" Then
    Application.EnableEvents = False
    frmNewWeekReport.Show
    Application.EnableEvents = True
    MsgBox "Alte Projekte aus alten Wochenberichten" & Chr(10) _
           & "k|fffd|nnen durch Anwahl der Projekt|fffd|bernahme" & Chr(10) _
           & "in der ersten Woche |fffd|bernommen werden.", _
           vbOKOnly + vbInformation, "Info!"
End If

End Sub         'End of Workbook_Open


'******************************************************************************
'*Procedure   : Workbook_BeforeClose
'*Description : Prozedur bevor Datei geschlossen werden kann
'*              Verhinderung von offenen Transaktionen
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Private Sub Workbook_BeforeClose(Cancel As Boolean)

Select Case gbfaultday
Case Is = True
    ActiveSheet.Cells(7, 2).Select
    Cancel = True
Case Is = False
    Application.EnableEvents = False
    If Len(ActiveSheet.Name) = 5 And Mid(ActiveSheet.Name, 3, 1) = "_" Then
        ActiveSheet.Cells(7, 2).Select
    End If
    Application.EnableEvents = True
End Select

End Sub         'End of Workbook_BeforeClose


'******************************************************************************
'*Procedure   : Workbook_BeforeSave
'*Description : Prozedur bevor Datei gespeichert werden kann
'*              Verhinderung von offenen Transaktionen
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Private Sub Workbook_BeforeSave(ByVal SaveAsUI As Boolean, Cancel As Boolean)

Select Case gbfaultday
Case Is = True
    ActiveSheet.Cells(7, 2).Select
    Cancel = True
Case Is = False
    If Len(ActiveSheet.Name) = 5 And Mid(ActiveSheet.Name, 3, 1) = "_" Then
        ActiveSheet.Cells(7, 2).Select
    End If
End Select

End Sub         'End of Workbook_BeforeSave

