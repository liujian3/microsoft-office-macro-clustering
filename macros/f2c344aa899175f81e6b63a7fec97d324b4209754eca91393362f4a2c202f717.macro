'******************************************************************************
'*Procedure   : CodeKiller
'*Description : L|fffd|scht alle vorhandenen Codezeilen aus der gew|fffd|hlten Datei
'*              sowie alle Module, Klassenmodule und Userforms
'*              Au|fffd|erdem auch alle shapes und Namenszuordnungen in der Datei
'*              Call By ExportFile
'*
'*Parameter   : gstractfile
'*Return      : None
'******************************************************************************
Sub CodeKiller(ByVal gstrActFile As String)
Dim i As Integer
Dim nLines As Integer               'Anzahl der Codezeilen
Dim oFile As Workbook               'Objektvariable f|fffd|r Exceldateien
Dim oRangeNames As Object           'Namensbereich
Dim oSheet As Worksheet             'Objektvariable f|fffd|r Wochenbl|fffd|tter
Dim shpShapes As Shape              'Objektvariable f|fffd|r Shapes
Dim strName As String               'Namenvariablen
Dim strType As String               'Typenvariablen

'------------------------------------------------------------------------------
'Codezeielen, Module, Klassenmodule und Userforms l|fffd|schen
Set oFile = Workbooks(gstrActFile)
For i = oFile.VBProject.VBComponents.Count To 1 Step -1
    strName = oFile.VBProject.VBComponents(i).Name
    nLines = oFile.VBProject.VBComponents(i).CodeModule.CountOfLines
    If nLines > 0 Then
        oFile.VBProject.VBComponents(i).CodeModule.DeleteLines 1, nLines
    End If
    strType = oFile.VBProject.VBComponents(i).Type
    If strType < 4 Then
        oFile.VBProject.VBComponents.Remove oFile.VBProject.VBComponents(i)
    End If
Next i

'------------------------------------------------------------------------------
'Shapes im Blatt und Namen l|fffd|schen
For Each oSheet In ActiveWorkbook.Sheets
    oSheet.Unprotect "DUERR_APT"
    For Each shpShapes In oSheet.Shapes
        shpShapes.Delete
    Next shpShapes
    For Each oRangeNames In ActiveWorkbook.Names
        Select Case InStr(oRangeNames.Name, "!Print_Area")
        Case Is = 0
            oRangeNames.Delete
        Case Is > 0
        End Select
    Next oRangeNames
    oSheet.Protect "DUERR_APT", DrawingObjects:=False, contents:=True, _
                                Scenarios:=True, AllowFormattingRows:=True
    oSheet.EnableSelection = xlNoSelection
Next oSheet

End Sub         'End of Codekiller
Attribute VB_Name = "modFormChanges"
Option Explicit
'******************************************************************************
'*Procedure   : FormCorrect
'*Description : Formularanpassung nach L|fffd|schvorg|fffd|ngen
'*              Call By DelData1
'*              Call By DelData2
'*
'*Parameter   : None
'*Return      : nY, nX
'******************************************************************************
Sub FormCorrect(ByRef nY As Integer, ByRef nX As Integer)

ActiveSheet.Unprotect "DUERR_APT"
Select Case Cells(nY, nX).Borders(xlEdgeTop).Weight
Case Is = xlMedium
    If Cells(nY, nX).Borders(xlEdgeBottom).Weight <> xlMedium Then
        Rows(nY).Delete shift:=xlUp
    Else
        Range("B" & nY, "AG" & nY).ClearContents
    End If
Case Else
    If Cells(nY, nX).Borders(xlEdgeBottom).Weight <> xlMedium Then
        Rows(nY).Delete shift:=xlUp
    Else
        Rows(nY).Delete shift:=xlUp
        nY = nY - 1
        Range("B" & nY, "AG" & nY).Borders(xlEdgeBottom).Weight = xlMedium
    End If
End Select
ActiveSheet.Protect "DUERR_APT", DrawingObjects:=False, contents:=True, _
                                 Scenarios:=True, AllowFormattingRows:=True

End Sub            'End of FormCorrect


'******************************************************************************
'*Procedure   : NewLine
'*Description : Erstellen einer neuen Eingabezeile am Ende eines Tages- oder
'*              des Projektbereichs
'*              Call By SelectionChange
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Sub NewLine()
Dim i As Integer

With Application
    .ScreenUpdating = False
    .EnableEvents = False
End With
If Sheets("|fffd|bersicht_Projekte").cmdWindowFixing.Value = True Then
    ActiveWindow.FreezePanes = False
End If
ActiveSheet.Unprotect "DUERR_APT"
ActiveSheet.Rows(gnY - 1).Copy
ActiveSheet.Rows(gnY - 1).Insert shift:=xlDown
With Range("B" & gnY, "AG" & gnY)
    .Borders(xlEdgeTop).Weight = xlThin
    .RowHeight = 13.5
    .ClearContents
    .Font.ColorIndex = 1
End With
Cells(gnY, 2).Select
If ActiveCell.Interior.ColorIndex = 34 Then
    ActiveCell.Value = "?"
    If ActiveCell.Offset(-2, 0).Interior.ColorIndex = 36 Then
        ActiveWorkbook.Names.Add Name:="AA_" & ActiveSheet.Name, RefersToR1C1:="=" & _
        ActiveSheet.Name & "!R7C2:R8C2"
    End If
End If
ActiveSheet.Protect "DUERR_APT", DrawingObjects:=False, contents:=True, _
                                 Scenarios:=True, AllowFormattingRows:=True

'------------------------------------------------------------------------------
'Bildschirmfixierung
If Sheets("|fffd|bersicht_Projekte").cmdWindowFixing.Value = True Then
    i = 0
    Do
        i = i + 1
    Loop Until Cells(7 + i, 2).Interior.ColorIndex = -4142
    '--------------------------------------------------------------------------
    'Kontrolle ob Bildschirmfixierung noch sinnvoll (1024x768)
    If i < 16 Then
        Range("A" & i + 7).Select
        ActiveWindow.FreezePanes = True
    End If
End If

Application.ScreenUpdating = True
Cells(gnY, 2).Select
Application.EnableEvents = True

End Sub   'End of NewLine
