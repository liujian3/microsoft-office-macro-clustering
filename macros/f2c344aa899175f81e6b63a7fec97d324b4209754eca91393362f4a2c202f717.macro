'******************************************************************************
'*Procedure   : Takeover
'*Description : Schon angelegte Projekte aus Vorg|fffd|ngerbericht |fffd|bernehmen
'*              Call By cmdProjectSelect_Click (Only first week in Report)
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Sub TakeOver()
Dim y As Integer
Dim strOldFile As String            'Name der Vorg|fffd|ngerdatei zur |fffd|bernahme von
                                    'schon angelegten Projekten

'------------------------------------------------------------------------------
'Option Daten aus altem Wochenbericht |fffd|bernehmen aktivieren
strOldFile = InputBox("Zur |fffd|bernahme von Werten aus anderen Wochen- " _
               & "berichten, erst alten Wochenbericht |fffd|ffnen." & Chr(10) _
               & "Danach diesen Dialog noch ein Mal aufrufen und in " & Chr(10) _
               & "Eingabezeile Dateinamen eingeben." & Chr(10) & Chr(10) _
               & "Option steht nur in erster Woche (Blatt " _
               & ActiveSheet.Name & ") zur" & Chr(10) _
               & "Verf|fffd|gung! Kann dort aber zu jedem Zeitpunkt immer" _
               & Chr(10) & "wieder aufgerufen werden." & Chr(10) & Chr(10) _
               & "Zur Beachtung:" & Chr(10) _
               & "3-stellige Typnummern werden nicht mehr |fffd|bernommen!" & Chr(10), _
               "Optionale |fffd|bernahme von Projekten aus altem Wochenbericht")
If strOldFile <> "" Then
    On Error GoTo ErrorHandler
    Workbooks(strOldFile).Sheets("|fffd|bersicht_Projekte").Activate
    Range("C500").End(xlUp).Select
    y = ActiveCell.Row
    gnDataCount = y - 8
    Workbooks(gstrActFile).Activate
    '--------------------------------------------------------------------------
    '|fffd|bergabe der Bezeichnung und Setzen des Indexes
    gstrActFile = strOldFile
    gbTakeOver = True
End If
Exit Sub
'************
ErrorHandler:
MsgBox "Datei nicht gefunden!" & Chr(10) & _
       "Bitte Datei |fffd|ffnen oder" & Chr(10) & _
       "Dateinamen korrigieren.", _
       vbOKOnly + vbInformation, "Fehler!"
       
End Sub         'End of TakeOver

Attribute VB_Name = "modUpdate"
Option Explicit
Dim mstrUpdFile As String           'Name der Updatedatei
Dim mstrUpdSheet As String          'Name des UpdateBlattes
Dim mstrOrgSheet As String          'Blattname des zu updatenden Blatts

'******************************************************************************
'*Procedure   : Update
'*Description : Update interner Programmparameter
'*              Call By cmdUpdate
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Sub Update()
Dim i As Integer
Dim nX As Integer                   'Koordinate - Spaltenwert
Dim nY As Integer                   'Koordinate - Zeilenwert
Dim oRange As Range                 'Bereich zur Namenszuweisung
Dim oNames As Object                'Angelegte Namen der Arbeitsmappe

If bUpdateCheck = True Then
    '--------------------------------------------------------------------------
    'Alte Namen + Originalblatt l|fffd|schen / Neue |fffd|bertragen
    Sheets(mstrOrgSheet).Visible = True
    Sheets(mstrOrgSheet).Select
    Select Case mstrOrgSheet
    Case Is = "Kunden"
        For Each oNames In ActiveWorkbook.Names
            If InStr(oNames.Name, "!Print_Area") = 0 _
              And InStr(oNames.Name, "!Print_Titles") = 0 _
              And InStr(oNames.Name, "AA_") = 0 _
              And oNames.Name <> "T|fffd|tigkeiten" _
              And oNames.Name <> "Dauer" Then
                oNames.Delete
            End If
        Next oNames
    Case Is = "T|fffd|tigkeit"
        For Each oNames In ActiveWorkbook.Names
            If oNames.Name = "T|fffd|tigkeiten" _
              Or oNames.Name = "Dauer" Then
                oNames.Delete
            End If
        Next oNames
    End Select
    ActiveWindow.SelectedSheets.Delete
    Sheets(mstrUpdSheet).Select
    Sheets(mstrUpdSheet).Name = mstrOrgSheet
    Sheets(mstrOrgSheet).Move before:=Sheets(3)
    '--------------------------------------------------------------------------
    'Namen wieder erstellen
    Sheets(mstrOrgSheet).Select
    Select Case mstrOrgSheet
    Case Is = "Kostenstellen"
    Case Is = "Kunden"
        Range("IV1").End(xlToLeft).Select
        nX = ActiveCell.Column
        Set oRange = Range(Cells(1, 2), Cells(1, nX))
        ActiveWorkbook.Names.Add Name:=Cells(1, 1).Value, _
          RefersTo:=oRange, Visible:=True
        For i = 2 To nX
            If InStr(Cells(1, i).Value, " ") > 0 Then
                GoTo ErrorHandler
            End If
        Next i
        For i = 2 To nX
            If Cells(4, i).Value = "" Then
            Set oRange = Cells(3, i)
        Else
            Range(Cells(3, i), Cells(50, i)).End(xlDown).Select
            nY = ActiveCell.Row
            Set oRange = Range(Cells(3, i), Cells(nY, i))
        End If
        ActiveWorkbook.Names.Add Name:=Cells(1, i).Value, _
          RefersTo:=oRange, Visible:=True
        Next i
    Case Is = "T|fffd|tigkeiten"
        Range("A100").End(xlUp).Select
        nY = ActiveCell.Row
        Set oRange = Range(Cells(2, 1), Cells(nY, 1))
        ActiveWorkbook.Names.Add Name:="T|fffd|tigkeiten", _
          RefersTo:=oRange, Visible:=True
        Range("B100").End(xlUp).Select
        nY = ActiveCell.Row
        Set oRange = Range(Cells(2, 2), Cells(nY, 2))
        ActiveWorkbook.Names.Add Name:="Dauer", RefersTo:=oRange, Visible:=True
    End Select
    Sheets(mstrOrgSheet).Visible = xlVeryHidden
    Sheets("|fffd|bersicht_Projekte").Activate
    MsgBox "Update wurde erfolgreich durchgef|fffd|hrt", _
           vbOKOnly + vbInformation, "Info!"
End If
Exit Sub

'************
ErrorHandler:
MsgBox "Jegliche Leerzeichen (f|fffd|hrende wie auch nachfolgende) bei den" _
  & Chr(10) & "Kundenbezeichnungen im Blatt Kunden_Update entfernen." _
  & Chr(10) & "Bei zusammengesetzten Bezeichnungen einen Unterstrich setzen." _
  & Chr(10) & Chr(10) & "Vorgang Update dann einfach wiederholen." _
  & Chr(10) & "Kundenliste ist jetzt unvollst|fffd|ndig!" _
  & Chr(10) & Chr(10) & "Fehlerhafte Kundenbezeichnung: " & Cells(1, i).Value, _
  vbOKOnly + vbCritical, "Update fehlgeschlagen"
Sheets(mstrOrgSheet).Visible = xlVeryHidden

End Sub         'End of Update


'******************************************************************************
'*Procedure   : bUpdateCheck
'*Description : Kontrolle ob Updatedatei auffindbar
'*              Call By Update
'*
'*Parameter   : None
'*Return      : bUpdateCheck
'******************************************************************************
Function bUpdateCheck() As Boolean
Dim strFileOld As String            '

bUpdateCheck = False
strFileOld = ActiveWorkbook.Name
On Error GoTo ErrorHandler
mstrUpdFile = InputBox("Bitte Update Dateinamen eingeben" & Chr(10) _
              & "Vorgabe entsprechend ver|fffd|ndern" & Chr(10) _
              & "(Datei muss in Excel ge|fffd|ffnet sein!)", "Update", _
              "myESS_Upd_Kunden_TA_mitL|fffd|ndercodes.xls / Update_T|fffd|tigkeiten.xls")
mstrUpdSheet = Workbooks(mstrUpdFile).Sheets(1).Name

'------------------------------------------------------------------------------
'Originaldatei zum Updaten festlegen
If InStr(1, mstrUpdFile, "Kunden", vbTextCompare) > 0 Then
    mstrOrgSheet = "Kunden"
End If
If InStr(1, mstrUpdFile, "T|fffd|tigkeiten", vbTextCompare) > 0 Then
    mstrOrgSheet = "T|fffd|tigkeiten"
End If

'------------------------------------------------------------------------------
'Verschieben des Updateblatts in Originaldatei
Workbooks(mstrUpdFile).Activate
Sheets(mstrUpdSheet).Select
Sheets(mstrUpdSheet).Move before:=Workbooks(strFileOld).Sheets(1)
Sheets(mstrUpdSheet).Select
bUpdateCheck = True
Exit Function

'************
ErrorHandler:
MsgBox "Kein Update m|fffd|glich!" & Chr(10) & Chr(10) & "Update nicht auffindbar." _
        & Chr(10) & Chr(10) & "Bitte Datei erst in" & Chr(10) & "Excel |fffd|ffnen.", _
        vbOKOnly + vbCritical, "Fehler!"
        
End Function    ' End of bUpdateCheck


