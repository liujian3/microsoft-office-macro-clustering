Private Sub UserForm_Initialize()
Dim i As Integer
Dim dtDateMonday As Date            'Aktuelles Montagsdatum der neuen Woche

'------------------------------------------------------------------------------
'Datenfelder des Dialogs mit Daten aus aktueller Woche f|fffd|llen
txtDescription.Value = Cells(3, 15).Value
txtShortMark.Value = Cells(1, 30).Value
txtDepartment.Value = Cells(2, 30).Value
txtPersonalNo.Value = Cells(3, 30).Value
txtCostCenter.Value = Cells(4, 30).Value

'------------------------------------------------------------------------------
'Erstes Tagesdatum in aktueller Woche lokalisieren
i = 0
Do
    i = i + 1
Loop Until Cells(8 + i, 2).Interior.ColorIndex = 36

If ActiveSheet.Name = "xx" Then
    '--------------------------------------------------------------------------
    'Berechnung des Montags- Datums bei Ersteingabe
    dtDateMonday = Date
    dtDateMonday = (Application.WorksheetFunction. _
                   RoundDown((dtDateMonday - 2) / 7, 0)) * 7 + 2
    txtDate.Value = dtDateMonday
    txtDescription.SetFocus
Else
    '--------------------------------------------------------------------------
    'Berechnung des neuen Montags- Datums aus aktueller Woche
    dtDateMonday = Cells(8 + i, 2).Value + 7
    txtDate.Value = dtDateMonday
    txtDate.SetFocus
    txtDate.SelStart = 0
    txtDate.SelLength = Len(txtDate.Value)
End If

End Sub         'End of Userform_Initialize


