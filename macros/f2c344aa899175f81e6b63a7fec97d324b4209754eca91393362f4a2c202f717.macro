Attribute VB_Name = "modSelection"
Option Explicit
'******************************************************************************
'*Procedure   : SelectionChange
'*Description : Cursorkoordinaten
'*              Call By Change (every Sheet)
'*              Call By SelectionChange (every Sheet)
'*              Call CalculateSumHours
'*              Call ConvertCountry
'*              Call ConvertTime
'*              Call bCtrlCalculate
'*              Call bCtrlValidArea
'*              Call CtrlDelData1
'*              Call DataCplDay
'*              Call DataHelpAndSet
'*              Call DayDataCpl
'*              Call NewLine
'*              Call OldData
'*
'*Parameter   : None
'*Return      : None
'************** ****************************************************************
Sub SelectionChange()

'------------------------------------------------------------------------------
'Zeitkonvertierung und Pr|fffd|fung L|fffd|ndercodeeinstellung
If Cells(gnY, gnX).Interior.ColorIndex = -4142 And Cells(gnY, 3).Value <> "" Then
    If Cells(gnY, 6).Value = "" Then
        '----------------------------------------------------------------------
        'Neueintrag
        Application.EnableEvents = False
        modConvert.ConvertTime
        modConvert.ConvertCountry
        Application.EnableEvents = True
    Else
        '----------------------------------------------------------------------
        '|fffd|nderung
        modConvert.ConvertTime
        modConvert.ConvertCountry
    End If
End If

'------------------------------------------------------------------------------
'Berechnung und Schutz von Datens|fffd|tzen
If modCtrlGeneral.bCtrlCalculate = True Then
    Application.EnableEvents = False
    modCtrlGeneral.CtrlDelData1
    modCalculate.CalculateSumHours gnY
    Application.EnableEvents = True
End If

'------------------------------------------------------------------------------
'Aktuelle Zellkoordinaten/Blattdaten speichern
gnX = ActiveCell.Column
gnY = ActiveCell.Row
gstrSheetOld = ActiveSheet.Name

'------------------------------------------------------------------------------
'Eingabekontrolle1 - Pr|fffd|fung auf Vollst|fffd|ndigkeit der Dateneingabe
modCtrlWeek.DayDataCpl

'------------------------------------------------------------------------------
'Eingabekontrolle2 - Pr|fffd|fung des korrekten Bereichs zur Eingabe
If modCtrlGeneral.bCtrlValidArea = True And gbfaultday = False Then

    '--------------------------------------------------------------------------
    'Eingabekontrolle3 - Pr|fffd|fung der Stundeneintr|fffd|ge
    modCtrlWeek.DataHelpAndSet

    '--------------------------------------------------------------------------
    'Altwerte bedingt zum optionalen R|fffd|cksetzen speichern
    modSelection.OldData gnY, gnX

    '--------------------------------------------------------------------------
    'Optionales Einf|fffd|gen einer neuen Zeile
    If Selection.Borders(xlEdgeTop).Weight = xlMedium _
      And Selection.Borders(xlLeft).LineStyle = xlNone _
      And Cells(gnY - 1, 2).Value <> "?" And Cells(gnY - 1, 2).Value <> "" Then
        modFormChanges.NewLine
    ElseIf Cells(gnY - 1, 2).Value = "?" Or Cells(gnY - 1, 2).Value = "" Then
        Cells(gnY - 1, 3).Select
    End If

    '--------------------------------------------------------------------------
    'Zellkoordinaten alt speichern
    gnOldX = gnX
    gnOldY = gnY
End If

End Sub         'End of SelectionChange


'******************************************************************************
