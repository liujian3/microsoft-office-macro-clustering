'*Procedure   : cmdTakeOver_Click
'*Description : Werte aus Dialog |fffd|bernehmen
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Private Sub cmdTakeOver_Click()
Dim i As Integer
Dim j As Integer
Dim nOffset As Integer              'Erste Zeile des neuen Projekteintrags
Dim vValue As Double                'notwendige Deklaration der Feldwerte als
                                    'Zahl vor |fffd|bergabe an Blatt

'------------------------------------------------------------------------------
'D|fffd|rrWert aktualisieren - Eigene Kostenstelle
vValue = Sheets("|fffd|bersicht_Projekte").Cells(9, 31).Value
Cells(7, 30).Value = vValue

'------------------------------------------------------------------------------
'Ort der ersten Eintragung lokalisieren
If Cells(mnYSheetNew, 2).Value >= mnYSheetNew - 7 Then
    nOffset = Cells(mnYSheetNew, 2).Value
Else
    nOffset = 0
End If

'------------------------------------------------------------------------------
'|fffd|bernahme der ausgew|fffd|hlten Werte aus dem Auswahlfensters
For i = 1 To (gnDataCount - 1)
  If Controls("CheckBox" & CStr(i)).Value = True And _
     Controls("CheckBox" & CStr(i)).Locked = False Then
    j = j + 1
    Cells(mnYSheetNew + j, 2).Select
    Application.EnableEvents = False
    ActiveSheet.Unprotect "DUERR_APT"
    ActiveCell.Value = nOffset + j
    Cells(mnYSheetNew + j, 3).Value = Controls("TextBox" & CStr(i + (i - 1) * 7))
    Cells(mnYSheetNew + j, 8).Value = Controls("TextBox" & CStr(i + (i - 1) * 7 + 1))
    Cells(mnYSheetNew + j, 14).Value = Controls("TextBox" & CStr(i + (i - 1) * 7 + 2))
    Cells(mnYSheetNew + j, 20).Value = Controls("TextBox" & CStr(i + (i - 1) * 7 + 3))
    Cells(mnYSheetNew + j, 24).Value = Controls("TextBox" & CStr(i + (i - 1) * 7 + 4))
    Cells(mnYSheetNew + j, 26).Value = Controls("TextBox" & CStr(i + (i - 1) * 7 + 5))
    vValue = Controls("TextBox" & CStr(i + (i - 1) * 7 + 6))
    Cells(mnYSheetNew + j, 28).Value = Application.Round(vValue, 2)
    vValue = Controls("TextBox" & CStr(i + (i - 1) * 7 + 7))
    Cells(mnYSheetNew + j, 30).Value = Application.Round(vValue, 2)
    Application.EnableEvents = True
    ActiveSheet.Protect "DUERR_APT", DrawingObjects:=False, contents:=True, _
                                     Scenarios:=True, AllowFormattingRows:=True
  End If
Next i
Unload Me

'------------------------------------------------------------------------------
'Einstellung der korrekten Farbgebung in der Projektansicht
Application.EnableEvents = False
modCalculate.SetHourColors
Application.EnableEvents = True

End Sub         'End of cmdTakeOver_Click


