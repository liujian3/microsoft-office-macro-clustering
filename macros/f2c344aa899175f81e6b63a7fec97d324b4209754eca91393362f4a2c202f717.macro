'******************************************************************************
'*Procedure   : DelData1
'*Description : L|fffd|schen von kompletten Datens|fffd|tzen im Projekt- und Wochenbereich
'*              Call By cmdDelete_Click (every Sheet)
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Sub DelData1()
Dim i As Integer                    'Laufvariable zur Tageseintragspr|fffd|fung
Dim strNoProject As String          'Aktive Projektnummer des zur L|fffd|schung
                                    'anstehenden Datensatzes

strNoProject = Cells(gnY, 2).Value

'------------------------------------------------------------------------------
'Kontrolle des L|fffd|schbereichs
If modCtrlGeneral.bCtrlValidArea = False Then
    GoTo ErrorHandler
End If
With Application
    .ScreenUpdating = False
    .EnableEvents = False
End With
If Cells(gnY, gnX).Interior.ColorIndex = 34 Then
    Select Case strNoProject
    Case Is = "0"
        MsgBox "Eigene Kostenstelle nicht zu l|fffd|schen", _
               vbOKOnly + vbCritical, "Fehler!"
    Case Is <> "0"
        '----------------------------------------------------------------------
        'Formular im Projektbereich anpassen - L|fffd|schung der Projektdaten
        modFormChanges.FormCorrect gnY, gnX
        i = 0
        '----------------------------------------------------------------------
        'Ersten Tageseintrag im Wochenbereich lokalisieren (Offsetwert)
        Do
            i = i + 1
        Loop Until Cells(gnY, 2).Offset(i, 0).Interior.ColorIndex = -4142
        '----------------------------------------------------------------------
        'Zum Projekt geh|fffd|rige Tageseintr|fffd|ge l|fffd|schen
        Do While Cells(gnY, 2).Offset(i, 0).Interior.ColorIndex <> 33
            If Cells(gnY, 2).Offset(i, 0).Value = strNoProject Then
                modFormChanges.FormCorrect gnY + i, 2
                i = i - 1
            End If
        i = i + 1
        Loop
    End Select
Else
    '--------------------------------------------------------------------------
    'Nur Tageseintrag l|fffd|schen
    If Cells(gnY, 3).Value <> "" Then
        Cells(gnY, 3).Value = 0
        '----------------------------------------------------------------------
        'Korrekturberechnung
        modCalculate.CalculateSumHours gnY
    End If
    modFormChanges.FormCorrect gnY, gnX
End If
With Application
    .ScreenUpdating = True
    .EnableEvents = True
End With
Cells(gnY, gnX).Select
Exit Sub

'************
ErrorHandler:
MsgBox "Kein Eintrag ausgew|fffd|hlt!", vbOKOnly + vbCritical, "Fehler!"

End Sub            'End of DelData1


