'******************************************************************************
'*Procedure   : bSheetNewDouble
'*Description : Pr|fffd|fung ob neu angelegtes Wochenblatt schon vorhanden
'*              Call By cmdOK_Click
'*
'*Parameter   : nYear,nWeek
'*Return      : bSheetNewDouble
'******************************************************************************
Function bSheetNewDouble(ByVal nYear As Integer, ByVal nWeek As Integer) As Boolean
Dim oSheet As Worksheet             'Objektvariable zum Pr|fffd|fen ob Neues Blatt
                                    'schon angelegt

bSheetNewDouble = False
Select Case nWeek
Case Is < 10
    Select Case nYear
    Case Is < 2010
        gstrSheetAct = "0" & nWeek & "_" & "0" & nYear - 2000
    Case Else
        gstrSheetAct = "0" & nWeek & "_" & nYear - 2000
    End Select
Case Else
    Select Case nYear
    Case Is < 2010
        gstrSheetAct = nWeek & "_" & "0" & nYear - 2000
    Case Else
        gstrSheetAct = nWeek & "_" & nYear - 2000
    End Select
End Select

For Each oSheet In ActiveWorkbook.Sheets
    If oSheet.Name = gstrSheetAct Then
        MsgBox "Kalenderwoche ist schon vorhanden!!!!" & Chr(10) & Chr(10) _
               & "Bitte entweder Eingabe korrigieren, korrektes KW- Blatt" _
               & Chr(10) & "ausw|fffd|hlen oder schon vorhandenes KW- Blatt l|fffd|schen!" _
               & Chr(10) & Chr(10) & "Nach Schlie|fffd|en dieses Dialogs wird die schon" _
               & Chr(10) & "vorhandene KW- Woche zur weiteren Bearbeitung" _
               & Chr(10) & "aktiviert!", vbOKOnly + vbCritical, "Fehler!"
        Unload Me
        Sheets(gstrSheetAct).Activate
        bSheetNewDouble = True
    End If
Next oSheet

End Function    'End of bSheetNewDouble
