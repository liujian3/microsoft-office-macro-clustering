Attribute VB_Name = "modCustomer"
Option Explicit
'******************************************************************************
'*Procedure   : DropDownPlants
'*Description : Erstellung des Werksbezugs zum ausgew|fffd|hlten Kunden
'*              Call By DataHelpAndSet
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Sub DropDownPlants()
Dim oCustomerName As Name           'Hinterlegte Kundennamen im Workbook
Dim strCustomer As String           'Kunde aus Userauswahl
Dim strVCustomer As String          'Kunde aus aktuellem Werksbezug
Dim strPlant As String              'Werksadresse im Blatt Kunden

Application.EnableEvents = False
If Cells(gnY, 3).Value <> "" Then
    strCustomer = Cells(gnY, 3).Value
    ActiveSheet.Unprotect "DUERR_APT"
    
    '--------------------------------------------------------------------------
    'Falschen Werks-/Kundenbezug l|fffd|schen
    If gnX = 3 And gbSheetChange = True Then
        With Cells(gnY, 8)
            On Error Resume Next
            strVCustomer = .Validation.Formula1
        End With
        If strCustomer <> Mid(strVCustomer, 2, Len(strVCustomer) - 1) Then
            With Cells(gnY, 8)
                .Validation.Delete
            End With
            Cells(gnY, 8).Value = ""
        End If
    End If
    
    '--------------------------------------------------------------------------
    'Neuen Werkbezug setzen
    If gnX = 8 Then
            With Selection.Validation
                .Delete
                .Add Type:=xlValidateList, AlertStyle:=xlValidAlertStop, _
                 Operator:=xlBetween, Formula1:="=" & strCustomer
            End With
        ActiveSheet.Protect "DUERR_APT", DrawingObjects:=False, contents:=True, _
                                         Scenarios:=True, AllowFormattingRows:=True
    End If
    
    '--------------------------------------------------------------------------
    'Bei nur einem Werk zu Kunde; Werk schon eintragen
    For Each oCustomerName In ActiveWorkbook.Names
        If gnX = 3 And oCustomerName.Name = strCustomer _
          And Left(oCustomerName.Value, 8) = "=Kunden!" Then
            Select Case Len(oCustomerName.Value)
            Case Is = 12
                strPlant = Mid(oCustomerName.Value, 10, 1) & _
                Right(oCustomerName.Value, 1)
                Cells(gnY, 8).Value = Sheets("Kunden").Range(strPlant).Value
            Case Is = 13
                If IsNumeric(Right(oCustomerName.Value, 2)) = True Then
                    strPlant = Mid(oCustomerName.Value, 10, 1) & _
                    Right(oCustomerName.Value, 2)
                Else
                    strPlant = Mid(oCustomerName.Value, 10, 2) & _
                    Right(oCustomerName.Value, 1)
                End If
                Cells(gnY, 8).Value = Sheets("Kunden").Range(strPlant).Value
            Case Is = 14
                strPlant = Mid(oCustomerName.Value, 10, 2) & _
                Right(oCustomerName.Value, 2)
                Cells(gnY, 8).Value = Sheets("Kunden").Range(strPlant).Value
            Case Else
                strPlant = ""
            End Select
        End If
    Next oCustomerName
ElseIf Cells(gnY, 3).Value = "" And Cells(gnY, 8).Value <> "" Then
    '--------------------------------------------------------------------------
    'Werkbezug und Zellinhalt l|fffd|schen
    Cells(gnY, 8).Select
    Selection.Validation.Delete
    Cells(gnY, 8).Value = ""
    Cells(gnY, 3).Select
End If
Application.EnableEvents = True

End Sub         'End of DropDownPlants
