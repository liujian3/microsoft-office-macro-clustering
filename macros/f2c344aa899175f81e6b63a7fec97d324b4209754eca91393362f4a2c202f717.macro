'******************************************************************************
'*Procedure   : AllProjects
'*Description : Erstellung der Projekt|fffd|bersicht im Blatt |fffd|bersicht Projekte
'*              Call by ExportFile
'*              Call by frmProjectSelect
'*              Call by Worksheet_Deactivate (every Sheet)
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Sub AllProjects()
Dim i As Integer
Dim j As Integer
Dim nPageReq As Integer             'Notwendige Seitenanzahl
Dim nPageExist As Integer           'Vorhandene Seitenanzahl
Dim oRange As Range                 'Bereich zur L|fffd|schung
Dim oSheet As Object                'Objektvariable f|fffd|r Wochenbl|fffd|tter
Dim nItem As Integer                'Letzte Item-Nummer im Blatt "|fffd|bersicht_Projekte"

'------------------------------------------------------------------------------
'Eventuelle Reste von Arrays l|fffd|schen
Erase marrstrCustomer, marrstrPlant, marrstrNotice, marrstrProject, marrstrType, _
      marrstrHGR, marrdTarget, marrdActual

'------------------------------------------------------------------------------
'Sicherheitsabfrage wegen eventuell unbeabsichtigter |fffd|nderungen
modOverview.NewTargetHours

'------------------------------------------------------------------------------
'Layout |fffd|bersicht an Eintragsumfang anpassen
nPageReq = Int(gnDataCount / 58)
If gnDataCount Mod 58 > 0 Then
    nPageReq = nPageReq + 1
End If

nItem = Sheets("|fffd|bersicht_Projekte").Range("B500").End(xlUp).Value
nPageExist = Int((nItem + 1) / 58)

Sheets("|fffd|bersicht_Projekte").Unprotect "DUERR_APT"
Set oRange = Sheets("|fffd|bersicht_Projekte").Range("C9", "AG" & nItem + 9)
With oRange
    .ClearContents
    .Font.ColorIndex = 1
End With

Select Case nPageReq - nPageExist
Case Is = 0
    '--------------------------------------------------------------------------
    'Notwendige Seiten gleich vorhandene Seiten
Case Is > 0
    '--------------------------------------------------------------------------
    'Neue Seiten anlegen
    For i = 1 To nPageReq - nPageExist
        Sheets("|fffd|bersicht_Projekte").Range("A9:AH66").Copy
        Sheets("|fffd|bersicht_Projekte").Range("A9").Insert shift:=xlDown
    Next i
Case Is < 0
    '--------------------------------------------------------------------------
    'Vorhandene Seiten l|fffd|schen
    For i = 1 To nPageExist - nPageReq
        Sheets("|fffd|bersicht_Projekte").Range("A9:AH66").Delete
    Next i
End Select

For i = 0 To 57 * nPageReq + nPageReq
    Sheets("|fffd|bersicht_Projekte").Cells(9 + i, 2).Value = i
Next i

'------------------------------------------------------------------------------
'Neue Liste im Blatt "|fffd|bersicht Projekte" erstellen
modOverview.ListNewValues 8 + nPageReq * 58
Sheets("|fffd|bersicht_Projekte").PageSetup.PrintArea = "$B$1:$AG$" & 8 + nPageReq * 58

With Sheets("|fffd|bersicht_Projekte")
    .Protect "DUERR_APT", DrawingObjects:=False, contents:=True, _
                          Scenarios:=True, AllowFormattingRows:=False
    .EnableSelection = xlNoSelection
End With

End Sub         'End of AllProjects


'******************************************************************************
'*Procedure   : NewTargetHours
'*Description : Neue Sollstunden eintragen
'*              Call By AllProjects
'*              Call By cmdNewTarget_Click (every Sheet)
'*              Call SetNewValues
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Sub NewTargetHours()
Dim i As Integer
Dim j As Integer
Dim dSetNew As Double               'Neuer Sollstundeneintrag
Dim dSetOld As Double               'Alter Sollstundeneintrag
Dim nA As Integer                   'Indexvariable f|fffd|r Datensatzaufruf Array
Dim nI As Integer                   'Indexvariable f|fffd|r Pr|fffd|fdurchlauf Array
Dim nMSG As Integer                 'R|fffd|ckgabewert Dialogfeldabfrage
Dim oSheet As Object                'Objektvariable f|fffd|r Wochenbl|fffd|tter
Dim IndexAktiv As Integer           'Index des aktiven Blattes

IndexAktiv = ActiveSheet.Index
'------------------------------------------------------------------------------
'Projektdaten auslesen und Stunden aufsummieren
For Each oSheet In Worksheets
    If Len(oSheet.Name) = 5 And Mid(oSheet.Name, 3, 1) = "_" Then
        i = 0
        Do While oSheet.Cells(7 + i, 3).Value <> "" And _
                 oSheet.Cells(7 + i, 3).Interior.ColorIndex = 34
            nI = 0
            Do
                '--------------------------------------------------------------
                'Vergleich der schon hinterlegten Datens|fffd|tze im Array mit denen
                'des aufgerufenen Wochenblattes
                If UCase(marrstrCustomer(nI)) = UCase(oSheet.Cells(7 + i, 3).Value) _
                  And UCase(marrstrPlant(nI)) = UCase(oSheet.Cells(7 + i, 8).Value) _
                  And marrstrNotice(nI) = oSheet.Cells(7 + i, 14).Value _
                  And marrstrProject(nI) = oSheet.Cells(7 + i, 20).Value _
                  And marrstrType(nI) = oSheet.Cells(7 + i, 24).Value _
                  And marrstrHGR(nI) = oSheet.Cells(7 + i, 26).Value Then
                    Select Case marrdTarget(nI)
                    Case Is = oSheet.Cells(7 + i, 28).Value
                        marrdActual(nI) = _
                        marrdActual(nI) + oSheet.Cells(7 + i, 32).Value
                        i = i + 1
                        nI = 0
                    Case Is <> oSheet.Cells(7 + i, 28).Value
                        For j = 0 To nA
                            If UCase(marrstrCustomer(j)) = _
                              UCase(oSheet.Cells(7 + i, 3).Value) _
                              And UCase(marrstrPlant(j)) = _
                              UCase(oSheet.Cells(7 + i, 8).Value) _
                              And marrstrNotice(j) = _
                              oSheet.Cells(7 + i, 14).Value _
                              And marrstrProject(j) = _
                              oSheet.Cells(7 + i, 20).Value _
                              And marrstrType(j) = _
                              oSheet.Cells(7 + i, 24).Value _
                              And marrstrHGR(j) = _
                              oSheet.Cells(7 + i, 26).Value Then
                                Select Case Sheets(oSheet.Name).Index
                                Case Is < IndexAktiv
                                    dSetOld = oSheet.Cells(7 + i, 28).Value
                                    dSetNew = marrdTarget(nI)
                                Case Is >= IndexAktiv
                                    dSetOld = marrdTarget(nI)
                                    dSetNew = oSheet.Cells(7 + i, 28).Value
                                End Select
                                nMSG = MsgBox("Neue Sollstundenzahl f|fffd|r alle " _
                                  & "KW|fffd|s |fffd|bernehmen?" & Chr(10) & Chr(10) _
                                  & "Zu |fffd|ndernde Projektdaten:" & Chr(10) & Chr(10) _
                                  & "Kunde:    " & marrstrCustomer(nI) & " " _
                                  & marrstrPlant(nI) & Chr(10) _
                                  & "Zusatz:    " & marrstrNotice(nI) & Chr(10) _
                                  & Chr(10) & "Projekt:   " & marrstrProject(nI) _
                                  & "/" & marrstrType(nI) & Chr(10) _
                                  & "HGR:      " & marrstrHGR(nI) & Chr(10) & Chr(10) _
                                  & "Soll Alt:    " & dSetOld & Chr(10) _
                                  & "Soll Neu: " & dSetNew, vbYesNo + vbQuestion, _
                                  "Abweichung bei Sollstundeneintrag")
                                Select Case nMSG
                                Case Is = 6  'Ja
                                    marrdTarget(nI) = dSetNew
                                Case Is = 7  'Nein
                                    marrdTarget(nI) = dSetOld
                                End Select
                            '--------------------------------------------------
                            'Wocheneintr|fffd|ge mit neuem Sollwerten aktualisieren
                            modOverview.SetNewValues (nI)
                            End If
                        Next j
                    i = i + 1
                    nI = 0
                    End Select
                Else
                    Select Case nI
                    Case Is < nA
                        nI = nI + 1
                    Case Is >= nA
                        '------------------------------------------------------
                        'Zuweisung eines neuen Datensatzes zum Array
                        marrstrCustomer(nA) = oSheet.Cells(7 + i, 3).Value
                        marrstrPlant(nA) = oSheet.Cells(7 + i, 8).Value
                        marrstrNotice(nA) = oSheet.Cells(7 + i, 14).Value
                        marrstrProject(nA) = oSheet.Cells(7 + i, 20).Value
                        marrstrType(nA) = oSheet.Cells(7 + i, 24).Value
                        marrstrHGR(nA) = oSheet.Cells(7 + i, 26).Value
                        marrdTarget(nA) = oSheet.Cells(7 + i, 28).Value
                        marrdActual(nA) = oSheet.Cells(7 + i, 30).Value
                        nA = nA + 1
                        i = i + 1
                        nI = 0
                    End Select
                End If
            Loop Until oSheet.Cells(7 + i, 3).Interior.ColorIndex = -4142
            '------------------------------------------------------------------
            '|fffd|bernahme Anzahl Datens|fffd|tze mit Ber|fffd|cksichtigung von Datensatz 0
            '- Eigene Kostenstelle
            gnDataCount = nA
            If gnDataCount > 464 Then
                GoTo ErrorHandler
            End If
            i = i + 1
        Loop
    End If
Next oSheet
Exit Sub

'************
ErrorHandler:
MsgBox "Zuviele Projekteintr|fffd|ge bitte KW|fffd|s l|fffd|schen" & Chr(10) _
       & "oder neue Datei beginnen." & Chr(10) & Chr(10) _
       & "Vorgang wird beendet!", vbOKOnly + vbCritical, "Fehler!"

End Sub         'End of TargetHours


'******************************************************************************
'*Procedure   : SetNewValues
'*Description : Zuweisen oder R|fffd|cksetzen von Sollwert|fffd|nderungen
'*              Call By NewTargetHours
'*
'*Parameter   : ArrayNo
'*Return      : None
'******************************************************************************
Sub SetNewValues(ByVal ArrayNo As Integer)
Dim i As Integer
Dim j As Integer
    
For i = Sheets.Count To 5 Step -1
    j = 0
    Do While Sheets(i).Cells(7 + j, 3).Value <> "" And _
             Sheets(i).Cells(7 + j, 3).Interior.ColorIndex = 34
        If UCase(marrstrCustomer(ArrayNo)) = _
          UCase(Sheets(i).Cells(7 + j, 3).Value) _
          And UCase(marrstrPlant(ArrayNo)) = _
          UCase(Sheets(i).Cells(7 + j, 8).Value) _
          And marrstrNotice(ArrayNo) = _
          Sheets(i).Cells(7 + j, 14).Value _
          And marrstrProject(ArrayNo) = _
          Sheets(i).Cells(7 + j, 20).Value _
          And marrstrType(ArrayNo) = _
          Sheets(i).Cells(7 + j, 24).Value _
          And marrstrHGR(ArrayNo) = _
          Sheets(i).Cells(7 + j, 26).Value Then
            Sheets(i).Unprotect "DUERR_APT"
            Sheets(i).Cells(7 + j, 28).Value = marrdTarget(ArrayNo)
            '--------------------------------------------------------------
            'Farbumschlag Soll-/Ist-Wert
            Select Case Sheets(i).Cells(7 + j, 28).Value
            Case Is < Sheets(i).Cells(7 + j, 30).Value
                Sheets(i).Cells(7 + j, 30).Font.ColorIndex = 3
            Case Is > Sheets(i).Cells(7 + j, 30).Value
                Sheets(i).Cells(7 + j, 30).Font.ColorIndex = 1
            End Select
            Select Case Sheets(i).Cells(7 + j, 28).Value
            Case Is < Sheets(i).Cells(7 + j, 32).Value
                Sheets(i).Cells(7 + j, 32).Font.ColorIndex = 7
            Case Is > Sheets(i).Cells(7 + j, 32).Value
                Sheets(i).Cells(7 + j, 32).Font.ColorIndex = 1
            End Select
            Sheets(i).Protect "DUERR_APT", DrawingObjects:=False, contents:=True, _
                                           Scenarios:=True, AllowFormattingRows:=True
        End If
        j = j + 1
    Loop
Next i

End Sub         'End of SetNewValues

'******************************************************************************
'*Procedure   : ListNewValues
'*Description : Neue |fffd|bersicht erstellen, Liste mit Werten f|fffd|llen
'*              Call By AllProjects
'*
'*Parameter   : YDataRange
'*Return      : None
'******************************************************************************
Sub ListNewValues(ByVal YDataRange As Integer)
Dim i As Integer

'------------------------------------------------------------------------------
'Projekte eintragen - Seitenumbr|fffd|che aktualisieren
Sheets("|fffd|bersicht_Projekte").ResetAllPageBreaks
For i = 0 To gnDataCount - 1
    Sheets("|fffd|bersicht_Projekte").Cells(9 + i, 3).Value = marrstrCustomer(i)
    Sheets("|fffd|bersicht_Projekte").Cells(9 + i, 8).Value = marrstrPlant(i)
    Sheets("|fffd|bersicht_Projekte").Cells(9 + i, 14).Value = marrstrNotice(i)
    Sheets("|fffd|bersicht_Projekte").Cells(9 + i, 20).Value = marrstrProject(i)
    Sheets("|fffd|bersicht_Projekte").Cells(9 + i, 24).Value = marrstrType(i)
    Sheets("|fffd|bersicht_Projekte").Cells(9 + i, 26).Value = marrstrHGR(i)
    Sheets("|fffd|bersicht_Projekte").Cells(9 + i, 28).Value = marrdTarget(i)
    Sheets("|fffd|bersicht_Projekte").Cells(9 + i, 31).Value = marrdActual(i)
    If marrdTarget(i) < marrdActual(i) Then
        Sheets("|fffd|bersicht_Projekte").Cells(9 + i, 31).Font.ColorIndex = 3
    Else
        Sheets("|fffd|bersicht_Projekte").Cells(9 + i, 31).Font.ColorIndex = 1
    End If
    If i > 0 And i Mod 58 = 0 Then
        Sheets("|fffd|bersicht_Projekte").HPageBreaks.Add before:=Cells(9 + i, 2)
    End If
Next i

'------------------------------------------------------------------------------
'Projekte sortieren
With Sheets("|fffd|bersicht_Projekte").Range("C10", "AG" & YDataRange)
    .Sort Key1:=Sheets("|fffd|bersicht_Projekte").Range("T10"), Order1:=xlAscending, _
     Key2:=Sheets("|fffd|bersicht_Projekte").Range("X10"), Order2:=xlAscending, _
     Key3:=Sheets("|fffd|bersicht_Projekte").Range("Z10"), Order3:=xlAscending, _
     Header:=xlNo, OrderCustom:=1, MatchCase:=False, Orientation:= _
     xlTopToBottom, DataOption1:=xlSortNormal, DataOption2:=xlSortNormal, _
     DataOption3:=xlSortNormal
    .Sort Key1:=Sheets("|fffd|bersicht_Projekte").Range("C10"), Order1:=xlAscending, _
     Key2:=Sheets("|fffd|bersicht_Projekte").Range("H10"), Order2:=xlAscending, _
     Header:=xlNo, OrderCustom:=1, MatchCase:=False, Orientation:= _
     xlTopToBottom, DataOption1:=xlSortNormal, DataOption2:=xlSortNormal
End With

End Sub         'End of ListNewValues
