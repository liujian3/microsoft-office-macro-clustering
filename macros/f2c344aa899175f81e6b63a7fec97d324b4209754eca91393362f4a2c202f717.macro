'******************************************************************************
'*Procedure   : CalculateSumHours
'*Description : Berechnung der Stundenwerte zur Eintragung
'*              Call By SelectionChange
'*              Call By DelData1
'*              Call SetHourColors
'*
'*Parameter   : nY
'*Return      : None
'******************************************************************************
Sub CalculateSumHours(ByVal nY As Integer)
Dim i As Integer
Dim bstrToDo As String              'Wochenbereich: Aktueller T|fffd|tigkeitseintrag
                                    'Neuwert bei Stunden|fffd|nderung oder Neueintrag
Dim dHour As Double                 'Wochenbereich: Aktueller Stundeneintrag
Dim dHourDifference As Double       'Wochenbereich: Differenz zwischen Alt- und
                                    'Neueintrag
Dim nNo As Integer                  'Wochenbereich: Aktueller Projekteintrag

nNo = Cells(nY, 2).Value
dHour = Cells(nY, 3).Value
bstrToDo = Cells(nY, 6).Value
ActiveSheet.Unprotect "DUERR_APT"
dHourDifference = dHour - gdHourOld

Do
    i = i + 1
    '--------------------------------------------------------------------------
    'Berechnung bei |fffd|nderung Projektzuordnung
    If nNo <> gnNoOld And gnNoOld >= 0 Then
        If Cells(6 + i, 2).Value = nNo Then
            Cells(6 + i, 32).Value = _
                Application.Round(Cells(6 + i, 32).Value + gdHourOld, 2)
            Cells(6 + i, 30).Value = _
                Application.Round(Cells(6 + i, 30).Value + gdHourOld, 2)
        ElseIf Cells(6 + i, 2).Value = gnNoOld Then
            Cells(6 + i, 32).Value = _
                Application.Round(Cells(6 + i, 32).Value - gdHourOld, 2)
            Cells(6 + i, 30).Value = _
                Application.Round(Cells(6 + i, 30).Value - gdHourOld, 2)
        End If
    Else
    '--------------------------------------------------------------------------
    'Berechnung bei |fffd|nderung Stundenwert und Neueintrag
        If Cells(6 + i, 2).Value = nNo Then
            Cells(6 + i, 32).Value = _
                Application.Round(Cells(6 + i, 32).Value + dHourDifference, 2)
            Cells(6 + i, 30).Value = _
                Application.Round(Cells(6 + i, 30).Value + dHourDifference, 2)
        End If
    End If
Loop Until Cells(7 + i, 2).Interior.ColorIndex = -4142
ActiveSheet.Protect "DUERR_APT", DrawingObjects:=False, contents:=True, _
                                 Scenarios:=True, AllowFormattingRows:=True

modCalculate.SetHourColors

End Sub         'End of CalculateSumHours


'******************************************************************************
'*Procedure   : SetHourColors
'*Description : Farbumschlag Stundenwerte in Abh|fffd|ngigkeit von der Sollstunden-
'*              vorgabe
'*              Call By CalculateSumHours
'*              Call By frmprojectselect
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Sub SetHourColors()
Dim i As Integer

ActiveSheet.Unprotect "DUERR_APT"
Do
    Select Case Cells(7 + i, 28).Value
        Case Is < Cells(7 + i, 30).Value: Cells(7 + i, 30).Font.ColorIndex = 3
        Case Is >= Cells(7 + i, 30).Value: Cells(7 + i, 30).Font.ColorIndex = 1
    End Select
    Select Case Cells(7 + i, 28).Value
        Case Is < Cells(7 + i, 32).Value: Cells(7 + i, 32).Font.ColorIndex = 7
        Case Is >= Cells(7 + i, 32).Value: Cells(7 + i, 32).Font.ColorIndex = 1
    End Select
    i = i + 1
Loop Until Cells(7 + i, 2).Interior.ColorIndex = -4142
ActiveSheet.Protect "DUERR_APT", DrawingObjects:=False, contents:=True, _
                                 Scenarios:=True, AllowFormattingRows:=True

End Sub         'End of SetHourColors
Attribute VB_Name = "modConvert"
Option Explicit
'******************************************************************************
'*Procedure   : ConvertTime
'*Description : Umrechnung von Zeitangaben in Dezimalformat
'*              Call By SelectionChange
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Sub ConvertTime()

Application.EnableEvents = False
If Left(Cells(gnY, 3).Value, 1) = "*" _
  And IsNumeric(Right(Cells(gnY, 3).Value, Len(Cells(gnY, 3).Value) - 1)) Then
    Select Case Len(Cells(gnY, 3).Value)
    Case Is > 5
        MsgBox "Inklusive * nur maximal" & Chr(10) & _
               "5 Stellen eingeben" & Chr(10) & _
               "Bitte Eingabe korrigieren!", _
               vbOKOnly + vbCritical, "Fehler!"
        Cells(gnY, 3).Value = ""
    Case Is = 5
        '----------------------------------------------------------------------
        '24h Kontrolle
        If Mid(Cells(gnY, 3).Value, 2, 4) > 2400 Then
            MsgBox "24h und immer noch kein Ende??" & Chr(10) & _
                   "Bitte Eingabe |fffd|berpr|fffd|fen!", _
                   vbOKOnly + vbCritical, "Fehler!"
            Cells(gnY, 3).Value = gdHourOld
        Else
            Cells(gnY, 3).Value = Mid(Cells(gnY, 3).Value, 2, 2) & ":" & _
                                  Right(Cells(gnY, 3).Value, 2)
            Cells(gnY, 3).Value = Application.Round(Cells(gnY, 3).Value * 24, 2)
        End If
    Case Is = 4
        Cells(gnY, 3).Value = Mid(Cells(gnY, 3).Value, 2, 1) & ":" & _
                              Right(Cells(gnY, 3).Value, 2)
        Cells(gnY, 3).Value = Application.Round(Cells(gnY, 3).Value * 24, 2)
    Case Is = 3
        Cells(gnY, 3).Value = "0:" & Right(Cells(gnY, 3).Value, 2)
        Cells(gnY, 3).Value = Application.Round(Cells(gnY, 3).Value * 24, 2)
    Case Is = 2
        Cells(gnY, 3).Value = "0:0" & Right(Cells(gnY, 3).Value, 1)
        Cells(gnY, 3).Value = Application.Round(Cells(gnY, 3).Value * 24, 2)
    Case Is = 1
        Cells(gnY, 3).Value = ""
    End Select
ElseIf IsNumeric(Cells(gnY, 3).Value) = False Or Cells(gnY, 3).Value < 0 Then
            MsgBox "Falsche Eingabe - TippFehler?" & Chr(10) _
                   & "Bitte Eingabe |fffd|berpr|fffd|fen!", vbOKOnly + vbCritical, "Fehler!"
            Cells(gnY, 3).Value = gdHourOld
End If
Application.EnableEvents = True

End Sub


'******************************************************************************
'*Procedure   : ConvertCountry
'*Description : Ber|fffd|cksichtigung der PC- L|fffd|ndereinstellung - Korrektur von
'*              Fehleingaben generell
'*              Call By SelectionChange
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Sub ConvertCountry()

Application.EnableEvents = False
If Cells(gnY, 3).Value <> "" And IsNumeric(Cells(gnY, 3).Value) = True Then
    '--------------------------------------------------------------------------
    'Indirekte Abfrage der L|fffd|nderienstellung Punkt/Komma
    Select Case InStr(Cells(5, 2).Value, ",")
    Case Is = 0
        '----------------------------------------------------------------------
        'L|fffd|ndereinstellung Punkt - Umsetzung von Komma auf Punkt
        If InStr(Cells(gnY, 3).Value, ",") > 0 Then
            Cells(gnY, 3).Value = _
            WorksheetFunction.Substitute(Cells(gnY, 3).Value, ",", ".")
        End If
        If Cells(gnY, 3).Value > 24 Then
            MsgBox "24h und immer noch kein Ende??" & Chr(10) _
                   & "Bitte Eingabe |fffd|berpr|fffd|fen!", vbOKOnly + vbCritical, "Fehler!"
            Cells(gnY, 3).Value = gdHourOld
        End If
    Case Is > 0
        '----------------------------------------------------------------------
        'L|fffd|ndereinstellung Komma - Umsetzung von Punkt auf Komma
        If InStr(Cells(gnY, 3).Value, ".") > 0 Then
            Cells(gnY, 3).Value = _
            WorksheetFunction.Substitute(Cells(gnY, 3).Value, ".", ",")
        End If
        If Cells(gnY, 3).Value > 24 Then
            If Cells(gnY, 3).Value > 24 Then
                MsgBox "Zeit gr|fffd||fffd|er 24h eingegeben!" & Chr(10) & Chr(10) _
                       & "Ursache 1:" & Chr(10) _
                       & "Tippfehler - Bitte korrigieren" & Chr(10) _
                       & "Ursache 2:" & Chr(10) _
                       & "Falsches Dezimaltrennzeichen", _
                       vbOKOnly + vbCritical, "Fehler!"
                Cells(gnY, 3).Value = gdHourOld
            End If
        End If
    End Select
Else
    '--------------------------------------------------------------------------
    'Genereller Fehler bzw. Wert nicht numerisch
    If Cells(gnY, 3).Value <> "" Then
        MsgBox "Falscher Wert!", vbOKOnly + vbCritical, "Fehler!"
        Cells(gnY, 3).Value = "0"
    End If
End If
Application.EnableEvents = True

End Sub


'******************************************************************************
'*Procedure   : nISOWeek
'*Description : Kalenderwochenberechnung
'*              Call By frmNewWeekReport
'*
'*Parameter   : None
'*Return      : nISOWeek
'******************************************************************************
Function nISOWeek(ByVal dtDat As Date) As Integer
Dim dDateDec As Double              'Dezimalwert Datum

dDateDec = DateSerial(Year(dtDat + (8 - Weekday(dtDat)) Mod 7 - 3), 1, 1)
nISOWeek = (dtDat - dDateDec - 3 + (Weekday(dDateDec) + 1) Mod 7) \ 7 + 1

End Function
