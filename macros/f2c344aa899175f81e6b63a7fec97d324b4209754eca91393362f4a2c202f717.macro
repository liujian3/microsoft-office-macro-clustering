'******************************************************************************
'*Procedure   : cmdWindowFixing_Click
'*Description : Blattfixierung An- und/oder Ausschalten
'*
'*Parameter   : None
'*Return      : None
'******************************************************************************
Sub cmdWindowFixing_Click()
Dim i As Integer
Dim oSheet As Object                'Objektvariable f|fffd|r Wochenbl|fffd|tter

DoEvents
With Application
    .ScreenUpdating = False
    .EnableEvents = False
End With

If cmdWindowFixing.Value = False Then
    cmdWindowFixing.Caption = "Fixierung Aus"
    For Each oSheet In Worksheets
        If Len(oSheet.Name) = 5 And Mid(oSheet.Name, 3, 1) = "_" Then
            oSheet.Select
            ActiveWindow.FreezePanes = False
        End If
        If oSheet.Name = "NEU" Then
            oSheet.Visible = True
            oSheet.Select
            ActiveWindow.FreezePanes = False
            oSheet.Visible = False
        End If
    Next oSheet
Else
    cmdWindowFixing.Caption = "Fixierung An"
    For Each oSheet In Worksheets
        If Len(oSheet.Name) = 5 And Mid(oSheet.Name, 3, 1) = "_" Then
            oSheet.Select
            i = 0
            Do
                i = i + 1
            Loop Until oSheet.Cells(7 + i, 2).Interior.ColorIndex = -4142
            '------------------------------------------------------------------
            'Kontrolle ob Bildschirmfixierung noch sinnvoll (1024x768)
            If i < 16 Then
                oSheet.Range("A" & i + 7).Select
                ActiveWindow.FreezePanes = True
                oSheet.Range("B7").Select
            End If
        End If
        If oSheet.Name = "NEU" Then
            oSheet.Visible = True
            oSheet.Select
            oSheet.Range("A9").Select
            ActiveWindow.FreezePanes = True
            oSheet.Range("B7").Select
            oSheet.Visible = False
        End If
    Next oSheet
End If

Sheets("|fffd|bersicht_Projekte").Select
With Application
    .ScreenUpdating = True
    .EnableEvents = True
End With

If cmdWindowFixing.Value = True Then
    MsgBox "Blattfixierung im Wochenbereich aktiviert!" & Chr(10) _
           & "Bl|fffd|tter mit mehr als 15 Projekten bleiben" & Chr(10) _
           & "hiervon ausgenommen.", vbOKOnly + vbInformation, "Info!"
Else
    MsgBox "Blattfixierung im Wochenbereich deaktiviert!", _
    vbOKOnly + vbInformation, "Info!"
End If

End Sub         'End of cmdWindowFixing_Click


