Dim marrstrCustomer(0 To 500) As String 'Kunde
Dim marrstrPlant(0 To 500) As String    'Kundenwerk/-ort
Dim marrstrNotice(0 To 500) As String   'Pers|fffd|nlicher Kommentar zur Anlage
