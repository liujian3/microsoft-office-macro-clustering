End Sub
Attribute VB_Name = "mdlSwitchPages"


Sub Language()
  Call ShowPage("Language")
End Sub

Sub see_pictures()
  Call ShowPage("Mold pictures")
End Sub

Sub Toolshop()
On Error GoTo ErrHandler

  Sheets("Mold pictures").Visible = True
  Call ShowPage("Mold data sheet")
Exit Sub

ErrHandler:
  MsgBox Err.Number & ": " & Err.Description, vbCritical + vbOKOnly, "Toolshop"
End Sub

Sub back()
On Error GoTo ErrHandler

  ActiveWindow.SelectedSheets.Visible = False
  Sheets("Mold pictures").Visible = False
  Sheets("Start sheet").Select
Exit Sub

ErrHandler:
  MsgBox Err.Number & ": " & Err.Description, vbCritical + vbOKOnly, "back"
End Sub

Sub backToMold()
On Error GoTo ErrHandler

  ActiveWindow.SelectedSheets.Visible = False
  Call ShowPage("Mold data sheet")
Exit Sub

ErrHandler:
  MsgBox Err.Number & ": " & Err.Description, vbCritical + vbOKOnly, "back"
End Sub


Private Sub ShowPage(ByVal sPageName As String)
On Error GoTo ErrHandler

  Sheets(sPageName).Visible = True
  Sheets(sPageName).Select
Exit Sub

ErrHandler:
  MsgBox Err.Number & ": " & Err.Description, vbCritical + vbOKOnly, "ShowPage: " & sPageName
End Sub



Sub close_all()

  Sheets("Mold Pictures").Visible = False
  Sheets("Mold data sheet").Visible = False
  Sheets("Language").Visible = False

End Sub

Sub open_all()

  Call ShowPage("Mold pictures")
  Call ShowPage("Mold data sheet")
  Call ShowPage("Language")
  Sheets("Start sheet").Select
  
End Sub



