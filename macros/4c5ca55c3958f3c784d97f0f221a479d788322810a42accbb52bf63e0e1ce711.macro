                ecount = ecount + 1
                pi.Parent.Log "|fffd||fffd||fffd||fffd||fffd||fffd| " & Err.Number & " |fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd| |fffd| |fffd||fffd||fffd||fffd| " & key$ & ": " & Err.Description
