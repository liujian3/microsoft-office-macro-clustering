Attribute VB_Name = "GetJobHist"

Option Explicit


' Gets a specified Job Budget History value
Private Function GetJCValue(ValueReq%, BT$, TType%, PCChar$, Company$, TheYear%, ThePeriod%, TheCcy%, JobCode$, StockCode$) As Variant
On Error GoTo GetJCValue_Error
    Dim CompStr$, Ty%, Tp%, TC%, Mode As Integer
    Dim DblVal As Double, RetVal%, BudgType$, TotType%, Commit%, JCode$, SCode$
     
    DblVal# = 0#
    
    If ValidCompany(Company) And ValidYear(TheYear) And ValidPeriod(ThePeriod) And ValidCcy(TheCcy) And ValidTotType(TType%) And ValidJobCode(JobCode$) And ValidStockCode(StockCode$) And ValidPC(PCChar$) Then
        If LocalCheckServer() Then
            Mode = ValueReq%
            CompStr$ = UCase(Company$)
            BudgType$ = BT$
            TotType% = TType%
            If (UCase(Trim(PCChar$)) = "P") Then
                Commit% = 0
            Else
                Commit% = 1
            End If
            Ty% = TheYear%
            Tp% = ThePeriod%
            TC% = TheCcy%
            JCode$ = UCase$(JobCode$)
            SCode$ = UCase$(StockCode$)
                        
            RetVal% = EntServer.GetJCBudgetValue(Mode%, CompStr$, BudgType$, TotType%, Commit%, Ty%, Tp%, TC%, JCode$, SCode$, DblVal#)
            
            If (RetVal% <> 0) Then
                Error RetVal%
            End If
        End If
    End If
      
    GetJCValue = DblVal
    
    GoTo GetJCValue_Exit
        
GetJCValue_Error:
    GetJCValue = GetErrorNumStr

GetJCValue_Exit:
    
End Function
 

' Gets a specified Job Budget History value
Private Function GetJCValueYearRange(ValueReq%, BT$, TType%, PCChar$, Company$, StartYear%, EndYear%, ThePeriod%, TheCcy%, JobCode$, StockCode$) As Variant
On Error GoTo GetJCValueYearRange_Error
    Dim CompStr$, Ty%, Tp%, TC%, Mode As Integer
    Dim TotVal#, DblVal#, RetVal%, BudgType$, TotType%, Commit%, JCode$, SCode$
     
    TotVal# = 0#
    
    If ValidCompany(Company) And ValidYear(StartYear) And ValidYear(EndYear) And ValidPeriod(ThePeriod) And ValidCcy(TheCcy) And _
       ValidTotType(TType%) And ValidJobCode(JobCode$) And ValidStockCode(StockCode$) And ValidPC(PCChar$) Then
        If (StartYear% <= EndYear%) Then
            If LocalCheckServer() Then
                Mode = ValueReq%
                CompStr$ = UCase(Company$)
                BudgType$ = BT$
                TotType% = TType%
                If (UCase(Trim(PCChar$)) = "P") Then
                    Commit% = 0
                Else
                    Commit% = 1
                End If
                Tp% = ThePeriod%
                TC% = TheCcy%
                JCode$ = UCase$(JobCode$)
                SCode$ = UCase$(StockCode$)
                        
                For Ty% = StartYear% To EndYear%
                    DblVal# = 0#
                    RetVal% = EntServer.GetJCBudgetValue(Mode%, CompStr$, BudgType$, TotType%, Commit%, Ty%, Tp%, TC%, JCode$, SCode$, DblVal#)
            
                    If (RetVal% = 0) Then
                      TotVal# = TotVal# + DblVal#
                    Else
                        Error RetVal%
                    End If
                Next Ty%
            End If
        Else
            ' Invalid StartYear/EndYear combo
            Error 512
        End If
    End If
      
    GetJCValueYearRange = TotVal#
    
    GoTo GetJCValueYearRange_Exit
        
GetJCValueYearRange_Error:
    GetJCValueYearRange = GetErrorNumStr

GetJCValueYearRange_Exit:
    
End Function
 
 
