Private Function GetLocMiscInt(Company$, LocCode$, WantNo%) As Variant
On Error GoTo GetLocMiscInt_Error
    Dim CompStr As String, IntVal%, Location$
    Dim RetVal As Long
    
    If ValidCompany(Company) And ValidLocation(LocCode$) Then
        If LocalCheckServer() Then
            CompStr = UCase(Company$)
            Location$ = UCase(LocCode$)
            
            IntVal% = EntServer.GetLocationMiscInt(CompStr, Location$, WantNo%, RetVal&)
            
            If (IntVal% > 0) Then
                Error IntVal%
            End If
        End If
    End If
     
    GetLocMiscInt = RetVal
    
    GoTo GetLocMiscInt_Exit
    
GetLocMiscInt_Error:
