Attribute VB_Name = "JobMisc"

Option Explicit


'*******************************************************************************
' Name:        EntJCCategoryName
' Description: Gets the Job Costing Category description
'*****************************************************************************************
Public Function EntJCCategoryName(Company$, Category%) As String
Attribute EntJCCategoryName.VB_Description = "Returns the Category name from the Job Costing System Setup for the requested code"
Attribute EntJCCategoryName.VB_HelpID = 20140
Attribute EntJCCategoryName.VB_ProcData.VB_Invoke_Func = " \n14"
On Error GoTo EntJCCategoryName_Error
    Dim TheCat%, RetVal%, CatName$
    
    RetVal% = 0
    
    If ValidCompany(Company) And ValidTotType(Category%) Then
        If LocalCheckServer() Then
            Company$ = UCase(Company$)
            CatName$ = ""
            Select Case Category%
            Case 10, 20, 30, 40, 50, 60
                TheCat% = (Category% \ 10)
            Case 99
                TheCat% = 99
            Case 160, 170, 180, 190
                TheCat% = (Category% - 90) \ 10
                
            ' HM 23/03/04: Added Apps&Vals Catagory Descriptions
            Case 5   ' Sales Applications
                TheCat% = 15
            Case 14  ' Sales Deductions
                TheCat% = 14
            Case 23  ' Sub Contract Labour
                TheCat% = 11
            Case 53  ' Materials 2
                TheCat% = 12
            Case 63  ' Overheads 2
                TheCat% = 13
            Case 67  ' Purchase Deductions
                TheCat% = 17
            Case 173 ' Purchase Applications
                TheCat% = 16
            Case Else
                Error 531
            End Select
                        
            RetVal% = EntServer.GetJobTotalStr(TheCat%, Company$, CatName$)
            
            If (RetVal% <> 0) Then
                Error RetVal%
            End If
        End If
    End If
      
    GoTo EntJCCategoryName_Exit
        
EntJCCategoryName_Error:
    CatName$ = GetErrorStr$

EntJCCategoryName_Exit:
    EntJCCategoryName = CatName$
End Function


