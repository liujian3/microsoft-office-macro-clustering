' Gets a particular stock value
Private Function GetStockValue(ReqVal%, Company$, TheYear%, ThePeriod%, TheCcy%, StockCode$, Location$) As Variant
On Error GoTo GetStockValue_Error
    Dim CompStr$, Ty%, Tp%, TC%, Stock$, Mode%, DblVal#, IntVal%
     
    If ValidCompany(Company) And ValidYear(TheYear) And ValidPeriod(ThePeriod) And ValidCcy(TheCcy) And ValidStockCode(StockCode) Then
        If LocalCheckServer() Then
            CompStr$ = UCase(Company$)
            Ty% = TheYear%
            Tp% = ThePeriod%
            TC% = TheCcy%
            Stock$ = UCase(StockCode$)
            Mode = ReqVal%
            
            IntVal% = EntServer.GetStockValue(Mode%, CompStr$, Ty%, Tp%, TC%, Stock$, Location$, DblVal#)
            
            If (IntVal% > 0) Then
                Error IntVal%
            End If
        End If
    End If
    
    GetStockValue = DblVal
    
    GoTo GetStockValue_Exit
        
GetStockValue_Error:
    GetStockValue = GetErrorNumStr

GetStockValue_Exit:
    
End Function


