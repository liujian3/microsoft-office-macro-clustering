Attribute VB_Name = "GetLoc"

Option Explicit

' Returns a specified string from the specified location record
Private Function GetLocMiscStr(Company$, LocCode$, WantNo%) As String
On Error GoTo GetLocMiscStr_Error
    Dim RetStr$, CompStr$, Loc$, IntVal%
    
    If ValidCompany(Company) And ValidLocation(LocCode$) Then
        If LocalCheckServer() Then
            CompStr = UCase(Company$)
            Loc$ = UCase(LocCode$)
            IntVal% = EntServer.GetLocationMiscStr(CompStr, Loc$, WantNo%, RetStr$)
            
            If (IntVal% > 0) Then
                Error IntVal%
            End If
        End If
    End If
       
    GoTo GetLocMiscStr_Exit
    
GetLocMiscStr_Error:
    RetStr = GetErrorStr$
     
GetLocMiscStr_Exit:
    GetLocMiscStr = RetStr
End Function


