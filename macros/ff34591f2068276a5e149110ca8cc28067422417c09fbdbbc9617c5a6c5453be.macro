Attribute VB_Name = "AltStk"

Option Explicit

' Returns a specified string from the specified stock record
Private Function GetAltStkMiscStr(Company$, StockCode$, AltCode$, AccCode$, WantNo%) As Variant
On Error GoTo GetAltStkMiscStr_Error
    Dim RetStr$, CompStr$, Stock$, IntVal%, RetInt%, RetDub#
    
    If ValidCompany(Company) And ValidStockCode(StockCode) Then
        If LocalCheckServer() Then
            CompStr = UCase(Company$)
            Stock = UCase(StockCode$)
            AltCode$ = UCase(AltCode$)
            RetStr$ = ""
            RetInt% = 0
            RetDub# = 0#
            IntVal% = EntServer.GetAltStkMiscStr(CompStr, Stock, AltCode$, AccCode$, WantNo%, RetStr$, RetInt%, RetDub#)
            
            If (IntVal% > 0) Then
                Error IntVal%
            Else
                Select Case WantNo%
                Case 1  ' Description
                    GetAltStkMiscStr = RetStr$
                Case 2  ' Re-Order Currency
                    GetAltStkMiscStr = RetInt%
                Case 3  ' Re-Order Price
                    GetAltStkMiscStr = RetDub#
                End Select
            End If
        End If
    End If
       
    GoTo GetAltStkMiscStr_Exit
    
GetAltStkMiscStr_Error:
    GetAltStkMiscStr = GetErrorStr$
     
GetAltStkMiscStr_Exit:
    
End Function


