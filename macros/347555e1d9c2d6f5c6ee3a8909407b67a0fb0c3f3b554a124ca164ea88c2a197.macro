                    ws.Range("B6:I6").ClearContents: ws.Range("K8:AG8").ClearContents
                    ws.Range("B10:AG42").ClearContents:  ws.Visible = xlSheetVeryHidden
                    
                Case "sh_SSP_STRmkgMth" 'delete any MKG month shapes if any
