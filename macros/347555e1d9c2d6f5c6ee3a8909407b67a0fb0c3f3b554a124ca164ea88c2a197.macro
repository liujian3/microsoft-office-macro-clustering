        Select Case ws.Visible '-1 visible, 0 hidden, 2 veryhidden
            Case Is <> -1
               Me.lbShowHide.Selected(Me.lbShowHide.ListCount - 1) = False
            Case Else
               Me.lbShowHide.Selected(Me.lbShowHide.ListCount - 1) = True
        End Select
    End If
Next

