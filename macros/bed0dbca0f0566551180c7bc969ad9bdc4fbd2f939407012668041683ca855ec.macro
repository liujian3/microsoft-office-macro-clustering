    Set g_aDialog.rgeOutBgn = rgeOut
    
    g_aDialog.bColData = True
    g_aDialog.bTitle = True
    
    '|fffd||fffd||fffd||fffd||fffd||3d0||fffd||fffd||311|I|fffd||fffd|
