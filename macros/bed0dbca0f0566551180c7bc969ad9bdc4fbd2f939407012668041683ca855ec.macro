    tmpRge.Copy
    toBgn.Worksheet.Activate
    toBgn.Select
    If bCol Then
        Selection.PasteSpecial Paste:=xlValues, Operation:=xlNone, SkipBlanks:=False, Transpose:=False
    Else
        Selection.PasteSpecial Paste:=xlValues, Operation:=xlNone, SkipBlanks:=False, Transpose:=True
    End If
    'H14.11.27 LSQ moved
