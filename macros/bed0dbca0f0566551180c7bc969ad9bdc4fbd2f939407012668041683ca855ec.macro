            End If
        End If
    Else
        Set tmpRge = getDataRange_SmpTitle(RgeObj, bCol, bTitle, False, bHasSelRow)
    End If
    
    If tmpRge Is Nothing Then
        Set moveData_Y = Nothing
        Exit Function
    End If
