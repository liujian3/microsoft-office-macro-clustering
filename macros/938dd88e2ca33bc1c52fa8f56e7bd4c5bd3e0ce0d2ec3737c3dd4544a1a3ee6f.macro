Private Sub ToggleButton1_Click()
If ActiveSheet.Cells(15, 8).Value = "November 07" Then
    ActiveSheet.Cells(25, 7).Value = Worksheet("Auszahlung Stunden").Cells(5, 12).Value
    ActiveSheet.Cells(26, 7).Value = Worksheet("Auszahlung Stunden").Cells(5, 13).Value
    ActiveSheet.Cells(27, 7).Value = Worksheet("Auszahlung Stunden").Cells(5, 14).Value
    ActiveSheet.Cells(28, 7).Value = Worksheet("Auszahlung Stunden").Cells(5, 15).Value
ElseIf ActiveSheet.Cells(15, 8).Value = "Dezember 07" Then
    ActiveSheet.Cells(25, 7).Value = Worksheet("Auszahlung Stunden").Cells(7, 12).Value
    ActiveSheet.Cells(26, 7).Value = Worksheet("Auszahlung Stunden").Cells(7, 13).Value
    ActiveSheet.Cells(27, 7).Value = Worksheet("Auszahlung Stunden").Cells(7, 14).Value
    ActiveSheet.Cells(28, 7).Value = Worksheet("Auszahlung Stunden").Cells(7, 15).Value
End If

End Sub
