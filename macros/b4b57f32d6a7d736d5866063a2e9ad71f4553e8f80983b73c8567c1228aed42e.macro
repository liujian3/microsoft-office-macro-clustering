 If TXT_Tabname.Value <> "" Then
 
 If ListBox1.ListIndex <> -1 Then
 i = ListBox1.ListIndex
 If i = 0 Then Exit Sub
   s = ListBox1.List(i)
   If TabExists(TXT_Tabname) = False Then
     ListBox1.RemoveItem i
