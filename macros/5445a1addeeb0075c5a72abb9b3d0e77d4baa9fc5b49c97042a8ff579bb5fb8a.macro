Private Sub UserForm_Initialize()
    Dim XLSTART As String
    Dim i As Integer
    XLSTART = Application.StartupPath
    For i = Len(XLSTART) - 2 To 1 Step -1
        If Mid(XLSTART, i, 1) = Application.PathSeparator Then Exit For
    Next
    XLSTART = UCase(Mid(XLSTART, i + 1, 255))
    Xlstart_OpB.Enabled = False
    AsAddIn_OpB.Value = True
