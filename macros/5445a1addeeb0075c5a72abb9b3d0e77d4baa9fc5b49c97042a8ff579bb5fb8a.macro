Attribute VB_Name = "Mod1Main"
Option Explicit
Public bSilence As Boolean
Const AddInTitle As String = "Orlando's Create ShortCut"

Sub Install()
    Dim PathTemp As String
    Dim PathInst As String
    Dim i As Integer
    IsPortg = Application.International(xlCountryCode) = 55

    If Val(Application.Version) < 8 Then
        Msg1 = IIf(IsPortg, "S|fffd| |fffd| poss|fffd|vel instalar este utilit|fffd|rio no Excel 97 ou mais recente!", _
                "Only it is possible to install this utility on Excel 97 or later!")
        Msg2 = IIf(IsPortg, "Falha!", "Fail!")
        MyMsgBox Msg1, 1, Msg2
        Exit Sub
    End If

    If Val(Application.Version) > 11 + 1 + 2 + 1 + 1 Then
        Msg1 = IIf(IsPortg, "Nova vers|fffd|o do Excel desconhecida para esta vers|fffd|o do CreateShortCut.xla! Clique no link Ajuda, logo abaixo, para baixar uma nova vers|fffd|o compat|fffd|vel.", _
                "New Excel version unknown to this CreateShortCut.xla version! Click Help link, soon below, to download a new compatible version.")
        Msg2 = IIf(IsPortg, "Falha!", "Fail!")
        MyMsgBox Msg1, 1, Msg2
        Exit Sub
    End If

    PathTemp = TempFolder()
    'PathInst = GetRegValue(HKEY_LOCAL_MACHINE, "Software\Microsoft\Windows\CurrentVersion", "ProgramFilesDir")
    'If PathInst = "Error" Then PathInst = GetRegValue(HKEY_LOCAL_MACHINE, "Software\Microsoft\Windows\CurrentVersion", "ProgramFilesDir")
    'If PathInst = "Error" Or Len(PathInst) < 5 Then PathInst = "C:"
    'If Dir(PathInst, vbDirectory) = "" Then MkDir PathInst
    'PathInst = PathInst & "\OrlandoApp"
    PathInst = Application.UserLibraryPath & "OrlandoApp"    'This folder doesn't need privileg elevation and moreover add-in is installed only for active limited user ou logge adm user

    FormInst.folder_Tb.Value = PathInst
    FormInst.Show
    If BotAcionado = 3 Then Exit Sub

    If FormInst.Xlstart_OpB.Value Then PathInst = Application.StartupPath
    If Dir(PathInst, vbDirectory) = "" Then MkDir PathInst

    'Uninstall early version
    bSilence = True
    UnInstall

    'Extraindo Pacote
    Dim lTipoPacote As Long
    lTipoPacote = 1
    Select Case lTipoPacote
    Case 1    'Usando o aplicativo XLtoExe
        PathTemp = ThisWorkbook.Path
        i = 40 - 35
    Case 2    'Usando Join and Split from Me > Ok mas o arquivo do Excel tem que ser read only, preferi o XLtoEXE
        Dim sAutoExtract
        '        sAutoExtract = SplitMe(PathTemp)
        If sAutoExtract = False Then
            i = 40
        Else
            Shell sAutoExtract
        End If
    Case 3    'Usando pacote OLE embeded na planilha > No Excel 2003 os antivirus impedem estra|fffd||fffd|o
        If Val(Application.Version) < 11 Then   'By pass Excel security alert
            Application.DisplayAlerts = False
        Else
            Application.SendKeys "{LEFT}~"

        End If
        ThisWorkbook.Sheets(1).OLEObjects("Package").Verb Verb:=xlPrimary
        Application.DisplayAlerts = True
        If NTVersion > 5 Then Application.SendKeys "{LEFT}~"   'By pass Windows XP security alert
    End Select

    MyWait 1
    PleaseWait True
    MyWait 3

    'Conferindo extra|fffd||fffd|o
    Do While Dir(PathTemp & "\CreateShortCut.xla") = ""
        MyWait 1
        i = i + 1
        If i > 40 Then
            MsgBox IIf(IsPortg, "Ao extrair arquivos para a pasta " & PathTemp & ".", "When extract files to " & PathTemp & " folder."), vbCritical, IIf(IsPortg, "Falha!", "Fail!")
            Exit Sub
        End If
    Loop

    'Install xla
    MyMoveFile "CreateShortCut.xla", PathTemp, PathInst, lTipoPacote = 1
    If FormInst.AsAddIn_OpB.Value Then
        If Dir(PathTemp & "\CreateShortCut.ico") <> "" Then MyMoveFile "CreateShortCut.ico", PathTemp, PathInst, lTipoPacote = 1
        '      AddIns.Add Filename:=PathInst & "\CreateShortCut.xla"
        '      AddIns(AddInTitle).Installed = True
        Application.EnableEvents = False
        Workbooks.Open PathInst & "\CreateShortCut.xla"
        Application.EnableEvents = True
        Application.Run "CreateShortCut.xla!ThisOnDesktop"
        If WbIsOpen("CreateShortCut.xla") Then Workbooks("CreateShortCut.xla").Close False
    Else
        If Dir(PathTemp & "\CreateShortCut.ico") <> "" Then MyMoveFile "CreateShortCut.ico", PathTemp, Application.Path, lTipoPacote = 1
        Workbooks.Open PathInst & "\CreateShortCut.xla"
    End If

    PleaseWait False

    If IsPortg Then
        Msg1 = "Um atalho para acionar CreateShortCut.xla foi criado em sua |fffd|rea de Trabalho do Windows de onde voc|fffd| pode mover ou copiar para qualquer outra pasta que achar mais apropriada." & Chr(10) & Chr(10) & "Sair?"
        Msg2 = "CreateShortCut.xla - Instala|fffd||fffd|o Conclu|fffd|da"
    Else
        Msg1 = "A Shortcut to launch CreateShortCut.xla was created on your Windows Desktop from where you can move or copy to any other folder that you think more appropriated." & Chr(10) & Chr(10) & "Exit?"
        Msg2 = "CreateShortCut.xla - Installation Finished"
    End If
    MyMsgBox Msg1, 2, Msg2
    If BotAcionado = 1 Then ThisWorkbook.Close False
End Sub

Sub MyMoveFile(vFiles As String, vOrigPath As String, vDestPath As String, bOnlyCopy As Boolean)
    Dim vFilename As String
    vFilename = Dir(vOrigPath & "\" & vFiles)
    While vFilename <> ""
        If bOnlyCopy Then
            FileCopy vOrigPath & "\" & vFilename, vDestPath & "\" & vFilename
            vFilename = Dir()
        Else
            If Dir(vDestPath & "\" & vFilename) <> "" Then Kill vDestPath & "\" & vFilename
            Name vOrigPath & "\" & vFilename As vDestPath & "\" & vFilename
            vFilename = Dir(vOrigPath & "\" & vFiles)
        End If
    Wend
End Sub

Function NTVersion()
'Windows (32-bit) 4.10        (98)
'Windows (32-bit) NT 5.00     (Win 2000)
'Windows (32-bit) NT 5.01     (XP)
    Dim NTpos As Integer
    NTVersion = Application.OperatingSystem
    NTpos = InStr(1, NTVersion, " NT ")
    If NTpos = 0 Then
        NTVersion = 4.1   'Not Windows NT
        Exit Function
    End If
    NTVersion = Mid(NTVersion, NTpos + 4, 4)
    If IsNumeric(NTVersion) Then
        NTVersion = Val(NTVersion)
    Else
        NTVersion = 4    'UnKnown NT
    End If
End Function

Sub UnInstall()
    Dim PathInst As String
    Dim ad As AddIn
    IsPortg = Application.International(xlCountryCode) = 55
    If IsPortg Then
        Msg1 = "Ser|fffd| verificada a exist|fffd|ncia de uma instala|fffd||fffd|o anterior e, em seguida, ela ser|fffd| devidamente removida. OK?"
        Msg2 = "CreateShortCut.xla - DesInstalar"
    Else
        Msg1 = "It will be verified a previous installation and, after that, it will be duly removed. OK?"
        Msg2 = "CreateShortCut.xla - UnInstall"
    End If
    If Not bSilence Then
        MyMsgBox Msg1, 2, Msg2
        If BotAcionado = 2 Then Exit Sub
    End If

    PathInst = ""
    For Each ad In AddIns
        If ad.Name = "CreateShortCut.xla" Then
            PathInst = ad.Path
            Exit For
        End If
    Next

    On Error Resume Next

    'Deactivate Add
    AddIns(AddInTitle).Installed = False
    If WbIsOpen("CreateShortCut.xla") Then Workbooks("CreateShortCut.xla").Close False

    'Delete files and folder...
    'In Anywhere detected if loaded
    If PathInst <> "" Then
        If Dir(PathInst & "\CreateShortCut.xla") <> "" Then Kill PathInst & "\CreateShortCut.xla"
        If Dir(PathInst & "\CreateShortCut.ini") <> "" Then Kill PathInst & "\CreateShortCut.ini"
        If Dir(PathInst & "\CreateShortCut.ico") <> "" Then Kill PathInst & "\CreateShortCut.ico"
    End If

    'InDefautOrlando
    PathInst = GetRegValue(HKEY_LOCAL_MACHINE, "Software\Microsoft\Windows\CurrentVersion", "ProgramFilesDir")
    If PathInst = "Error" Then PathInst = GetRegValue(HKEY_LOCAL_MACHINE, "Software\Microsoft\Windows\CurrentVersion", "ProgramFilesDir")
    If PathInst = "Error" Or Len(PathInst) < 5 Then
        PathInst = ""
    Else
        If Dir(PathInst & "\OrlandoApp", vbDirectory) = "" Then
            PathInst = ""
        Else
            PathInst = PathInst & "\OrlandoApp"
        End If
    End If
    If PathInst <> "" Then
        If Dir(PathInst & "\CreateShortCut.xla") <> "" Then Kill PathInst & "\CreateShortCut.xla"
        If Dir(PathInst & "\CreateShortCut.ini") <> "" Then Kill PathInst & "\CreateShortCut.ini"
        If Dir(PathInst & "\CreateShortCut.ico") <> "" Then Kill PathInst & "\CreateShortCut.ico"
        If Dir(PathInst & "\*.*") = "" And Not bSilence Then RmDir PathInst
    End If

    'InDefautOrlando-InUserLibrary
    PathInst = Application.UserLibraryPath & "OrlandoApp"
    If Dir(PathInst, vbDirectory) = "" Then PathInst = ""
    If PathInst <> "" Then
        If Dir(PathInst & "\CreateShortCut.xla") <> "" Then Kill PathInst & "\CreateShortCut.xla"
        If Dir(PathInst & "\CreateShortCut.ini") <> "" Then Kill PathInst & "\CreateShortCut.ini"
        If Dir(PathInst & "\CreateShortCut.ico") <> "" Then Kill PathInst & "\CreateShortCut.ico"
        If Dir(PathInst & "\*.*") = "" And Not bSilence Then RmDir PathInst
    End If

    'In XLSTART
    PathInst = Application.StartupPath
    If PathInst <> "" Then
        If Dir(PathInst & "\CreateShortCut.xla") <> "" Then Kill PathInst & "\CreateShortCut.xla"
        If Dir(Application.Path & "\CreateShortCut.ico") <> "" Then Kill Application.Path & "\CreateShortCut.ico"
        If Dir(Application.StartupPath & Application.PathSeparator & ".." & Application.PathSeparator _
                & "CreateShortCut.ini") <> "" Then Kill Application.StartupPath & Application.PathSeparator _
                & ".." & Application.PathSeparator & "CreateShortCut.ini"
        If Dir(Application.StartupPath & Application.PathSeparator & ".." & Application.PathSeparator _
                & "CreateShortCut.ico") <> "" Then Kill Application.StartupPath & Application.PathSeparator _
                & ".." & Application.PathSeparator & "CreateShortCut.ico"
    End If

    'Delete Menu
    '   AddMenu "Cell", False
    '   AddMenu "XLM Cell", False

    'Delete Shortcut
    PathInst = IIf(IsPortg, "Atalho para ", "Shortcut to ") & "CreateShortCut.xla" & ".lnk"
    PathInst = GetRegValue(HKEY_CURRENT_USER, "Software\Microsoft\Windows\CurrentVersion\Explorer\Shell Folders", "Desktop") & "\" & PathInst
    If Dir(PathInst) <> "" Then Kill PathInst

    If bSilence Then bSilence = False: Exit Sub

    Application.Wait Now + TimeValue("00:00:03")   'Dispens|fffd|vel
    If IsPortg Then
        Msg1 = "Sair?"
        Msg2 = "CreateShortCut.xla - DesInstala|fffd||fffd|o Conclu|fffd|da"
    Else
        Msg1 = "Exit?"
        Msg2 = "CreateShortCut.xla - UnInstall Finished"
    End If
    MyMsgBox Msg1, 2, Msg2
    If BotAcionado = 1 Then ThisWorkbook.Close False
End Sub

Sub AddMenu(vBarName As String, vAdd As Boolean, vPosDown As Boolean)
    Dim vBarra, Cmd
    Dim vBeginGroup As Boolean
    vBeginGroup = True
    Set vBarra = CommandBars(vBarName)
    Dim c As CommandBarControl
    IsPortg = Application.International(xlCountryCode) = 55
    On Error GoTo ExitNow   'Se falhar ao deletar, |fffd| porque foi chamada do menu
    For Each c In vBarra.Controls
        If (c.Tag = "Orlando's Calendar" Or c.Tag = "Orlando's Stopwatch") And c.Index > 3 Then vBeginGroup = False
        If c.Tag = "Orlando's Calculator" Then c.Delete
    Next
    If vAdd = False Then GoTo ExitNow
    Set Cmd = vBarra.Controls.Add(IIf(vBarra.Type = msoBarTypePopup, msoControlButton, msoControlPopup), , , IIf(vPosDown, vBarra.Controls.Count, 1))
    With Cmd
        .BeginGroup = vBeginGroup And vPosDown
        .Caption = IIf(IsPortg, "Calculadora", "Calculator")
        .OnAction = "CalculatorFromCmB"
        .Tag = "Orlando's Calculator"
        .FaceId = 50
    End With
ExitNow:
End Sub

