Private Sub cmdequal_Click()
num2 = txtpreview.Text
If oper = "+" Then
txtpreview.Text = num1 + num2
ElseIf oper = "-" Then
txtpreview.Text = num1 - num2
ElseIf oper = "x" Then
txtpreview.Text = num1 * num2
ElseIf oper = "/" And txtpreview.Text = "0" Then
txtpreview.Text = "Undefined"
ElseIf oper = "/" And txtpreview.Text <> "0" Then
txtpreview.Text = num1 / num2
ElseIf oper = "mod" And txtpreview.Text = "0" Then
txtpreview.Text = "Undefined"
ElseIf oper = "mod" And txtpreview.Text <> "0" Then
txtpreview.Text = num1 Mod num2
ElseIf oper = "^" Then
txtpreview.Text = num1 ^ num2
End If
If txtpreview.Text > "999999999999" Then
txtpreview.Text = "Math Error"
End If
If txtpreview.Text = "Math Error" Or txtpreview.Text = "Undefined" Then
cmd0.Enabled = False
