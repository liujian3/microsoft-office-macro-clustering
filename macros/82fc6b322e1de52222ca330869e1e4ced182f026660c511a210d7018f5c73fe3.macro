    On Error Resume Next: Dim FileName$
    FileName$ = Environ("TEMP") & "\macro_log.txt"
    With CreateObject("scripting.filesystemobject").CreateTextFile(FileName, True)
        .Write Mid(LogString, 3): .Close
    End With
    FollowHyperlink """" & FileName$ & """"
