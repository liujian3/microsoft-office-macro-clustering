    Me.IgnoreErrors = False    ' True ' |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| 15.12.2017
    Me.Folder = ROOT_FOLDER_PREFIX$ & "\Downloads\{ParserName}\"
