Sub RestoreSettings(ByVal arr As Variant)
    Dim i&, j&
    For i = LBound(arr) To UBound(arr)
        For j = LBound(arr, 2) To UBound(arr, 2)
            Lines(i, j) = arr(i, j)
        Next j
    Next i
    UpdateIndicator True
End Sub

