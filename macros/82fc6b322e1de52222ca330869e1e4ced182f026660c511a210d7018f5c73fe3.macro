    Dim LastLen&, i&: Const SLIDER_CHAR_CODE& = 9607, CHAR_INTERVAL& = 100    '25
    
    Static TextLine$, TextBlock$
    If TextBlock$ = "" Then TextBlock$ = ChrW(SLIDER_CHAR_CODE&) & " " & ChrW(SLIDER_CHAR_CODE&) & " " & ChrW(SLIDER_CHAR_CODE&)
    If TextLine$ = "" Then
