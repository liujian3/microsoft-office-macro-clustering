Public Function toUnicode(txt As String) As String
    
    Dim X As Long
    Dim uStr As New cStringBuilder
    Dim uChrCode As Integer
    
    For X = 1 To Len(txt)
        uChrCode = Asc(Mid(txt, X, 1))
        Select Case uChrCode
            Case 8:        ' backspace
                uStr.Append "\b"
            Case 9:        ' tab
                uStr.Append "\t"
            Case 10:        ' line feed
                uStr.Append "\n"
            Case 12:        ' formfeed
                uStr.Append "\f"
            Case 13:        ' carriage return
                uStr.Append "\r"
            Case 34:        ' quote
                uStr.Append "\"""
            Case 39:        ' apotxtophe
                uStr.Append "\'"
            Case 92:        ' backslash
                uStr.Append "\\"
            Case 123, 125:        ' "{" and "}"
                uStr.Append ("\u" & Right("0000" & Hex(uChrCode), 4))
            Case Is < 32, Is > 127:        ' non-ascii characters
                uStr.Append ("\u" & Right("0000" & Hex(uChrCode), 4))
            Case Else
                uStr.Append Chr$(uChrCode)
        End Select
    Next
    toUnicode = uStr.toString
    Exit Function
    
End Function

