    v3 = LineLength& - v1& - v2&
    'GetUnicodeSlider = String(5, ChrW(9632)) & String(5, ChrW(9637)) & String(5, ChrW(9633))
    TextBlock$ = ChrW(9632) & " " & ChrW(9632) & " " & ChrW(9632)
