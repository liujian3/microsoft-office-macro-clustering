            v& = 0: v& = Val(.Attributes.getNamedItem("Interval").text)
            If v& > 0 Then Me.ReplaceInterval = v&
            Me.ReplaceWith = .Attributes.getNamedItem("ReplaceWith").text
        End With
    End With
End Sub



