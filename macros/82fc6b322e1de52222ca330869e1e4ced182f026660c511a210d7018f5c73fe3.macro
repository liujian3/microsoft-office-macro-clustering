    Set SelectedItem = All_Items.Add("", "")
    NewItem$ = SelectedItem.Name
    RefreshItemsList
    
    ' |fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd| |fffd| |fffd||fffd||fffd||fffd||fffd||fffd|
