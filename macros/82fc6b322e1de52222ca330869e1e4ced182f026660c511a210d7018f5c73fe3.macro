    With XML
        .preserveWhiteSpace = True
        .Load FileName$
        
        LoadParserNameFromFile = .SelectSingleNode("Parser").Attributes.getNamedItem("Name").text
        If LoadParserNameFromFile = "" Then LoadParserNameFromFile = Replace(Dir(FileName$), PARSER_EXT$, "")
    End With
    Set XML = Nothing
End Function

