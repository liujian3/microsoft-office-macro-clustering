Sub ProducerGenerate()
'   *** Generates Draft for Approval***
'   * Defines Conditions *
    Application.ScreenUpdating = False
'   * Confirms Action *
    If MsgBox("Are you sure you want to proceed?", vbYesNo + vbQuestion, "Product Master") = vbYes Then
'   * Saves Workbook *
    ActiveWorkbook.Save
'   * Unhides Designer Sheet *
    Sheets("Designer").Visible = True
'   * Selects Designer Sheet *
    Sheets("Designer").Select
'   * Creates Duplicate of the Designer Sheet *
    Sheets("Designer").Copy After:=Sheets("Designer")
'   * Hides the Desinger Sheet *
    Sheets("Designer").Visible = False
'   * Hides the Producer Sheet *
    Sheets("Producer").Visible = False
'   * Selectes the Duplicated Designer Sheet *
    Sheets("Designer (2)").Select
'   * Renames the Duplicated Sheets as Products *
    Sheets("Designer (2)").Name = "Current Products"
'   * Selects Control Cell A *
    Range("P31").Select
'   * Activates Filter on Control Cell **
    Selection.AutoFilter
'   * Selects Header Cells *
    Rows("17:32").Select
'   * Applies Filter to Return Blank Product Rows *
    ActiveSheet.Range("$A$30:$P$495").AutoFilter Field:=16, Criteria1:="0"
'   * Selects Control Cell B *
    Range("P30").Select
'   * Selects Blank Colums *
    Range(Selection, Selection.End(xlToLeft)).Select
'   * Selects Blank Rows *
    Range(Selection, Selection.End(xlDown)).Select
'   * Deletes Blank Rows *
    Selection.EntireRow.Delete
'   * Selects Control Cell C *
    Range("A31").Select
'   * Deactivates Filer on Control Cell *
    Selection.AutoFilter
'   * Calls Additional Process *
    Call ProducerGenerateNew
'   * Calls Additional Process *
    Call ProducerGenerateWithdrawn
'   * Selects Products Sheet *
    Sheets("Current Products").Select
'   * Unhides Introduction Sheet *
    Sheets("Introduction").Visible = True
'   * Unhides Additional Sheet *
    Sheets("Additional").Visible = True
'   * Selects Products Sheet *
    Sheets("Current Products").Select
'   * Saves Workbook *
    ChDir "\\apps1\deptwork\salesmarketing\ccare\MKTGDIR\Lewis\"
    ActiveWorkbook.SaveAs Filename:=("\\apps1\deptwork\salesmarketing\ccare\MKTGDIR\Products\Mortgages\Product Matrix\RANGE\2019\Outputs\ACQ Provisional " & Format(Now(), "ddmmyyyy") & ".xlsm") _
    , FileFormat:=xlOpenXMLWorkbookMacroEnabled, CreateBackup:=False
'   * Alerts User to End of Process *
    If MsgBox("The provisional document has been generated.", vbOKOnly + vbInformation, "Process Complete") = vbOK Then
'   * Ends Alert *
    End If
'   * Provides Option to Alert Team *
    If MsgBox("Would you like to distribute a request for approval?", vbYesNo + vbQuestion, "Product Master") = vbYes Then
'   * Actions Distribution *
    Call ProducerAlert
'   * Cancels Distribution *
    End If
'   * Completes Action *
    End If
End Sub
Sub ProducerAlert()
'   *** Generates Distribution Alert ***
'   * Defines OutApp Dimension as Object *
    Dim OutApp As Object
'   * Defines OutMail Diminsions as Object *
    Dim OutMail As Object
'   * Defines AlertContent as String *
    Dim AlertContent As String
'   * Checks Path *
    If ActiveWorkbook.Path <> "" Then
'   * Sets OutApp as Opening Outlook *
    Set OutApp = CreateObject("Outlook.Application")
'   * Sets OutMail as Creating a New Email *
    Set OutMail = OutApp.CreateItem(0)
'   * Defines Alert Content *
    AlertBody = "<font size=""2"" face=""Arial"">" & _
                "Hello,<br><br>" & _
                "A provisional product guide has been generated and is awaiting approval.<br><br>" & _
                "<A HREF=""file://" & ActiveWorkbook.FullName & _
                """>Click Here to View</A>" & _
                "<br><br>Regards," & _
                "<br><br>Product Team Robot</font>"
'   * Defines Error Conditions *
    On Error Resume Next
'   * Starts Sending Process *
    With OutMail
'   * Defines Main Recipients *
    .To = "bcharlesworth@leedsbuildingsociety.co.uk;cleavesley@leedsbuildingsociety.co.uk;gsagoo@leedsbuildingsociety.co.uk;lnewbould2@leedsbuildingsociety.co.uk;jabrahams@leedsbuildingsociety.co.uk"
'   * Defines CC Recipients *
    .CC = ""
'   * Defines BCC Recipients *
    .BCC = ""
'   * Defines Alert Subject *
    .Subject = "Product Guide Awaiting Approval"
'   * Populates Alert *
    .HTMLBody = AlertBody
'   * Sends Alert *
    .Send
'   * Completes Sending Proecess *
    End With
'   * End Sending Process *
    On Error GoTo 0
'   * Defines Error Function *
    Set OutMail = Nothing
'   * Defines Error Function *
    Set OutApp = Nothing
'   * Defines Error Function *
    Else
'   * Alerts User to Failed Process *
    MsgBox "This process has failed, please see the Help section."
'   * Completes Action *
    End If
End Sub
