'   *** Copy From APR Calculator ***
    Public Function monthly_payment(principal As Long, annual_rate As Double, year_term As Single) As Double
    Dim monthly_rate, subtotal As Double
    Dim month_term, index As Integer
    monthly_rate = annual_rate / 365
    month_term = Int(year_term * 365)
    subtotal = 0
    For index = 1 To month_term
        subtotal = subtotal + 1 / ((1 + monthly_rate) ^ index)
    Next index
    monthly_payment = principal * 365 / 12 / subtotal
    End Function
'   *** Copy From APR Calculator ***
    Public Function annual_payment(principal As Long, annual_rate As Double, year_term As Integer) As Double
    Dim subtotal As Double
    Dim index As Integer
    subtotal = 0
    For index = 1 To year_term
    subtotal = subtotal + 1 / ((1 + annual_rate) ^ index)
    Next index
    annual_payment = principal / subtotal
    End Function
