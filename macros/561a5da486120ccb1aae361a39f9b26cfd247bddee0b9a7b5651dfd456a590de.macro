'Ambient temperature minimum
Private Sub TextBox55_Change()

Worksheets("Hulpsheet").Range("H120") = TextBox55.Value

If TextBox55.Value <> "" Then

    If (TextBox55.Value >= -40 And TextBox55.Value <= 45) Then

        Image307.Visible = True
        
    ElseIf TextBox55.Value = "-" Then
        
        Image307.Visible = False
    
    ElseIf TextBox55.Value < -40 Then
        Dim Msg, Style, Title, Response
        Msg = "Please enter a value between -40 and 45"
        Style = vbOKOnly + vbExclamation
        Title = "Incorrect value"
        Response = MsgBox(Msg, Style, Title)
        TextBox55.Value = -40
'        Image307.Visible = False
    
    ElseIf TextBox55.Value > 45 Then
        Dim Msg1, Style1, Title1, Response1
        Msg1 = "Please enter a value between -40 and 45"
        Style1 = vbOKOnly + vbExclamation
        Title1 = "Incorrect value"
        Response1 = MsgBox(Msg1, Style1, Title1)
        TextBox55.Value = 45
'        Image307.Visible = False
    
    Else
        Dim Msg2, Style2, Title2, Response2
        Msg2 = "Please enter a value between -40 and 45"
        Style2 = vbOKOnly + vbExclamation
        Title2 = "Incorrect value"
        Response2 = MsgBox(Msg2, Style2, Title2)
        
        Image307.Visible = False
    
    End If
    
Else
    Image307.Visible = False
End If

'Check_Complete_GI
GI_Complete = Check_Complete_GI()
'MsgBox ("compleet: " & GI_Complete)

End Sub

