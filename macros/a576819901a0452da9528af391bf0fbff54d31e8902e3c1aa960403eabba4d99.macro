            Case 9:     PDFtk_File = "Docs\PDFtk.1.HTML"
            Case 10:    PDFtk_File = "Docs\PDFtk.1.TXT"

            Case 11:    PDFtk_File = "License_GPL_PDFtk\Reference\Apache_License_2.TXT"
            Case 12:    PDFtk_File = "License_GPL_PDFtk\Reference\GCC_Runtime_Library_Exception_3.1.HTML"
            Case 13:    PDFtk_File = "License_GPL_PDFtk\Reference\GCC_Runtime_Library_Exception_3.1.TXT"
            Case 14:    PDFtk_File = "License_GPL_PDFtk\Reference\GNU_General_Public_License_2.TXT"
            Case 15:    PDFtk_File = "License_GPL_PDFtk\Reference\GNU_General_Public_License_3.0.TXT"
            Case 16:    PDFtk_File = "License_GPL_PDFtk\Reference\GNU_LGPL_License_2.1.TXT"
            Case 17:    PDFtk_File = "License_GPL_PDFtk\Reference\GNU_LGPL_License_2.TXT"

            Case 18:    PDFtk_File = "License_GPL_PDFtk\Third_Party\Apache_Batik.TXT"
            Case 19:    PDFtk_File = "License_GPL_PDFtk\Third_Party\Apache_Batik_Notice.TXT"
            Case 20:    PDFtk_File = "License_GPL_PDFtk\Third_Party\Bouncy_Castle.TXT"
            Case 21:    PDFtk_File = "License_GPL_PDFtk\Third_Party\ClassPath.TXT"
            Case 22:    PDFtk_File = "License_GPL_PDFtk\Third_Party\CRT1_CRT2.TXT"
            Case 23:    PDFtk_File = "License_GPL_PDFtk\Third_Party\IText-Paulo.TXT"
            Case 24:    PDFtk_File = "License_GPL_PDFtk\Third_Party\LibGCC.TXT"
            Case 25:    PDFtk_File = "License_GPL_PDFtk\Third_Party\LibGCJ.TXT"
            Case 26:    PDFtk_File = "License_GPL_PDFtk\Third_Party\LibIconv.TXT"
            Case 27:    PDFtk_File = "License_GPL_PDFtk\Third_Party\LibStdC++.TXT"
            Case 28:    PDFtk_File = "License_GPL_PDFtk\Third_Party\Libz.TXT"
            Case 29:    PDFtk_File = "License_GPL_PDFtk\Third_Party\MinGW.TXT"

            Case 30:    PDFtk_File = "Unins000.EXE"
