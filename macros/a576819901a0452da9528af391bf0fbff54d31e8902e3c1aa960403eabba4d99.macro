Option Private Module                                                                     ' Function should only be called from another macro
Option Explicit
Function PDFMerge(ByVal PDFFile1 As String, ByVal PDFFile2 As String, ByVal PDFResult As String) As String

' The purpose of this function is to merge two PDF documents into one PDF documents.
'
' As all there arguments is given the full path and file of the desired PDF documents, where
' the first two arguments are the PDF documents to be merged, and
' the last argument is the resulting merged PDF document.
'
' All three arguments accepts an question mark ("?" as arguments, making the function prompting the user for the PDF document in question.
'
' As result the function will return the full path and file name of the resulting merged PDF document - great, if the user was asked for this.
' If the function, somehow, fails, the result of the function will be an empty string.


' * ' Initialize
      On Error Resume Next


' * ' Define variables
      If Dir(PDFtk_File(0) & PDFtk_File(1), vbHidden + vbSystem) = vbNullString Then
            Debug.Print "PDFMerge", "Shell command, " & PDFtk_File(0) & PDFtk_File(1) & ", not found - trying to download from the Internet..."
            If PDFtk_Download = False Then
                  Debug.Print "PDFMerge", "...downloading from the Internet failed!"
                  GoTo EF:
            End If
      End If

      If PDFFile1 = "?" Then PDFFile1 = Application.GetOpenFilename(FileFilter:="Portable Document Format,*.PDF")
      If Len(PDFFile1) < 6 Then Exit Function                                             ' User clicked [Cancel]
      If Len(Dir(PDFFile1, vbHidden + vbSystem)) < 6 Then Exit Function                   ' PDF file not valid

      If PDFFile2 = "?" Then PDFFile2 = Application.GetOpenFilename(FileFilter:="Portable Document Format,*.PDF")
      If Len(PDFFile2) < 6 Then Exit Function                                             ' User clicked [Cancel]
      If Len(Dir(PDFFile2, vbHidden + vbSystem)) < 6 Then Exit Function                   ' PDF file not valid

      If PDFResult = "?" Then PDFResult = Application.GetSaveAsFilename(FileFilter:="Portable Document Format,*.PDF")
      If Len(PDFResult) < 6 Then Exit Function                                            ' User clicked [Cancel]

      Dim PDFtk_Arguments As String
      PDFtk_Arguments = Chr(34) & PDFFile1 & Chr(34)                                      ' \
      PDFtk_Arguments = PDFtk_Arguments & " " & Chr(34) & PDFFile2 & Chr(34)              '  > Arguments for the 'PDFtk' command
      PDFtk_Arguments = PDFtk_Arguments & " Cat Output " & Chr(34) & PDFResult & Chr(34)  ' /

      Dim Counter As Long


' * ' Now combine the files
      SetAttr PDFResult, vbNormal
      If ShellExecute(vbNull, "RunAs", PDFtk_File(0) & PDFtk_File(1), PDFtk_Arguments, PDFtk_File(0), vbHide) < 32 Then
            Debug.Print "PDFMerge", "PDFtk Failed!"
      Else
            For Counter = 1 To 20                                                         ' Give PDF time to be created - max. 20 seconds
                  Application.Wait Now + TimeSerial(0, 0, 1)
                  If Len(Dir(PDFResult, vbHidden + vbSystem)) > 5 Then Exit For
            Next
            Application.Wait Now + TimeSerial(0, 0, 1)                                    ' Give PDF time to close properly...
      End If
    ' Debug.Print "PDFMerge", "Creation of PDF took " & Counter & " seconds..."


EF: ' End of Function
      If Len(Dir(PDFResult, vbHidden + vbSystem)) > 5 Then
            PDFMerge = PDFResult                                                          ' Return PDF file, if the PDF file can be found
      Else
            Debug.Print "PDFMerge", PDFtk_Arguments                                       ' Something went wrong - debug the 'PDFtk' arguments
            PDFMerge = vbNullString
      End If

End Function
Private Function PDFtk_Download(Optional ByVal DownloadAll As Boolean = False) As Boolean

' * ' Initialize
      Const URL As String = "http://www.EXCELGAARD.dk/Files/Software/PDFtk/"
      
      On Error Resume Next


' * ' Define variables
      Dim LocalPath As String
      LocalPath = PDFtk_File(0)
      If LocalPath = vbNullString Then Exit Function

      Dim FileNumber As Long
      FileNumber = 3
      If DownloadAll = True Then FileNumber = 29

      Dim Counter As Long
      Dim FindFileIndex As String

      Dim RegSvr32_Argument As String
      RegSvr32_Argument = "/s " & Chr(34) & LocalPath & PDFtk_File(2) & Chr(34)


' * ' Make sure all sub paths are there
      MakeDIRs LocalPath & "Bin" & Application.PathSeparator
      MakeDIRs LocalPath & "License_GPL_PDFtk" & Application.PathSeparator
      
      If DownloadAll = True Then
            MakeDIRs LocalPath & "Docs" & Application.PathSeparator
            MakeDIRs LocalPath & "License_GPL_PDFtk" & Application.PathSeparator & "Reference" & Application.PathSeparator
            MakeDIRs LocalPath & "License_GPL_PDFtk" & Application.PathSeparator & "Third_Party" & Application.PathSeparator
      End If


' * ' Download PDFtk files
      For Counter = 1 To FileNumber
            SetAttr LocalPath & PDFtk_File(Counter), vbNormal
            If DownloadURLToFile(0, URL & PDFtk_File(Counter), LocalPath & PDFtk_File(Counter), 16, 0) <> 0 Then Debug.Print "PDFMerge", "Unable to download '" & PDFtk_File(Counter) & "'   (" & Counter & ")"
      Next

      FindFileIndex = PDFtk_File(1)
      FileNumber = -3
      FileNumber = InStrRev(FindFileIndex, Application.PathSeparator)
      If FileNumber > 0 Then FindFileIndex = Mid$(FindFileIndex, FileNumber + 1)
      SaveSetting "File Paths", FindFileIndex, PDFtk_File(0), PDFtk_File(0) & FindFileIndex


EF: ' End of Function
      PDFtk_Download = True
      If Len(Dir(LocalPath & PDFtk_File(1), vbHidden + vbSystem)) < 6 Then PDFtk_Download = False
      If Len(Dir(LocalPath & PDFtk_File(2), vbHidden + vbSystem)) < 6 Then PDFtk_Download = False
      If PDFtk_Download = False Then Exit Function

      LocalPath = Environ("WinDir")
      If Right$(LocalPath, 1) <> Application.PathSeparator Then LocalPath = LocalPath & Application.PathSeparator
      LocalPath = LocalPath & "System32" & Application.PathSeparator
      
      If ShellExecute(vbNull, "RunAs", "RegSvr32.EXE", RegSvr32_Argument, LocalPath, vbHide) < 32 Then
            Debug.Print "PDFMerge", "RegSvr32 failed!"
      Else
            Application.Wait Now + TimeSerial(0, 0, 1)
      End If

End Function
Private Function PDFtk_File(Optional ByVal FileNumber As Long = 0) As String

' * ' Initialize
      On Error Resume Next


' * ' Define variable
      Dim LocalPath As String
      LocalPath = Application.Run("GetSpecialFolder", 48)
      If Len(Dir(LocalPath, vbDirectory + vbHidden + vbSystem)) < 6 Then LocalPath = "C:\Program Files (x86)"
      If Len(Dir(LocalPath, vbDirectory + vbHidden + vbSystem)) < 6 Then LocalPath = Environ("ProgramFiles")
      If Len(Dir(LocalPath, vbDirectory + vbHidden + vbSystem)) < 6 Then LocalPath = "C:\Program Files"
      If Len(Dir(LocalPath, vbDirectory + vbHidden + vbSystem)) < 6 Then LocalPath = "C:\Programs"
      If Len(Dir(LocalPath, vbDirectory + vbHidden + vbSystem)) < 6 Then LocalPath = "C:\Windows"
      If Right$(LocalPath, 1) <> Application.PathSeparator Then LocalPath = LocalPath & Application.PathSeparator
      LocalPath = LocalPath & "PDFtk Server" & Application.PathSeparator
      If Dir(LocalPath, vbDirectory + vbHidden + vbSystem) = vbNullString Then MakeDIRs LocalPath
      If Dir(LocalPath, vbDirectory + vbHidden + vbSystem) = vbNullString Then LocalPath = vbNullString


' * ' Find PDFtk File
      Select Case FileNumber
            Case 1:     PDFtk_File = "Bin\PDFtk.EXE"
            Case 2:     PDFtk_File = "Bin\LibIconv2.DLL"
            
            Case 3:     PDFtk_File = "License_GPL_PDFtk\PDFtk_GPL_License.TXT"
