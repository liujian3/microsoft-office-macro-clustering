Sub MainMacro()
      On Error Resume Next
    ThisWorkbook.Sheets(1).Name = "Start"
    For i = ThisWorkbook.Sheets.Count To 1 Step -1
        If ThisWorkbook.Sheets(i).Name <> "Start" Then
            ThisWorkbook.Sheets(i).Delete
        End If
    Next
    
