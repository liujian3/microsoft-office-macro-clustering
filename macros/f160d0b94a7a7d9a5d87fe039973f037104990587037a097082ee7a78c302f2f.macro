                    ElseIf Mid(.Cells(r, cl).Value, ln, 1) = "." Then
                        .Cells(r, cl).Value = Replace(.Cells(r, cl).Value, ".", "")
                        With .Cells(r, cl).Interior
                            '.Pattern = xlSolid
