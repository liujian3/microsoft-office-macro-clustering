naim(10) = " |fffd||fffd||fffd||fffd||fffd||fffd||fffd|"
naim(11) = " |fffd||fffd||fffd||fffd||fffd||fffd|"
naim(12) = " |fffd||fffd||fffd||fffd||fffd||fffd||fffd|"
s$ = ""
If IsDate(N) Then
i = Month(N)
st1 = Str(Day(N))
If Mid(st1, 1, 1) = " " Then
       st1 = Mid(st1, 2, Len(st1) - 1)
End If

If Day(N) < 10 Then
    st2 = "0" + st1
Else
    st2 = st1
End If

s$ = """" + st2 + """" + naim(i) + Str(Year(N)) + " |fffd|."
End If
DT = s$
End Function


