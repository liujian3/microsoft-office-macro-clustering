'Material ausgew|fffd|hlt und |fffd|bertragung der Eigenschaften aus Excelsheet DATA

Private Sub CommandButton_MaterialeingabeBest_Click()
'Eigenschaften anzeigen // ComboBoxen f|fffd|r unterschiedliche Hoehen !
Dim lfdNr As Integer
Dim RG As Integer
Dim Stauchh As Double
Dim Farbe As String
Dim laenge As Integer
Dim Breite As Integer
Dim nutzhoehe As Integer
Dim Eingabe As Range
Dim EKProBlock As Double
Dim VKProBlock As Double
Dim EKProPlatte As Double
Dim VKProPlatte As Double
Dim EKProKilo As Double
Dim abfallhoehe As Double

'lfdNr suchen
Dim zeile As Integer
Set Eingabe = Worksheets("DATA").Columns(2).Find(what:=ComboBox_Material.Value, _
                lookat:=xlWhole)
zeile = Eingabe.Row
'Eigenschaften des ausgewaehlten Materials
lfdNr = zeile - 2
RG = Worksheets("DATA").Cells(lfdNr + 2, 3).Value
Stauchh = Worksheets("DATA").Cells(lfdNr + 2, 4).Value
laenge = Worksheets("DATA").Cells(lfdNr + 2, 6).Value
abfallhoehe = Worksheets("DATA").Cells(lfdNr + 2, 10).Value
EKProBlock = Worksheets("DATA").Cells(lfdNr + 2, 11).Value
VKProBlock = Worksheets("DATA").Cells(lfdNr + 2, 12).Value
EKProPlatte = Worksheets("DATA").Cells(lfdNr + 2, 13).Value
VKProPlatte = Worksheets("DATA").Cells(lfdNr + 2, 14).Value
EKProKilo = Worksheets("DATA").Cells(lfdNr + 2, 15).Value
'Mehr Farben zur Auswahl
ComboBox_Farbe.AddItem (Worksheets("DATA").Cells(lfdNr + 2, 5).Value)
Dim i As Integer
i = 1
Do While IsEmpty(Worksheets("DATA").Cells(lfdNr + 2 + i, 2).Value) = True
    If IsEmpty(Worksheets("DATA").Cells(lfdNr + 2 + i, 5).Value) = False Then
        ComboBox_Farbe.AddItem (Worksheets("DATA").Cells(lfdNr + 2 + i, 5).Value)
        i = i + 1
    Else: Exit Do
    End If
