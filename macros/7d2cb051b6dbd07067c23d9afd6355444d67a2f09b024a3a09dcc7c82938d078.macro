'Quadratmeter-------------------------------------------------

plattenflaeche = TextBox_Laenge.Value / 1000 * ComboBox_Breite.Value / 1000 'in m|fffd|
If bool_ekm2 = True Then
    gesamtpreis_Material = TextBox_verwPlatten.Value * plattenflaeche * verechnungspreis
End If
If bool_vkm2 = True Then
    gesamtpreis_Material = TextBox_verwPlatten.Value * plattenflaeche * verechnungspreis
End If
'-------------------------------------------------------------------------------
TextBox_preisproblock.Value = preisProBlock

'f|fffd|r m3 und m2  unterschiedliche berechnungen des Plattenpreises--> LxBxH f|fffd|r m3
If bool_ekm3 = True Or bool_vkm3 = True Then
    TextBox_preisproplatte.Value = plattenVolumen * verechnungspreis
End If
If bool_ekm2 = True Or bool_vkm2 = True Then
    TextBox_preisproplatte.Value = plattenflaeche * verechnungspreis
End If
TextBox_preisprozuschnitt.Value = TextBox_preisproplatte.Value / TextBox_zuschnitteProPlatte.Value
TextBox_Materialpreis.Value = gesamtpreis_Material

End Sub



