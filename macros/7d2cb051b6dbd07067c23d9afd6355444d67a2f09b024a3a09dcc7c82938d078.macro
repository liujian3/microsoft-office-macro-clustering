        End If
    ElseIf TextBox_resthoehemehrbloecke.Value >= TextBox_abfallhoehe.Value And TextBox_verwendeteBloecke.Value > 1 Then 'mehr als 1 Block und rest exklusive Preis
        If bool_ekm3 = True Then
            preisProBlock = TextBox_plattenProBlock.Value * plattenVolumen * verechnungspreis
            If extraBlock = True Then
                If TextBox_restextrablock.Value < TextBox_abfallhoehe.Value Then
                    preisExtraBlock = preisProBlock
                Else:
                    preisExtraBlock = TextBox_plattenextraBlock.Value * plattenVolumen * verechnungspreis
                End If
            End If
            gesamtpreis_Material = TextBox_verwendeteBloecke.Value * preisProBlock + preisExtraBlock
        ElseIf bool_vkm3 = True Then
            preisProBlock = TextBox_plattenProBlock.Value * plattenVolumen * verechnungspreis
            If extraBlock = True Then
                If TextBox_restextrablock.Value < TextBox_abfallhoehe.Value Then
                    preisExtraBlock = preisProBlock
                Else:
                    preisExtraBlock = TextBox_plattenextraBlock.Value * plattenVolumen * verechnungspreis
                End If
            End If
            gesamtpreis_Material = TextBox_verwendeteBloecke.Value * preisProBlock + preisExtraBlock
        End If
    End If
