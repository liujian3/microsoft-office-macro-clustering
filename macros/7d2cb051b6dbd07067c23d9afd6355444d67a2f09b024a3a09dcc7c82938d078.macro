    TextBox_gesamtpreis_Arbeitsgaenge.Value = CDbl(TextBox_Preis_SpaltenPU.Value) + CDbl(TextBox_Preis_SpaltenPE.Value) _
    + CDbl(TextBox_Preis_SchneidenRechteck.Value) + CDbl(TextBox_Preis_SchneidenKontur.Value) _
    + CDbl(TextBox_Preis_Stanzen.Value) + CDbl(TextBox_Preis_Ausbrechen.Value) + CDbl(TextBox_Preis_Fr|fffd|sen.Value) _
    + CDbl(TextBox_Preis_VerklebenF.Value) + CDbl(TextBox_Preis_VerklebenP.Value) + CDbl(TextBox_Preis_VerschweissenF.Value) _
