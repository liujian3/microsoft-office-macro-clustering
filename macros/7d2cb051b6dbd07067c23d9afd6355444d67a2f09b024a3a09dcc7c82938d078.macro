'ZuschnitteProPlatte/scheibe bestimmen
Dim zuschnitteProPlatte As Integer
zuschnitteProPlatte = anz_plattenProBlockL1 * anz_plattenProBlockB1 + (anz_plattenRestL1 * anz_plattenRestB1)
'TextBox_zuschnitteProPlatte.Value = zuschnitteProPlatte
'TextBox_plattenProBlock.Value = anz_plattenProBlockH1
anz_vorkalkulation = zuschnitteProPlatte

End Function

