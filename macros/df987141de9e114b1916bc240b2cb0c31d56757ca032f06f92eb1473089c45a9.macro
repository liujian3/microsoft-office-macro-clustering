Function RndProCon()
'On Error Resume Next
        subjx = CLng(Rnd * ProGroup.Count) + 1
        subjy = CLng(Rnd * ConGroup.Count) + 1
    Do Until subjx <> subjy And subjy <= ConGroup.Count And subjx <= ProGroup.Count
        subjx = CLng(Rnd * ProGroup.Count) + 1
        subjy = CLng(Rnd * ConGroup.Count) + 1
    Loop
End Function
