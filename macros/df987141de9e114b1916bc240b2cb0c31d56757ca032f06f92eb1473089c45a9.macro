                AnsTog = 1
        ActivePresentation.Slides(s).Shapes("Return").Visible = True
        ActivePresentation.Slides(s).Shapes("Right").ActionSettings(ppMouseClick).SoundEffect.Play
        Exit Sub
    End If
    If Ans = "C" And oShp.Name = "AnswerC" Then
        oShp.Fill.ForeColor.RGB = RGB(0, 255, 0)
                AnswerTog = 1
                AnsTog = 1
        ActivePresentation.Slides(s).Shapes("Return").Visible = True
        ActivePresentation.Slides(s).Shapes("Right").ActionSettings(ppMouseClick).SoundEffect.Play
        Exit Sub
    End If
    If Ans = "D" And oShp.Name = "AnswerD" Then
        oShp.Fill.ForeColor.RGB = RGB(0, 255, 0)
                AnswerTog = 1
                AnsTog = 1
        ActivePresentation.Slides(s).Shapes("Return").Visible = True
        ActivePresentation.Slides(s).Shapes("Right").ActionSettings(ppMouseClick).SoundEffect.Play
        Exit Sub
    End If
    AnsTog = 1
    'Wrong Answer, Show correct button
    oShp.Fill.ForeColor.RGB = RGB(255, 0, 0)
