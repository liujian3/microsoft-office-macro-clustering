Sub Peek()
Dim s As Long
Dim sld As Slide
s = ActivePresentation.SlideShowWindow.View.CurrentShowPosition
If Not AnsTog >= 1 Then
 ActivePresentation.Slides(s).Shapes("Select").ActionSettings(ppMouseClick).SoundEffect.Play
Call FillStudentSlate
    On Error GoTo ErrorHandler
For Each sld In ActivePresentation.Slides
sld.Shapes("PEEK").Fill.ForeColor.RGB = RGB(89, 89, 89)
sld.Shapes("PEEKX").Visible = True
Next
ActivePresentation.Slides(s).Shapes("SlateGroup").Visible = True
ActivePresentation.Slides(s).Shapes("Continue").Visible = True
End If

NormalExit:
    Exit Sub
ErrorHandler:
    Resume Next
End Sub
