'Change fill to yellow
        oShp.Fill.ForeColor.RGB = RGB(255, 255, 0)

 ActivePresentation.Slides(s).Shapes("Select").ActionSettings(ppMouseClick).SoundEffect.Play
 'Wait 3 seconds for dramatic pause
         t = Timer + 2.7
            Do Until Timer > t
                'ActivePresentation.Slides(2).Shapes("TEST").Visible = True
            DoEvents
            Loop
    
    
    If Ans = "A" And oShp.Name = "AnswerA" Then
        oShp.Fill.ForeColor.RGB = RGB(0, 255, 0)
