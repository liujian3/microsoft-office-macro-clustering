Private Sub Workbook_BeforeClose(Cancel As Boolean)


Sheets("Shipping Order Form").Select

With ActiveSheet
.Unprotect Password:="pahkgbooking"
End With

    Range("BN8:BO31").Select
    Selection.Copy
    Range("BP8").Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    Application.CutCopyMode = False
    ActiveWorkbook.Worksheets("Shipping Order Form").Sort.SortFields.Clear
    ActiveWorkbook.Worksheets("Shipping Order Form").Sort.SortFields.Add Key:= _
        Range("BP8:BP31"), SortOn:=xlSortOnValues, Order:=xlAscending, CustomOrder _
        :="SNA,SCD,CNAA,MOT,CRD,TOD,PPTD,POL,POD,DRT,FPA,NOG,DG,UNNO,CDG,SWP,WPC,MN, ET, QTY, PKG, DOG, GW, MEAS, Z", DataOption:= _
        xlSortNormal
    With ActiveWorkbook.Worksheets("Shipping Order Form").Sort
        .SetRange Range("BP8:BQ31")
        .Header = xlGuess
        .MatchCase = False
        .Orientation = xlTopToBottom
        .SortMethod = xlPinYin
        .Apply
    End With
    Range("BQ8:BQ31").Select
    Selection.Replace What:="Z", Replacement:="", LookAt:=xlPart, _
        SearchOrder:=xlByRows, MatchCase:=False, SearchFormat:=False, _
        ReplaceFormat:=False
    Range("F8:AH8").Select
Dim Confirm As Variant

If Range("D7:D7").Value > 0 Then

MsgBox "Please provide:" & Chr(10) & _
