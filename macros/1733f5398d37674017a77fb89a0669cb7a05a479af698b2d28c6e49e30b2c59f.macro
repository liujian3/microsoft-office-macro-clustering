Option Explicit

Private Sub Workbook_SheetActivate(ByVal Sh As Object)
On Error GoTo Workbook_SheetActivate_Exit

  Dim strCharacter As String
  Dim lngCounter As Long

  ' Sorteer de groepsvolgorde
  If Sh.Name = "Stand groepswedstrijden" Then
    For lngCounter = 1 To 4
      strCharacter = Choose(lngCounter, "A", "B", "C", "D")
      Range("Sorteer" & strCharacter).Select
      Selection.Sort _
          Key1:=Range("C:C"), Order1:=xlDescending, _
          Key2:=Range("B:B"), Order2:=xlAscending, _
          Header:=xlNo, _
          OrderCustom:=1, _
          MatchCase:=False, _
          Orientation:=xlTopToBottom
    Next
    Range("A2").Select
  End If

Workbook_SheetActivate_Exit:
End Sub



