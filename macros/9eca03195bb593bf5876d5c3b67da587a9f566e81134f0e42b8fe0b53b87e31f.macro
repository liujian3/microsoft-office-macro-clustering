 Public Function legal_char_email(p_string As String) As Boolean
     Dim i As Integer
     legal_char_email = True
     For i = 1 To Len(p_string)
         Select Case UCase(Mid(p_string, i, 1))
             Case "A" To "Z", "0" To "9", "@", "_", ".", "-"
             Case Else
                 legal_char_email = False
                 
         End Select
     Next i
 End Function
  Public Function remove_illegalchars(p_string As String) As String
     Dim i As Integer
     Dim legal_char As String
     legal_char = ""
     
     If (p_string <> "") Then
        For i = 1 To Len(p_string)
            Select Case Mid(p_string, i, 1)
                Case "A" To "Z", "a" To "z", " ", "0" To "9", "@", "_", ".", "-", "/", "&", "(", ")", "%", "$", "+", ","
                    legal_char = legal_char & Mid(p_string, i, 1)
                Case Else
                
                    
            End Select
        Next i
     End If
     remove_illegalchars = legal_char
 End Function

