Public Function get_fin_Year(pdate As String) As String
    Dim l_finyear As String
    If Mid(pdate, 5, 4) >= 401 Then
        l_finyear = Mid(pdate, 1, 4) & "-" & Val(Mid(pdate, 1, 4)) + 1
    Else
        l_finyear = Val(Mid(pdate, 1, 4)) - 1 & "-" & (Mid(pdate, 1, 4))
    End If
    get_fin_Year = l_finyear
    
End Function
Public Function remove_illegal_char(pstring As String) As String
    Dim lstring As String
    lstring = ""
    If Trim(pstring) <> "" Then
        For i = 1 To Len(pstring)
            If Asc(Mid(pstring, i, 1)) >= 33 And Asc(Mid(pstring, i, 1)) <= 126 Then
                lstring = lstring & Mid(pstring, i, 1)
            End If
        Next
    End If
    remove_illegal_char = lstring
End Function

