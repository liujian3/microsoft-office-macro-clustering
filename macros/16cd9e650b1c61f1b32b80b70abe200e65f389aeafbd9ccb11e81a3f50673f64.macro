Private Sub Workbook_SheetChange(ByVal Sh As Object, ByVal Target As Range)
  On Error Resume Next
  If Not Target.Address Like Range("Filter").Address & "*" Then
  ElseIf Target.Validation.Type <> xlValidateList Then
  Else
    Dim s As String, i As Long, bShow As Boolean
  
    s = "D"
    s = "D" & Left$("" & Target.Value, 1)
    
    For i = Target.Row + 1 To ActiveSheet.UsedRange.Rows.Count
      bShow = (s = Cells(i, 2)) Or s = "D" Or Not Cells(i, 2) Like "D[1-9]"
      
      If Rows(i).Hidden <> Not bShow Then
        Rows(i).Hidden = Not bShow
      End If
    Next i
  End If
End Sub


