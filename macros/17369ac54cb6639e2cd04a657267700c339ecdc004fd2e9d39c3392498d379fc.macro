Sub Grab_ZBACKLOG()
Dim A As Long, B As Long, C As Long, xrate As Long
Dim x As Range
Dim DatArray() As Variant
Dim MyBook As String, fname As String, fpath As String, strdate As String
Dim shtyear As Single, shtmth As Single

Application.Calculation = xlCalculationManual

MyBook = ThisWorkbook.Name
fname = Range("fname_ZBACKLOG").Value 'Filename from Data tab
fpath = Range("fpath_ZBACKLOG").Value 'File path from Data tab

xrate = Range("XRATE").Value
shtyear = Range("SHEETYR").Value
shtmth = Range("SHEET_MTH").Value

'On Error GoTo ErrHandler
'Open ZBACKLOG Download
Workbooks.OpenText Filename:= _
    fpath & fname, Origin:=437, _
    StartRow:=1, DataType:=xlDelimited, TextQualifier:=xlDoubleQuote, _
    ConsecutiveDelimiter:=False, Tab:=True, Semicolon:=False, Comma:=False _
    , Space:=False, Other:=False, TrailingMinusNumbers:=True

A = Range("A1000000").End(xlUp).Row 'Last Row of table

ReDim DatArray(1 To A, 1 To 12)
    B = 1
    For C = 2 To A 'Put items into an array
        DatArray(B, 1) = Range("X" & C).Value 'Plant
        DatArray(B, 2) = Range("H" & C).Value 'Material #
        DatArray(B, 3) = Range("A" & C).Value & "/" & Range("G" & C).Value 'Sales Order/Line Item
        DatArray(B, 4) = Range("F" & C).Value 'Sold-To Party
            strdate = Range("L" & C).Value
        DatArray(B, 5) = CDate(Mid(strdate, 5, 2) & "/" & Right(strdate, 2) & "/" & Left(strdate, 4)) 'Schedule Line Date
        DatArray(B, 6) = Range("I" & C).Value 'Material Group
            ''Convert other material groups to general category of "Others"
            If DatArray(B, 6) = "100" Or DatArray(B, 6) = "PKGN" Or DatArray(B, 6) = "SVC" Or _
            DatArray(B, 6) = "COM" Or DatArray(B, 6) = "MISC" Or DatArray(B, 6) = "RM" Then
                DatArray(B, 6) = "OTHERS"
            End If
        DatArray(B, 7) = Range("K" & C).Value 'Currency
        DatArray(B, 8) = Range("J" & C).Value 'Net Value
        ''Convert currencies to match forecasting currency in shipping plant
            If DatArray(B, 7) = "USD" And DatArray(B, 1) = "CA77" Then
                DatArray(B, 8) = DatArray(B, 8) * xrate
                DatArray(B, 7) = "CAD"
            ElseIf DatArray(B, 7) = "CAD" And Left(DatArray(B, 1), 2) = "US" Then
                DatArray(B, 8) = DatArray(B, 8) / xrate
                DatArray(B, 7) = "USD"
            End If
        DatArray(B, 9) = "" 'CM
        ''Convert to metric tons
            If Range("AE" & C).Value = "LB" Then
                DatArray(B, 10) = Range("AD" & C).Value / 2204.62 'Tons
            ElseIf Range("AE" & C).Value = "KG" Then
                DatArray(B, 10) = Range("AD" & C).Value / 1000 'Tons
            End If
        DatArray(B, 11) = Year(DatArray(B, 5)) 'Year
        ''Converts Schedule Line date to date/month used in sheet
            If DatArray(B, 11) < shtyear Then
                DatArray(B, 12) = "Past Dues"
                DatArray(B, 11) = shtyear
            ElseIf DatArray(B, 11) > shtyear Then
                DatArray(B, 12) = "Future"
            ElseIf Month(DatArray(B, 5)) < shtmth And DatArray(B, 11) <= shtyear Then
                DatArray(B, 12) = "Past Dues"
                DatArray(B, 11) = shtyear
            Else
                DatArray(B, 12) = MonthName(Month(DatArray(B, 5)), False)
            End If
            B = B + 1
    Next C

ActiveWindow.Close SaveChanges:=False
Windows(MyBook).Activate
  
Range("DATE_ZBACKLOG").Value = FileDateTime(fpath & fname)

Sheet4.Activate 'Select ZBACKLOG sheet
Show_All

'Clear Old Data
Range("A3:P1000000").ClearContents
    Range("A2").Select
Set x = Range("A2:L" & B)
    x = DatArray

Grab_ZBACKLOG_CA

A = Range("C1000000").End(xlUp).Row 'Last Row of table

'Range("O2").Select
'    ActiveCell.FormulaR1C1 = _
'        "=IF(RC14<SHEETYR,""Past Dues"",IF(RC14>SHEETYR,""FUTURE"",IF(AND(RC13<SHEET_MTH,RC14<=SHEETYR),""Past Dues"",IF(LEFT(RC5,2)=""20"",TEXT(DATE(LEFT(RC5,4),MID(RC5,5,2),RIGHT(RC5,2)),""mmmm""),TEXT(RC5,""mmmm"")))))"
Range("M2").Select
    ActiveCell.FormulaR1C1 = "=INDEX(CustMaster!C2,MATCH(RC4,CustMaster!C1,0))"
Range("M2").Select
    Selection.AutoFill Destination:=Range("M2:M" & A)

'Sort Data
Range("A2:L" & A).Sort Key1:=Range("A2"), Order1:=xlAscending, Key2:=Range("E2"), _
    Order2:=xlAscending, Header:=xlNo

ActiveSheet.Calculate 'Calculate Sheet

'Copy/Paste values over the equations
Range("M2:M" & A).Select
    Selection.Copy
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    Application.CutCopyMode = False

ActiveSheet.AutoFilterMode = False
Range("A1:M" & A).AutoFilter

Application.Calculation = xlCalculationAutomatic

Sheet13.Activate

End Sub

