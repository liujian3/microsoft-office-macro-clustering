Sub Refresh_RunRates_MidMonth()

Application.ScreenUpdating = False
Application.DisplayAlerts = False

RunRates_MidMonth

Application.DisplayAlerts = True
Application.ScreenUpdating = True

MsgBox "All Plant Run Rates Updated"

End Sub

Sub RunRates_MidMonth()
Dim A As Long, B As Long, C As Long
Dim PltArray() As Variant
Dim mth As Single, basecol As Single
'
' Pastes run rate calculation
'

mth = Range("SHEET_MTH").Value
basecol = (mth * 4) + 3

ReDim PltArray(1 To 7)
PltArray(1) = "US71"
PltArray(2) = "US72"
PltArray(3) = "US73"
PltArray(4) = "US74"
PltArray(5) = "US75"
PltArray(6) = "US76"
PltArray(7) = "CA77"

For A = 1 To 7

Sheets(PltArray(A)).Activate
    
    For C = 78 To 2487 Step 219
        Range(Cells(C, (basecol)), Cells((C + 31), (basecol + 2))).ClearContents 'Clear current month adjustments
    Next C
    
    Cells(79, basecol).Select
        ActiveCell.FormulaR1C1 = "=IF((((R[-69]C/DAYS_DONE)*DAYS_LEFT)-R[-45]C)<=1,0,((R[-69]C/DAYS_DONE)*DAYS_LEFT)-R[-45]C)"
    Cells(79, basecol + 1).Select
        ActiveCell.FormulaR1C1 = "=IF((((R[-69]C/DAYS_DONE)*DAYS_LEFT)-R[-45]C)<=1,0,((R[-69]C/DAYS_DONE)*DAYS_LEFT)-R[-45]C)"
    Cells(79, basecol + 2).Select
        ActiveCell.FormulaR1C1 = "=0"
    Range(Cells(79, basecol), Cells(79, basecol + 2)).Select
        Selection.Copy
        
    Cells(80, basecol).Select 'Range("W80").Select
        ActiveSheet.Paste
    Range(Cells(82, basecol), Cells(83, basecol + 2)).Select 'Range("W82:Y83").Select
        ActiveSheet.Paste
    Cells(85, basecol).Select 'Range("W85").Select
        ActiveSheet.Paste
    Cells(87, basecol).Select 'Range("W87").Select
        ActiveSheet.Paste
    Cells(89, basecol).Select 'Range("W89").Select
        ActiveSheet.Paste
    Cells(92, basecol).Select 'Range("W89").Select
        ActiveSheet.Paste
    Cells(93, basecol).Select 'Range("W93").Select
        ActiveSheet.Paste
    Range(Cells(95, basecol), Cells(96, basecol + 2)).Select 'Range("W93:Y96").Select
        ActiveSheet.Paste
    Cells(98, basecol).Select 'Range("W98").Select
        ActiveSheet.Paste
    Cells(100, basecol).Select 'Range("W100").Select
        ActiveSheet.Paste
    
    Range(Cells(78, basecol), Cells(109, basecol + 2)).Select 'Range("W78:Y109").Select
        Selection.Copy

    For C = 297 To 2487 Step 219
        Cells(C, basecol).Select
        ActiveSheet.Paste
    Next C

    ActiveSheet.Calculate

    For C = 78 To 2487 Step 219
        Range(Cells(C, (basecol)), Cells((C + 31), (basecol + 2))).Select
            Selection.Copy
            Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
                :=False, Transpose:=False

    Next C
    
Cells(4, basecol).Select

Next A

Sheet17.Calculate

Sheet13.Select

End Sub

