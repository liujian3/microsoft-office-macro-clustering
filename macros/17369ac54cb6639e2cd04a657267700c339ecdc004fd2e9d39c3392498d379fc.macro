Sub Grab_KE30()
Dim A As Long, B As Long, C As Long, xrate As Long
Dim x As Range
Dim DatArray() As Variant
Dim MyBook As String, fname As String, fpath As String

Application.Calculation = xlCalculationManual

MyBook = ThisWorkbook.Name
fname = Range("fname_KE30").Value 'Filename from Data tab
fpath = Range("fpath_KE30").Value 'File path from Data tab

xrate = Range("XRATE").Value

'On Error GoTo ErrHandler
'Open KE30 Download
    Workbooks.OpenText Filename:= _
        fpath & fname, Origin:=437, _
        StartRow:=1, DataType:=xlDelimited, TextQualifier:=xlDoubleQuote, _
        ConsecutiveDelimiter:=False, Tab:=True, Semicolon:=False, Comma:=False _
        , Space:=False, Other:=False, TrailingMinusNumbers:=True

A = Range("A500000").End(xlUp).Row 'Last Row of table

ReDim DatArray(1 To A, 1 To 9)
    B = 1
    For C = 2 To A 'Put items into an array
        DatArray(B, 1) = Range("A" & C).Value 'Invoice Date
        DatArray(B, 2) = Range("B" & C).Value 'Plant
        DatArray(B, 3) = Range("C" & C).Value 'Material Group
            ''Convert other material groups to general category of "Others"
            If DatArray(B, 3) = "100" Or DatArray(B, 3) = "PKGN" Or DatArray(B, 3) = "SVC" Or _
            DatArray(B, 3) = "COM" Or DatArray(B, 3) = "MISC" Or DatArray(B, 3) = "RM" Then
                DatArray(B, 3) = "OTHERS"
            End If
        DatArray(B, 4) = Range("F" & C).Value 'Customer #
        DatArray(B, 5) = Range("I" & C).Value 'Sales
        ''Convert currencies to match forecasting currency in shipping plant
            If Range("H" & C).Value = "USD" And DatArray(B, 2) = "CA77" Then
                DatArray(B, 5) = DatArray(B, 5) * xrate
            ElseIf Range("H" & C).Value = "CAD" And Left(DatArray(B, 2), 2) = "US" Then
                DatArray(B, 5) = DatArray(B, 5) / xrate
            End If
        DatArray(B, 6) = Range("J" & C).Value 'Margin
        ''Convert currencies to match forecasting currency in shipping plant
            If Range("H" & C).Value = "USD" And DatArray(B, 2) = "CA77" Then
                DatArray(B, 6) = DatArray(B, 6) * xrate
            ElseIf Range("H" & C).Value = "CAD" And Left(DatArray(B, 2), 2) = "US" Then
                DatArray(B, 6) = DatArray(B, 6) / xrate
            End If
        ''Convert weight to metric tons
            If Range("L" & C).Value = "LB" Then
                DatArray(B, 7) = Range("K" & C).Value / 2204.62 'Tons
            ElseIf Range("L" & C).Value = "KG" Then
                DatArray(B, 7) = Range("K" & C).Value / 1000 'Tons
            End If
        DatArray(B, 8) = Year(DatArray(B, 1)) 'Year
        DatArray(B, 9) = MonthName(Month(DatArray(B, 1)), False) 'Month Name
            B = B + 1
    Next C

