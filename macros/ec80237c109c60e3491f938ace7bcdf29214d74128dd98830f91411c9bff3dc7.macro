Sub procSetzteKopfFusszeilen()
    ' Routine zum Setzen der Kopf- und Fu|fffd|zeilen
    ' Wird aufgerufen beim |fffd|ffnen der Datei, beim Wechseln der Sprache sowie vor dem Drucken
    ' Vor dem Druck Titel der Seiten erneuern
    
    Dim Sh As Worksheet

    For Each Sh In ThisWorkbook.Worksheets
      Sh.PageSetup.LeftHeader = Sheets("Translator").Range("B3").Value
    Next Sh
    
    For Each Sh In ThisWorkbook.Worksheets
      Sh.PageSetup.CenterHeader = Sheets("Translator").Range("B4").Value
    Next Sh
    
    For Each Sh In ThisWorkbook.Worksheets
      Sh.PageSetup.RightHeader = Sheets("Translator").Range("B5").Value
    Next Sh
    
   

End Sub
