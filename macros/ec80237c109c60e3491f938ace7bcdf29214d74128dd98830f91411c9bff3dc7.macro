Sub Workbook_Open()
  On Error GoTo ErrorHandler    ' Fehlerbehandlung aktivieren
    ' Translator auf jeden Fall ausblenden
 '  Call Worksheets("Cover_Sheet").BlattSchutzAufheben
    Worksheets("Translator").Visible = 2
    Sheets("Translator").Range("C2:J1663").Locked = True
    ' Beim 1. |fffd|ffnen der Exceldatei 1 = Bitte aus|fffd|hlen
    Application.ScreenUpdating = False
        ' Blattschutz aufheben
    Sheets("How_to_fill_out").Unprotect "N1!"
    Worksheets("How_to_fill_out").Cells(3, 1).Interior.Color = 16777215
    Sheets("How_to_fill_out").Protect "N1!", AllowFiltering:=True
    If Worksheets("Cover_Sheet").Range("D7").Value = Worksheets("Cover_Sheet").Range("K1") Then
        Sheets("Cover_Sheet").Visible = True
        Sheets("How_to_fill_out").Visible = True
        Sheets("FAQ").Visible = True
    End If
    ' Mache piFlagSprache auf 1 damit KopfFusszeilen beim Start nicht gesetzt werden
    piFlagSprache = 1
    If piFlagSprache = 0 Then
        
        piFlagSprache = 1
        procSetzteKopfFusszeilen
    End If
    piFlagSprache = 0
    On Error Resume Next
    Call Sheets("Cover_Sheet").BlattSchutzSetzen
    piFlagSprache = 0
    Application.ScreenUpdating = True
    Exit Sub        ' Vor Fehlerbehandlung beenden.
ErrorHandler:       ' Fehlerbehandlungsroutine.
        Application.ScreenUpdating = True
'        MsgBox (vbOKOnly)
        MsgBox Err.Description, vbCritical
End Sub

