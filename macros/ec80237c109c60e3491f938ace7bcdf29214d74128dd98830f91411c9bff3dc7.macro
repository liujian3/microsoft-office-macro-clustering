      Sh.PageSetup.LeftHeader = Sheets("Translator").Range("B3").Value
      Sh.PageSetup.CenterHeader = Sheets("Translator").Range("B4").Value
      Sh.PageSetup.RightHeader = Sheets("Translator").Range("B5").Value
    

End Sub

Sub ChangeLanguageAuswahlMaterial()
    piFlagAuswahlMaterial = -1
    Application.ScreenUpdating = False
    On Error Resume Next
    Dim swert As String
    swert = Worksheets("Cover_Sheet").Cells(7, 4)
    Dim i As Integer
    Dim iflag As Integer
    Dim iC As Integer
    iflag = 0
    Dim s1, s2 As Range
    Dim irow As Integer
    For i = 1 To 6
        If Worksheets("Cover_Sheet").Cells(i, 11) = swert Then
            iflag = 1
        End If
    Next
    If iflag = 0 Then
        
        For i = 1 To 6
            s1 = Worksheets("Cover_Sheet").Cells(i, 11).Formula
            Set s2 = Range(s1)
            irow = s2.Row
            For iC = 2 To 12
                If Worksheets("Translator").Cells(irow, iC) = swert Then
                    Worksheets("Cover_Sheet").Cells(7, 4) = Worksheets("Translator").Cells(irow, 2)
                    piFlagAuswahlMaterial = 0
                    Exit Sub
                End If
            Next
        Next
    End If
    piFlagAuswahlMaterial = 0
 Application.ScreenUpdating = True
End Sub
