Sub CalcGesamtGewichtBattery()
    Application.ScreenUpdating = False
        
        ' Blattschutz aufheben
        Sheets("Cover_Sheet").Unprotect "N1!"
    Dim iAnz As Long
    Dim dGes As Double
    Dim ws As Worksheet
    Set ws = Worksheets("8_Battery directive")
    Dim irow As Integer
    For irow = 9 To 18
        
        If ws.Cells(irow, 5) <> "" Then
            If ws.Cells(irow, 1) <> "" Then
                dGes = dGes + (CLng(ws.Cells(irow, 1)) * CDbl(ws.Cells(irow, 5)))
            Else
                dGes = dGes + CDbl(ws.Cells(irow, 5))
            End If
        End If
    Next
    ws.Cells(7, 5) = dGes
    ' Blattschutz setzen
     Sheets("Cover_Sheet").Protect "N1!", AllowFiltering:=True
    Application.ScreenUpdating = True
    
End Sub
