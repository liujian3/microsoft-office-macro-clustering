Sub procForActivate()
    On Error GoTo errActivate
    ' Kein Bildschirmupdate
    Application.ScreenUpdating = False
    Sheets("GB").Unprotect "N1!"
    ' Call Worksheets("Cover_Sheet").BlattSchutzAufheben
    ' Wenn in CoverSheet Material Unbekannt
    If Worksheets("Cover_Sheet").Range("D7").Value = Worksheets("Cover_Sheet").Range("K6") Then
        Worksheets("GB").ListBox1.ListFillRange = Me.Range("$I$2:$I$20").Address
        ListBox1.Selected(18) = True
    Else
        Worksheets("GB").ListBox1.ListFillRange = Me.Range("$H$2:$H$19").Address
        ListBox1.Selected(17) = True
    End If
    SetListboxGBSelectDimensions
    If Sheets("GB").Visible = False And Sheets("Materialart").Visible = False Then
    
        
        For i = 1 To Worksheets.Count
            If i <> 19 Then 'Hier geht es um GB
                Sheets(i).Visible = False
            End If
        Next i
        If Sheets("How_to_fill_out").Cells(3, 1).Interior.Color = 49407 Then
            Sheets("Translator").Visible = True
            Sheets("Translator").Range("C2:J1663").Locked = False
        Else
            Sheets("Translator").Visible = 2
            Sheets("Translator").Range("C2:J1663").Locked = True
        End If
    End If
    Sheets("GB").Protect "N1!", AllowFiltering:=True
    ' Bildschirmupdate einschalten
    Application.ScreenUpdating = True
    Exit Sub
errActivate:
    Sheets("GB").Protect "N1!", AllowFiltering:=True
    Application.ScreenUpdating = True

End Sub
