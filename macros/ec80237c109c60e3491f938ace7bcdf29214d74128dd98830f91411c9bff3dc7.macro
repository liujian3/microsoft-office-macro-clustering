Sub TestFillCover_Sheet(ByRef filled As Boolean)

    Dim i As Integer

    filled = True
    
    i = 2
    If Rows(i).Hidden = False Then
       If Trim(Worksheets("Cover_Sheet").Cells(i, 4).Value) = "" Then
           filled = False
       End If
    End If
    i = 8
    If Rows(i).Hidden = False Then
       If Trim(Worksheets("Cover_Sheet").Cells(i, 4).Value) = "" Then
           filled = False
       End If
    End If
    i = 10
    If Rows(i).Hidden = False Then
       If Trim(Worksheets("Cover_Sheet").Cells(i, 4).Value) = "" Then
           filled = False
       End If
    End If
    i = 12
    If Rows(i).Hidden = False Then
       If Trim(Worksheets("Cover_Sheet").Cells(i, 4).Value) = "" Then
           filled = False
       End If
    End If

    For i = 15 To 34
        If i <> 24 And i <> 20 Then
            If Rows(i).Hidden = False Then
                If Trim(Worksheets("Cover_Sheet").Cells(i, 4).Value) = "" Then
                    filled = False
                End If
                If Trim(Worksheets("Cover_Sheet").Cells(i, 4).Value) = Worksheets("Translator").Range("B663").Value Then
                    Worksheets("Cover_Sheet").Cells(i, 5).Value = Worksheets("Translator").Range("B28").Value
                End If
                If i <> 25 And i <> 27 Then
                    If Trim(Worksheets("Cover_Sheet").Cells(i, 5).Value) = "" Then
                        filled = False
                    End If
                End If
            End If
        End If
    Next i

End Sub

