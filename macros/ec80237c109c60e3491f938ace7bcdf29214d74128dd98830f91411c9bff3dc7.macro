Sub ZeilenAusblendenReach(ZeilenAusblendenSpalte)

    For i = 0 To 60
        If Sheets("Zeilen").Cells(i + 4, ZeilenAusblendenSpalte).Value = 1 Then
            ArrayReach(i, 0) = ArrayReach(i, 0) + 1
        End If
    Next i
    
    For i = 0 To 161
     '   If i = 154 And ZeilenAusblendenSpalte = 3 Then Stop
        If Sheets("Zeilen").Cells(i + 161, ZeilenAusblendenSpalte).Value = 1 Then
            ArrayREACHSVHC(i, 0) = ArrayREACHSVHC(i, 0) + 1
        End If
    Next i
       
    For i = 0 To 12
        If Sheets("Zeilen").Cells(i + 102, ZeilenAusblendenSpalte).Value = 1 Then
            ArrayDiverses(i, 0) = ArrayDiverses(i, 0) + 1
        End If
    Next i

End Sub

