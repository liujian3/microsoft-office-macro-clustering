Public Sub changeComboValueLanguage(sBereich As String)
    Dim ws1 As Worksheet
    Dim wsTrans As Worksheet
    Dim sQuelle As String
    Dim sRange  As String
    Dim sZiel As String
    Dim r2 As Range
    Dim r1 As Range
    Dim irow As Integer
    If UCase(sBereich) = UCase("ConflictMineralsEnthaltenIn") Then
        Set ws1 = Worksheets("12_CONFLICT_MINERALS")
        Set wsTrans = Worksheets("Translator")
        sRange = "C1254:J1256"
        sQuelle = ws1.Cells(4, 3)
        If sQuelle <> "" Then
            sZiel = sQuelle
            Set r1 = wsTrans.Range(sRange)
            For Each r2 In r1
                If r2.Value = sQuelle Then
                    sZiel = wsTrans.Cells(r2.Row, 2)
                    Exit For
                End If
            Next
            If sZiel <> sQuelle Then
                ws1.Cells(4, 3) = sZiel
            End If
        End If
        sQuelle = ws1.Cells(5, 3)
        If sQuelle <> "" Then
            sZiel = sQuelle
            Set r1 = wsTrans.Range(sRange)
            For Each r2 In r1
                If r2.Value = sQuelle Then
                    sZiel = wsTrans.Cells(r2.Row, 2)
                    Exit For
                End If
            Next
            If sZiel <> sQuelle Then
                ws1.Cells(5, 3) = sZiel
            End If
        End If
        sQuelle = ws1.Cells(6, 3)
        If sQuelle <> "" Then
            sZiel = sQuelle
            Set r1 = wsTrans.Range(sRange)
            For Each r2 In r1
                If r2.Value = sQuelle Then
                    sZiel = wsTrans.Cells(r2.Row, 2)
                    Exit For
                End If
            Next
            If sZiel <> sQuelle Then
                ws1.Cells(6, 3) = sZiel
            End If
