    TestFilled = False
    If Worksheets("Cover_Sheet").Range("D7").Value <> Worksheets("Cover_Sheet").Range("K1") And ActiveSheet.Name = "Cover_Sheet" Then
'    If Worksheets("Cover_Sheet").ComboBoxGBSelect.ListIndex > 0 And ActiveSheet.Name = "Cover_Sheet" Then
        Call TestFillCover_Sheet(TestFilled)
        If TestFilled = False Then
            a = MsgBox(Worksheets("Translator").Range("B1075").Value & " " & Worksheets("Translator").Range("B1076").Value, vbYesNo)
