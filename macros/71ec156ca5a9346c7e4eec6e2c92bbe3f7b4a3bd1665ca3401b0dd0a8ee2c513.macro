Attribute VB_Name = "ArenaMod"
'SETUP ARENA - CALLS ARENASTART, SPAWNPLAYER, SELECTENEMY, MAKES THE ATTACK AND DEFEND BUTTON VISIBLE
'ARENA START -  CLEARS ARENA, PPUTS GREY BG, COPY PASTES ARENA ART
'ARENA CLEAR - CLEARS THE AREA
'SPAWN PLAYER - COPY PASTES SPRITE, DISPLAYS HEALTH, ATTACK AND DEFENCE
'SPAWN ENEMY - CHOSES WHICH ENEMY TO SPAWN
'SELECT ENEMY - LOGIC FOR WHICH ENEMY TO SPAWN - UPDATES THE NUMBER OF ENEMY NUMBER, TYPE AND VALUE ON GAMEDATA SHEET
'ENEMY 1 ART - ART FOR ENEMY 1 + STAGES GAMEDATA VALUES FOR FIGHT (FROM TABLE TO TEMP LOCATION)
'ENEMY 2 ART - ART FOR ENEMY 2 + STAGES GAMEDATA VALUES FOR FIGHT (FROM TABLE TO TEMP LOCATION)
'ENEMY 3 ART - ART FOR ENEMY 3 + STAGES GAMEDATA VALUES FOR FIGHT (FROM TABLE TO TEMP LOCATION)
'ENEMY 4 ART - ART FOR ENEMY 4 + STAGES GAMEDATA VALUES FOR FIGHT (FROM TABLE TO TEMP LOCATION)
'ENEMY 5 ART - ART FOR ENEMY 5 + STAGES GAMEDATA VALUES FOR FIGHT (FROM TABLE TO TEMP LOCATION)

Sub setupArena()

Worksheets("Arena").Activate
'setup
    Call arenaStart
'player setup
    Call spawnPlayer
'setup enemy test - ideally should call spawn enemy
    Call selectEnemy
    
Worksheets("Arena").Shapes("Oval 1").Visible = True 'attack button
Worksheets("Arena").Shapes("Oval 2").Visible = True 'defend button

'SET TURN COUNTER BACK TO 1 - THE THING AI USES FOR PATTERNS
Worksheets("GameData").Range("K2").Value = 1


Worksheets("Game").Shapes("Rectangle 3").Visible = False 'disable quest selection
Worksheets("Game").Shapes("Rectangle 5").Visible = False
Worksheets("Game").Shapes("Rectangle 8").Visible = False

'setting up element related values

Call elementalEffect

Call displayEnemyElement
Call displayPlayerElement

Call matchStartVictorySound



End Sub

Sub arenaStart()
    
    Dim clr_bgGrey As Long
    clr_bgGrey = RGB(40, 40, 40)
    
With Sheets("Arena")
    .Cells.Clear
    .Cells.ColumnWidth = 0.9
    .Cells.RowHeight = 9
    .Cells.Interior.Color = clr_bgGrey
End With

    Sheets("GameSprites").Select
    Range("dd1:gt45").Copy
    Sheets("Arena").Select
    Range("A1").Select
    ActiveSheet.Paste
    Range("A1").Select

End Sub

Sub arenaClear()
    
    Dim clr_bgGrey As Long
    clr_bgGrey = RGB(40, 40, 40)
    
With Sheets("Arena")
    .Cells.Clear
    .Cells.ColumnWidth = 0.9
    .Cells.RowHeight = 9
    .Cells.Interior.Color = clr_bgGrey
End With


End Sub

