Attribute VB_Name = "startGame"
Sub startGame()

Worksheets("Game").Activate
    
'setup
    Call cleanSheet
'setup newspaper format
    Call setupNewspaper

'choose left panel art
Dim randNo As Integer
randNo = Application.WorksheetFunction.RandBetween(0, 1)
If randNo = 0 Then
    Call leftpanel_rocket
Else
    Call leftpanel_Skull
End If

'choose right panel art
randNo = Application.WorksheetFunction.RandBetween(0, 2)
If randNo = 0 Then
    Call centerpanel_robber
ElseIf randNo = 1 Then
    Call centerpanel_ogre
Else
    Call centerpanel_mage
End If

'choose right panel art
randNo = Application.WorksheetFunction.RandBetween(0, 1)
If randNo = 0 Then
    Call rightpanel_bomb
Else
    Call rightpanel_joker
End If

'left quest
Call setQuestNarrativeLeft
'center quest
Call setQuestNarrativeCenter
'right quest
Call setQuestNarrativeRight

'set values
Range("CM4").Value = Worksheets("GameData").Range("B2").Value
Range("CM17").Value = Worksheets("GameData").Range("C2").Value
Range("CM30").Value = Worksheets("GameData").Range("D2").Value
'Range("CN30").Value = Worksheets("GameData").Range("B2").Value
'Range("CN36").Value = Worksheets("GameData").Range("B2").Value


Worksheets("Game").Shapes("Rectangle 3").Visible = True
Worksheets("Game").Shapes("Rectangle 5").Visible = True
Worksheets("Game").Shapes("Rectangle 8").Visible = True

Call checkGameOver



'Sheets("Game").Protect Password = "tanu"
End Sub

