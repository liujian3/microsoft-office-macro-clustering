Sub selectEnemy()

Dim turns As Integer
    turns = Worksheets("GameData").Range("a2").Value
Dim enemy_type As Integer
    enemy_type = 0
Dim randNo As Integer
    randNo = 0
Dim randNo2 As Integer
    randNo2 = 0
Dim enemyNumber As Integer
    enemyNumber = 1
Dim difficulty As Integer
    difficulty = 1

If turns = 1 Then
    enemy_type = 1
    difficulty = 1
    
ElseIf turns = 2 Then
    enemy_type = 2
    difficulty = 1
    
ElseIf turns = 3 Then
    enemy_type = 3
    difficulty = 1
    
ElseIf turns >= 4 And turns < 6 Then
    randNo = Application.WorksheetFunction.RandBetween(1, 3)
    If randNo = 1 Then
        enemy_type = 1
        ElseIf randNo = 2 Then
        enemy_type = 2
        Else
        enemy_type = 3
    End If
    difficulty = 1
    
ElseIf turns = 6 Then
    enemy_type = 4
    difficulty = 2
    
    
ElseIf turns >= 7 And turns < 11 Then
    randNo = Application.WorksheetFunction.RandBetween(1, 4)
    If randNo = 1 Then
        enemy_type = 1
        ElseIf randNo = 2 Then
        enemy_type = 2
        ElseIf randNo = 3 Then
        enemy_type = 3
        Else
        enemy_type = 4
    End If
    
    randNo = Application.WorksheetFunction.RandBetween(0, 100)
    If (randNo < Worksheets("GameData").Range("c41").Value) Then
            difficulty = 1
        Else
            difficulty = 2
            enemy_type = 4
    End If
    

    
ElseIf turns = 11 Then
    enemy_type = 5
    difficulty = 3
    
ElseIf turns >= 12 And turns < 15 Then
     
    randNo = Application.WorksheetFunction.RandBetween(0, 100)
    If (randNo < Worksheets("GameData").Range("d41").Value) Then
            difficulty = 1
            randNo2 = Application.WorksheetFunction.RandBetween(1, 3)
            If randNo2 = 1 Then
            enemy_type = 1
            ElseIf randNo2 = 2 Then
            enemy_type = 2
            Else
            enemy_type = 3
            End If
    ElseIf (randNo < Worksheets("GameData").Range("h42").Value) Then
            difficulty = 2
            enemy_type = 4
    Else
            difficulty = 3
            enemy_type = 5
    End If

ElseIf turns = 15 Then
    randNo = Application.WorksheetFunction.RandBetween(1, 3)
    If randNo = 1 Then
        enemy_type = 1
        ElseIf randNo = 2 Then
        enemy_type = 2
        ElseIf randNo = 3 Then
        enemy_type = 3
    End If
      
    difficulty = 4
    enemyNumber = 2
    
ElseIf turns > 15 And turns < 18 Then

      
    randNo = Application.WorksheetFunction.RandBetween(0, 100)
    If (randNo < Worksheets("GameData").Range("e41").Value) Then
            difficulty = 1
            randNo2 = Application.WorksheetFunction.RandBetween(1, 3)
            If randNo2 = 1 Then
            enemy_type = 1
            ElseIf randNo2 = 2 Then
            enemy_type = 2
            Else
            enemy_type = 3
            End If
    ElseIf (randNo < Worksheets("GameData").Range("i42").Value) Then
            difficulty = 2
            enemy_type = 4
    ElseIf (randNo < Worksheets("GameData").Range("i43").Value) Then
            difficulty = 3
            enemy_type = 5
    Else
            difficulty = 4
            randNo2 = Application.WorksheetFunction.RandBetween(1, 3)
            If randNo2 = 1 Then
            enemy_type = 1
            ElseIf randNo2 = 2 Then
            enemy_type = 2
            Else
            enemy_type = 3
            End If
            enemyNumber = 2
    End If
    
ElseIf turns = 18 Then
    randNo = Application.WorksheetFunction.RandBetween(4, 5)
    If randNo = 4 Then
        enemy_type = 4
        Else
        enemy_type = 5
    End If
      
    difficulty = 5
    enemyNumber = 2
    
ElseIf turns > 18 Then

    randNo = Application.WorksheetFunction.RandBetween(0, 100)
    If (randNo < Worksheets("GameData").Range("e41").Value) Then
            difficulty = 1
            randNo2 = Application.WorksheetFunction.RandBetween(1, 3)
            If randNo2 = 1 Then
            enemy_type = 1
            ElseIf randNo2 = 2 Then
            enemy_type = 2
            Else
            enemy_type = 3
            End If
        ElseIf (randNo < Worksheets("GameData").Range("j42").Value) Then
            difficulty = 2
            enemy_type = 4
        ElseIf (randNo < Worksheets("GameData").Range("j43").Value) Then
            difficulty = 3
            enemy_type = 5
        ElseIf (randNo < Worksheets("GameData").Range("j44").Value) Then
            difficulty = 4
            randNo2 = Application.WorksheetFunction.RandBetween(1, 3)
            If randNo2 = 1 Then
            enemy_type = 1
            ElseIf randNo2 = 2 Then
            enemy_type = 2
            Else
            enemy_type = 3
            End If
            enemyNumber = 2
        Else
            difficulty = 5
            randNo2 = Application.WorksheetFunction.RandBetween(4, 5)
            If randNo2 = 4 Then
            enemy_type = 4
            Else
            enemy_type = 5
            End If
            enemyNumber = 2
    End If

End If
Worksheets("GameData").Range("h2").Value = enemy_type
Worksheets("GameData").Range("i2").Value = enemyNumber
Worksheets("GameData").Range("j2").Value = difficulty

Call spawnEnemy

End Sub


