Sub checkGameOver()

Dim gameIsOver As Boolean
    gameIsOver = False

' check if game is oveer
If (Worksheets("GameData").Range("B2").Value <= 0) Then
    Call gameover
    MsgBox "Justice is too low! You failed to save the city. Game over. Start a new game from main menu to play again"
'    MsgBox "Thank you for playing! Start a new game from main menu to play again"
    gameIsOver = True
    
ElseIf (Worksheets("GameData").Range("C2").Value >= 10) Then
    Call gameover
    MsgBox "Heat is too high! Police arrested you for obstruction. Game over. Start a new game from main menu to play again"
'    MsgBox "Thank you for playing! Start a new game from main menu to play again"
    gameIsOver = True
     
ElseIf (Worksheets("GameData").Range("D2").Value >= 10) Then
    Call gameover
    MsgBox "Power is too high! You injured yourself and cannot fight crime anymore. Game over. Start a new game from main menu to play again"
'    MsgBox "Thank you for playing! Start a new game from main menu to play again"
    gameIsOver = True
 
ElseIf (Worksheets("GameData").Range("D2").Value <= 0) Then
    Call gameover
    MsgBox "Power is too low! You injured yourself and cannot fight crime anymore. Game over. Start a new game from main menu to play again"
'    MsgBox "Thank you for playing! Start a new game from main menu to play again"
    gameIsOver = True
 
End If

If (gameIsOver = True) Then
    'disable buttons
    Worksheets("Game").Shapes("Rectangle 3").Visible = False
    Worksheets("Game").Shapes("Rectangle 5").Visible = False
    Worksheets("Game").Shapes("Rectangle 8").Visible = False
