Sub reducePlayerHealth()

'check armor
If Worksheets("GameData").Range("F2").Value > 0 Then
    'yes armor
    'take damage - update game data and UI
    Worksheets("GameData").Range("F2").Value = Worksheets("GameData").Range("F2").Value - Worksheets("GameData").Range("M2").Value
    'check if armor broke and damage has to be applied to health also
    If Worksheets("GameData").Range("F2").Value < 0 Then
        'find how much extra damage is there
        Worksheets("GameData").Range("F2").Value = Worksheets("GameData").Range("F2").Value * -1
        'subtract that from health
        Worksheets("GameData").Range("G2").Value = Worksheets("GameData").Range("G2").Value - Worksheets("GameData").Range("F2").Value
        'turn armor to zero
        Worksheets("GameData").Range("F2").Value = "0"
    End If
Else
    'no armor - subtract player power from enemy health
    Worksheets("GameData").Range("G2").Value = Worksheets("GameData").Range("G2").Value - Worksheets("GameData").Range("M2").Value
End If

    'update arena UI - armor and health
    Worksheets("Arena").Range("Y11").Value = Worksheets("GameData").Range("F2").Value
    Worksheets("Arena").Range("S5").Value = Worksheets("GameData").Range("G2").Value

End Sub


Sub reduceEnemyHealth()

'check armor
If Worksheets("GameData").Range("N2").Value > 0 Then
    'yes armor
    'take damage - update game data and UI
    Worksheets("GameData").Range("N2").Value = Worksheets("GameData").Range("N2").Value - Worksheets("GameData").Range("D2").Value
    'check if armor broke and damage has to be applied to health also
    If Worksheets("GameData").Range("N2").Value < 0 Then
        'find how much extra damage is there
        Worksheets("GameData").Range("N2").Value = Worksheets("GameData").Range("N2").Value * -1
        'subtract that from health
        Worksheets("GameData").Range("L2").Value = Worksheets("GameData").Range("L2").Value - Worksheets("GameData").Range("N2").Value
        'turn armor to zero
        Worksheets("GameData").Range("N2").Value = "0"
    End If
Else
    'no armor - subtract player power from enemy health
    Worksheets("GameData").Range("L2").Value = Worksheets("GameData").Range("L2").Value - Worksheets("GameData").Range("D2").Value
End If

    'update arena UI - armor and health
    Worksheets("Arena").Range("BQ11").Value = Worksheets("GameData").Range("N2").Value
    Worksheets("Arena").Range("BK5").Value = Worksheets("GameData").Range("L2").Value

End Sub

