'is enemy dead?
If Worksheets("GameData").Range("L2").Value <= "0" Then
    Call enemyDead
    'sound
    Call whichEnemyDeathSoundToMake
    MsgBox "Enemy Dead!"
    
    ' check if there is another enemy
    If (Worksheets("GameData").Range("I2").Value > 1) Then
    
        MsgBox "Wait! another enemy appeared"
        Worksheets("GameData").Range("I2").Value = 1
            'SET HEALTH
        Worksheets("Arena").Range("BK5").Value = Worksheets("GameData").Range("D13").Value
