Sub Saisonwechsel()

Dim Laenge As Integer, Von As Integer, Bis As Integer
Dim Zelle As Range
Dim Liga As String

Dim y As Integer, i As Integer, j As Integer, a As Integer, b As Integer, x As Integer, z As Integer, c As Integer
Dim Daten_Array() As String

Call aendern

'----------------------------------------------------------------------------------
'Saison
'Tabelle: Spielplan
'Zelle: E2

Set Zelle = Sheets("Spielplan").Range("E2")

Liga = Left(Zelle, Len(Zelle) - 11)
Von = Left(Right(Zelle, 11), 4) + 1
Bis = Right(Zelle, 4) + 1

Sheets("Spielplan").Range("E2").Value = Liga & Von & " - " & Bis

'----------------------------------------------------------------------------------
'-Liga+Saison
'Tabelle: Spielplan
'Zelle: AS25

Set Zelle = Sheets("Spielplan").Range("AS25")

Liga = Left(Zelle, Len(Zelle) - 5)
Von = Left(Right(Zelle, 5), 2) + 1
Bis = Right(Zelle, 2) + 1

Sheets("Spielplan").Range("AS25").Value = Liga & Von & "-" & Bis
'----------------------------------------------------------------------------------
'Vereine
'Tabelle: Kreuztabelle
'Zellen: B4:B21 (18) oder B4:B23 (20)

If Sheets("Hinweise").Range("N38") = "38." Then
    y = 10
    x = y * 2
    b = 10
    z = 2
    c = 38
Else
    y = 9
    x = y * 2
    b = 9
    z = 3
    c = 34
End If

ReDim Daten_Array(1 To x)

For i = 1 To y
    Daten_Array(i) = Sheets("Spielplan").Range("E" & i + 37).Value
    Daten_Array(b + i) = Sheets("Spielplan").Range("G" & i + 37).Value
Next i

SelectionSort Daten_Array

For i = 1 To x
    Sheets("Kreuztabelle").Range("B" & i + 3).Value = Daten_Array(i)
    Sheets("Fieberkurve").Range("A" & i + 3).Value = Daten_Array(i)
    If y = 9 Then
        Sheets("Fieberkurve").Range("AP" & i + 2).Value = Daten_Array(i)
    ElseIf y = 10 Then
        Sheets("Fieberkurve").Range("AQ" & i + 1).Value = Daten_Array(i)
    End If
        
Next i

Sheets("Fieberkurve").Select
'ActiveSheet.Unprotect Password:=Passwort
Columns("AP:AQ").EntireColumn.Hidden = False

If y = 9 Then
    Sheets("Fieberkurve").Range("AP3:AP20").Copy
    x = 24
ElseIf y = 10 Then
    Sheets("Fieberkurve").Range("AQ2:AQ21").Copy
    x = 23
End If

For i = 2 To c
    If y = 9 Then
        Sheets("Fieberkurve").Range("AP" & x).Select
        ActiveSheet.Paste
'        x = x + 21
    ElseIf y = 10 Then
        Sheets("Fieberkurve").Range("AQ" & x).Select
        ActiveSheet.Paste
    End If
    x = x + 21
Next i

Columns("AP:AQ").EntireColumn.Hidden = True
Range("A1").Select
'ActiveSheet.Protect Password:=Passwort
'----------------------------------------------------------------------------------
'Stand

Sheets("Hinweise").Range("I4").Value = Format(Now, "DD.MM.YYYY")
Sheets("Hinweise").Range("M34").Value = 1
Sheets("Hinweise").Range("M35").Value = 10
'----------------------------------------------------------------------------------

End Sub

Function SelectionSort(TempArray As Variant)
          Dim MaxVal As Variant
          Dim MaxIndex As Integer
          Dim i, j As Integer

          ' Step through the elements in the array starting with the
          ' last element in the array.
          For i = UBound(TempArray) To 1 Step -1

              ' Set MaxVal to the element in the array and save the
              ' index of this element as MaxIndex.
              MaxVal = TempArray(i)
              MaxIndex = i

              ' Loop through the remaining elements to see if any is
              ' larger than MaxVal. If it is then set this element
              ' to be the new MaxVal.
              For j = 1 To i
                  If TempArray(j) > MaxVal Then
                      MaxVal = TempArray(j)
                      MaxIndex = j
                  End If
              Next j

              ' If the index of the largest element is not i, then
              ' exchange this element with element i.
              If MaxIndex < i Then
                  TempArray(MaxIndex) = TempArray(i)
                  TempArray(i) = MaxVal
              End If
          Next i

End Function


