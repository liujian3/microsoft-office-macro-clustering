Private Sub cmdStart_Click()
Dim A, B, C As Single
Dim D As String
A = Val(txt1.Text)
B = Val(txt2.Text)
D = txt4.Text
Select Case D
  Case Is = "+"
     C = A + B
     txt3.Text = Str(C)
  Case Is = "*"
     C = A * B
     txt3.Text = Str(C)
  Case Is = "-"
     C = A - B
     txt3.Text = Str(C)
     Case Is = "/"
     C = A / B
     txt3.Text = Str(C)
     Case Is = "^"
     C = A ^ B
     txt3.Text = Str(C)
     Case Is = "cos"
     A = Val(txt1.Text)
     C = Cos(A * 2 * 3.14 / 360)
     txt3.Text = (Str(C))
     Case Is = "sin"
     A = Val(txt1.Text)
     C = Sin(A * 2 * 3.14 / 360)
     txt3.Text = Str(C)
     Case Is = "tg"
     A = Val(txt1.Text)
     C = Tan(A * 2 * 3.14 / 360)
     txt3.Text = Str(C)
     Case Is = "ctg"
     A = Val(txt1.Text)
     C = 1 / Tan(A * 2 * 3.14 / 360)
     txt3.Text = Str(C)
     Case Is = "sqr"
     C = Sqr(A)
     txt3.Text = Str(C)
     Case Is = "mod"
     C = A Mod B
     txt3.Text = Str(C)
     Case Is = "log"
     C = Log(A) / Log(B)
     txt3.Text = Str(C)
     
  Case Else
     txt3.Text = "|fffd||fffd||fffd||fffd||fffd||fffd|"
End Select
End Sub

