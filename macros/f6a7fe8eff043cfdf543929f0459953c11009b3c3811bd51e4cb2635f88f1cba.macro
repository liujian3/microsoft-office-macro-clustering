'Callback for customUI.onLoad
Sub ribbonLoaded(ribbon As IRibbonUI)
    Set rib = ribbon
    
    'Show the Tab
    rib.Invalidate
    
    rib.ActivateTab ("TabSSSKKS108")
End Sub

Sub UpdateRibbon()
    rib.InvalidateControl ("TabSSSKKS108")
    rib.Invalidate
End Sub
