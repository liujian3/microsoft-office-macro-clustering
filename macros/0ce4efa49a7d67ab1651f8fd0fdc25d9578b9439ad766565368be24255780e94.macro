Private Sub Worksheet_Activate()
Dim r, c, i As Integer

    Set thissh = ThisWorkbook.Sheets("Jaar")
    menu_row = 23
    act_row = 4
    Call Do_Month(1, 4, 2)      '' Januari
    Call Do_Month(2, 4, 11)     '' Februari
    Call Do_Month(3, 4, 20)     '' Maart
    Call Do_Month(4, 4, 29)     '' April
    Call Do_Month(5, 13, 2)     '' Mei
    Call Do_Month(6, 13, 11)    '' Juni
    Call Do_Month(7, 13, 20)    '' Juli
    Call Do_Month(8, 13, 29)    '' Augustus
    Call Do_Month(9, 22, 2)     '' September
    Call Do_Month(10, 22, 11)   '' Oktober
    Call Do_Month(11, 22, 20)   '' November
    Call Do_Month(12, 22, 29)   '' December
    
End Sub


Private Sub Do_Month(mo As Integer, ro As Integer, co As Integer)
    For r = ro To ro + 5
        For c = co To co + 6
            If thissh.Cells(ro - 1, c).Value = "Zo" Or thissh.Cells(ro - 1, c).Value = "Za" Then
                thissh.Cells(r, c).Interior.Color = RGB(220, 220, 220)
            Else
                thissh.Cells(r, c).Interior.Color = vbWhite
            End If
            If thissh.Cells(r, c).Value <> "" Then
                da = thissh.Cells(r, c) & "-" & CStr(mo) & "-" & "2020"
                While ThisWorkbook.Sheets("Menu").Cells(menu_row, 1).Value < da
                    menu_row = menu_row + 1
                Wend
                If ThisWorkbook.Sheets("Menu").Cells(menu_row, 1).Value = da Then
                    thissh.Cells(r, c).Interior.Color = RGB(180, 180, 180)
                    menu_row = menu_row + 1
                End If
                While ThisWorkbook.Sheets("ContentPlanner").Cells(act_row, 1).Value < da And ThisWorkbook.Sheets("ContentPlanner").Cells(act_row, 1).Value <> Empty
                    act_row = act_row + 1
                Wend
                If ThisWorkbook.Sheets("ContentPlanner").Cells(act_row, 1).Value = da Then
                    If ThisWorkbook.Sheets("ContentPlanner").Cells(act_row, 7).Value <> "" Then
                        thissh.Cells(r, c).Interior.Color = ThisWorkbook.Sheets("ContentPlanner").Cells(act_row, 3).Interior.Color
                    End If
                    act_row = act_row + 1
                End If
            End If
        Next c
    Next r
End Sub
