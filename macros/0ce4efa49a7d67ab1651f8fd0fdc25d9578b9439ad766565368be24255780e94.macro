Private Sub Worksheet_Activate()
Dim i, m As Integer

    If ThisWorkbook.Sheets("ContentPlanner").Cells(1, 79).Value = 0 Or ThisWorkbook.Sheets("ContentPlanner").Cells(1, 79).Value = "" Then
        ThisWorkbook.Sheets("ContentPlanner").Cells(1, 79).Value = 1
    End If
    Call HideRows
End Sub

Private Sub Worksheet_Change(ByVal Target As Range)
Dim i, j As Integer
Dim res As VbMsgBoxResult
Dim arr(9) As String

    If Target.Column = 3 Then
        arr(1) = "=thema_A"
        arr(2) = "=thema_B"
        arr(3) = "=thema_C"
        arr(4) = "=thema_D"
        arr(5) = "=thema_E"
        arr(6) = "=thema_F"
        arr(7) = "=thema_G"
        arr(8) = "=thema_H"
        ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column + 1).Validation.Delete
        ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column + 1).Value = ""
        ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column + 1).Interior.Color = ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, 1).Interior.Color
        If ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column).Value <> "" Then
            For i = 6 To 13
                If Target.Value = ThisWorkbook.Sheets("Menu").Cells(i, 1).Value Then
                    j = i
                    i = 13
                End If
            Next
            If j > 5 Then
                ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column + 1).Validation.Add Type:=xlValidateList, AlertStyle:=xlValidAlertStop, Operator:=xlBetween, Formula1:=arr(j - 5)
                ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column).Interior.Color = ThisWorkbook.Sheets("Menu").Cells(j, 1).Interior.Color
                ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column + 1).Interior.Color = ThisWorkbook.Sheets("Menu").Cells(j, 1).Interior.Color
            Else
                res = MsgBox("Kies een waarde uit de lijst.", vbCritical)
                ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column).Interior.Color = ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, 1).Interior.Color
                ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column + 1).Interior.Color = ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, 1).Interior.Color
                Target.Value = ""
            End If
        Else
            ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column).Interior.Color = ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, 1).Interior.Color
            ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column + 1).Interior.Color = ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, 1).Interior.Color
        End If
    Else
        If Target.Column = 4 Then
            If ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column).Value <> "" Then
                ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column).Interior.Color = ThisWorkbook.Sheets("ContentPlanner").Cells(Target.Row, Target.Column - 1).Interior.Color
            End If
        End If
    End If
End Sub

