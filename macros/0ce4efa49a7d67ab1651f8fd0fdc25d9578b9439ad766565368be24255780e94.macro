Sub HideRows()
Dim m, i, beg, eind As Integer
Dim b, e As Boolean

    MonthSelected = ThisWorkbook.Sheets("ContentPlanner").Cells(1, 79).Value
    i = 4
    b = False
    e = False
    While ThisWorkbook.Sheets("ContentPlanner").Cells(i, 1).Value <> ""
        m = Format(ThisWorkbook.Sheets("ContentPlanner").Cells(i, 1).Value, "m")
        If b = False Then
            If e = False Then
                If m = CStr(MonthSelected) Then
                    beg = i
                    b = True
                End If
            End If
        End If
        If b = True Then
            If e = False Then
                If m = CStr(MonthSelected) Then
                    eind = i
                Else
                    e = True
                End If
            End If
        End If
        
        If ThisWorkbook.Sheets("ContentPlanner").Cells(i, 2).Value = "zaterdag" Or ThisWorkbook.Sheets("ContentPlanner").Cells(i, 2).Value = "zondag" Then
            Range(ThisWorkbook.Sheets("ContentPlanner").Cells(i, 1), ThisWorkbook.Sheets("ContentPlanner").Cells(i, 14)).Interior.Color = RGB(204, 204, 204)
        Else
            Range(ThisWorkbook.Sheets("ContentPlanner").Cells(i, 1), ThisWorkbook.Sheets("ContentPlanner").Cells(i, 14)).Interior.Color = vbWhite
        End If
        If ThisWorkbook.Sheets("ContentPlanner").Cells(i, 3).Value <> "" Then
            j = 6
            For j = 6 To 13
                If ThisWorkbook.Sheets("ContentPlanner").Cells(i, 3).Value = ThisWorkbook.Sheets("Menu").Cells(j, 1).Value Then
                    ThisWorkbook.Sheets("ContentPlanner").Cells(i, 3).Interior.Color = ThisWorkbook.Sheets("Menu").Cells(j, 1).Interior.Color
                    ThisWorkbook.Sheets("ContentPlanner").Cells(i, 4).Interior.Color = ThisWorkbook.Sheets("Menu").Cells(j, 1).Interior.Color
                End If
            Next j
        End If
        i = i + 1
    Wend
    If beg <> 4 Then
        ThisWorkbook.Sheets("ContentPlanner").Rows(CStr(4) & ":" & CStr(beg - 1)).EntireRow.Hidden = True
    End If
    ThisWorkbook.Sheets("ContentPlanner").Rows(CStr(beg) & ":" & CStr(eind)).EntireRow.Hidden = False
    ThisWorkbook.Sheets("ContentPlanner").Rows(CStr(eind + 1) & ":" & CStr(i)).EntireRow.Hidden = True
End Sub

