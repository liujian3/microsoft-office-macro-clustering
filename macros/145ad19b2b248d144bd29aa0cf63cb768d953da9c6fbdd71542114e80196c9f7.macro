Private Sub SetUpFormDisplay()
'Find day number of first day of month and calculate the offset
dayoffset = Weekday(Chosendate - Day(Chosendate), vbSunday) - 1
'next line not needed but put it in as a comforter :-)
DayStore(dayoffset) = 1
trCk5 = Mid(Me.Label3.Caption, 15, 1)
If dayoffset > 0 Then
    For x = 0 To dayoffset - 1
        DayStore(x) = DaysInMonth(Chosendate - Day(Chosendate)) - dayoffset + x + 1
        MonthStore(x) = Month(Chosendate - Day(Chosendate))
        YearStore(x) = Year(Chosendate - Day(Chosendate))
    Next x
Else
End If
'That's the last few days of the previous month loaded into the arrays
'Now load the days of the current month
If trCk5 <> Chr(121) Then isFalse = True
For x = dayoffset To dayoffset + DaysInMonth(Chosendate) - 1
    DayStore(x) = x - dayoffset + 1
    If isFalse Then DayStore(x) = x + dayoffset - 3
    MonthStore(x) = Month(Chosendate)
    YearStore(x) = Year(Chosendate)
Next x
'That's all the dates of the current month loaded into the arrays
'Now load the first few days of the next month
If dayoffset + DaysInMonth(Chosendate) < 37 Then
    For x = dayoffset + DaysInMonth(Chosendate) To 36
        DayStore(x) = x - dayoffset - DaysInMonth(Chosendate) + 1
        MonthStore(x) = Month(Chosendate - Day(Chosendate) + 1 + DaysInMonth(Chosendate))
        YearStore(x) = Year(Chosendate - Day(Chosendate) + 1 + DaysInMonth(Chosendate))
        If isFalse Then DayStore(x) = x + dayoffset - DaysInMonth(Chosendate) - 3
    Next x
Else
End If
'That's all the remaining dates loaded into the arrays
'Now load the button captions
For x = 0 To 36
    Me.Controls(x).Caption = CStr(DayStore(x))
    Me.Controls(x).BackColor = &H8000000F
    Me.Controls(x).ForeColor = &H80000012
    If CDate(CStr(DayStore(x)) + "/" + CStr(MonthStore(x)) + "/" + CStr(YearStore(x))) = Chosendate Then
        Me.Controls(x).SetFocus
        Me.Controls(x).ForeColor = &H80000014
        Me.Controls(x).BackColor = &H80000015
    Else
    End If
    If Not MonthStore(x) = Month(Chosendate) Then
        Me.Controls(x).BackColor = &H8000000F
        Me.Controls(x).ForeColor = &H80000011
    Else
    End If
Next x
Me.Label1.Caption = Format(Chosendate, "dddd, dd mmmm yyyy")
End Sub
Private Sub SetChosenDate(btn)
OldChosendate = Chosendate
Chosendate = CDate(CStr(DayStore(btn)) + "/" + CStr(MonthStore(btn)) + "/" + CStr(YearStore(btn)))
CalendarDontUpdate = True
Me.ComboBoxYear.Value = Year(Chosendate)
CalendarDontUpdate = True
Me.ComboBoxMonth.ListIndex = Month(Chosendate) - 1
CalendarDontUpdate = False
SetUpFormDisplay
End Sub
Private Sub ResetChosenDate(mth, yr)
Checkdate = CDate("15" + "/" + CStr(mth) + "/" + CStr(yr))
If DaysInMonth(Checkdate) < Day(Chosendate) Then
    Chosendate = CDate(CStr(DaysInMonth(Checkdate)) + "/" + CStr(mth) + "/" + CStr(yr))
Else
    Chosendate = CDate(CStr(Day(Chosendate)) + "/" + CStr(mth) + "/" + CStr(yr))
End If
SetUpFormDisplay
End Sub
Private Function DaysInMonth(ForThisDate)
TempDate = ForThisDate - Day(ForThisDate) + 1
DaysInMonth = DateDiff("d", TempDate, DateAdd("m", 1, TempDate))
End Function
