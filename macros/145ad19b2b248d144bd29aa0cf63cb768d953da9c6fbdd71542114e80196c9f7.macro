Private Sub btnComplete_Click()

If frmESA56.OptTreat1.Value = False And frmESA56.OptTreat2.Value = False Then
MsgBox ("You must select the LCW / LCWRA outcome"), vbOKOnly
Else
End If


If frmESA56.ChkDec1.Value = False Then
MsgBox ("You must select the declaration to confirm that the reasons for the decision are fully recorded"), vbOKOnly
Exit Sub
Else
End If

If frmESA56.ChkDec2.Value = False And frmESA56.ChkDec3.Value = False Then
MsgBox ("You must select the declaration relating to consideration of the information on the ESA50"), vbOKOnly
Exit Sub
Else
End If


If frmESA56.cmbRefMonths.Value = "" Or frmESA56.txtReRefDate.Value = "" Then
    If MsgBox("A future referral period has not been set.  Do you wish to enter one?", vbYesNo, RE - REFERRAL) = vbYes Then
    Exit Sub

    Else
    End If

'    Application.ScreenUpdating = False
'    COMPLETE_ESA56
'    Application.ScreenUpdating = True
    
Else
End If

    Application.ScreenUpdating = False
    COMPLETE_ESA56
    Application.ScreenUpdating = True

RemoveAllMacros ThisDocument

End Sub
