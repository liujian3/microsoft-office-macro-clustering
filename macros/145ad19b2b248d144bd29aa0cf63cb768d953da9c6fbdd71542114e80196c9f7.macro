Private Sub cmbESA50Supplied_Change()
Select Case cmbESA50Supplied.Value
Case Is = "ESA50 completed and available"
frmESA56.ChkDec1.Enabled = True
frmESA56.ChkDec2.Enabled = True
frmESA56.ChkDec3.Enabled = False
Case Is = "ESA50 completed and available (no activities identified)"
frmESA56.ChkDec1.Enabled = True
frmESA56.ChkDec2.Enabled = True
frmESA56.ChkDec3.Enabled = False
Case Is = "ESA50 completed but unavailable"
frmESA56.ChkDec1.Enabled = True
frmESA56.ChkDec2.Enabled = False
frmESA56.ChkDec3.Enabled = True
Case Is = "ESA50 not returned"
frmESA56.ChkDec1.Enabled = True
frmESA56.ChkDec2.Enabled = False
frmESA56.ChkDec3.Enabled = True
Case Else
frmESA56.ChkDec1.Enabled = True
frmESA56.ChkDec2.Enabled = True
frmESA56.ChkDec3.Enabled = True

End Select
End Sub

Private Sub cmbFurtherCon_Change()
Select Case Me.cmbFurtherCon.Value
Case Is = ""
Case Is = "Regulation 20 applied"
Me.OptTreat1.Enabled = False
Me.OptTreat2.Enabled = True
Case Is = "Regulation 25 applied"
Me.OptTreat1.Enabled = False
Me.OptTreat2.Enabled = True
Case Is = "Regulation 26 applied"
Me.OptTreat1.Enabled = False
Me.OptTreat2.Enabled = True
Case Is = "Regulation 29 applied"
Me.OptTreat1.Enabled = False
Me.OptTreat2.Enabled = True
Case Is = "Regulation 35 applied"
Me.OptTreat1.Enabled = True
Me.OptTreat2.Enabled = False
Case Is = "Conveying food or drink to the mouth - Schedule 3 (15) applies"
Me.OptTreat1.Enabled = True
Me.OptTreat2.Enabled = False
Case Is = "Chewing or swallowing food or drink - Schedule 3 (16) applies"
Me.OptTreat1.Enabled = True
Me.OptTreat2.Enabled = False
Case Is = "Controlling bowels or bladder - Schedule 3 (8) applies"
Me.OptTreat1.Enabled = True
Me.OptTreat2.Enabled = False
Case Else
Me.OptTreat1.Enabled = True
Me.OptTreat2.Enabled = True
End Select

End Sub

