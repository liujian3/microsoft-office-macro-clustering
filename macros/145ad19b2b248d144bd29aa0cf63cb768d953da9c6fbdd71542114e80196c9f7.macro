Public Function FormatDate(DateTxt As String)
Dim Sep1Pos, Sep2Pos, Pos As Integer
Dim NewTxt As String
Dim FormatOK As Boolean
FormatOK = True
'Check if date already formatted properly by the calendar (Date Selector)
If Len(DateTxt) <> 11 Then FormatOK = False
For Pos = 1 To Len(DateTxt)
    If Pos < 3 Or Pos > 7 Then
        If Asc(Mid(DateTxt, Pos, 1)) < 48 Or Asc(Mid(DateTxt, Pos, 1)) > 57 Then FormatOK = False
    End If
    If Pos = 3 Or Pos = 7 Then
        If Asc(Mid(DateTxt, Pos, 1)) <> 45 Then FormatOK = False
    End If
Next Pos

Select Case Mid(DateTxt, 4, 3)
    Case "Jan"
    Case "Feb"
    Case "Mar"
    Case "Apr"
    Case "May"
    Case "Jun"
    Case "Jul"
    Case "Aug"
    Case "Sep"
    Case "Oct"
    Case "Nov"
    Case "Dec"
    Case Else
        FormatOK = False
End Select

If FormatOK = True Then
    FormatDate = DateTxt
Else
    NewTxt = ""
    For Pos = 1 To Len(DateTxt)
        If Mid(DateTxt, Pos, 1) = "/" Or Mid(DateTxt, Pos, 1) = "." Or Mid(DateTxt, Pos, 1) = "-" Or Mid(DateTxt, Pos, 1) = "_" Or Mid(DateTxt, Pos, 1) = " " Or Mid(DateTxt, Pos, 1) = "," Then
            If Sep1Pos = 0 Then
                Sep1Pos = Pos
            Else
                Sep2Pos = Pos
            End If
        Else
        End If
    Next Pos
    
    If Sep1Pos > 0 And Sep2Pos > 0 Then
        Select Case Sep1Pos
            Case 2
                NewTxt = "0" & Mid(DateTxt, 1, 1)
            Case 3
                NewTxt = Mid(DateTxt, 1, 2)
            Case Else
                'Can't decipher date
                Txt = ""
                NewTxt = ""
                Sep1Pos = 0
                Sep2Pos = 0
        End Select
    End If
    
    If Sep1Pos > 0 And Sep2Pos > 0 Then
        Select Case Sep2Pos - Sep1Pos
            Case 2
                NewTxt = NewTxt & "0" & Mid(DateTxt, Sep2Pos - 1, 1)
            Case 3
                NewTxt = NewTxt & Mid(DateTxt, Sep1Pos + 1, 2)
            Case Else
                'Can't decipher date
                DateTxt = ""
                NewTxt = ""
                Sep1Pos = 0
                Sep2Pos = 0
        End Select
    End If
    
    If Sep1Pos > 0 And Sep2Pos > 0 Then
        DateTxt = NewTxt & Mid(DateTxt, Sep2Pos + 1, Len(DateTxt) - Sep2Pos)
    End If
    
    'Clean out any extra seperators
    NewTxt = ""
    If DateTxt <> "" Then
        For Pos = 1 To Len(DateTxt)
        Select Case Mid(DateTxt, Pos, 1)
            Case "/"
            Case "."
            Case "-"
            Case "_"
            Case " "
            Case Else
                NewTxt = NewTxt & Mid(DateTxt, Pos, 1)
        End Select
        Next Pos
    End If
    DateTxt = NewTxt
    
    Dim AllDigits As Boolean
    AllDigits = True
    For Pos = 1 To Len(DateTxt)
        If Asc(Mid(DateTxt, Pos, 1)) < 48 Or Asc(Mid(DateTxt, Pos, 1)) > 57 Then
            AllDigits = False
        End If
    Next Pos
    
    If AllDigits Then
        On Error GoTo NotADate
        Select Case Len(DateTxt)
            Case 6
                FormatDate = Format(CDate(Mid(DateTxt, 1, 2) & "/" & Mid(DateTxt, 3, 2) & "/" & Mid(DateTxt, 5, 2)), "dd-mmm-yyyy")
            Case 8
                FormatDate = Format(CDate(Mid(DateTxt, 1, 2) & "/" & Mid(DateTxt, 3, 2) & "/" & Mid(DateTxt, 5, 4)), "dd-mmm-yyyy")
            Case Else
        End Select
    Else
    End If
End If
Exit Function
NotADate:
MsgBox "The date entered could not be resolved. Please use the calendar."
FormatDate = ""
End Function

