'if it is checked then uncheck it and
'show the user instructions
If chkStat = 1 Then
    'uncheck
    WordBasic.SetFormResult "Check39", 0
    
    WordBasic.BeginDialog 489, 166, "Persons-in-Home"
        WordBasic.Text 25, 6, 437, 20, "Click OK to create an additional Persons-in-Home page. ", "Text1"
        WordBasic.Text 27, 25, 441, 13, "Each additional page created will be a seperate document", "Text4"
        WordBasic.Text 29, 40, 341, 16, "and will need to be saved with its own name.", "Text5"
        WordBasic.Text 31, 82, 383, 13, "To return to the original document select its name ", "Text6"
        WordBasic.Text 33, 98, 187, 13, "from the 'Window' menu.", "Text2"
        WordBasic.OKButton 128, 129, 88, 21
        WordBasic.CancelButton 250, 129, 88, 21
        WordBasic.Text 90, 58, 272, 13, "_____________________________", "Text3"
WordBasic.EndDialog
Set dlg = WordBasic.CurValues.UserDialog

'store the user's response to 'x'
    x = WordBasic.Dialog.UserDialog(dlg)

' -1 is true or Ok
    If x = -1 Then
       WordBasic.FileNew Template:=PRSDotPath$ + "people" + ".dot"
    End If
End If

