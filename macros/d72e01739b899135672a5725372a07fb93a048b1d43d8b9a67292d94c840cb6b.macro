Attribute VB_Name = "FormsToolBar"

Public Sub MAIN()
Dim Protect
Dim I

WordBasic.ScreenUpdating 0

WordBasic.ViewToolbars Toolbar:="Standard", Show:=1
WordBasic.ViewToolbars Toolbar:="Formatting", Show:=1

'Use this syntax on the other macros that unprotect documents
Protect = -1
If WordBasic.CommandValid("ToolsUnprotectDocument") = -1 Then
    WordBasic.ToolsUnprotectDocument DocumentPassword:="PRSIRT"
Else
    Protect = 0
End If

Dim nope
nope = -1

For I = 1 To WordBasic.CountToolbars()
    If WordBasic.[ToolbarName$](I) = "Forms Toolbar" Then
        nope = 0
    End If
Next

If nope = -1 Then
    WordBasic.NewToolbar Name:="Forms Toolbar", Context:=0
Else
    WordBasic.Organizer Delete:=1, Source:=WordBasic.[DefaultDir$](2) + "\normal.dot", Name:="Forms Toolbar", Tab:=2
WordBasic.NewToolbar Name:="Forms Toolbar", Context:=0
End If

WordBasic.AddButton "Forms Toolbar", 1, 2, "Corrpasswd", 0, 1
WordBasic.ChooseButtonImage Face:=276, Button:=1, Context:=1, Text:="", Toolbar:="Forms Toolbar"

WordBasic.AddButton "Forms Toolbar", 2, 2, "Relock", 0, 1
WordBasic.ChooseButtonImage Face:=235, Button:=2, Context:=1, Text:="", Toolbar:="Forms Toolbar"

WordBasic.AddButton "Forms Toolbar", 3, 2, "AddParagraph", 0, 1
WordBasic.ChooseButtonImage Face:=0, Button:=3, Context:=1, Text:="AddParagraph", Toolbar:="Forms Toolbar"

WordBasic.AddButton "Forms Toolbar", 4, 2, "formpagebreak", 0, 1
WordBasic.ChooseButtonImage Face:=0, Button:=4, Context:=1, Text:="PgBreak", Toolbar:="Forms Toolbar"

WordBasic.AddButton "Forms Toolbar", 5, 2, "SpCheck", 0, 1
WordBasic.ChooseButtonImage Face:=0, Button:=5, Context:=1, Text:="SpCheck", Toolbar:="Forms Toolbar"

