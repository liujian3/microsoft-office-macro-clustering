Attribute VB_Name = "formpagebreak"

Public Sub MAIN()
Attribute MAIN.VB_Description = "Inserts a page break in a protected document. If your cursor is in a table, it will insert the pgbk in between rows"
Attribute MAIN.VB_ProcData.VB_Invoke_Func = "TemplateProject.formpagebreak.MAIN"
Dim Protect
Dim x

Protect = -1

If WordBasic.CommandValid("ToolsUnprotectDocument") = -1 Then
    WordBasic.ToolsUnprotectDocument DocumentPassword:="PRSIRT"
Else
    Protect = 0
End If

'this statement delares an object variable to hold values and
'parameters for the userDialog
'=========================================
Dim dlg As Object
'=========================================

WordBasic.BeginDialog 330, 144, "Microsoft Word"
    WordBasic.Text 10, 6, 299, 13, "If your cursor is at the END of your text", "Text1"
    WordBasic.Text 11, 20, 315, 13, "your Page Break will be inserted BELOW.", "Text2"
    WordBasic.Text 10, 44, 285, 13, "If your cursor is at the BEGINNING of", "Text3"
    WordBasic.Text 10, 57, 261, 13, "your text, your Page Break will be ", "Text4"
    WordBasic.Text 10, 71, 127, 13, "inserted ABOVE.", "Text5"
    WordBasic.Text 10, 101, 199, 13, "Do you wish to continue ?", "Text6"
    WordBasic.OKButton 24, 120, 88, 21
    WordBasic.CancelButton 133, 120, 88, 21
WordBasic.EndDialog
Set dlg = WordBasic.CurValues.UserDialog

'the Dialog can be used to 'display' or 'display & return a value'
