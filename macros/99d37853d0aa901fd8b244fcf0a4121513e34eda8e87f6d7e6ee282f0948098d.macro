Attribute VB_Name = "m19_InputFromINI"
Public Function InputFromINI(FileLine)
a = 3
While (Mid$(FileLine, a, 1) <> "=")
    a = a + 1
    If a = Len(FileLine) Then
        InputFromINI = ""
        Exit Function
    End If
Wend
InputFromINI = Mid$(FileLine, a + 1)
End Function

