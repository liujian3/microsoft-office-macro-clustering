Attribute VB_Name = "m04_FindWord"
Public Function FindWord(w)
FindWord = 0 '|fffd||fffd||fffd||fffd||fffd||fffd||fffd|
 b = True
 
 Windows(IFileForControl).Selection.GoTo What:=wdGoToLine, Which:=wdGoToFirst, name:=""
 Windows(IFileForControl).Selection.Find.ClearFormatting
 With Windows(IFileForControl).Selection.Find
        .Text = w
        .Replacement.Text = ""
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = False
        .MatchWholeWord = True
        .MatchWildcards = False
        .MatchSoundsLike = False
        .MatchAllWordForms = False
    End With
    While b
    
    If Windows(IFileForControl).Selection.Find.Execute = True Then
            FindWord = FindWord + 1
      
     Else
            b = False
    End If
    Wend
End Function

