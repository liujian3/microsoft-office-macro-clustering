Public Function LineSpacingParagraph(par)
Select Case Windows(IFileForControl).Document.Paragraphs.Item(CLng(par)).LineSpacingRule
    Case wdLineSpaceExactly
        LineSpacingParagraph = Windows(IFileForControl).Document.Paragraphs.Item(CLng(par)).LineSpacing
    
    Case wdLineSpaceMultiple
        LineSpacingParagraph = Round((Windows(IFileForControl).Document.Paragraphs.Item(CLng(par)).LineSpacing / 100) / 12, 3)
        If LineSpacingParagraph >= 1 Then
        LineSpacingParagraph = Windows(IFileForControl).Document.Paragraphs.Item(CLng(par)).LineSpacing
        End If
    
    Case wdLineSpaceSingle
        LineSpacingParagraph = 0.01
        
    Case wdLineSpace1pt5
        LineSpacingParagraph = 0.015
    LineSpacingParagraph = Windows(IFileForControl).Document.Paragraphs.Item(CLng(par)).LineSpacing
    
    Case wdLineSpaceDouble
        LineSpacingParagraph = 0.02
    
    Case wdLineSpaceAtLeast
        LineSpacingParagraph = Windows(IFileForControl).Document.Paragraphs.Item(CLng(par)).LineSpacing
      
    Case Else
        LineSpacingParagraph = Windows(IFileForControl).Document.Paragraphs.Item(CLng(par)).LineSpacing
   
End Select


End Function

