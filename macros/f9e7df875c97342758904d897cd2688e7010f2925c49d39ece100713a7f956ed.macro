        For Each c In int_range
           If c.Value = "Glass" Then c.Offset(3, -11).Value = "Supplier acknowledges that this product is subject to applicable state labeling and bottle deposit regulations."
    Next
    End If
    Set int_range = Intersect(Target, Range("AT9"))
    If Not (int_range Is Nothing) Then
        For Each c In int_range
           If c.Value = "Select from drop down" Then c.Offset(3, -11).Value = ""
    Next
