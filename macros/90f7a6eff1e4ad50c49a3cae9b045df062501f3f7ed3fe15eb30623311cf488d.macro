Attribute VB_Name = "AggiungiTipicoVuoto"
Sub AggiungiNuovoTipicoVuotoSingleSheet()
Attribute AggiungiNuovoTipicoVuotoSingleSheet.VB_ProcData.VB_Invoke_Func = " \n14"
'
' AggiugniNuovoTipicoVuoto Macro
'
'
   Application.DisplayAlerts = False
   Sheets("Empty_single_Sheet").Visible = True
   If Sheets(Sheets.Count).Name = "MATERIAL SUMMARY" Then
        Sheets("Empty_single_Sheet").Copy Before:=Sheets("MATERIAL SUMMARY")
   Else: Sheets("Empty_single_Sheet").Copy After:=Sheets(Sheets.Count)
   End If

    'ActiveSheet.Name = "PROC-XXX"
    Range("A1").Select
    Sheets("Empty_single_Sheet").Visible = False
    Application.DisplayAlerts = True
    MsgBox "RENAME THE NEW ADDED SHEET"
        
End Sub

