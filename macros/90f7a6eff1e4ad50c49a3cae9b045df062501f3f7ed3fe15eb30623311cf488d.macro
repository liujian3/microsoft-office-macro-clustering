Attribute VB_Name = "Stampa"
Sub StampaDettagli()
' Stampa tutti i data sheets ad esclusione dei fogli MMS TABLE e Template
    
    'scegli la stampante
    Application.Dialogs(xlDialogPrinterSetup).Show
    ' stampa dal folgio CoverPage in avanti
    Application.ScreenUpdating = False
    Sheets("CoverPage").Select
    x = ActiveSheet.Index
    For i = x To Sheets.Count
        Sheets(i).Select
        With ActiveSheet.PageSetup
           .BlackAndWhite = True
        End With
        ActiveSheet.PrintOut
    Next
    Sheets("CoverPage").Select
    Application.ScreenUpdating = True

End Sub

