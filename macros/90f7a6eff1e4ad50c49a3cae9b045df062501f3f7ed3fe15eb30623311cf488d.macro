Attribute VB_Name = "Indice"
Sub AggiornaIndice()
Application.ScreenUpdating = False
Sheets("INDEX").Select
Range("a18: j163 ").ClearContents
IndirizzoTypicalNoPage = Range("a18").Address
IndirizzoTypicalNo = Range("b18").Address
IndirizzoTypicalNoDescr = Range("c18").Address
IndirizzoTypicalNoService = Range("d18").Address
IndirizzoTypicalNoIPS = Range("e18").Address
IndirizzoRevision = Range("f18").Address
IndirizzoModule = Range("g18").Address
IndirizzoUnit = Range("i18").Address
Sheets("GENERAL NOTES").Select
If ActiveSheet.Index = Sheets.Count Then
    Sheets("INDEX").Select
    Application.ScreenUpdating = True
    Exit Sub
Else
    ActiveSheet.Next.Select
    x = ActiveSheet.Index
    For i = x To Sheets.Count
    If Not (Sheets(i).Name = "MATERIAL SUMMARY") Then
             Sheets(i).Select
             Range("TYP_NAME").Copy
             Sheets("INDEX").Select
             Range(IndirizzoTypicalNo).Select
             ActiveSheet.Paste Link:=True
             Range(IndirizzoTypicalNo).Cells(2, 1).Select
             IndirizzoTypicalNo = ActiveCell.Address
             Sheets(i).Select
             Range("TYP_TITLE").Copy
             Sheets("INDEX").Select
             Range(IndirizzoTypicalNoDescr).Select
             ActiveSheet.Paste Link:=True
             Range(IndirizzoTypicalNoDescr).Cells(2, 1).Select
             IndirizzoTypicalNoDescr = ActiveCell.Address
             Sheets(i).Select
             Range("Service").Copy
             Sheets("INDEX").Select
             Range(IndirizzoTypicalNoService).Select
             ActiveSheet.Paste Link:=True
             Range(IndirizzoTypicalNoService).Cells(2, 1).Select
             IndirizzoTypicalNoService = ActiveCell.Address
             Sheets(i).Select
             Range("Instr_Pip_Spec").Copy
             Sheets("INDEX").Select
             Range(IndirizzoTypicalNoIPS).Select
             ActiveSheet.Paste Link:=True
             Range(IndirizzoTypicalNoIPS).Cells(2, 1).Select
             IndirizzoTypicalNoIPS = ActiveCell.Address
             Sheets(i).Select
             Range("REV_NR").Copy
             Sheets("INDEX").Select
             Range(IndirizzoRevision).Select
             ActiveSheet.Paste Link:=True
             Range(IndirizzoRevision).Cells(2, 1).Select
             IndirizzoRevision = ActiveCell.Address
             Sheets(i).Select
             Range("Module").Copy
             Sheets("INDEX").Select
             Range(IndirizzoModule).Select
             ActiveSheet.Paste Link:=True
             Range(IndirizzoModule).Cells(2, 1).Select
             IndirizzoModule = ActiveCell.Address
             Sheets(i).Select
             Range("Unit").Copy
             Sheets("INDEX").Select
             Range(IndirizzoUnit).Select
             ActiveSheet.Paste Link:=True
             Range(IndirizzoUnit).Cells(2, 1).Select
             IndirizzoUnit = ActiveCell.Address
             Sheets(i).Select
            If Range("sheet_format") = "s-s" Then
                PageNo = Range("Sheet_No")
                Sheets("INDEX").Select
                Range(IndirizzoTypicalNoPage).Select
                Range(IndirizzoTypicalNoPage) = PageNo
                Range(IndirizzoTypicalNoPage).Cells(2, 1).Select
                IndirizzoTypicalNoPage = ActiveCell.Address
             Else
                PageNo = Range("Sheet_No") & "/" & Range("Sheet_No_A")
                Sheets("INDEX").Select
                Range(IndirizzoTypicalNoPage).Select
                Range(IndirizzoTypicalNoPage) = PageNo
                Range(IndirizzoTypicalNoPage).Cells(2, 1).Select
                IndirizzoTypicalNoPage = ActiveCell.Address
            End If
    Else
        If Sheets(i).Name = "MATERIAL SUMMARY" Then
            Range(IndirizzoTypicalNoPage) = Sheets("MATERIAL SUMMARY").Range("v4")
            Range(IndirizzoTypicalNo) = "-"
            Range(IndirizzoTypicalNoDescr) = "INSTRUMENT MATERIAL SUMMARY"
            Range(IndirizzoTypicalNoService) = "-"
            Range(IndirizzoTypicalNoIPS) = "-"
             Range(IndirizzoModule) = "-"
            Range(IndirizzoRevision) = Sheets("MATERIAL SUMMARY").Range("V2")
        End If
    End If
    Next
    Sheets("INDEX").Select
    Application.ScreenUpdating = True
End If
End Sub


        
Attribute VB_Name = "MTO"
Sub MTO1()
Attribute MTO1.VB_ProcData.VB_Invoke_Func = " \n14"

' Crea MTO
'
    Dim LibreriaMateriali, DettagliDiInstallazione As Workbook
    Set DettagliDiInstallazione = ActiveWorkbook
    Application.ScreenUpdating = False
    
    Sheets("MTO").Select
    Range("A5").Select
    Application.DisplayAlerts = False
    MsgBox "click on <OK> then select the  excel file with the material library (Libreria materiali_PROCESS_PNEUMATIC.xlsm)"
    Application.Dialogs(xlDialogOpen).Show
    Set LibreriaMateriali = ActiveWorkbook
    Application.DisplayAlerts = True
    DettagliDiInstallazione.Activate
       
    IndirizzoUnit = Range("a5").Address
    IndirizzoArea = Range("b5").Address
    IndirizzoTOS = Range("c5").Address
    IndirizzoTagCode = Range("d5").Address
    IndirizzoTagDescription = Range("e5").Address
    IndirizzoMaterial = Range("f5").Address
    IndirizzoSTD = Range("g5").Address
    IndirizzoDetails = Range("h5").Address
    IndirizzoRating = Range("i5").Address
    IndirizzoNotes = Range("j5").Address
    IndirizzoSize = Range("k5").Address
    IndirizzoQty = Range("l5").Address
    IndirizzoMU = Range("m5").Address
    IndirizzoFur = Range("n5").Address
    IndirizzoDrawingNr = Range("o5").Address
    IndirizzoDwgRev = Range("p5").Address
    IndirizzoReferenceObject = Range("q5").Address
       

    Sheets("GENERAL NOTES").Select
        If Sheets("GENERAL NOTES").Index = Sheets.Count Then
            MsgBox "no items included in appendix 2"
            Application.ScreenUpdating = True
        Exit Sub
    End If
    ActiveSheet.Next.Select

    x = ActiveSheet.Index
    If Sheets(Sheets.Count).Name = "MATERIAL SUMMARY" Then
        NumeroFogli = Worksheets.Count - 1
    Else
        NumeroFogli = Worksheets.Count
    End If
    'per ciascun dettaglio di installazione
    For i = x To NumeroFogli
        Sheets(i).Activate
        Range("Inizio_Elenco_Strumenti").Select
        IndirizzoElencoStrumenti = Range("Inizio_Elenco_Strumenti").Address
        'Supply = ""
        

        'finch|fffd| nel typical detail ci sono item elencati
        While Range(IndirizzoElencoStrumenti) <> ""
              
        
                TagNumber = Range(IndirizzoElencoStrumenti)
                Unit = Range("Unit")
                               IndirizzoCodice = Range("Inizio_Quantity_Table").Address
                IndirizzoCodiceQuantity = Range("Inizio_Quantity_Table").Offset(0, 2).Address
                TOS = Range(IndirizzoElencoStrumenti).Offset(0, 5)
                AREA = Range(IndirizzoElencoStrumenti).Offset(0, 4)
                IndirizzoSupply = Range("Inizio_Quantity_Table").Offset(0, 3).Address
                Range(IndirizzoCodice).Select
                Codice = Range(IndirizzoCodice).Text
                Range(IndirizzoCodiceQuantity).Select
                CodiceQuantity = Range(IndirizzoCodiceQuantity)
                Range(IndirizzoSupply).Select
                Supply = Range(IndirizzoSupply).Text
                IndirizzoCodiceFur = Range("Inizio_Quantity_Table").Offset(0, 3).Address
                Range(IndirizzoCodiceFur).Select
                CodiceFur = Range(IndirizzoCodiceFur)
                               
                While Codice <> ""
                    Sheets("MTO").Select
                    'se il codice va comperato
                    If Supply <> "" Then
                       'copia l'Unit|fffd|
                        Range(IndirizzoUnit) = Unit
                        Range(IndirizzoUnit).NumberFormat = "@"
                        Range(IndirizzoUnit).Cells(2, 1).Select
                        IndirizzoUnit = ActiveCell.Address
                       'copia la Construction Area
                        Range(IndirizzoArea) = AREA
                        Range(IndirizzoArea).NumberFormat = "@"
                        Range(IndirizzoArea).Cells(2, 1).Select
                        IndirizzoArea = ActiveCell.Address
                        'copia il TOS
                        Range(IndirizzoTOS) = TOS
                        Range(IndirizzoTOS).NumberFormat = "@"
                        Range(IndirizzoTOS).Cells(2, 1).Select
                        IndirizzoTOS = ActiveCell.Address
                       'copia il Codice Materiale
                        Range(IndirizzoTagCode) = Codice
                        Range(IndirizzoTagCode).NumberFormat = "@"
                        Range(IndirizzoTagCode).Cells(2, 1).Select
                        IndirizzoTagCode = ActiveCell.Address
                       'copia la descrizione del codice
                        Range(IndirizzoTagDescription) = Application.VLookup(Codice, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 3, False)
                        Range(IndirizzoTagDescription).NumberFormat = "@"
                        Range(IndirizzoTagDescription).Cells(2, 1).Select
                        IndirizzoTagDescription = ActiveCell.Address
                        'copia il materiale
                        Range(IndirizzoMaterial) = Application.VLookup(Codice, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 8, False)
                        Range(IndirizzoMaterial).NumberFormat = "@"
                        Range(IndirizzoMaterial).Cells(2, 1).Select
                        IndirizzoMaterial = ActiveCell.Address
                       'copia la normativa
                        Range(IndirizzoSTD) = Application.VLookup(Codice, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 5, False)
                        Range(IndirizzoSTD).NumberFormat = "@"
                        Range(IndirizzoSTD).Cells(2, 1).Select
                        IndirizzoSTD = ActiveCell.Address
                        'copia i dettagli costruttivi
                        Range(IndirizzoDetails) = Application.VLookup(Codice, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 9, False)
                        Range(IndirizzoDetails).NumberFormat = "@"
                        Range(IndirizzoDetails).Cells(2, 1).Select
                        IndirizzoDetails = ActiveCell.Address
                        'copia il rating
                        Range(IndirizzoRating) = Application.VLookup(Codice, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 4, False)
                        Range(IndirizzoRating).NumberFormat = "@"
                        Range(IndirizzoRating).Cells(2, 1).Select
                        IndirizzoRating = ActiveCell.Address
                        'copia le note materiale
                        Range(IndirizzoNotes) = Application.VLookup(Codice, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 10, False)
                        Range(IndirizzoNotes).NumberFormat = "@"
                        Range(IndirizzoNotes).Cells(2, 1).Select
                        IndirizzoNotes = ActiveCell.Address
                        'copia il size
                        Range(IndirizzoSize) = Application.VLookup(Codice, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 7, False)
                        Range(IndirizzoSize).NumberFormat = "@"
                        Range(IndirizzoSize).Cells(2, 1).Select
                        IndirizzoSize = ActiveCell.Address
                        'copia la CodiceQuantity in Qty
                        Range(IndirizzoQty) = CodiceQuantity
                        Range(IndirizzoQty).NumberFormat = "######"
                        Range(IndirizzoQty).Cells(2, 1).Select
                        IndirizzoQty = ActiveCell.Address
                        'copia l'unit|fffd| di misura
                        Range(IndirizzoMU) = Application.VLookup(Codice, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 11, False)
                        Range(IndirizzoMU).NumberFormat = "@"
                        Range(IndirizzoMU).Cells(2, 1).Select
                        IndirizzoMU = ActiveCell.Address
                        'copia la fornitura
                        Range(IndirizzoFur) = CodiceFur
                        Range(IndirizzoFur).NumberFormat = "######"
                        Range(IndirizzoFur).Cells(2, 1).Select
                        IndirizzoFur = ActiveCell.Address
                        'copia il drawing Number
                        DrawingNr = Sheets("CoverPage").Range("Drawing_Number")
                        Range(IndirizzoDrawingNr) = DrawingNr
                        Range(IndirizzoDrawingNr).NumberFormat = "@"
                        Range(IndirizzoDrawingNr).Cells(2, 1).Select
                        IndirizzoDrawingNr = ActiveCell.Address
                        'copia la revisione del disegno
                        DwgRev = Sheets("CoverPage").Range("REV_NR")
                        Range(IndirizzoDwgRev) = DwgRev
                        Range(IndirizzoDwgRev).NumberFormat = "@"
                        Range(IndirizzoDwgRev).Cells(2, 1).Select
                        IndirizzoDwgRev = ActiveCell.Address
                        'copia il tagNumber nel reference object
                        Range(IndirizzoReferenceObject) = TagNumber
                        Range(IndirizzoReferenceObject).NumberFormat = "@"
                        Range(IndirizzoReferenceObject).Cells(2, 1).Select
                        IndirizzoReferenceObject = ActiveCell.Address
                   End If
                    
                    'passa al codice successivo dello stesso dettaglio
                    Sheets(i).Select
                    Range(IndirizzoCodice).Cells(2, 1).Select
                    IndirizzoCodice = ActiveCell.Address
                    Range(IndirizzoCodiceQuantity).Cells(2, 1).Select
                    
                    IndirizzoCodiceQuantity = ActiveCell.Address
                    Range(IndirizzoCodiceFur).Cells(2, 1).Select
                    IndirizzoCodiceFur = ActiveCell.Address
                    Range(IndirizzoCodice).Select
                    Codice = Range(IndirizzoCodice).Text
                    Range(IndirizzoCodiceQuantity).Select
                    Range(IndirizzoCodiceFur).Select
                    CodiceQuantity = Range(IndirizzoCodiceQuantity)
                    CodiceFur = Range(IndirizzoCodiceFur)
                                       
                Wend
           
           'passa al tagnumber successivo
           Sheets(i).Select
           Range(IndirizzoElencoStrumenti).Cells(2, 1).Select
           IndirizzoElencoStrumenti = ActiveCell.Address
        Wend
        
   Next i
   LibreriaMateriali.Close
   Sheets("MTO").Select
   ActiveWorkbook.Save
   Application.ScreenUpdating = True
   
    ActiveCell.SpecialCells(xlLastCell).Select
    IndirizzoUltimaCella = ActiveCell.Address
    Range("A4", IndirizzoUltimaCella).Select
    ActiveWorkbook.Worksheets("MTO").Sort.SortFields.Clear
    ActiveWorkbook.Worksheets("MTO").Sort.SortFields.Add Key:=Range("A4:A50"), _
        SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:=xlSortNormal
    ActiveWorkbook.Worksheets("MTO").Sort.SortFields.Add Key:=Range("B4:B50"), _
        SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:=xlSortNormal
    ActiveWorkbook.Worksheets("MTO").Sort.SortFields.Add Key:=Range("N4:N50"), _
        SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:=xlSortNormal
    ActiveWorkbook.Worksheets("MTO").Sort.SortFields.Add Key:=Range("D4:D50"), _
        SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:=xlSortNormal
    With ActiveWorkbook.Worksheets("MTO").Sort
        .SetRange Range("A4", IndirizzoUltimaCella)
        .Header = xlYes
        .MatchCase = False
        .Orientation = xlTopToBottom
        .SortMethod = xlPinYin
        .Apply
        End With
         
        End Sub
        
        Sub ClearMTO()

    Sheets("MTO").Select
    'svuota la tabella
    Range("A5").Select
    ActiveCell.SpecialCells(xlLastCell).Select
    IndirizzoUltimaCella = ActiveCell.Address
    Range("A5", IndirizzoUltimaCella).Select
    Selection.ClearContents
    ActiveWorkbook.Save
    Range("A5").Select

End Sub

Attribute VB_Name = "NumeraPagine"
Sub AggiornaNumerazionePagine()
Application.ScreenUpdating = False
Sheets("INDEX").Select
Message = "Type the number of first page after the general notes (i.e. 4)"
    Predefinito = 4
    Title = "Material Summary"
    NumeroPagina = InputBox(Message, Title, Predefinito)
Sheets("GENERAL NOTES").Select
    ActiveSheet.Next.Select
    x = ActiveSheet.Index
    For i = x To Sheets.Count
        If Not (Sheets(i).Name = "MATERIAL SUMMARY") Then
            Sheets(i).Select
            Range("Sheet_No") = NumeroPagina
            NumeroPagina = NumeroPagina + 1
        End If
    Next
    
    If Sheets(Sheets.Count).Name = "MATERIAL SUMMARY" Then
        Sheets("MATERIAL SUMMARY").Range("v4") = NumeroPagina
        Sheets("INDEX").Range("TOTAL_PAGES") = NumeroPagina
    Else
        Sheets("INDEX").Range("TOTAL_PAGES") = NumeroPagina - 1
    End If
    Application.ScreenUpdating = True
    Sheets("INDEX").Select
End Sub

        

