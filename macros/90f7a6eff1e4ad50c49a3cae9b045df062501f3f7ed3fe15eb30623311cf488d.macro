Attribute VB_Name = "BOM"
Sub CreaBOM()
'
' Crea BOM
'
    Dim LibreriaMateriali, DettagliDiInstallazione As Workbook
    Set DettagliDiInstallazione = ActiveWorkbook
    Application.ScreenUpdating = False
    
    Sheets("BOM Import Data").Select
    'svuota la tabella
    Range("A5").Select
    ActiveCell.SpecialCells(xlLastCell).Select
    IndirizzoUltimaCella = ActiveCell.Address
    Range("A5", IndirizzoUltimaCella).Select
    Selection.ClearContents
  
    Message1 = "Type the Requisition Set (two digits)"
    Message2 = "Type the MTO Level (one digit)"
    Message3 = "Type the Unit No. (five digits max)"
    Message4 = "Type the Construction Area (three digits)"
    Message5 = "Type the TOS No. (three digits)"
    Message6 = "Type the MTO Info (two digits)"
    Message7 = "Type IFC (one digit Y/N)"
    Message8 = "Type Mat. Req. Long description"
    Title = "Automatic BOM Import Data Table generation"
    Predefinito1 = "A"
    Predefinito2 = "1"
    Predefinito3 = ""
    Predefinito4 = ""
    Predefinito5 = ""
    Predefinito6 = ""
    Predefinito7 = ""
    Predefinito8 = "Material supply according to FW Job Specification BDXXXXY-60A10"
    
    ReqSet = InputBox(Message1, Title, Predefinito1)
    MtoLevel = InputBox(Message2, Title, Predefinito2)
    'Unit = InputBox(Message3, Title, Predefinito3)
    'AREA = InputBox(Message4, Title, Predefinito4)
    'TOS = InputBox(Message5, Title, Predefinito5)
    MTOInfo = InputBox(Message6, Title, Predefinito6)
    IFC = InputBox(Message7, Title, Predefinito7)
    'ReqLongDescr = InputBox(Message8, Title, Predefinito8)
    
    Application.DisplayAlerts = False
    MsgBox "click on <OK> then select the  excel file with the material library (Libreria materiali_PROCESS_PNEUMATIC.xlsm)"
    Application.Dialogs(xlDialogOpen).Show
    Set LibreriaMateriali = ActiveWorkbook
    Application.DisplayAlerts = True
    DettagliDiInstallazione.Activate
    
    IndirizzoJob = Range("a5").Address
    IndirizzoUnit = Range("c5").Address
    IndirizzoArea = Range("d5").Address
    IndirizzoDisc = Range("e5").Address
    IndirizzoSA = Range("f5").Address
    IndirizzoDrawingNr = Range("g5").Address
    IndirizzoDwgRev = Range("h5").Address
    IndirizzoReferenceObject = Range("i5").Address
    IndirizzoMtoLevel = Range("j5").Address
    IndirizzoIFC = Range("k5").Address
    IndirizzoECA = Range("l5").Address
    IndirizzoReqSeqNo = Range("m5").Address
    IndirizzoReqSet = Range("n5").Address
    IndirizzoReqTitle = Range("o5").Address
    IndirizzoReqAliasCode = Range("p5").Address
    IndirizzoItemNo = Range("q5").Address
    IndirizzoTagCode = Range("r5").Address
    IndirizzoTagDescription = Range("s5").Address
    IndirizzoQty = Range("t5").Address
    IndirizzoMU = Range("u5").Address
    IndirizzoTOS = Range("v5").Address
    IndirizzoMTOInfo = Range("w5").Address
    IndirizzoReqLongDescr = Range("x5").Address
    IndirizzoTagLongDescr = Range("y5").Address
    IndirizzoOriginalDrawing = Range("y5").Address

    Sheets("GENERAL NOTES").Select
        If Sheets("GENERAL NOTES").Index = Sheets.Count Then
            MsgBox "no items included in appendix 2"
            Application.ScreenUpdating = True
        Exit Sub
    End If
    ActiveSheet.Next.Select

    x = ActiveSheet.Index
    If Sheets(Sheets.Count).Name = "MATERIAL SUMMARY" Then
        NumeroFogli = Worksheets.Count - 1
    Else
        NumeroFogli = Worksheets.Count
    End If
    'per ciascun dettaglio di installazione
    For i = x To NumeroFogli
        Sheets(i).Activate
        Range("Inizio_Elenco_Strumenti").Select
        IndirizzoElencoStrumenti = Range("Inizio_Elenco_Strumenti").Address
        Supply = ""

        'finch|fffd| nel typical detail ci sono item elencati
        While Range(IndirizzoElencoStrumenti) <> ""
                TagNumber = Range(IndirizzoElencoStrumenti)
                Unit = Range("Unit")
                ReqLongDescr = Worksheets("GENERAL NOTES").Range("Mat_Req_Long_Description")
                IndirizzoCodice = Range("Inizio_Quantity_Table").Address
                IndirizzoCodiceQuantity = Range("Inizio_Quantity_Table").Offset(0, 2).Address
                TOS = Range(IndirizzoElencoStrumenti).Offset(0, 5)
                AREA = Range(IndirizzoElencoStrumenti).Offset(0, 4)
                IndirizzoSupply = Range("Inizio_Quantity_Table").Offset(0, 3).Address
                Range(IndirizzoCodice).Select
                Codice = Range(IndirizzoCodice).Text
                Range(IndirizzoCodiceQuantity).Select
                CodiceQuantity = Range(IndirizzoCodiceQuantity)
                Range(IndirizzoSupply).Select
                Supply = Range(IndirizzoSupply).Text
                While Codice <> ""
                    Sheets("BOM Import Data").Select
                    'se il codice va comperato
                    If Supply = "FW" Then
                       
                        'copia il tagNumber nel reference object
                        Range(IndirizzoReferenceObject) = TagNumber
                        Range(IndirizzoReferenceObject).NumberFormat = "@"
                        Range(IndirizzoReferenceObject).Cells(2, 1).Select
                        IndirizzoReferenceObject = ActiveCell.Address
                        'copia il Codice in Tag code
                        Range(IndirizzoTagCode) = Codice
                        Range(IndirizzoTagCode).NumberFormat = "@"
                        Range(IndirizzoTagCode).Cells(2, 1).Select
                        IndirizzoTagCode = ActiveCell.Address
                        'copia l'ECA
                        ECA = "16" & Left(Codice, 3)
                        Range(IndirizzoECA) = ECA
                        Range(IndirizzoECA).NumberFormat = "@"
                        Range(IndirizzoECA).Cells(2, 1).Select
                        IndirizzoECA = ActiveCell.Address
                        'copia la CodiceQuantity in Qty
                        Range(IndirizzoQty) = CodiceQuantity
                        Range(IndirizzoQty).NumberFormat = "######"
                        Range(IndirizzoQty).Cells(2, 1).Select
                        IndirizzoQty = ActiveCell.Address
                        ' copia l'MTO level
                        Range(IndirizzoMtoLevel).Select
                        Range(IndirizzoMtoLevel) = MtoLevel
                        Selection.NumberFormat = "###"
                        Range(IndirizzoMtoLevel).Cells(2, 1).Select
                        IndirizzoMtoLevel = ActiveCell.Address
                        'copia il Req Set
                        Range(IndirizzoReqSet) = ReqSet
                        Range(IndirizzoReqSet).NumberFormat = "@"
                        Range(IndirizzoReqSet).Cells(2, 1).Select
                        IndirizzoReqSet = ActiveCell.Address
                        'copia il drawing Number
                        DrawingNr = Sheets("CoverPage").Range("Drawing_Number")
                        Range(IndirizzoDrawingNr) = DrawingNr
                        Range(IndirizzoDrawingNr).NumberFormat = "@"
                        Range(IndirizzoDrawingNr).Cells(2, 1).Select
                        IndirizzoDrawingNr = ActiveCell.Address
                        'copia la revisione del disegno
                        DwgRev = Sheets("CoverPage").Range("REV_NR")
                        Range(IndirizzoDwgRev) = DwgRev
                        Range(IndirizzoDwgRev).NumberFormat = "@"
                        Range(IndirizzoDwgRev).Cells(2, 1).Select
                        IndirizzoDwgRev = ActiveCell.Address
                        'copia in numero di contratto
                        Job = Sheets("CoverPage").Range("Contract_No")
                        Range(IndirizzoJob) = Job
                        Range(IndirizzoJob).NumberFormat = "@"
                        Range(IndirizzoJob).Cells(2, 1).Select
                        IndirizzoJob = ActiveCell.Address
                        'copia la disciplina
                        Range(IndirizzoDisc) = "6"
                        Range(IndirizzoDisc).NumberFormat = "@"
                        Range(IndirizzoDisc).Cells(2, 1).Select
                        IndirizzoDisc = ActiveCell.Address
                        'copia la sottosezione
                        Range(IndirizzoSA) = "65"
                        Range(IndirizzoSA).NumberFormat = "@"
                        Range(IndirizzoSA).Cells(2, 1).Select
                        IndirizzoSA = ActiveCell.Address
                        'copia l'Unit|fffd|
                        Range(IndirizzoUnit) = Unit
                        Range(IndirizzoUnit).NumberFormat = "@"
                        Range(IndirizzoUnit).Cells(2, 1).Select
                        IndirizzoUnit = ActiveCell.Address
                        'copia la Construction Area
                        Range(IndirizzoArea) = AREA
                        Range(IndirizzoArea).NumberFormat = "@"
                        Range(IndirizzoArea).Cells(2, 1).Select
                        IndirizzoArea = ActiveCell.Address
                        'copia il TOS
                        Range(IndirizzoTOS) = TOS
                        Range(IndirizzoTOS).NumberFormat = "@"
                        Range(IndirizzoTOS).Cells(2, 1).Select
                        IndirizzoTOS = ActiveCell.Address
                        'copia l'MTO Info
                        Range(IndirizzoMTOInfo) = MTOInfo
                        Range(IndirizzoMTOInfo).NumberFormat = "@"
                        Range(IndirizzoMTOInfo).Cells(2, 1).Select
                        IndirizzoMTOInfo = ActiveCell.Address
                        'copia IFC
                        Range(IndirizzoIFC) = IFC
                        Range(IndirizzoIFC).NumberFormat = "@"
                        Range(IndirizzoIFC).Cells(2, 1).Select
                        IndirizzoIFC = ActiveCell.Address
                        'copia la Mat Req Long Description
                        Range(IndirizzoReqLongDescr) = ReqLongDescr
                        Range(IndirizzoReqLongDescr).NumberFormat = "@"
                        Range(IndirizzoReqLongDescr).Cells(2, 1).Select
                        IndirizzoReqLongDescr = ActiveCell.Address
                        'copia il titolo delle requisition
                        Range(IndirizzoReqTitle) = Application.VLookup(Codice, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 2, False)
                        Range(IndirizzoReqTitle).NumberFormat = "@"
                        Range(IndirizzoReqTitle).Cells(2, 1).Select
                        IndirizzoReqTitle = ActiveCell.Address
                        'copia la tag desciption requisition
                        Range(IndirizzoTagDescription) = Application.VLookup(Codice, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 3, False)
                        Range(IndirizzoTagDescription).NumberFormat = "@"
                        Range(IndirizzoTagDescription).Cells(2, 1).Select
                        IndirizzoTagDescription = ActiveCell.Address
                        ' copia la tag long description
                        Range(IndirizzoTagLongDescr) = Application.VLookup(Codice, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 6, False)
                        Range(IndirizzoTagLongDescr).NumberFormat = "@"
                        Range(IndirizzoTagLongDescr).Cells(2, 1).Select
                        IndirizzoTagLongDescr = ActiveCell.Address
                        'copia l'unit|fffd| di misura
                        Range(IndirizzoMU) = Application.VLookup(Codice, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 11, False)
                        Range(IndirizzoMU).NumberFormat = "@"
                        Range(IndirizzoMU).Cells(2, 1).Select
                        IndirizzoMU = ActiveCell.Address
                    End If
                    
                    'passa al codice successivo dello stesso dettaglio
                    Sheets(i).Select
                    Range(IndirizzoCodice).Cells(2, 1).Select
                    IndirizzoCodice = ActiveCell.Address
                    Range(IndirizzoCodiceQuantity).Cells(2, 1).Select
                    IndirizzoCodiceQuantity = ActiveCell.Address
                    Range(IndirizzoSupply).Cells(2, 1).Select
                    IndirizzoSupply = ActiveCell.Address
                    Range(IndirizzoCodice).Select
                    Codice = Range(IndirizzoCodice).Text
                    Range(IndirizzoCodiceQuantity).Select
                    CodiceQuantity = Range(IndirizzoCodiceQuantity)
                    Range(IndirizzoSupply).Select
                    Supply = Range(IndirizzoSupply).Text
                                        
                Wend
           
           'passa al tagnumber successivo
           Sheets(i).Select
           Range(IndirizzoElencoStrumenti).Cells(2, 1).Select
           IndirizzoElencoStrumenti = ActiveCell.Address
        Wend
        
   Next i
   LibreriaMateriali.Close
   Sheets("BOM Import Data").Select
   ActiveWorkbook.Save
   Application.ScreenUpdating = True
   Range("a5").Select
    
End Sub


