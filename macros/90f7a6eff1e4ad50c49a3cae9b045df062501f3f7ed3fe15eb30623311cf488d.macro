    ActiveSheet.Next.Select
    x = ActiveSheet.Index
    For i = x To Sheets.Count
        Sheets(i).Select
        If Not (Sheets(i).Name = "SOMMARIO MATERIALI" Or _
            Sheets(i).Name = "MATERIAL SUMMARY") Then
            Sheets(i).Name = Range("TYP_NAME")
         
        End If
    Next
    Sheets("INDEX").Select
    Application.ScreenUpdating = True
End Sub
