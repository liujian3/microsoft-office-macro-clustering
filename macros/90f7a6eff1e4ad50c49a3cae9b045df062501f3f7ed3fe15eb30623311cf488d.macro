Attribute VB_Name = "SommarioMateriali"
Sub CreaSommarioMateriali()
'
' Crea Sommario Materiali di Prcoesso
'
' Keyboard Shortcut: Ctrl+k
'
    Dim LibreriaMateriali, DettagliDiInstallazione As Workbook
    Set DettagliDiInstallazione = ActiveWorkbook
    Application.ScreenUpdating = False
    Message = "Type the required Overage Percentage (i.e. 20)"
    Predefinito = 0
    Title = "Material Summary"
    Message1 = "Type the required Revision Number (i.e. C00)"
    Title1 = "Material Summary"
    Predefinito1 = "C00"
    Message2 = "Type the required Revision Date (i.e. 25/11/2013)"
    Title2 = "Material Summary"
    Predefinito2 = Now
    Application.DisplayAlerts = False
    MsgBox "click on <OK> then select the  excel file with the material library (Libreria materiali_PORCESS_PNEUMATIC.xlsm)"
    Application.Dialogs(xlDialogOpen).Show
    Set LibreriaMateriali = ActiveWorkbook
    PercentualeOverage = InputBox(Message, Title, Predefinito)
    RevNumber = InputBox(Message1, Title1, Predefinito1)
    RevDate = InputBox(Message2, Title2, Predefinito2)
    DettagliDiInstallazione.Activate
    For Each Sh In Sheets
      If Sh.Name = "SOMMARIO MATERIALI" Then
           Application.DisplayAlerts = False
           Sh.Delete
      End If
    Next
    For Each Sh In Sheets
      If Sh.Name = "MATERIAL SUMMARY" Then
           Application.DisplayAlerts = False
           Sh.Delete
      End If
    Next
    Application.DisplayAlerts = True
    Sheets.Add After:=Sheets(Sheets.Count)
    Sheets(Sheets.Count).Name = "SOMMARIO MATERIALI"
    Sheets("GENERAL NOTES").Select
    ActiveSheet.Next.Select
    x = ActiveSheet.Index
    Range("SommarioPerTipico").Select
    Selection.Copy
    Sheets("SOMMARIO MATERIALI").Activate
    Range("a1") = "Typical No."
    Range("b1") = "Code"
    Range("c1") = "Total Quantity"
    Range("A2").Select
    ActiveSheet.Paste Link:=True
    Sheets(x).Activate
    ActiveSheet.Next.Select
    x = ActiveSheet.Index
    For i = x To Worksheets.Count - 1
        Sheets(i).Activate
        Range("SommarioPerTipico").Select
        Selection.Copy
        Sheets("SOMMARIO MATERIALI").Activate
        Range("A1").Select
        Selection.End(xlDown).Select
        ActiveCell.Offset(1, 0).Select
        ActiveSheet.Paste Link:=True
    Next
    ActiveWorkbook.Save
'crea tabella Pivot
    Sheets("SOMMARIO MATERIALI").Activate
    Range("A1").Select
    With Sheets("SOMMARIO MATERIALI")
        .PivotTableWizard SourceType:=xlDatabase, _
        SourceData:=ActiveSheet.UsedRange, _
        TableDestination:="", _
        TableName:="MATERIAL SUMMARY"
        RowGrand = False
        ColumnGrand = False
    End With
    With ActiveSheet
    .PivotTables("MATERIAL SUMMARY").AddFields RowFields:="Code"
    .PivotTables("MATERIAL SUMMARY").PivotFields("TOTAL QUANTITY"). _
    Orientation = xlDataField
 
    .Name = "MATERIAL SUMMARY"
    End With
'completa il sommario materiali
    DettagliDiInstallazione.Sheets("MATERIAL SUMMARY").Activate
    Dim UltimaCella As Range
    Dim AngoloDiStampa As Range
    Rows(2).Font.Bold = True
    Range("A1").Select
    Set UltimaCella = ActiveCell.CurrentRegion.End(xlDown)
    For Each Cella In Range(Cells(3, 1), UltimaCella.Offset(-1, 0))
        If Cella <> 0 Then
            Cella.Offset(0, 4) = Cella
            Cella.Offset(0, 4).ColumnWidth = 10
            Cella.Offset(0, 5) = Application.VLookup(Cella, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 3, False)
            Cella.Offset(0, 5).ColumnWidth = 20
            Cella.Offset(0, 5).WrapText = True
            Cella.Offset(0, 6) = Application.VLookup(Cella, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 8, False)
            Cella.Offset(0, 6).ColumnWidth = 20
            Cella.Offset(0, 6).WrapText = True
            Cella.Offset(0, 7) = Application.VLookup(Cella, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 5, False)
            Cella.Offset(0, 7).ColumnWidth = 15
            Cella.Offset(0, 7).WrapText = True
            Cella.Offset(0, 8) = Application.VLookup(Cella, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 9, False)
            Cella.Offset(0, 8).ColumnWidth = 20
            Cella.Offset(0, 8).WrapText = True
            Cella.Offset(0, 9) = Application.VLookup(Cella, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 4, False)
            Cella.Offset(0, 9).ColumnWidth = 20
            Cella.Offset(0, 9).WrapText = True
            Cella.Offset(0, 10) = Application.VLookup(Cella, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 10, False)
            Cella.Offset(0, 10).ColumnWidth = 20
            Cella.Offset(0, 10).WrapText = True
            Cella.Offset(0, 11) = Application.VLookup(Cella, LibreriaMateriali.Sheets("MATERIAL").Range("$C$1:$O$16000"), 7, False)
            Cella.Offset(0, 11).ColumnWidth = 20
            Cella.Offset(0, 11).WrapText = True
            Cella.Offset(0, 12) = Cella.Offset(0, 1)
            Cella.Offset(0, 12).ColumnWidth = 15
            Cella.Offset(0, 13) = Round(Cella.Offset(0, 1) * PercentualeOverage / 100)
            Cella.Offset(0, 13).ColumnWidth = 15
