Sub bank_doc_c_mandatory()

If Range("bank_doc_fields_c").EntireRow.Hidden = False And Range("bank_doc_count_c").Value < 1 And Range("SRC").Value <> "New Creation" Then

    Range("bank_doc_fields_c").Select
    Selection.FormatConditions.Add Type:=xlExpression, Formula1:="=bank_doc_count_c<1"
    Selection.FormatConditions(Selection.FormatConditions.Count).SetFirstPriority
    
    With Selection.FormatConditions(1).Interior
        .Pattern = xlPatternLinearGradient
        .Gradient.Degree = 90
        .Gradient.ColorStops.Clear
    End With
    With Selection.FormatConditions(1).Interior.Gradient.ColorStops.Add(0)
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = 0
    End With
    With Selection.FormatConditions(1).Interior.Gradient.ColorStops.Add(1)
        .Color = 4390113
        .TintAndShade = 0
    End With
    Selection.FormatConditions(1).StopIfTrue = False
    
    Range("status_c").Value = "Validation Fail!!"
    

Else

End If
    

    rn = "bank_ac_n_iban_vf_c" ' name of affected fields
    rv = "bank_ac_n_iban_count_c" 'rule field based on which
    visible_range = "bank_ac_n_iban_count_c"
    
            If Range(visible_range).EntireRow.Hidden = False And Range(rv).Value = "" Then
            Range(rn).Select
            'Call UnPro
            Selection.FormatConditions.Add Type:=xlExpression, Formula1:="=bank_ac_n_iban_count_c="""""
            Selection.FormatConditions(Selection.FormatConditions.Count).SetFirstPriority
            With Selection.FormatConditions(1).Interior
                .Pattern = xlPatternLinearGradient
                .Gradient.Degree = 90
