Private Sub Worksheet_Change(ByVal target As Range)

    If target.Row = 1 And target.Column = 3 Then
            Application.EnableEvents = False
        With Worksheets("|fffd|\|fffd||fffd||fffd||fffd|")
            Select Case .Cells(target.Row, target.Column).Value
                Case "NCHK App No."
                    .Cells(50, 6).Value = "HKD"
                    .Cells(62, 14).Value = "NCHK P."
                    .Cells(62, 16).Value = "NCHK S.A."
                    .Cells(62, 18).Value = "P."
                    .Cells(62, 20).Value = "Manager"
                    .Cells(62, 22).Value = "Applicant"
'                   Call Nitc_On
                Case "NCSS App No."
                    .Cells(50, 6).Value = "USD"
                    .Cells(62, 14).Value = "SA"
                    .Cells(62, 16).Value = "P."
                    .Cells(62, 18).Value = "Manager"
                    .Cells(62, 20).Value = "Sales"
                    .Cells(62, 22).Value = "Coordinator"
'                   Call Nitc_On
            End Select

        End With
        Application.EnableEvents = True
    End If
    
End Sub
