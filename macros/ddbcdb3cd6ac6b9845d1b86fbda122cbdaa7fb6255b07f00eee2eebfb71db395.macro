Sub performCalc2(Optional RunMode As Integer)
    Dim numberMBXServer1, numberMBXServer2, numDAGs, numDAGsA, numDAGsB, x As Integer
    Dim numberMBXServerA1, numberMBXServerA2, numberMBXServerB1, numberMBXServerB2
    Dim SolText As String
    Dim ReCalc As Integer
    Dim ServerCount As Integer
    Dim cpuUtil1, cpuUtilA1, cpuUtilB1, cpuUtil2, cpuUtilA2, cpuUtilB2
    Dim numLUNs, numLUNsA, numLUNsB
    Dim RecMem1, RecMemA1, RecMemB1, RecMem2, RecMemA2, RecMemB2
    Dim SiteResilient As Boolean
    Dim ValidA, ValidB As Boolean
    Dim SpecRateCheck As Boolean

    ReCalc = Application.Calculation
    Application.Calculation = xlCalculationAutomatic

    If ThisWorkbook.Sheets("Input").Range("numSpecRatePDC").Value = 0 Or (ThisWorkbook.Sheets("Input").Range("SiteResilienceEnabled").Value = "Yes" And ThisWorkbook.Sheets("Input").Range("numSpecRateSDC").Value = 0) Then
        x = MsgBox("Cannot calculate DAG until SPECInt2006 values are populated", vbCritical, "Role Calulator")
        SpecRateCheck = False
    Else
        SpecRateCheck = True
    End If
    
'Enabling this will require some though regarding which validation checks to ignore because the auto-seek function will change them anyway.
'    If ThisWorkbook.Sheets("Role Requirements").Range("ValidationCheck").Value Then
'        x = MsgBox("Cannot calculate DAG because the starting configuration is invalid", vbCritical, "Role Calulator")
'    End If
    
    
    If SpecRateCheck And ThisWorkbook.Sheets("Role Requirements").Range("ValidationCheck").Value = False Then

        If ThisWorkbook.Sheets("Input").Range("SiteResilienceEnabled").Value = "Yes" Then
            SiteResilient = True
        Else
            SiteResilient = False
        End If

        ValidB = False
        performCalcB
        If numDAGs >= ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value + 1 Then
            numberMBXServerB1 = MaxServers
            numDAGsB = ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value
        Else
            If ThisWorkbook.Sheets("Role Requirements").Range("NumDAGServersPDC").Value = "--" Then
                numberMBXServerB1 = MaxServers + 1
            Else
                numberMBXServerB1 = ThisWorkbook.Sheets("Role Requirements").Range("NumDAGServersPDC").Value
            End If
            If ThisWorkbook.Sheets("Role Requirements").Range("NumDAGServersSDC").Value = "--" Then
                numberMBXServerB2 = 0
            Else
                numberMBXServerB2 = ThisWorkbook.Sheets("Role Requirements").Range("NumDAGServersSDC").Value
            End If
            numDAGsB = ThisWorkbook.Sheets("Input").Range("numDAGs").Value
            cpuUtilB1 = ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value
            cpuUtilB2 = ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC2").Value
            numLUNsB = ThisWorkbook.Sheets("Volume Requirements").Range("RecDatabaseVols").Value
            RecMemB1 = ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value
            RecMemB2 = ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXSDC").Value
            If numberMBXServerA1 <> 0 Or numberMBXServerA2 <> 0 Then
                ValidB = True
            End If
        End If

        ValidA = False
        If ThisWorkbook.Sheets("Input").Range("OptimizeChoice").Value = "Servers" Then
            performCalcA
            If numDAGs >= ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value + 1 Then
                numberMBXServerA1 = MaxServers
                numDAGsA = ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value
            Else
                If ThisWorkbook.Sheets("Role Requirements").Range("NumDAGServersPDC").Value = "--" Then
                    numberMBXServerA1 = MaxServers + 1
                Else
                    numberMBXServerA1 = ThisWorkbook.Sheets("Role Requirements").Range("NumDAGServersPDC").Value
                End If
                If ThisWorkbook.Sheets("Role Requirements").Range("NumDAGServersSDC").Value = "--" Then
                    numberMBXServerA2 = 0
                Else
                    numberMBXServerA2 = ThisWorkbook.Sheets("Role Requirements").Range("NumDAGServersSDC").Value
                End If
                numDAGsA = ThisWorkbook.Sheets("Input").Range("numDAGs").Value
                cpuUtilA1 = ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value
                cpuUtilA2 = ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC2").Value
                numLUNsA = ThisWorkbook.Sheets("Volume Requirements").Range("RecDatabaseVols").Value
                RecMemA1 = ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value
                RecMemA2 = ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXSDC").Value
                If numberMBXServerA1 <> 0 Or numberMBXServerA2 <> 0 Then
                    ValidA = True
                End If
            End If
        Else
            numberMBXServerA1 = MaxServers
            numDAGsA = ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value
        End If
        
        If ValidA Or ValidB Then
            If (numberMBXServerA1 * numDAGsA + numberMBXServerA2 * numDAGsA) > (numberMBXServerB1 * numDAGsB + numberMBXServerB2 * numDAGsB) Then
                numberMBXServer1 = numberMBXServerB1
                numberMBXServer2 = numberMBXServerB2
                numDAGs = numDAGsB
                cpuUtil1 = cpuUtilB1
                cpuUtil2 = cpuUtilB2
                numLUNs = numLUNsB
                RecMem1 = RecMemB1
                RecMem2 = RecMemB2
            Else
                numberMBXServer1 = numberMBXServerA1
                numberMBXServer2 = numberMBXServerA2
                numDAGs = numDAGsA
                cpuUtil1 = cpuUtilA1
                cpuUtil2 = cpuUtilA2
                numLUNs = numLUNsA
                RecMem1 = RecMemA1
                RecMem2 = RecMemA2
            End If

            If RunMode = 0 Then
                'SolText = "Number of DAGS: " & Chr(9) & numDAGs & Chr(13) & "Servers per DAG: " & Chr(9) & ServerCount & Chr(13) & "RAM per Server: " & Chr(9) & RecMem & Chr(13) & "Disks per Server: " & Chr(9) & numLUNs & Chr(13) & "CPU Utilization: " & Chr(9) & Format(cpuUtil, "00%")
                SolText = Chr(9) & Chr(9) & Chr(9) & "Primary" & Chr(9) & "Secondary" & Chr(13)
                SolText = SolText & "Servers / DAG: " & Chr(9) & Chr(9) & numberMBXServer1 & Chr(9) & numberMBXServer2 & Chr(13)
                SolText = SolText & "RAM / Server: " & Chr(9) & Chr(9) & RecMem1 & Chr(9) & RecMem2 & Chr(13)
                SolText = SolText & "CPU Utilization: " & Chr(9) & Chr(9) & Format(cpuUtil1, "00%") & Chr(9) & Format(cpuUtil1, "00%") & Chr(13)
                SolText = SolText & Chr(13) & "Data Volumes / Server: " & Chr(9) & numLUNs & Chr(13)
                SolText = SolText & "Number of DAGs: " & Chr(9) & Chr(9) & numDAGs
                x = MsgBox(SolText, vbInformation, "DAG Solution Found")
            End If
        Else
            If RunMode = 0 Then
                SolText = "Exceeded maxiumum number of DAGs for goal seek: " & ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value
                x = MsgBox(SolText, vbCritical, "Solution Not Found")
            Else
                ThisWorkbook.Sheets("Input").Range("numDAGs").Value = 999
                ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = 1
            End If
        End If
    End If
    Application.Calculation = ReCalc
End Sub

