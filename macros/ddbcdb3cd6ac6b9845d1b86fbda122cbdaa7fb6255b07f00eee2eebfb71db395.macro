    End If
        
    ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = MaxServers / 2
    ThisWorkbook.Sheets("Input").Range("numDAGs").Value = ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value
            
    ThisWorkbook.Sheets("Role Requirements").Calculate
    ThisWorkbook.Sheets("Volume Requirements").Calculate

    'setup max disks to be used
    If ThisWorkbook.Sheets("Input").Range("DiskLimitOverride").Value = "Yes" Then
        maxDisks = ThisWorkbook.Sheets("Variables").Range("varMaxDisksPerServer").Value
    Else
        maxDisks = ThisWorkbook.Sheets("Input").Range("NumDisksPerServer").Value
    End If
    
    Do 'do the DAG checks then loop if needed
        'setup the counters
    
        numberMBXServer = ThisWorkbook.Sheets("Input").Range("numMBXServers").Value
        numDAGs = ThisWorkbook.Sheets("Input").Range("numDAGs").Value
        numLUNs = ThisWorkbook.Sheets("Volume Requirements").Range("RecDatabaseVols").Value
    
        ThisWorkbook.Sheets("Role Requirements").Calculate
        ThisWorkbook.Sheets("Volume Requirements").Calculate
        
        If SiteResilient Then
            If ((ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value = "--")) Or ((ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC2").Value = "--")) Then
                cpuUtil = ThisWorkbook.Sheets("Variables").Range("varMaxRecCpuUtil").Value + 1
            Else
                If ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value > ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC2").Value Then
                    cpuUtil = ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value
                Else
                    cpuUtil = ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC2").Value
                End If
            End If
            If ((ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value = "--")) Or ((ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXSDC").Value = "--")) Then
                RecMem = 9999
            Else
                If ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value > ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXSDC").Value Then
                    RecMem = ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value
                Else
                    RecMem = ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXSDC").Value
                End If
            End If
        Else
            If (ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value = "--") Then
                cpuUtil = ThisWorkbook.Sheets("Variables").Range("varMaxRecCpuUtil").Value + 1
            Else
                cpuUtil = ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value
            End If
            'setup memory
            If (ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value = "--") Then
                RecMem = 9999
            Else
                RecMem = ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value
            End If
        End If

        If (numLUNs > maxDisks) Or (cpuUtil > ThisWorkbook.Sheets("Variables").Range("varMaxRecCpuUtil").Value) Or (RecMem > MaxMem) Or (ThisWorkbook.Sheets("Role Requirements").Range("ValidationCheck").Value = True) Or (ThisWorkbook.Sheets("Role Requirements").Range("JbodValidationCheck").Value = True) Then
                'Test to see if config fails with max number of DAGs
                If ThisWorkbook.Sheets("Input").Range("numDAGs").Value = ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value Then
                    ThisWorkbook.Sheets("Input").Range("numDAGs").Value = ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value + 1
                    FindDAG = False
                Else
                    If FindDAG Then 'Check to see if still looking for number of DAGs or switched to looking for number of servers
                        If SearchUp Then
                            'Do nothing
                        Else
                            SearchUp = True
                        End If
                        ThisWorkbook.Sheets("Input").Range("numDAGs").Value = numDAGs + 1
                    Else
                        ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = numberMBXServer + 1
                    End If
                End If
        Else
                If FindDAG Then 'Check to see if still looking for number of DAGs or switched to looking for number of servers
                    If SearchUp Then
                        FindDAG = False
                        ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = 1
                    Else 'Passed validation test so decrease number of DAGs
                        Temp1 = numDAGs / 2
                        If Temp1 < 1 Then 'down to just 1 DAG so start searching up
                            ThisWorkbook.Sheets("Input").Range("numDAGs").Value = 1
                            SearchUp = True
                        Else
                            ThisWorkbook.Sheets("Input").Range("numDAGs").Value = Round(Temp1, 0)
                        End If
                    End If
                Else
                    Optimized = True
                End If
        End If
    
    Loop While (Optimized = False Or FindDAG = True) And numDAGs < ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value + 1
End Sub

Public Sub RunTimer()
    Dim t As String
    Dim d As String
    Dim x As String
    d = Date
    t = Time

    If TimerFile.Number > 0 Then
        Write #TimerFile.Number, "  EndTime: ", t, vbCrLf
        Close TimerFile.Number
        TimerFile.Number = 0
    Else
        TimerFile.Name = "Timer.txt"
        TimerFile.Number = FreeFile()
        On Error Resume Next
        If FileStatus(TimerFile.Name) <> -53 Then
            Open TimerFile.Name For Append As #TimerFile.Number
        Else
            Open TimerFile.Name For Output As #TimerFile.Number
        End If
            If Err.Number = 0 Then
                Write #TimerFile.Number, "StartTime: ", t
            Else
                If Err.Number = 55 Then
                    On Error GoTo 0
                    TimerFile.Number = 0
                    x = MsgBox("Only one instance of Role Calculator can be open at a time when in debug mode.  Current value for varDebugLevel is: " & DebugLevel & ". The Timer.txt file was not written.", vbCritical, "Debug Level")
                Else
                    x = Err.Number
                    On Error GoTo 0
                    Err.Raise x
                End If
            End If
    End If
End Sub

Sub Wait(Seconds As Variant)

    Dim Done As Variant
    
    Done = Timer + Seconds
    While Timer < Done
        DoEvents
    Wend
End Sub

