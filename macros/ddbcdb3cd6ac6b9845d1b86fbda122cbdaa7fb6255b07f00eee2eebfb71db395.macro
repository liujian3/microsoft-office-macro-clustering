Public Function FileStatus(ItemName As String) As Integer
    Dim FileExists As Boolean
    Dim objXML
    Dim x As Integer
    Dim DirName As String
    
    If DebugLevel > DebugMedium Then Call PushStack(38, ItemName)
    If Len(ItemName) > 0 Then
        On Error Resume Next
        Err.Clear
        If LCase(Left(ItemName, 8)) = "https://" Or LCase(Left(ItemName, 7)) = "http://" Then
            CloudSource = True
            Set objXML = CreateObject("Microsoft.XMLHTTP")
            objXML.Open "HEAD", ItemName, False
            objXML.Send ("")
            FileStatus = objXML.Status '200 = found, 404 = not found
        Else
            CloudSource = False
            FileStatus = GetAttr(ItemName)
            If Err.Number <> 0 Then
                FileStatus = -9998
                If Err.Number = 53 Then FileStatus = -53 'File not found or last directory not found
                If Err.Number = 76 Then FileStatus = -76 'Path not found
                Err.Clear
            End If
        End If
    Else
        FileStatus = -9999
    End If
    On Error GoTo 0
    If DebugLevel > DebugMedium Then Call PopStack
End Function

