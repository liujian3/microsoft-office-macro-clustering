    Dim Check As Variant
    Check = Left(Range("'Variables'!varEnableTrace").Value, 1)
    If UCase(Check) = "Y" Then
        If EnableTrace = False Then
            Call StartTrace
            EnableTrace = True
        End If
        Call Trace("CheckTrace", "TraceEnabled")
    Else
        If EnableTrace Then
            Call Trace("CheckTrace", "TracePause")
            EnableTrace = False
            Call EndTrace
        End If
    End If
End Sub

Public Function GetDBperVolIndex(Index As Integer, Site As Integer) As Integer
    Dim Copies As Integer
    If DebugLevel > DebugHigh Then Call PushStack(47, Str(Index) & ";" & Str(Site))
    If Application.Calculation <> xlCalculationManual Then
        If ThisWorkbook.Sheets("Variables").Range("varSupportAutoReseed").Value = "Yes" Then
            If Site > 1 Then
                'Second Site
                If ThisWorkbook.Sheets("Input").Range("DedLaggedCopyServer").Value = "Yes" Then
                    Copies = ThisWorkbook.Sheets("Role Requirements").Range("calcNumDBCopyInSDCActual").Value
                Else
                    Copies = ThisWorkbook.Sheets("Role Requirements").Range("calcNumDBCopyInSDCActual").Value + ThisWorkbook.Sheets("Role Requirements").Range("calcNumLagCopyInSDCActual").Value
                End If
            Else
                'First Site
                If ThisWorkbook.Sheets("Input").Range("DedLaggedCopyServer").Value = "Yes" Then
                    Copies = ThisWorkbook.Sheets("Role Requirements").Range("calcNumDBCopyInPDCActual").Value
                Else
                    Copies = ThisWorkbook.Sheets("Role Requirements").Range("calcNumDBCopyInPDCActual").Value + ThisWorkbook.Sheets("Role Requirements").Range("calcNumLagCopyInPDCActual").Value
                End If
            End If
        Else
            Copies = 1
        End If
        GetDBperVolIndex = Copies * Index
        If Index = 0 Then
            GetDBperVolIndex = 1
        Else
            If Copies = 1 Then
                GetDBperVolIndex = GetDBperVolIndex + 1
            End If
        End If
        If GetDBperVolIndex > ThisWorkbook.Sheets("Variables").Range("varMaxDBperVolume").Value Then
            GetDBperVolIndex = 0
        End If
    End If
    If DebugLevel > DebugHigh Then Call PopStack
End Function

Sub performCalc(Optional RunMode As Integer)
    Dim cpuUtil As Double
    Dim Optimized As Boolean
    Dim numberMBXServer, numDAGs, numLUNs, maxDisks, RecMem, MaxMem, x As Integer
    Dim SolText As String
    Dim ReCalc As Integer

    ReCalc = Application.Calculation
    Application.Calculation = xlCalculationAutomatic

    If ThisWorkbook.Sheets("Input").Range("numSpecRatePDC").Value = 0 Or ThisWorkbook.Sheets("Input").Range("numSpecRateSDC").Value = 0 Then
        x = MsgBox("Cannot calculate DAG until SPECInt2006 values are populated", vbCritical, "Role Calulator")
    Else
        Optimized = False
        x = 1
        If ThisWorkbook.Sheets("Input").Range("LimitServerMemory").Value = "Yes" Then
        If ThisWorkbook.Sheets("Input").Range("ServerVersion").Value = "2013" Then
            MaxMem = ThisWorkbook.Sheets("Variables").Range("varMaxRecMemory2013").Value
        Else
            MaxMem = ThisWorkbook.Sheets("Variables").Range("varMaxRecMemory").Value
        End If
        Else
            MaxMem = ThisWorkbook.Sheets("Input").Range("NumRamPerServer").Value
        End If
        ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = 1
        ThisWorkbook.Sheets("Input").Range("numDAGs").Value = 1

        Do 'do the DAG checks then loop if needed
            'setup the counters
        
            numberMBXServer = ThisWorkbook.Sheets("Input").Range("numMBXServers").Value
            numDAGs = ThisWorkbook.Sheets("Input").Range("numDAGs").Value
            numLUNs = ThisWorkbook.Sheets("Volume Requirements").Range("RecDatabaseVols").Value
        
            'cpuUtil = Workbooks(calcWrkBook).Sheets("Role Requirements").Range("C238").Value
            If (ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value = "--") Then
                cpuUtil = ThisWorkbook.Sheets("Variables").Range("varMaxRecCpuUtil").Value + 1
            Else
                cpuUtil = ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value
            End If
            'setup max disks to be used
            If ThisWorkbook.Sheets("Input").Range("DiskLimitOverride").Value = "Yes" Then
                maxDisks = ThisWorkbook.Sheets("Variables").Range("varMaxDisksPerServer").Value
            Else
                maxDisks = ThisWorkbook.Sheets("Input").Range("NumDisksPerServer").Value
            End If
            'setup memory
            If (ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value = "--") Then
                RecMem = 9999
            Else
                RecMem = ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value
            End If
        
            ThisWorkbook.Sheets("Role Requirements").Calculate
            ThisWorkbook.Sheets("Volume Requirements").Calculate

            If (numLUNs > maxDisks) Or (cpuUtil > ThisWorkbook.Sheets("Variables").Range("varMaxRecCpuUtil").Value) Or (RecMem > MaxMem) Or (ThisWorkbook.Sheets("Role Requirements").Range("ValidationCheck").Value = True) Or (ThisWorkbook.Sheets("Role Requirements").Range("JbodValidationCheck").Value = True) Then
                If ThisWorkbook.Sheets("Input").Range("OptimizeChoice").Value = "DAGs" Then
                    If (numberMBXServer >= 8) Then
                            ThisWorkbook.Sheets("Input").Range("numDAGs").Value = numDAGs + 1 'increment the number of DAGs by one
                            ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = 2 'start the # server back to 2 since we just incremented the DAG count
                    Else
                        ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = numberMBXServer + 1
                    End If
                Else
                    ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = WorksheetFunction.Index(ThisWorkbook.Sheets("Tables").Range("TblOptimizeServers"), x, 1)
                    ThisWorkbook.Sheets("Input").Range("numDAGs").Value = WorksheetFunction.Index(ThisWorkbook.Sheets("Tables").Range("TblOptimizeServers"), x, 2)
                    x = x + 1
                End If
            
            Else
                Optimized = True
            End If
        
        Loop While Optimized = False And numDAGs < ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value
        If numDAGs < ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value Then
            If RunMode = 0 Then
                SolText = "Number of DAGS: " & Chr(9) & numDAGs & Chr(13) & "Servers per DAG: " & Chr(9) & numberMBXServer & Chr(13) & "RAM per Server: " & Chr(9) & RecMem & Chr(13) & "Data Volumes per Server: " & Chr(9) & numLUNs & Chr(13) & "CPU Utilization: " & Chr(9) & Format(cpuUtil, "00%")
                x = MsgBox(SolText, vbInformation, "DAG Solution Found")
            End If
        Else
            If RunMode = 0 Then
                SolText = "Exceeded maxiumum number of DAGs for goal seek: " & ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value
                x = MsgBox(SolText, vbCritical, "Solution Not Found")
            Else
                ThisWorkbook.Sheets("Input").Range("numDAGs").Value = 999
                ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = 1
            End If
        End If
    End If
    Application.Calculation = ReCalc
End Sub
Sub MakeDAGOptions()
    Dim MinServers As Integer
    Dim TableName As String
    Dim Pointer, TablePointer As Integer
    
    Erase DAGOptions
    Erase ServerOptions
    MaxPointer = 0
    MinServers = ThisWorkbook.Sheets("Role Requirements").Range("calcTotNumHACopies").Value + ThisWorkbook.Sheets("Role Requirements").Range("calcTotNumLagCopies").Value
    If ThisWorkbook.Sheets("Input").Range("SiteResilienceEnabled").Value = "Yes" Then
        TableName = "Tbl2OptimizeServers"
    Else
        TableName = "Tbl1OptimizeServers"
    End If
    Pointer = 0
    
    For TablePointer = 1 To ThisWorkbook.Sheets("Tables").Range(TableName).Rows.Count
        If ThisWorkbook.Sheets("Tables").Range(TableName)(TablePointer, 1) >= MinServers Then
            DAGOptions(Pointer) = ThisWorkbook.Sheets("Tables").Range(TableName)(TablePointer, 2)
            ServerOptions(Pointer) = ThisWorkbook.Sheets("Tables").Range(TableName)(TablePointer, 1)
            Pointer = Pointer + 1
        End If
    Next TablePointer
    MaxPointer = Pointer - 1
End Sub

Sub performCalc3(Optional RunMode As Integer)
    Dim cpuUtil As Double
    Dim Optimized As Boolean
    Dim numberMBXServer, numDAGs, LastNumberMBXServer, LastNumDAGs, numLUNs, maxDisks, RecMem, MaxMem, x As Integer
    Dim SolText As String
    Dim ReCalc, FirstRow, LastRow, CurrentRow As Integer
    Dim FindDAG, SearchUp As Boolean
    Dim Temp1, Temp2 As Variant
    Dim TableName As String
    Dim MinServers, ServerCount As Integer
    
    ReCalc = Application.Calculation
    Application.Calculation = xlCalculationAutomatic

    If ThisWorkbook.Sheets("Input").Range("numSpecRatePDC").Value = 0 Or (ThisWorkbook.Sheets("Input").Range("SiteResilienceEnabled").Value = "Yes" And ThisWorkbook.Sheets("Input").Range("numSpecRateSDC").Value = 0) Then
        x = MsgBox("Cannot calculate DAG until SPECInt2006 values are populated", vbCritical, "Role Calulator")
    Else
        FindDAG = True
        SearchUp = False
        Optimized = False
        
        If ThisWorkbook.Sheets("Input").Range("SiteResilienceEnabled").Value = "Yes" Then
            LastNumberMBXServer = MaxServers / 2
            TableName = "Tbl2OptimizeServers"
            MinServers = ThisWorkbook.Sheets("Role Requirements").Range("calcNumDBCopyInPDC").Value + ThisWorkbook.Sheets("Role Requirements").Range("calcNumDBCopyInSDC").Value
        Else
            LastNumberMBXServer = MaxServers
            TableName = "Tbl1OptimizeServers"
            MinServers = ThisWorkbook.Sheets("Role Requirements").Range("calcNumDBCopyInPDC").Value
        End If
        
        LastNumDAGs = ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value

        MakeDAGOptions

'        x = 1
        FirstRow = 1
        LastRow = ThisWorkbook.Sheets("Tables").Range(TableName).Rows.Count
        CurrentRow = (LastRow - FirstRow) / 2
        If ThisWorkbook.Sheets("Input").Range("LimitServerMemory").Value = "Yes" Then
            If ThisWorkbook.Sheets("Input").Range("ServerVersion").Value = "2013" Then
                MaxMem = ThisWorkbook.Sheets("Variables").Range("varMaxRecMemory2013").Value
            Else
                MaxMem = ThisWorkbook.Sheets("Variables").Range("varMaxRecMemory").Value
            End If
        Else
            MaxMem = ThisWorkbook.Sheets("Input").Range("NumRamPerServer").Value
        End If
        If ThisWorkbook.Sheets("Input").Range("OptimizeChoice").Value = "DAGs" Then 'Optimizing for fewest DAGs
            ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = MaxServers / 2
            ThisWorkbook.Sheets("Input").Range("numDAGs").Value = ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value
        Else 'Optimizing for fewest servers
