                maxDisks = ThisWorkbook.Sheets("Variables").Range("varMaxDisksPerServer").Value
            Else
                maxDisks = ThisWorkbook.Sheets("Input").Range("NumDisksPerServer").Value
            End If
            'setup memory
            If (ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value = "--") Then
                RecMem = 9999
            Else
                RecMem = ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value
            End If
        
            ThisWorkbook.Sheets("Role Requirements").Calculate
            ThisWorkbook.Sheets("Volume Requirements").Calculate

            If (numLUNs > maxDisks) Or (cpuUtil > ThisWorkbook.Sheets("Variables").Range("varMaxRecCpuUtil").Value) Or (RecMem > MaxMem) Or (ThisWorkbook.Sheets("Role Requirements").Range("ValidationCheck").Value = True) Or (ThisWorkbook.Sheets("Role Requirements").Range("JbodValidationCheck").Value = True) Then
                If ThisWorkbook.Sheets("Input").Range("OptimizeChoice").Value = "DAGs" Then 'Optimizing for fewest DAGs
                    'Test to see if config fails with max number of DAGs
                    If ThisWorkbook.Sheets("Input").Range("numDAGs").Value = ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value Then
                        ThisWorkbook.Sheets("Input").Range("numDAGs").Value = ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value + 1
                        FindDAG = False
                    Else
                        If FindDAG Then 'Check to see if still looking for number of DAGs or switched to looking for number of servers
                            If SearchUp Then
                                'Do nothing
                            Else
                                SearchUp = True
                            End If
                            ThisWorkbook.Sheets("Input").Range("numDAGs").Value = numDAGs + 1
                        Else
                            ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = numberMBXServer + 1
                        End If
                    End If
                Else 'Optimizing for fewest servers
                    'Test to see if config fails with max number of DAGs
                    If CurrentRow = ThisWorkbook.Sheets("Tables").Range(TableName).Rows.Count Or FirstRow = ThisWorkbook.Sheets("Tables").Range(TableName).Rows.Count - 1 Then
                        numDAGs = ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value + 1
                    Else
                        If FirstRow = CurrentRow Then 'Found the answer
                            Optimized = True
                            FindDAG = False
                        Else
                            FirstRow = CurrentRow
                            CurrentRow = FirstRow + ((LastRow - FirstRow) / 2)
                            While ThisWorkbook.Sheets("Tables").Range(TableName)(CurrentRow, 1) < MinServers
                                CurrentRow = CurrentRow + 1
                            Wend
                            ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = ThisWorkbook.Sheets("Tables").Range(TableName)(CurrentRow, 1)
                            ThisWorkbook.Sheets("Input").Range("numDAGs").Value = ThisWorkbook.Sheets("Tables").Range(TableName)(CurrentRow, 2)
                        End If
                    End If
                End If
            Else
                If ThisWorkbook.Sheets("Input").Range("OptimizeChoice").Value = "DAGs" Then 'Optimizing for fewest DAGs
                    If FindDAG Then 'Check to see if still looking for number of DAGs or switched to looking for number of servers
                        If SearchUp Then
                            FindDAG = False
                            ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = 1
                        Else 'Passed validation test so decrease number of DAGs
                            Temp1 = numDAGs / 2
                            If Temp1 < 1 Then 'down to just 1 DAG so start searching up
                                ThisWorkbook.Sheets("Input").Range("numDAGs").Value = 1
                                SearchUp = True
                            Else
                                ThisWorkbook.Sheets("Input").Range("numDAGs").Value = Round(Temp1, 0)
                            End If
                        End If
                    Else
                        Optimized = True
                    End If
                Else 'Optimizing for fewest servers
                    If LastRow = CurrentRow Then 'Found the answer
                        Optimized = True
                        FindDAG = False
                    Else
                        LastRow = CurrentRow
                        CurrentRow = FirstRow + ((LastRow - FirstRow) / 2)
                        While ThisWorkbook.Sheets("Tables").Range(TableName)(CurrentRow, 1) < MinServers
                            CurrentRow = CurrentRow + 1
                        Wend
                        ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = ThisWorkbook.Sheets("Tables").Range(TableName)(CurrentRow, 1)
                        ThisWorkbook.Sheets("Input").Range("numDAGs").Value = ThisWorkbook.Sheets("Tables").Range(TableName)(CurrentRow, 2)
                    End If

'                    ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = WorksheetFunction.Index(ThisWorkbook.Sheets("Tables").Range("TblOptimizeServers"), x, 1)
'                    ThisWorkbook.Sheets("Input").Range("numDAGs").Value = WorksheetFunction.Index(ThisWorkbook.Sheets("Tables").Range("TblOptimizeServers"), x, 2)
'                    x = x + 1
                End If
            End If
        
        Loop While (Optimized = False Or FindDAG = True) And numDAGs < ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value + 1
        If numDAGs < ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value Then
            If RunMode = 0 Then
                If ThisWorkbook.Sheets("Input").Range("SiteResilienceEnabled").Value = "Yes" Then
                    ServerCount = numberMBXServer * 2
                Else
                    ServerCount = numberMBXServer
                End If
                SolText = "Number of DAGS: " & Chr(9) & numDAGs & Chr(13) & "Servers per DAG: " & Chr(9) & ServerCount & Chr(13) & "RAM per Server: " & Chr(9) & RecMem & Chr(13) & "Data Volumes per Server: " & Chr(9) & numLUNs & Chr(13) & "CPU Utilization: " & Chr(9) & Format(cpuUtil, "00%")
                x = MsgBox(SolText, vbInformation, "DAG Solution Found")
            End If
        Else
            If RunMode = 0 Then
                SolText = "Exceeded maxiumum number of DAGs for goal seek: " & ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value
                x = MsgBox(SolText, vbCritical, "Solution Not Found")
            Else
                ThisWorkbook.Sheets("Input").Range("numDAGs").Value = 999
                ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = 1
            End If
        End If
    End If
    Application.Calculation = ReCalc
End Sub

