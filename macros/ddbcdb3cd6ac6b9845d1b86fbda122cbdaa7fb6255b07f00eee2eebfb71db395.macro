Public Function SaveFile(Filebuffer As String, FilePath As String) As Boolean
    Dim FileExists As Boolean
    Dim objXML
    Dim x As Integer
    Dim TargetFile As Object
    Dim fs As Object
    Dim Stat As Integer
    Dim Overwrite As Boolean

    If DebugLevel > DebugMedium Then Call PushStack(49, FilePath & "Size: " & Len(Filebuffer))
    
    Stat = FileStatus(FilePath)
    Overwrite = True
    If (CloudSource = True And Stat = 200) Or (CloudSource = False And Stat <> -53) Then
            FileExists = True
            x = MsgBox(FilePath & " already exists." & vbCrLf & vbCrLf & "Do you want to overwrite it?", vbYesNo, "Role Calculator - Export Scripts")
            If x = vbYes Then
                Overwrite = True
            Else
                Overwrite = False
            End If
        Else
            FileExists = False
    End If
    If Overwrite Then
        If CloudSource Then
            Set objXML = CreateObject("Microsoft.XMLHTTP")
            If FileExists Then
                objXML.Open "DELETE", FilePath, False
                objXML.Send ("")
                If (objXML.Status <> 200) And (objXML.Status <> 204) Then
                    Overwrite = False
                End If
            End If
            If Overwrite Then
                objXML.Open "PUT", FilePath, False
                objXML.Send Filebuffer
                If objXML.Status = 201 Then
                    SaveFile = True
                Else
                    SaveFile = False
                End If
            Else
                x = MsgBox("Failure to overwrite. File: " & FilePath, vbCritical, "Role Calculator File Save")
            End If
            Set objXML = Nothing
        Else
            Set fs = CreateObject("Scripting.FileSystemObject")
            Set TargetFile = fs.CreateTextFile(FilePath, True)
            TargetFile.Write (Filebuffer)
            TargetFile.Close
            Set fs = Nothing
            SaveFile = True
        End If
    Else
        x = MsgBox("File save canceled. File: " & FilePath, vbExclamation, "Role Calculator File Save")
    End If
    If DebugLevel > DebugMedium Then Call PopStack

