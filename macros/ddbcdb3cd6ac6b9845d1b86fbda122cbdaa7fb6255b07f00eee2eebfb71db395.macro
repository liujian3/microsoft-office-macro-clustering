    ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = ThisWorkbook.Sheets("Tables").Range(TableName)(CurrentRow, 1)
    ThisWorkbook.Sheets("Input").Range("numDAGs").Value = ThisWorkbook.Sheets("Tables").Range(TableName)(CurrentRow, 2)
            
    ThisWorkbook.Sheets("Role Requirements").Calculate
    ThisWorkbook.Sheets("Volume Requirements").Calculate

    'setup max disks to be used
    If ThisWorkbook.Sheets("Input").Range("DiskLimitOverride").Value = "Yes" Then
        maxDisks = ThisWorkbook.Sheets("Variables").Range("varMaxDisksPerServer").Value
    Else
        maxDisks = ThisWorkbook.Sheets("Input").Range("NumDisksPerServer").Value
    End If

    
    Do 'do the DAG checks then loop if needed
        'setup the counters
    
        numberMBXServer = ThisWorkbook.Sheets("Input").Range("numMBXServers").Value
        numDAGs = ThisWorkbook.Sheets("Input").Range("numDAGs").Value
        numLUNs = ThisWorkbook.Sheets("Volume Requirements").Range("RecDatabaseVols").Value
    
        ThisWorkbook.Sheets("Role Requirements").Calculate
        ThisWorkbook.Sheets("Volume Requirements").Calculate
        
        If SiteResilient Then
            If ((ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value = "--")) Or ((ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC2").Value = "--")) Then
                cpuUtil = ThisWorkbook.Sheets("Variables").Range("varMaxRecCpuUtil").Value + 1
            Else
                If ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value > ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC2").Value Then
                    cpuUtil = ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value
                Else
                    cpuUtil = ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC2").Value
                End If
            End If
            If ((ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value = "--")) Or ((ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXSDC").Value = "--")) Then
                RecMem = 9999
            Else
                If ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value > ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXSDC").Value Then
                    RecMem = ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value
                Else
                    RecMem = ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXSDC").Value
                End If
            End If
        Else
            If (ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value = "--") Then
                cpuUtil = ThisWorkbook.Sheets("Variables").Range("varMaxRecCpuUtil").Value + 1
            Else
                cpuUtil = ThisWorkbook.Sheets("Role Requirements").Range("CpuUtilizationDC1").Value
            End If
            'setup memory
            If (ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value = "--") Then
                RecMem = 9999
            Else
                RecMem = ThisWorkbook.Sheets("Role Requirements").Range("RecRAMMBXPDC").Value
            End If
        End If

        If (numLUNs > maxDisks) Or _
           (cpuUtil > ThisWorkbook.Sheets("Variables").Range("varMaxRecCpuUtil").Value) Or _
           (RecMem > MaxMem) Or _
           (ThisWorkbook.Sheets("Role Requirements").Range("ValidationCheck").Value = True) Or _
           (ThisWorkbook.Sheets("Role Requirements").Range("JbodValidationCheck").Value = True) Then
            'Test to see if config fails with max number of DAGs
            If CurrentRow = MaxPointer Or FirstRow = MaxPointer - 1 Then
                numDAGs = ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value + 1
            Else
                If FirstRow = CurrentRow Then 'Found the answer
                    FirstRow = FirstRow + 1
                    CurrentRow = CurrentRow + 1
                    LastRow = LastRow + 1
                Else
                    FirstRow = CurrentRow
                    CurrentRow = FirstRow + Round((LastRow - FirstRow) / 2 + 0.5, 0)
                End If
                ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = ServerOptions(CurrentRow)
                ThisWorkbook.Sheets("Input").Range("numDAGs").Value = DAGOptions(CurrentRow)
            End If
        Else
            If LastRow = CurrentRow Then 'Found the answer
                Optimized = True
                FindDAG = False
            Else
                LastRow = CurrentRow
                CurrentRow = FirstRow + ((LastRow - FirstRow) / 2)
                ThisWorkbook.Sheets("Input").Range("numMBXServers").Value = ServerOptions(CurrentRow)
                ThisWorkbook.Sheets("Input").Range("numDAGs").Value = DAGOptions(CurrentRow)
            End If
        End If
    
    Loop While (Optimized = False Or FindDAG = True) And numDAGs < ThisWorkbook.Sheets("Variables").Range("VarMaxDags").Value + 1
End Sub

Sub performCalcB(Optional RunMode As Integer)
    Dim cpuUtil As Double
