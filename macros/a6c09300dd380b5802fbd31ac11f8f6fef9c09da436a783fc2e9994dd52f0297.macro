Sub Restart()
    ActivePresentation.SlideShowWindow.View.Slide.Shapes("results").TextFrame.TextRange.Text = ""
    ActivePresentation.SlideShowWindow.View.Slide.Shapes("scorebox").TextFrame.TextRange.Text = ""
    ActivePresentation.SlideShowWindow.View.Slide.Shapes("scorebox2").TextFrame.TextRange.Text = ""
    ActivePresentation.SlideShowWindow.View.First
End Sub
Sub Results()
    ActivePresentation.SlideShowWindow.View.Last
    On Error GoTo err1
    If AddToThePoints(0) > AddToThePoints2(0) Then
        ActivePresentation.SlideShowWindow.View.Slide.Shapes("results").TextFrame.TextRange.Text = "Player 1 Wins!!"
    End If
    If AddToThePoints(0) < AddToThePoints2(0) Then
        ActivePresentation.SlideShowWindow.View.Slide.Shapes("results").TextFrame.TextRange.Text = "Player 2 Wins!!"
    End If
    If AddToThePoints(0) = AddToThePoints2(0) Then
        ActivePresentation.SlideShowWindow.View.Slide.Shapes("results").TextFrame.TextRange.Text = "It's a TIE!!"
    End If
    Exit Sub
err1:

End Sub









