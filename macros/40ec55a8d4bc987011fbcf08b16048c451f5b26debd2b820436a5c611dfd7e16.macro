Sub SetProtect()

Dim Response As String, Answer As Integer, ws As Worksheet
Response = InputBox("Enter password")
    For Each ws In Worksheets
SetProtect:
    If Response = "H0using" Then
        ws.Protect Response
        Else
    MsgBox "Incorrect Password"
    End If
    ws.Protect Password:="H0using", AllowFormattingCells:=True, AllowFormattingColumns:=True, AllowFormattingRows:=True
    Next ws
End Sub

Sub SetUnprotect()

Dim Response As String, Answer As Integer, ws As Worksheet
Response = InputBox("Enter password")
For Each ws In Worksheets
    If Response = "H0using" Then
ws.Unprotect "H0using"
    End If
    Next ws
End Sub

