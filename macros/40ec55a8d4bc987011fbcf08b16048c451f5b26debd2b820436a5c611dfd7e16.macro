Attribute VB_Name = "Module3"
'This module is used for hiding/unhiding lines and tabs based on what source of funding is being used.


Sub ECShowAll()

' The code below (as well as at the end of the macro) is to prevent the user from seeing the changes as they happen.

    Application.ScreenUpdating = False
    
' ECShowAll Macro

'Un-hides all rows and columns on exhibit checklist

    Sheets("Exhibit Checklist").Select
    Cells.Select
    Selection.EntireRow.Hidden = False
    Selection.EntireColumn.Hidden = False


'Un-hide all sections of Application Tab

    Sheets("Application").Activate
    Cells.Select
    Selection.EntireRow.Hidden = False
    Range("A1").Select
    

'Un-hide all sheets in workbook.
    
    Dim ws As Worksheet
    For Each ws In ActiveWorkbook.Worksheets
    ws.Visible = xlSheetVisible
    Next ws
    
    Sheets("Drop Downs").Select
    ActiveSheet.Visible = False

    
'This section will add page breaks above the specified row(s) on the Exhibit Checklist tab

    Worksheets("Exhibit Checklist").ResetAllPageBreaks
    Worksheets("Exhibit Checklist").Range("ECSection6").PageBreak = xlPageBreakManual
    Worksheets("Exhibit Checklist").Range("ECSection11").PageBreak = xlPageBreakManual
    
    
'The code below shows that this button was the last to be selected on exhibit checklist
    Sheets("Exhibit Checklist").Select
    Range("K11:AD11").Select
    ActiveCell = "Show All"
    
'The code below removes the highlighted color from selected buttons
    Sheets("Exhibit Checklist").Select
    Range("I14:AF14").Select
    With Selection.Interior
        .Pattern = xlNone
        .TintAndShade = 0
        .PatternTintAndShade = 0
    End With

'This section will add page breaks above the specified row(s) on the application tab

    Worksheets("Application").ResetAllPageBreaks
    Worksheets("Application").Range("AppFinancialStatements").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppRangeSection4").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppDevelopmentScope").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppHistoricPreservationCompliance").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppSiteControlandInformation").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppZoning").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppPhaseI").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppDevelopmentDesign").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppReductionofaffordableunits").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppRangeSection5").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppRangeSection6").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppExistingReserves").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppRangeSection8").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppRangeSection11").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppRangeSection12").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppRangeSection14").PageBreak = xlPageBreakManual
    Worksheets("Application").Range("AppSignatureSection").PageBreak = xlPageBreakManual
    
'The code below selects the top left cell of the exhibit checklist
    
    Sheets("Exhibit Checklist").Select
    Range("B14").Select
    ActiveWindow.SmallScroll Down:=-246
     
'The code below (as well as that at the beginning of the macro) is to prevent changes from being seen by the user.
    
    Application.ScreenUpdating = True

End Sub
