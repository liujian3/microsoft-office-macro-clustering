Public Sub OnRefreshBeforeReports()
    Dim DateFrom As String: DateFrom = Range("DateFrom")
    Dim dateto As String: dateto = Range("dateto")
    Dim DateFrom2 As String: DateFrom2 = Range("DateFrom2")
    Dim dateto2 As String: dateto2 = Range("dateto2")
    Dim AfterEntry As Integer: AfterEntry = Range("AfterEntry")
    Dim AAA As String
    Dim Faktor As Integer: Faktor = Range("faktor")
    
    If AfterEntry = 0 Then AAA = " 0 <> a.verwerknrl and" Else AAA = ""
    
    Range("stm") = _
   "select b.freefield1 as kod, sum(bdr_hfl)/" & Faktor & " as summa from gbkmut a inner join grtbk b on a.reknr=b.reknr where " & AAA & " ((a.docdate is not null and a.docdate >=convert(datetime,'" & DateFrom & "', 101) and a.docdate <=convert(datetime,'" & dateto & "', 101)) or (a.docdate is null and a.datum >=convert(datetime,'" & DateFrom & "', 101) and a.datum <=convert(datetime,'" & dateto & "', 101))) and b.freefield1 is not null and (a.transtype='N' or a.transtype='C' ) group by b.freefield1 order by b.freefield1 asc"
   Range("stm2") = _
   "select b.freefield1 as kod, sum(bdr_hfl)/" & Faktor & " as summa from gbkmut a inner join grtbk b on a.reknr=b.reknr where " & AAA & " ((a.docdate is not null and a.docdate >=convert(datetime,'" & DateFrom2 & "', 101) and a.docdate <=convert(datetime,'" & dateto2 & "', 101)) or (a.docdate is null and a.datum >=convert(datetime,'" & DateFrom2 & "', 101) and a.datum <=convert(datetime,'" & dateto2 & "', 101))) and b.freefield1 is not null and (a.transtype='N' or a.transtype='C' ) group by b.freefield1 order by b.freefield1 asc"

End Sub

