Sub TransformSelectedEquations()
' |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd|
' "|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|" |fffd||fffd||fffd||fffd||fffd||fffd||fffd|, |fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd| |fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|
    
On Error Resume Next
    
Dim F As Word.Field
Dim N1 As Long, n2 As Long, N3 As Long
Dim percent As String
Dim tempP

    Application.ScreenUpdating = False
    N1 = 0: n2 = 0: N3 = ActiveDocument.Fields.Count
    percent = ""
    
'    For Each F In ActiveDocument.Fields
    For Each F In Selection.Fields
        If F.Type <> Word.wdFieldEmbed Then
        ElseIf Not (F.OLEFormat.ClassType Like "Equation.*") Then
        Else
            N1 = N1 + 1
            Err.Clear
            F.OLEFormat.ConvertTo ClassType:=F.OLEFormat.ClassType
            
            ' |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd| |fffd| |fffd||fffd||fffd||fffd||fffd||fffd| 100%, |fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd|.
            F.InlineShape.ScaleWidth = 100
            F.InlineShape.ScaleHeight = 100
            
            If Err.Number <> 0 Then ' |fffd||fffd||fffd||fffd||fffd||fffd|
                n2 = n2 + 1
                Application.ScreenUpdating = True
                F.Select
                Select Case MsgBox(Prompt:="|fffd||fffd||fffd||fffd||fffd||fffd|! " & vbLf & vbLf & _
                                           "|fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd|: " & vbLf & _
                                           F.OLEFormat.ClassType & vbLf & vbLf & _
                                           "|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|?", _
                                   Buttons:=VBA.vbCritical + VBA.vbYesNo)
                    Case VBA.vbOK, VBA.vbYes
                    Case Else: Exit For
                End Select
                Application.ScreenUpdating = False
            End If
        End If
        
        tempP = Format(N1 / N3 * 100, "0")
        
        If Not percent = tempP Then
            percent = tempP
            StatusBar = "|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|: " & percent & "%"
        End If
        
    Next F
