Private Sub Worksheet_Calculate()
If ActiveSheet.AutoFilterMode Then
Application.Calculation = xlManual
ThisWorkbook.Sheets(ActiveSheet.Name).EnableCalculation = False
CalcDisabled = True
Else
CalcDisabled = False
End If

End Sub

Private Sub Worksheet_SelectionChange(ByVal Target As Range)
If CalcDisabled = False Then
Application.Calculation = xlCalculationAutomatic
ThisWorkbook.Sheets(ActiveSheet.Name).EnableCalculation = True
End If

If ActiveSheet.AutoFilterMode Then
Application.Calculation = xlManual
ThisWorkbook.Sheets(ActiveSheet.Name).EnableCalculation = False
CalcDisabled = True
Else
CalcDisabled = False
End If
End Sub
