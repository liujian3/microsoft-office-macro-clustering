Private Sub Workbook_Open()
    Dim myWorksheet As Worksheet
    For Each myWorksheet In ThisWorkbook.Worksheets
        myWorksheet.Protect Password:="makro", userinterfaceonly:=True
        myWorksheet.EnableOutlining = True 'f|fffd|r Gliederung
        myWorksheet.EnableAutoFilter = True 'f|fffd|r Autofilter
    Next myWorksheet
End Sub

Attribute VB_Name = "Tabelle1"
Attribute VB_Base = "0{00020820-0000-0000-C000-000000000046}"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Attribute VB_TemplateDerived = False
Attribute VB_Customizable = True
Private Sub Worksheet_Change(ByVal Target As Range)
'** Mehrfachauswahl |fffd|ber DropDown-Liste (G|fffd|ltigkeitspr|fffd|fung)
'** Einf|fffd|gen im Code-Container des betreffenden Arbeitsblattes

'** Dimensionierung der Variablen
Dim rngDV As Range
Dim wert_old As String
Dim wertnew As String

'** Errorhandling
On Error GoTo Errorhandling

'** Mehrfachauswahl im definierten Bereich (Bsp. B4:B14) durchf|fffd|hren
If Not Application.Intersect(Target, Range("L4:L4450,N4:N4450,AA4:AA4450,AC4:AC4450,AD4:AD4450,AE4:AE4450")) Is Nothing Then

   '**Range definieren
   Set rngDV = Target.SpecialCells(xlCellTypeAllValidation)
   If rngDV Is Nothing Then GoTo Errorhandling

   '** Pr|fffd|fen, ob eine g|fffd|ltige Zelle ausgew|fffd|hlt wurde und Werte eintragen
   If Not Application.Intersect(Target, rngDV) Is Nothing Then
      Application.EnableEvents = False
      wertnew = Target.Value
      Application.Undo
      wertold = Target.Value
      Target.Value = wertnew
      If wertold <> "" Then
         If wertnew <> "" Then
            Target.Value = wertold & "; " & wertnew
         End If
      End If
   End If

End If

Errorhandling:
Application.EnableEvents = True
End Sub
