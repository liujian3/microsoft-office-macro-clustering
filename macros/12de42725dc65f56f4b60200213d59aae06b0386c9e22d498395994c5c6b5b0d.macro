Attribute VB_Name = "Module1"
Sub HideComments()
Attribute HideComments.VB_Description = "Macro recorded 24/07/2006 by maria.christofi"
Attribute HideComments.VB_ProcData.VB_Invoke_Func = " \n14"
'
' HideComments Macro
' Macro recorded 24/07/2006 by maria.christofi
'

'
    Columns("P:R").Select
    ActiveSheet.Unprotect
    Selection.EntireColumn.Hidden = True
    ActiveSheet.Protect DrawingObjects:=True, Contents:=True, Scenarios:=True
    Range("G6").Select
End Sub
Sub UnhideComments()
'
' HideComments Macro
' Macro recorded 24/07/2006 by maria.christofi
'

'
    Columns("O:S").Select
    ActiveSheet.Unprotect
    Selection.EntireColumn.Hidden = False
    ActiveSheet.Protect DrawingObjects:=True, Contents:=True, Scenarios:=True
    Range("G6").Select
End Sub

