Attribute VB_Name = "Module1"
Sub Button1_Click()
myValue = InputBox("May I know your Name")

 Dim sTxt As String
 Dim CurrTime As Long

 CurrTime = Hour(Now)
 Select Case CurrTime
 Case Is > 18
 sTxt = "Hello, Good evening "
 Case Is > 12
 sTxt = "Hello, Good afternoon "
 Case Is < 6
 sTxt = "Hello, Good morning, wow you're here early "
 Case Else
 sTxt = "Hello, Good Morning "
End Select
 
 MsgBox sTxt + myValue
End Sub
Attribute VB_Name = "Module2"

Sub Button2_click()
Dim sName As String
 Dim sTxt As String
 Dim CurrTime As Long
 sName = Environ("UserName")
 
 CurrTime = Hour(Now)
 Select Case CurrTime
 Case Is > 18
 sTxt = "Good evening, "
 Case Is > 12
 sTxt = "Good afternoon, "
 Case Is < 6
 sTxt = "Good morning, wow you're here early, "
 Case Else
 sTxt = "Good Morning, "
End Select
 
 MsgBox sTxt & sName
End Sub

