     lineno = 3
     srno = 0

End Sub

Public Sub Numaric_validations_ancA()
        Dim i As Integer
        Dim l_ers As String
         For i = 7 To 104
         
 If Sheets("Annexure_A").Cells(i, 5).Value <> "" Then
     l_ers = check_Locationv(Sheets("Annexure_A").Cells(i, 5))
  If l_ers <> "" Then
  lineno = lineno + 1
                        srno = srno + 1
                        errors.Cells(lineno, 1) = srno
                        errors.Cells(lineno, 2) = "Annexure A"
                        errors.Cells(lineno, 3) = i
                        errors.Cells(lineno, 4) = "Type of return"
                        errors.Cells(lineno, 5) = "Not As Per Drop Down List"
    End If
      
 End If
        
         
         
            If Trim(Annexure_A.Cells(i, 2)) = "" Or _
            Trim(Annexure_A.Cells(i, 3)) = "" Or _
            Trim(Annexure_A.Cells(i, 4)) = "" Or _
            Trim(Annexure_A.Cells(i, 5)) = "" Or _
            Trim(Annexure_A.Cells(i, 7)) = "" Or _
            Trim(Annexure_A.Cells(i, 8)) = "" Or _
            Trim(Annexure_A.Cells(i, 10)) = "" Then
            If Trim(Annexure_A.Cells(i, 2)) <> "" Or _
               Trim(Annexure_A.Cells(i, 3)) <> "" Or _
               Trim(Annexure_A.Cells(i, 4)) <> "" Or _
               Trim(Annexure_A.Cells(i, 5)) <> "" Or _
               Trim(Annexure_A.Cells(i, 7)) <> "" Or _
               Trim(Annexure_A.Cells(i, 8)) <> "" Or _
                Trim(Annexure_A.Cells(i, 10)) <> "" Then
                lineno = lineno + 1
                srno = srno + 1
                errors.Cells(lineno, 1) = srno
                errors.Cells(lineno, 2) = "Annexure A"
                errors.Cells(lineno, 3) = i
                errors.Cells(lineno, 4) = i
                errors.Cells(lineno, 5) = "Fill the Row completely"
                
             End If
        End If
     Next
        If Trim(Annexure_A.Cells(105, 7)) = "" Or _
            Trim(Annexure_A.Cells(105, 9)) = "" Or _
            Trim(Annexure_A.Cells(105, 10)) = "" Then
            If Trim(Annexure_A.Cells(105, 7)) <> "" Or _
               Trim(Annexure_A.Cells(105, 9)) <> "" Or _
               Trim(Annexure_A.Cells(105, 10)) <> "" Then
                lineno = lineno + 1
                srno = srno + 1
                errors.Cells(lineno, 1) = srno
                errors.Cells(lineno, 2) = "Annexure A"
                errors.Cells(lineno, 3) = 105
                errors.Cells(lineno, 4) = 105
                errors.Cells(lineno, 5) = "Fill the row completely "
             End If
        End If
    For i = 110 To 123
           If Trim(Annexure_A.Cells(i, 2)) = "" Or _
            Trim(Annexure_A.Cells(i, 5)) = "" Or _
            Trim(Annexure_A.Cells(i, 8)) = "" Then
