  Public Sub DELAY_INT_WRITE_ANX_A()
Dim i As Integer
     For i = 7 To 104
        temp_date = Mid(Annexure_A.Cells(i, 8), 1, 2) & Mid(Annexure_A.Cells(i, 8), 4, 2) & Mid(Annexure_A.Cells(i, 8), 7, 2)
         temp_date1 = Mid(Annexure_A.Cells(i, 4), 1, 2) & Mid(Annexure_A.Cells(i, 4), 4, 2) & Mid(Annexure_A.Cells(i, 4), 7, 2)
         'srno = get_duration_days(temp_date, temp_date1)
         If Trim(temp_date) <> "" And Trim(temp_date1) <> "" Then
               If date_validation(Annexure_A.Cells(i, 8)) = True And date_validation(Annexure_A.Cells(i, 4)) = True Then
                    temp_no = Round((Val(get_duration_days(temp_date1, temp_date)) / 30) * 0.0125, 6)
                    If (temp_no < 0) Then
                        Annexure_A.Cells(i, 9) = 0
                    Else
                        Annexure_A.Cells(i, 9) = Round((Annexure_A.Cells(i, 7) * temp_no), 0)
                    End If
              End If
         End If
     Next
End Sub
