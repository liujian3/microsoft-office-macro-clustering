Private Sub Worksheet_Activate()
    

    If ActiveSheet.Name = "Audit Procedures" Then

        Application.ScreenUpdating = False
        Application.EnableEvents = False
        
'If no audit procedures were selected on the Estimates worksheet for SE1
        If Sheet1.chkSE1Q41.Value = False And _
         Sheet1.chkSE1Q42.Value = False And _
         Sheet1.chkSE1Q43.Value = False And _
         Sheet1.chkSE1Q44.Value = False Then
            Range("APItem1").Value = "No audit procedures were selected for " & _
             "Estimate #1"
            Range(Range("APItem1").Offset(1, 0), _
             Range("APItem1").Offset(4, 0)).Rows.Hidden = True
'If at least one audit procedures was selected on the Estimates worksheet for SE1
        Else
            Range("APItem1").Value = "Estimate #1"
            Range(Range("APItem1").Offset(1, 0), _
             Range("APItem1").Offset(4, 0)).Rows.Hidden = False
        End If
'What will appear from the SE1 "Test of Subsequent Transactions" audit section
        If Sheet1.chkSE1Q41.Value = False Then
            If txtSE1APQ01.Visible = True Then
                Call txtSE1APQ01_GotFocus
                txtSE1APQ01.Text = ""
                Call txtSE1APQ01_Change
                Call txtSE1APQ01_LostFocus
                txtSE1APQ01.BorderColor = &HC0C0C0
                txtSE1APQ01.Visible = False
            End If
            optSE1APQ02No.Value = False
            optSE1APQ02Yes.Value = False
            If txtSE1APQ03.Visible = True Then
                Call txtSE1APQ03_GotFocus
                txtSE1APQ03.Text = ""
                Call txtSE1APQ03_Change
                Call txtSE1APQ03_LostFocus
                txtSE1APQ03.BorderColor = &HC0C0C0
                txtSE1APQ03.Visible = False
            End If
            Range("SE1APMeth1").Rows.Hidden = True
        Else
            Range(Range("SE1APQ01Quest").Offset(-2, 0), _
             Range("SE1APQ03Quest").Offset(-1, 0)).Rows.Hidden = False
            txtSE1APQ01.Top = Range("SE1APQ01Quest").Top + 15
            txtSE1APQ01.Visible = True
            If optSE1APQ02No.Value = True Then
                Range(Range("SE1APQ03Quest"), _
                 Range("SE1APQ04Quest").Offset(-3, 0)).Rows.Hidden = False
                txtSE1APQ03.Top = Range("SE1APQ03Quest").Top + 15
                txtSE1APQ03.Visible = True
            Else
                Range(Range("SE1APQ03Quest"), _
                 Range("SE1APQ04Quest").Offset(-3, 0)).Rows.Hidden = True
                txtSE1APQ03.Visible = False
            End If
        End If
'What will appear from the SE1 "Test of Management's Method and Assumptions" audit section.
        If Sheet1.chkSE1Q42.Value = False Then
            If txtSE1APQ04.Visible = True Then
                Call txtSE1APQ04_GotFocus
                txtSE1APQ04.Text = ""
                Call txtSE1APQ04_Change
                Call txtSE1APQ04_LostFocus
                txtSE1APQ04.Visible = False
                txtSE1APQ04.BorderColor = &HC0C0C0
            End If
            If txtSE1APQ05.Visible = True Then
                Call txtSE1APQ05_GotFocus
                txtSE1APQ05.Text = ""
                Call txtSE1APQ05_Change
                Call txtSE1APQ05_LostFocus
