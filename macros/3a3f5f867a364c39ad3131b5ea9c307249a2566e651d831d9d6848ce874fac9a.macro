    Sheet3.Range(ActiveCell, ActiveCell.Offset(InsRow, 0)).EntireRow.Insert
ElseIf DelRow = (txtSE3APQ01BegLC - 4) - 1 Or DelRow = (txtSE3APQ01BegLC - txtSE3APQ01EndLC) - 1 Then
    Sheet3.Range(ActiveCell, ActiveCell.Offset(DelRow, 0)).EntireRow.Delete
End If
Application.Calculate
'Set textbox border to grey color
txtSE3APQ01.BorderColor = &HC0C0C0
'If Textbox is empty
If txtSE3APQ01.Value = "" Then
''Show red border
