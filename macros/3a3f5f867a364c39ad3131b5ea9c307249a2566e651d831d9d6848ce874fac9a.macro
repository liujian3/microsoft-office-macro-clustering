Public Sub InitForEditorial()
  '
  ' Code for editorial special setups that need to be called
  '  after saving or on worksheet initialization.
  '
    Application.ScreenUpdating = False
    Application.EnableEvents = False

    If Sheet1.optSE1Q3Yes.Value = True Or _
     Sheet1.optSE2Q3Yes.Value = True Or _
     Sheet1.optSE3Q3Yes.Value = True Or _
     Sheet1.optSE4Q3Yes.Value = True Or _
     Sheet1.optSE5Q3Yes.Value = True Then
        Sheet2.Visible = xlSheetVisible
    Else
        Sheet2.Visible = xlSheetHidden
    End If
    
    If Range("SE1Disc2").Rows.Hidden = True Then
        Sheet1.txtSE1TB2.Visible = False
    End If
    If Range("SE1Disc3").Rows.Hidden = True Then
        Sheet1.txtSE1TB3.Visible = False
    End If
    If Range("SE2Disc2").Rows.Hidden = True Then
        Sheet1.txtSE2TB2.Visible = False
    End If
    If Range("SE2Disc3").Rows.Hidden = True Then
        Sheet1.txtSE2TB3.Visible = False
    End If
    If Range("SE3Disc2").Rows.Hidden = True Then
        Sheet1.txtSE3TB2.Visible = False
    End If
    If Range("SE3Disc3").Rows.Hidden = True Then
        Sheet1.txtSE3TB3.Visible = False
    End If
    If Range("SE4Disc2").Rows.Hidden = True Then
        Sheet1.txtSE4TB2.Visible = False
    End If
    If Range("SE4Disc3").Rows.Hidden = True Then
        Sheet1.txtSE4TB3.Visible = False
    End If
    If Range("SE5Disc2").Rows.Hidden = True Then
        Sheet1.txtSE5TB2.Visible = False
    End If
    If Range("SE5Disc3").Rows.Hidden = True Then
        Sheet1.txtSE5TB3.Visible = False
    End If

    Application.ScreenUpdating = True
    Application.EnableEvents = True

End Sub

