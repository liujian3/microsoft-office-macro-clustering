Private Sub optSE1Q5No_Click()

    Application.ScreenUpdating = False
    Application.EnableEvents = False
        
'If Textbox2 or Textbox 3 are not empty
    If txtSE1TB2.Value <> "" Or txtSE1TB3.Value <> "" Then
''Warn user that information from the Textboxes will be deleted
        msg = "Selecting 'No' to this question will remove " & vbCrLf & _
              "the information from the textboxes below " & vbCrLf & _
              "for this item. Do you want to continue?"
        ans = MsgBox(msg, vbYesNo, "PPC's Workpapers")
        If ans = vbNo Then  ' User chose No.
            optSE1Q5Yes.Value = True
            Range("SE1Q5Quest").Resize(1, 1).Offset(0, 1).Select
            Application.EnableEvents = True
            Application.ScreenUpdating = True
            Exit Sub
        End If
    End If
'Clear and hide Questions 6 through 9, Textbox2 and Textbox3
    optSE1Q6No.Value = False
    optSE1Q6Yes.Value = False
    Range("SE1Q6Response").Value = ""
    optSE1Q7No.Value = False
    optSE1Q7Yes.Value = False
    Range("SE1Q7Response").Value = ""
    optSE1Q8No.Value = False
    optSE1Q8Yes.Value = False
    Range("SE1Q8Response").Value = ""
    optSE1Q9No.Value = False
    optSE1Q9Yes.Value = False
    Range("SE1EstimateAmount").ClearContents
    Range("SE1Q9Quest").Offset(1, 0).Value = ""
    If txtSE1TB2.Value <> "" Then
        Call txtSE1TB2_GotFocus
        txtSE1TB2.Text = ""
        Call txtSE1TB2_Change
        Call txtSE1TB2_LostFocus
    End If
    txtSE1TB2.Visible = False
    txtSE1TB2.BorderColor = &HC0C0C0
    If txtSE1TB3.Value <> "" Then
        Call txtSE1TB3_GotFocus
        txtSE1TB3.Text = ""
        Call txtSE1TB3_Change
        Call txtSE1TB3_LostFocus
    End If
    txtSE1TB3.Visible = False
    txtSE1TB3.BorderColor = &HC0C0C0
    Range(Range("SE1Q6Quest"), _
     Range("_SE2").Offset(-3, 0)).Rows.Hidden = True
'Add "disclosure not required" message
    Range("SE1Q5Response").Value = "Disclosure under FASB ASC 275, " & _
     "Risks and Uncertainties, is not required."
    Range("SE1Q5Response").Characters(32, 23).Font.Italic = True
'Place cursor for this worksheet
    Range("_SE2").Offset(-2, 0).Resize(1, 1).Select
       
    Application.EnableEvents = True
    Application.ScreenUpdating = True

End Sub

