clear_belt_length_s
End If

End Sub

Private Sub TextBox7_width_s_AfterUpdate()
If Range("belt_width_s").Value > 300 And Range("ser_joint_s").Value = "yes" Then
MsgBox ("Attention: max. width per belt for Set Fabrication = 300 mm in total 600 mm !!")
clear_serjoint
End If

If Range("total_width_s").Value > 600 And Range("ser_joint_s").Value = "yes" Then
MsgBox ("Attention: max. total width for Set Fabrication = 600 mm !!")
clear_serjoint
End If

If Range("belt_code_s") Like "5841*" And Range("belt_width_s").Value < 200 Then
MsgBox ("Attention: min. width for Soliflex PRO Duplex = 200 mm !!")
clear_belt_width_s
End If

If Range("belt_code_s") Like "5842*" And Range("belt_width_s").Value < 490 Then
MsgBox ("Attention: min. width for Soliflex PRO Quattro = 490 mm !!")
clear_belt_width_s
End If

If Range("belt_width_s").Value > 1200 And Not Range("soli_code").Value = 1 Then
MsgBox ("Attention: max. width for Soliflex PES and TPU R18 belts = 1.200 mm !!")
clear_belt_width_s
End If

If Range("belt_width_s").Value > 1200 And Not Range("belt_execution_s").Value = "open" Then
MsgBox ("Attention: max. width for endless Soliflex belts = 1.200 mm !!")
clear_belt_width_s
End If

If Range("belt_width_s").Value > 1800 And Range("belt_execution_s").Value = "open" Then
MsgBox ("Attention: max. width for open Soliflex belts  = 1.800 mm !!")
clear_belt_width_s
End If

End Sub

