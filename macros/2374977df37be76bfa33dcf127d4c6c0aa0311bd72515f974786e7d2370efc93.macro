Sub mail_order()

Name1 = Range("filename").Value

'Dim OutApp As Outlook.Application
'Dim OutMail As Outlook.MailItem

Dim OutApp      As Object
Dim OutMail     As Object


Set OutApp = CreateObject("Outlook.Application")
Set OutMail = OutApp.CreateItem(olMailItem)

With OutMail
        .To = Range("mailto").Value
        .CC = Range("mailcc").Value
        .BCC = Range("mailbcc").Value
        .Subject = "EuroFab Order Form Synthetic Belting OpCo: " & Range("opco").Value & "   Order:  " & Range("order_no").Value
        .Body = Range("mailbody").Value
        .Attachments.Add (Name1)
        .Display
    End With

End Sub
