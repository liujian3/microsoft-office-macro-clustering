Sub create_file_c()
   Name1 = Range("filename_c").Value

    sheetname = Range("sheet_c1").Value
    
    If sheetname = "no drawing" Then
  
    Sheets("print_Curves").Copy
'    Range("a1:P31").Select
    Application.ScreenUpdating = False
    Cells.Copy
    Cells.PasteSpecial Paste:=xlValues
    Application.CutCopyMode = False
    Range("A1").Select
    'ActiveSheet.CommandButton2.Cut
    ActiveSheet.OLEObjects("CommandButton2").Delete
    Application.ScreenUpdating = True
    
      
    Else
    
    Sheets(Array("print_Curves", sheetname)).Copy
    Application.ScreenUpdating = False
    Cells.Copy
    Cells.PasteSpecial Paste:=xlValues
    Application.CutCopyMode = False
    Range("A1").Select
    'ActiveSheet.CommandButton2.Cut
    ActiveSheet.OLEObjects("CommandButton2").Delete
    Sheets(sheetname).Select
    Cells.Copy
    Application.ScreenUpdating = False
    Cells.PasteSpecial Paste:=xlValues
    Range("A1").Select
    'ActiveSheet.CommandButton1.Cut
    ActiveSheet.OLEObjects("CommandButton1").Delete
    Application.ScreenUpdating = True
    Sheets("print_Curves").Activate
    End If
    
    'ActiveWorkbook.SaveAs Filename:=UserForm1.TextBox1.Value
    ActiveWorkbook.SaveAs (Name1)
    ActiveWorkbook.Close
   
    
End Sub
