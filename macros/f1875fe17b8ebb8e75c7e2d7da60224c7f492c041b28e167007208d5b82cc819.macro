Private Sub LitLetterTB_Change()

Application.EnableEvents = False

LitLetterTB.Width = 530.25
If LitLetterTB.LineCount <= 4 Then
    LitLetterTB.Height = 52.5
Else
    LitLetterTB.Height = (LitLetterTB.LineCount) * 13
End If

Application.EnableEvents = True

End Sub

Private Sub LitLetterTB_KeyDown(ByVal KeyCode As MSForms.ReturnInteger, ByVal Shift As Integer)
    
Dim MoveBack As Boolean
    
Select Case KeyCode
        ''' This is the only key necessary.
    Case vbKeyTab
        Application.ScreenUpdating = False
            ''' Determine if need to move backwards.
        MoveBack = CBool(Shift And 1)
            ''' Activate appropriate Control based on key(s) pressed.
        If MoveBack Then
            Call LitLetterTB_LostFocus
            Range("LitYN2").Select
            Range("LitYN2").Select
        Else
            Call LitLetterTB_LostFocus
'            Range("Lit1Case").Select
        End If
            
        Application.ScreenUpdating = True
    End Select

End Sub
