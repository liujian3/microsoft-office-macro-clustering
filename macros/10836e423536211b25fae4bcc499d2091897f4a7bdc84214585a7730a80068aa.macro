Private Sub Worksheet_Change(ByVal Target As Range)
    ''
    On Error Resume Next
    ''
    ''
    ' Se o valor da proposta for digitado, abre a possibilidade de informar os valores:
    If (Target.Address = "$B$10") Then
        If Trim(Range("B10").Value) <> "" Then
        ''
            Plan15.Unprotect "123"
            ActiveWorkbook.Unprotect "123"
            Range("B18:B22").Locked = False
            Call protegerPlanilha
        Else
            Plan15.Unprotect "123"
            ActiveWorkbook.Unprotect "123"
            Range("B18:B22").Locked = True
            Range("B18:B22").Value = ""
            Call protegerPlanilha
        End If
        ''
    End If
    ''
    ''
    If Target.Address = "$B$40" Then
        If Trim(Range("B40").Value) <> "" Then
            ''
            ' Desprotege a planilha temporariamente:
            Plan15.Unprotect "123"
            ActiveWorkbook.Unprotect "123"
            ''
            ''
            If Me.Range("B38").Value = "Lucro Real" And Me.Range("C38").Value = "Sim" Then   ' Linha 8
                ''
                If Not (Target.Value >= Planilha1.Range("B8").Value And Target.Value <= Planilha1.Range("C8").Value) Then
                    MsgBox "O valor informado n|fffd|o est|fffd| contido dentro de um intervalo v|fffd|lido!", vbCritical, "Valor fora do intervalo aceit|fffd|vel"
                    Me.Range("B40").Value = ""
                End If
                ''
            ElseIf Me.Range("B38").Value = "Lucro Real" And Me.Range("C38").Value = "N|fffd|o" Then    ' Linha 9
                ''
                If Not (Target.Value >= Planilha1.Range("B9").Value And Target.Value <= Planilha1.Range("C9").Value) Then
                    MsgBox "O valor informado n|fffd|o est|fffd| contido dentro de um intervalo v|fffd|lido!", vbCritical, "Valor fora do intervalo aceit|fffd|vel"
                    Me.Range("B40").Value = ""
                End If
                ''
            ElseIf Me.Range("B38").Value = "Lucro Presumido" And Me.Range("C38").Value = "Sim" Then   ' Linha 10
                ''
                If Not (Target.Value >= Planilha1.Range("B10").Value And Target.Value <= Planilha1.Range("C10").Value) Then
                    MsgBox "O valor informado n|fffd|o est|fffd| contido dentro de um intervalo v|fffd|lido!", vbCritical, "Valor fora do intervalo aceit|fffd|vel"
                    Me.Range("B40").Value = ""
                End If
                ''
            ElseIf Me.Range("B38").Value = "Lucro Presumido" And Me.Range("C38").Value = "N|fffd|o" Then    ' Linha 11
                ''
                If Not (Target.Value >= Planilha1.Range("B11").Value And Target.Value <= Planilha1.Range("C11").Value) Then
                    MsgBox "O valor informado n|fffd|o est|fffd| contido dentro de um intervalo v|fffd|lido!", vbCritical, "Valor fora do intervalo aceit|fffd|vel"
                    Me.Range("B40").Value = ""
                End If
                ''
            ElseIf Me.Range("B38").Value = "Simples" And Me.Range("C38").Value = "Sim" Then       ' Linha 6
                ''
                If Not (Target.Value >= Planilha1.Range("B6").Value And Target.Value <= Planilha1.Range("C6").Value) Then
                    MsgBox "O valor informado n|fffd|o est|fffd| contido dentro de um intervalo v|fffd|lido!", vbCritical, "Valor fora do intervalo aceit|fffd|vel"
                    Me.Range("B40").Value = ""
                End If
                ''
            ElseIf Me.Range("B38").Value = "Simples" And Me.Range("C38").Value = "N|fffd|o" Then       ' Linha 7
                ''
                If Not (Target.Value >= Planilha1.Range("B7").Value And Target.Value <= Planilha1.Range("C7").Value) Then
                    MsgBox "O valor informado n|fffd|o est|fffd| contido dentro de um intervalo v|fffd|lido!", vbCritical, "Valor fora do intervalo aceit|fffd|vel"
                    Me.Range("B40").Value = ""
                End If
                ''
            Else
                MsgBox "|fffd| preciso informar o Regime Tribut|fffd|rio e se |fffd| Optante pela CPPRB antes de inserir o valor.", vbExclamation, "Aten|fffd||fffd|o ao preenchimento"
                Me.Range("B40").Value = ""
            End If
            ''
            Call protegerPlanilha  ' Protege a planilha novamente
        End If
        ''
    End If
    ''
    If Intersect(Target, Me.Range("B18:B22")) Is Nothing Then Exit Sub ' Verifica se a mudan|fffd|a foi manual ou n|fffd|o
    ''
    ' Desprotege a planilha temporariamente:
    Plan15.Unprotect "123"
    ActiveWorkbook.Unprotect "123"
    ''
    Call verificarValores_PRINCIPAL(Target.Address)
    ''
    Call protegerPlanilha  ' Protege a planilha novamente
    ''
End Sub
''
Private Sub verificarValores_PRINCIPAL(enderecoCelula As String)   ' Bot|fffd|o de comando ATUALIZAR
    Dim j As Integer
    ''
    ' Analisa para os casos de 'RESUMO CUSTOS DIRETOS/INDIRETOS:
    ''
    If enderecoCelula = "$B$18" Then
        Call verificaValores_PESSOAL
    ElseIf enderecoCelula = "$B$19" Then
        Call verificaValores_EQUIPAMENTOS_VEICULOS_FERRAMENTAS
    ElseIf enderecoCelula = "$B$20" Then
        Call verificaValores_MATERIAL_CONSUMO
    ElseIf enderecoCelula = "$B$21" Then
        Call verificaValores_SUBCONTRATACAO
    ElseIf enderecoCelula = "$B$22" Then
        Call verificaValores_INSTALACOES_ESTRUTURAS
    End If
    ''
        For j = 18 To 31   ' Linhas atuais
            ''
            valor = Val(Replace(Plan15.Range("C" & j).Value, ",", "."))
            ''
                enderecoCelula = Plan15.Range("C" & j).Address
                If enderecoCelula = "$C$18" Then
                    Call verificaValores_PESSOAL
                ElseIf enderecoCelula = "$C$19" Then
                    Call verificaValores_EQUIPAMENTOS_VEICULOS_FERRAMENTAS
                ElseIf enderecoCelula = "$C$20" Then
                    Call verificaValores_MATERIAL_CONSUMO
                ElseIf enderecoCelula = "$C$21" Then
                    Call verificaValores_SUBCONTRATACAO
                ElseIf enderecoCelula = "$C$22" Then
                    Call verificaValores_INSTALACOES_ESTRUTURAS
                End If
            ''
        Next j
    ''
    ''
End Sub
''
Private Sub verificaValores_PESSOAL()
    ''
    valor = Val(Replace(Plan15.Range("C18").Value, ",", "."))
    If (valor < 0.1) Then
        ''
        With Plan15.Range("B18").Interior
             ' Se o valor N|fffd|O for maior do que 10%:
                .Pattern = xlNone
                .PatternColorIndex = xlAutomatic
                .TintAndShade = 0
                .PatternTintAndShade = 0
        End With
        ''
        Plan4.Visible = xlSheetVeryHidden
        ''
        ''
        With Plan15.Range("A18:B18")
                .Interior.Pattern = xlNone
                .Interior.TintAndShade = 0
                .Interior.PatternTintAndShade = 0
                ''
                .Font.ThemeColor = xlThemeColorLight1
                .Font.TintAndShade = 0
        End With
        Range("B18").ClearComments
        ''
    Else
        ''
        ' Se o valor for maior do que 10%:
        With Plan15.Range("B18").Interior
             ' Se o valor FOR MAIOR do que 10%:
                .Pattern = xlNone
                .TintAndShade = 0
                .PatternTintAndShade = 0
        End With
        ''
        Plan4.Visible = xlSheetVisible
        ''
        ''
        ' Compara o valor informado na c|fffd|lula com o valor da total da aba correspondente:
        ''
        valor = Val(Replace(Plan15.Range("B18").Value, ",", "."))
        totalAba = Plan4.Range("C12").Value   ' Obt|fffd|m o valor do total da aba 'PESSOAL'
        If valor <> totalAba Then
            With Plan15.Range("A18:B18")
                .Interior.Color = vbRed
                .Font.Color = vbYellow
            End With
            With Range("B18")
                .ClearComments
                .AddComment
                .Comment.Visible = False
                .Comment.Text Text:="ERRO:" & Chr(10) & "Valor digitado N|fffd|O CORRESPONDE ao da aba ""PESSOAL""."
            End With
                ' Exibe observa|fffd||fffd|o na coluna D:
                Range("D17").Value = "Obs."
                Range("D18").Value = "Valor da aba: " & Format(totalAba, "R$ ##,##0.00")
                
        Else
            With Plan15.Range("A18:B18")
                .Interior.Pattern = xlNone
                .Interior.TintAndShade = 0
                .Interior.PatternTintAndShade = 0
                ''
                .Font.ThemeColor = xlThemeColorLight1
                .Font.TintAndShade = 0
            End With
                Range("B18").ClearComments
                ' Oculta a observa|fffd||fffd|o na coluna D:
                Range("D17").ClearContents
                Range("D18").ClearContents
        End If
        ''
    End If
    ''
    ' PLAN4 |fffd| a planilha PESSOAL
    ''
    ''
End Sub
