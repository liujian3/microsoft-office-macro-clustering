Sub Desativar_Preparacao()
Attribute Desativar_Preparacao.VB_ProcData.VB_Invoke_Func = " \n14"
'
' Desativar_Preparacao Macro
'

'

    Dim resposta As Integer
    
    resposta = MsgBox("Voc|fffd| tem certeza que deseja fechar o modo prepara|fffd||fffd|o? J|fffd| selecionou o modelo de Tributo? Se Completo ou Simplificado.", vbYesNo + vbQuestion, "Confirmar tipo de Tributo")
    
    If resposta = vbYes Then
    
    Sheets("TRIBUTOS_LUCRO").Unprotect "123"
    
    Sheets("TRIBUTOS_LUCRO").Select
    Range("B11").Select
    Columns("E:G").EntireColumn.Hidden = True
    'Columns("E:G").Select
    'Selection.EntireColumn.Hidden = True
    Range("B11").Select
    
    ActiveSheet.Shapes.Range(Array("Rectangle 4")).Select
    Selection.ShapeRange.ZOrder msoSendToBack
    
    ActiveSheet.Shapes.Range(Array("Rectangle 2")).Select
    Selection.ShapeRange.ZOrder msoSendToBack
        
    ActiveSheet.Shapes.Range(Array("Rectangle 39")).Select
    Selection.ShapeRange.ZOrder msoSendToBack
    
    'Sheets("TRIBUTOS_LUCRO").Protect "123"
    
    ActiveSheet.Protect DrawingObjects:=True, Contents:=True, Scenarios:=True _
        , AllowFormattingRows:=True, Password:="123"
    
    Else
    
    'nao faz nada
    
    End If
    
End Sub
