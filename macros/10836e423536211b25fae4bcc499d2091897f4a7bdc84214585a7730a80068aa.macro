Sub Ativar_Preparacao()

Dim yourName As String

yourName = InputBox("Digite a SENHA ou Clique no Bot|fffd|o Cancelar")

If yourName = "123" Then '= neste caso defini a palavra CONTROLE como senha
'Ativar_Preparacao 'as macros que deve executar

   Sheets("TRIBUTOS_LUCRO").Select
    Sheets("TRIBUTOS_LUCRO").Unprotect "123"
    Range("B11").Select
    Columns("E:G").EntireColumn.Hidden = False
    'Columns("D:H").Select
    'Selection.EntireColumn.Hidden = False
    
    ActiveSheet.Shapes.Range(Array("Rectangle 4")).Select
    Selection.ShapeRange.ZOrder msoBringToFront
    
    ActiveSheet.Shapes.Range(Array("Rectangle 2")).Select
    Selection.ShapeRange.ZOrder msoBringToFront
        
    ActiveSheet.Shapes.Range(Array("Rectangle 39")).Select
    Selection.ShapeRange.ZOrder msoBringToFront
    
    Range("B11").Select


ElseIf yourName = "" Then
acesso_senha '= macro apenas com a msg que voc|fffd| precisa digitar a senha


Exit Sub

Else
acesso_senha
Exit Sub

End If

End Sub

Sub acesso_senha()

MSG = MsgBox("ATEN|fffd||fffd|O para Ativar, Digite a SENHA", vbExclamation, "Tempo de Carregamento ")

End Sub
