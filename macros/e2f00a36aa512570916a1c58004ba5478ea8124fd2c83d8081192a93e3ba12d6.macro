 Sub Powertrain()
'alle reiter ausblenden

    Sheets("BPF - SPF").Visible = False
    Sheets("Teilebuendel - common parts").Visible = False
    Sheets("Setzteile - directed parts").Visible = False
    Sheets("Optionale Teilebedarfssteuerung").Visible = False
    Sheets("Bsp. Teilebedarfssteuerung").Visible = False
    Sheets("Formular BAG").Visible = False
    Sheets("Teilebuendel").Visible = False
    Sheets("Formblattlebenslauf").Visible = False
    Sheets("Liste").Visible = False
    
   'PT reiter einblenden
    
    Sheets("Formular BAG").Visible = True
    Sheets("Teilebuendel").Visible = True
    Sheets("Liste").Visible = False
       
       
    Sheets("Formular BAG").Select
    Range("A1").Select

End Sub


 Sub Aufbau()
'alle reiter ausblenden

    Sheets("BPF - SPF").Visible = False
    Sheets("Teilebuendel - common parts").Visible = False
    Sheets("Setzteile - directed parts").Visible = False
    Sheets("Optionale Teilebedarfssteuerung").Visible = False
    Sheets("Bsp. Teilebedarfssteuerung").Visible = False
    
    Sheets("Formular BAG").Visible = False
    Sheets("Teilebuendel").Visible = False
    Sheets("Formblattlebenslauf").Visible = False
    Sheets("Liste").Visible = False
    
   'Aufbau reiter einblenden
    
    Sheets("BPF - SPF").Visible = True
    Sheets("Teilebuendel - common parts").Visible = True
    Sheets("Setzteile - directed parts").Visible = True
    Sheets("Optionale Teilebedarfssteuerung").Visible = True
    Sheets("Bsp. Teilebedarfssteuerung").Visible = True
       
    Sheets("BPF - SPF").Select
    Range("A1").Select

End Sub
