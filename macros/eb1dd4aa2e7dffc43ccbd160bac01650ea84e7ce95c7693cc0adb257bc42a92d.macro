Attribute VB_Name = "Module1"
Sub ValuesInMillions()
Attribute ValuesInMillions.VB_ProcData.VB_Invoke_Func = "M\n14"
'
' ValuesInMillions Macro
'
' Keyboard Shortcut: Ctrl+Shift+M
'
    Selection.NumberFormat = "#,##0.0,,"

End Sub
Attribute VB_Name = "Module2"
Sub Highlight_Value_Cells()
Attribute Highlight_Value_Cells.VB_Description = "Highlight_Value_Cells in this worksheet"
Attribute Highlight_Value_Cells.VB_ProcData.VB_Invoke_Func = "C\n14"
'
' Highlight_Value_Cells Macro
' Highlight_Value_Cells in this worksheet
'
' Keyboard Shortcut: Ctrl+Shift+C
'
    ActiveWindow.SmallScroll Down:=-6
    Selection.SpecialCells(xlCellTypeConstants, 1).Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .Color = 5296274
        .TintAndShade = 0
        .PatternTintAndShade = 0
    End With
    Range("A1").Select
End Sub
Attribute VB_Name = "Module3"
Sub InputCell_SheetProtect()
Attribute InputCell_SheetProtect.VB_ProcData.VB_Invoke_Func = " \n14"
'
' InputCell_SheetProtect Macro
'

'
    Selection.SpecialCells(xlCellTypeConstants, 1).Select
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .ThemeColor = xlThemeColorDark2
        .TintAndShade = -9.99786370433668E-02
        .PatternTintAndShade = 0
    End With
    Selection.Locked = False
    Selection.FormulaHidden = False
    Range("A1").Select
    ActiveSheet.Protect DrawingObjects:=True, Contents:=True, Scenarios:=True
End Sub
Attribute VB_Name = "Module4"
Sub CopyandPasteValue()
Attribute CopyandPasteValue.VB_ProcData.VB_Invoke_Func = "P\n14"
'
' CopyandPasteValue Macro
'
' Keyboard Shortcut: Ctrl+Shift+P
'
    Selection.Copy
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
End Sub
Sub PROPERandTRIM()
Attribute PROPERandTRIM.VB_ProcData.VB_Invoke_Func = "V\n14"
'
' PROPERandTRIM Macro
'

'
    Selection.FormulaR1C1 = "=PROPER(TRIM(RC[-1]))"
    ActiveCell.Offset(1, 0).Range("A1").Select
End Sub
