Private Sub CommandButton1_Click()
Dim A, B, C As Single
Dim D As String
A = Val(TextBox1.Text)
B = Val(TextBox2.Text)
D = TextBox4.Text
Select Case D
 Case Is = "+"
 C = A + B
 TextBox3.Text = Str(C)
Case Is = "*"
 C = A * B
 TextBox3.Text = Str(C)
 Case Is = "/"
 C = A / B
 TextBox3.Text = Str(C)
 Case Is = "-"
 C = A - B
 TextBox3.Text = Str(C)
 Case Is = "sin"
 C = Sin(A)
 TextBox3.Text = Str(C)
 Case Is = "cos"
 C = Cos(A)
 TextBox3.Text = Str(C)
 Case Is = "sqr"
 C = Sqr(A)
 TextBox3.Text = Str(C)
 Case Is = "tan"
 C = Tan(A)
 TextBox3.Text = Str(C)
 Case Else
 TextBox3.Text = "|fffd||fffd||fffd||fffd||fffd||fffd|"
End Select
End Sub

