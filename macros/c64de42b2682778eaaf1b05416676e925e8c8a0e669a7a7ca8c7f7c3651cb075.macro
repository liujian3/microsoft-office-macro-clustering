            Stil = vbYesNo + vbQuestion + vbDefaultButton2
            Title = "W|fffd|hrungs-Wandler"
            Ergebnis = MsgBox(Mldg, Stil, Title, Help, Kontext)
                If Ergebnis = vbNo Then
                Sheets("Konverter").Range("D5") = Sheets("Konverter").Range("B5").Value 'Synchronisierung der W|fffd|hrungen im Konverter
                Application.Run Macro:="ZurKostenberechnung" ' Alte W|fffd|hrung beibehalten
                Else: Application.Run Macro:="JaKonverter" ' W|fffd|hrung konvertieren
                End If
        Else:
            MsgBox "Sie haben die aktuelle W|fffd|hrung nicht ver|fffd|ndert. "
            Title = "W|fffd|hrungs-Wandler"
