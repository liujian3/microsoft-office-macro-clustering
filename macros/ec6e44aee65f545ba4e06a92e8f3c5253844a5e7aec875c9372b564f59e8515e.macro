Sub Cont_Aplic()
'
' Data_Center Macro
'

    lin = Sheets("Apoio macro").Range("B9").Value
   

    If Range("H" & lin) = False Then
    
        Range("D" & lin, "G" & lin).Select
        With Selection.Interior
            .Pattern = xlDown
            .PatternColorIndex = xlAutomatic
            .ThemeColor = xlThemeColorDark2
            .TintAndShade = -0.249977111117893
            .PatternTintAndShade = 0
        End With

    ElseIf Range("H" & lin) = True Then
        
        Range("D" & lin, "G" & lin).Select
        With Selection.Interior
            .Pattern = xlNone
            .TintAndShade = 0
            .PatternTintAndShade = 0
        End With

    Else
    
        Range("D" & lin, "G" & lin).Select
        With Selection.Interior
            .Pattern = xlNone
            .TintAndShade = 0
            .PatternTintAndShade = 0
        End With

    End If

End Sub


