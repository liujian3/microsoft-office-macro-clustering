Private Sub Worksheet_SelectionChange(ByVal Target As Range)
Dim lrXe As Byte
Dim i As Byte
Dim lrGN As Integer
Dim lrCua As Integer
Dim Xe
Dim GN
Dim Cua

'So xe
If Target.Column = Range("I1").Column Then
    lrXe = Range("Q" & Rows.Count).End(xlUp).Row
    For i = 2 To lrXe
        If Range("R" & i) <> "" Then
            If Xe = "" Then
                Xe = Range("R" & i)
                Else
                Xe = Xe & "," & Range("R" & i)
            End If
        End If
    Next i
    With Target.Validation
        .Delete
        .Add Type:=xlValidateList, AlertStyle:=xlValidAlertInformation, _
        Operator:=xlBetween, Formula1:=Xe
    End With
End If

'Giao nhan
If Target.Column = Range("J1").Column Then
    lrGN = Range("W" & Rows.Count).End(xlUp).Row
    For i = 2 To lrGN
        If Range("W" & i) <> "" Then
            If GN = "" Then
                GN = Range("W" & i)
                Else
                GN = GN & "," & Range("W" & i)
            End If
        End If
    Next i
    With Target.Validation
        .Delete
        .Add Type:=xlValidateList, AlertStyle:=xlValidAlertInformation, _
        Operator:=xlBetween, Formula1:=GN
    End With
End If

'So cua
If Target.Column = Range("K1").Column Then
    lrCua = Range("AA" & Rows.Count).End(xlUp).Row
    For i = 2 To lrCua
        If Range("AA" & i) <> "" Then
            If Cua = "" Then
                Cua = Range("AA" & i)
                Else
                Cua = Cua & "," & Range("AA" & i)
            End If
        End If
    Next i
    With Target.Validation
        .Delete
        .Add Type:=xlValidateList, AlertStyle:=xlValidAlertInformation, _
        Operator:=xlBetween, Formula1:=Cua
    End With
End If
End Sub

