Private Sub Worksheet_Change(ByVal Target As Range)
Dim Cel As Range
Dim Plg As Range
If Intersect(Target, [P:P]) Is Nothing Then Exit Sub
ActiveSheet.Unprotect ("")
Application.EnableEvents = False
Set Plg = Intersect(Target, [P:P])
For Each Cel In Plg
     If Cells(Cel.Row, "P").Value = 0 Then
        Cells(Cel.Row, "Q").Value = "MAINTIEN"
        Cells(Cel.Row, "R").Value = Now()
        Else
        Cells(Cel.Row, "Q").Value = "INDEXEE"
        Cells(Cel.Row, "R").Value = Now()
     End If
Next Cel
ActiveSheet.Protect ("")
Application.EnableEvents = True
End Sub
