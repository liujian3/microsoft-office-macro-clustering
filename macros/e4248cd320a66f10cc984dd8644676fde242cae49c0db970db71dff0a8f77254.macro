Private Sub Worksheet_BeforeDoubleClick(ByVal Target As Range, Cancel As Boolean)
Dim rango As Range
Dim FilasRango As Integer
Dim columnabusqueda As Range
Dim datoencontrado As Range
Application.ScreenUpdating = False


   'el c|fffd|digo siguiente es para evitar que al hacer doble clic me genere el ruido molesto de error
    Cancel = True
    'utilizamos un condicional if que funciona solamente cuando coincide la posici|fffd|n de la celda en la que hacemos doble clic con el rango determinado,
    'en este caso como es un ejemplo |fffd|D12:D100|fffd|
    If Not Intersect(Target, Range("D11:D100")) Is Nothing Then
    'seteamos el texbox con el valor de la celda seleccionada en la tabla din|fffd|mica
    TextBox1.Text = ActiveCell.Text
    'usamos la funci|fffd|n coincidir |fffd|match|fffd| de Excel para buscar el valor del ID para el empleado seleccionado.
    
    
    Sheets("Lista").PivotTables("TablaDin|fffd|mica1").PivotCache.Refresh 'Actualizar tabla dinamica 1
    
    Set rango = Sheets("Lista").Range("J1").CurrentRegion 'Selecciona todos los datos activos desde A1 hasta que se tope con filar y olumnas vacias

    FilasRango = rango.Rows.Count 'Cuenta las filas activas dentro del rango
    Set columnabusqueda = Sheets("Lista").Range("J1:J" & FilasRango)  'Estable la culumna donde se va a buscar el valor.("A2:A" & FilasRango) = A2:ultima A activa

    datoencontrado = columnabusqueda.Find(What:=TextBox1.Value, MatchCase:=False, LookAt:=xlWhole).Address 'Encuentra la ubicaci|fffd|n del codigo

   

    Range("D1").Value = Sheets("lista").Range(datoencontrado).Offset(0, 1).Value
 
    'Range("F1").Text = Sheets("buscar").TextBoxes("TextBox1").Text  'TEXTBOX!! CODE
    
     With Application.WorksheetFunction

    Range("D3").Value = .VLookup(TextBox1.Text, Sheets("Lista").Range("Tabla1[#All]"), 2, 0)
    Range("D5").Value = .VLookup(TextBox1.Text, Sheets("Lista").Range("Tabla1[#All]"), 4, 0)
    Range("D7").Value = .VLookup(TextBox1.Text, Sheets("Lista").Range("Tabla1[#All]"), 3, 0)
    'Range("D1").Value = .VLookup(TextBox1.Text, Sheets("Lista").Range("TablaDin|fffd|mica1"), 2, 0)
    End With
    
    
    'luego dejo vac|fffd|a la tabla din|fffd|mica ya que encontramos nuestro valor buscado.
    ActiveSheet.PivotTables("TablaDin|fffd|mica3").PivotFields("Nombre(s)").ClearAllFilters
    ActiveSheet.PivotTables("TablaDin|fffd|mica3").PivotFields("Nombre(s)").PivotFilters.Add Type:=xlCaptionEquals, Value1:=""
   
    'hacemos foco sobre el textbox
     TextBox1.Activate
     Columns("D:D").EntireColumn.AutoFit
    End If
End Sub

Private Sub Worksheet_Change(ByVal Target As Range)

Application.ScreenUpdating = False

On Error Resume Next

    If Not Intersect(Target, Range("D3")) Is Nothing Then
    
        If Range("D3").Text <> "" Then
        
           TextBox1.Text = WorksheetFunction.VLookup(Range("D3"), Sheets("lista").Range("Tabla1[#All]"), 2, False)
    
            ActiveSheet.PivotTables("TablaDin|fffd|mica3").PivotFields("Nombre(s)").ClearAllFilters
            ActiveSheet.PivotTables("TablaDin|fffd|mica3").PivotFields("Nombre(s)").PivotFilters.Add Type:=xlCaptionEquals, Value1:=""
            
            
            
            Range("D3").Select
        
        Else
        
        TextBox1.Text = ""
        
        End If
    
    End If

End Sub

