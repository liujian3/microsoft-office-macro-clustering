Private Sub puntos_Change()
'Validar que s|fffd|lo se ingresen n|fffd|meros (a-z y s|fffd|mbolos)
    Dim Texto As Variant
    Dim Caracter As Variant
    Dim Largo As Integer
    On Error Resume Next
    Texto = Me.Puntos.Value
    Largo = Len(Me.Puntos.Value)
    For i = 1 To Largo
        Caracter = Mid(Texto, i, 1)
        If Caracter <> "" Then
            If Caracter < Chr(48) Or Caracter > Chr(57) Then
                Me.Puntos.Value = Replace(Texto, Caracter, "")
            Else
            End If
        End If
    Next i
    On Error GoTo 0
    Caracter = 0
    Caracter1 = 0
End Sub

Private Sub Telefono_Change()
'Validar que s|fffd|lo se ingresen n|fffd|meros (a-z y s|fffd|mbolos)
