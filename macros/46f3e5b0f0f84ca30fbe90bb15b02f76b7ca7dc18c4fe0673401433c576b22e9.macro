Attribute VB_Name = "Module1"
Sub wikiHow()
Attribute wikiHow.VB_ProcData.VB_Invoke_Func = "A\n14"
'
' wikiHow Macro
'
' Keyboard Shortcut: Ctrl+Shift+A
'
    Range("A2").Select
    ActiveCell.FormulaR1C1 = "2"
    Range("B2").Select
    ActiveCell.FormulaR1C1 = "3"
    Range("C7").Select
    ActiveCell.FormulaR1C1 = "=SUM(A2B2)"
    Range("C13").Select
End Sub


