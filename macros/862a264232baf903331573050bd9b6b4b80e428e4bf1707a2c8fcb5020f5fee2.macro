Attribute VB_Name = "Module1"
Function finance(CF_before_overdraft, interest_rate)

 finance = -CF_before_overdraft * (1 + interest_rate)
 residual_finance = CF_before_overdraft + finance - finance * interest_rate
 
 While residual_finance < -0.0000001
  finance = finance - residual_finance * (1 + interest_rate)
  residual_finance = CF_before_overdraft + finance - finance * interest_rate
 Wend
  
 
End Function
