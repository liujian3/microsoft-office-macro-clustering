        SaveWithDocument:=True
        Selection.InlineShapes.AddPicture FileName:= _
        "S:\vic\Marketing\Marketing\1 MERCHANDISE BULLETIN TEMPLATES & NUMBERS\Protect Your Hands.bmp", LinkToFile:=False, _
        SaveWithDocument:=True
