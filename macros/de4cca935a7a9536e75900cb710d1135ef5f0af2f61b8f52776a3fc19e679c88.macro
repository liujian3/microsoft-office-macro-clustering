Public Sub cboSubject_Change()
    Application.ScreenUpdating = False

 
    If Me.cboSubject = "" Then Exit Sub

    Dim lngRow As Long
    Dim lngCol As Long
    Dim strSubject As String

    'Get Table
    Select Case Me.cboSubject

    Case "Public Recall to CRC"
        lngCol = 4: lngRow = 5
   
    Case "Public Recall to DC"
        lngCol = 4: lngRow = 5
        
    Case "Product Isolation"
        lngCol = 5: lngRow = 5
      
    Case "Product Withdrawal to CRC"
        lngCol = 4: lngRow = 5
   
    Case "Product Withdrawal to DC"
        lngCol = 4: lngRow = 5
        
    Case "Buy Back"
        lngCol = 4: lngRow = 5


    Case "Product Recall"
        lngCol = 3: lngRow = 3

    Case "Buy Back"
        lngCol = 4: lngRow = 5


    
    End Select


    'Display table
    Dim tbl As Table
    Dim docActive As Document
    Dim ongRange As Range
    Dim lngNbr As Long

    Set docActive = ActiveDocument

    On Error Resume Next
    '    'Delete extra text from document
    Call delete_text

    docActive.Tables(4).Delete    ' deletes extra table
    docActive.Tables(3).Delete    ' delete table

    'Insert Subject in first table
    If Me.cboSubject = "Public Recall to CRC" Or Me.cboSubject = "Public Recall to DC" Then
    With docActive.Tables(1).Cell(Row:=2, Column:=2).Range
        .Delete
        .InsertAfter Text:="Public Recall"
    End With
    ElseIf Me.cboSubject = "Product Withdrawal to CRC" Or Me.cboSubject = "Product Withdrawal to DC" Then
    With docActive.Tables(1).Cell(Row:=2, Column:=2).Range
        .Delete
        .InsertAfter Text:="Product Withdrawal"
    End With
    Else:
        With docActive.Tables(1).Cell(Row:=2, Column:=2).Range
        .Delete
        .InsertAfter Text:=Me.cboSubject
    End With
    End If
    
    'Set objRange = docActive.Bookmarks("bmkTable").Range
    'Set tbl = docActive.Tables.Add(objRange, NumRows:=lngRow, NumColumns:=lngCol)
    
' *****************************************************************************************************************************************************end display table.



