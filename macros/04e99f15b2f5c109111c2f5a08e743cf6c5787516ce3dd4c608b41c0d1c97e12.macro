Attribute VB_Control = "PhysAddFill, 20, 11, MSForms, CheckBox"
Private Sub PhysAddFill_Click()

'Auto -fills 'Physical Address' if same as "Mailing Address is Same as Physical Address" is checked('PhysAddFill').

If ActiveSheet.PhysAddFill.Value = True Then
    'When check box is clicked
    Sheets("Master Trans").Range("L27").Value = Range("C27")
    'Copies 'Mailing Address' to 'Physical Address'
    Sheets("Master Trans").Range("L31").Value = Range("C31")
    'Copies 'Phone #' from 'Mailing Address' to 'Physical Address'
Else
    'When check box is unclicked
    Sheets("Master Trans").Range("PhysicalAddress").MergeArea.ClearContents
    'Clears 'Physical Address' from 'Physical Address' section
    Sheets("Master Trans").Range("L31").MergeArea.ClearContents
    'Clears 'Phone#' from 'Physical Address' section
    
End If

End Sub


Private Sub Worksheet_SelectionChange(ByVal Target As Range)



'////BULKPAYMENT VISIBILTY - based upon client selection////
''BulkPayment' check box for ENVOY and SETERUS payments. Appears ONLY if user selects "ENVOY - 27640" or "SETERUS - 27600" in "F3" ('Client').
    'This check box is linked to cell "R40" where click returns TRUE, unclick returns FALSE:
    'TRUE modifies 'Workbook_BeforePrint(Cancel As Boolean)' arguement to allow sumbission of form without a single 'Borrower Name', 'Property Address', and/or 'City, State, Zip Code'.
    'FALSE resets 'Workbook_BeforePrint(Cancel As Boolean)' arugement to require submission of form with a single 'Borrower Name', 'Property Address', and/or 'City, State, Zip Code'.
    
If ActiveSheet.Range("F3").Value = "ENVOY - 27640" Or ActiveSheet.Range("F3").Value = "SETERUS - 27600" Then
    'If client is Envoy or Seterus...
    BulkPayment.Visible = True
    'show 'BulkPayment' check box.
Else
    BulkPayment.Visible = False
    'Do not show 'BulkPayment' check box.
    BulkPayment.Value = False
    'Set 'BulkPayment' value to FALSE.
End If


'////CLEAR LDR & TAX SALE DATES////

If ActiveSheet.Range("B19").Value = "" Then
    ActiveSheet.Range("H19").MergeArea.ClearContents
    '//Erases 'LDR (Date)' when user changes 'Critical Category' from 'Property Loss (Within 90 Days)' to 'Tax Sale (Within 90 Days)'
    End If

If ActiveSheet.Range("H15").Value = "" Then
    ActiveSheet.Range("H17").MergeArea.ClearContents
    '//Erases 'Tax Sale Date' when user deletes 'Critical Category' selection.
    End If



End Sub



