Attribute VB_Name = "MasterTrans_Checkboxes"
Sub Critical_Click()

'//Clicking "YES" auto-populates 'Tracking Required' and 'Reason for Tracking' sections.

Dim ws As Worksheet
Dim chk As CheckBox
Set ws = ActiveSheet
Set chk = ws.CheckBoxes(Application.Caller)

Select Case chk.Value
   Case 1   '//"YES" box is checked

    ActiveSheet.Range("I34").Value = "CRITICAL ITEM"
    ''Tracking Required' = YES
    ActiveSheet.Range("M33").Value = "YES"
    ''Reason for Tracking' = "CRITICAL ITEM"
    ActiveSheet.Range("S12").Value = False
    ''Deselects "No" critical check box

Case Else   '//"YES" box is not checked, also triggered when user UNCLICKS "YES" or changes "YES" to "NO" for 'Critical Item'.


    ActiveSheet.Range("I34").MergeArea.ClearContents
    '//Clears 'CRITICAL ITEM' from 'Reason for Tracking'
    ActiveSheet.Range("M33").MergeArea.ClearContents
    '//Clears 'YES' from 'Tracking Required'


    If ActiveSheet.Range("H17").Value <> "" Then
        ActiveSheet.Range("h17").MergeArea.ClearContents
        '//Clears the 'Tax Sale Date' in "H17"
    End If
        If ActiveSheet.Range("h19").Value <> "" Then
            ActiveSheet.Range("h19").MergeArea.ClearContents
            '//Clears the 'LDR (Date)' in cell "H19"
        End If
            If ActiveSheet.Range("h15").Value <> "" Then
                ActiveSheet.Range("h15").MergeArea.ClearContents
                '//Clears the 'Critical Category' selection in cell "H15"
            End If
End Select

End Sub
Sub NoneCritical_Click()

'//Triggered when user changes clicks "NO" for 'Critical Item'
'//Clicking "NO" removes user data for 'Critical Category' selection, 'Tax Sale Date' and 'LDR (Date)'.

Dim ws As Worksheet
Dim chk As CheckBox
Set ws = ActiveSheet
Set chk = ws.CheckBoxes(Application.Caller)

Select Case chk.Value
   Case 1   '//"NO" box is checked
    
    ActiveSheet.Range("M33").MergeArea.ClearContents
    '//Clears 'YES' from 'Tracking Required' upon click of NO.
    ActiveSheet.Range("I34").MergeArea.ClearContents
    '//Clears 'CRITICAL ITEM' from 'Reason for tracking'
    ActiveSheet.Range("h15").MergeArea.ClearContents
    '//Clears 'Critical Category' data upon click of NO.
    ActiveSheet.Range("h17").MergeArea.ClearContents
    '//Clears the 'Tax Sale Date' daa upon click of NO.
    ActiveSheet.Range("h19").MergeArea.ClearContents
    '//Clears the 'LDR (Date)' data upon click of NO.
    ActiveSheet.Range("r12").Value = False
   
Case Else   '//"NO" box is NOT checked
   
'//Triggered if "NO" is deselected or "YES" is select'Critical Item'
   
   
    ActiveSheet.Range("M33").Value = "YES"
    '//Auto-populate 'Tracking Required'
    ActiveSheet.Range("I34").Value = "CRITICAL ITEM"
    '//Auto-populate 'Reason for Tracking'
   
End Select

End Sub
