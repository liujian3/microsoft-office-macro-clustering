Private Sub Button_Next_Click()
    
Dim dateTime As Date
dateTime = now()

EmployeeNumberString = Field_EmployeeNumber.Value
    
    If Field_FirstName = "" Or Field_LastName = "" Or Field_EmployeeNumber = "" Then
        MsgBox "All fields must be be filled out to continue", vbExclamation
        Exit Sub
    End If
    
    If Len(Field_EmployeeNumber) < 7 Then
        MsgBox "The Employee Number is not long enough. Please enter a valid number.", vbExclamation
        Exit Sub
    End If
    
    If Not ((Field_EmployeeNumber Like "r*") Or (Field_EmployeeNumber Like "R*")) Then
        MsgBox "Employee Numbers must begin with the letter 'r'", vbExclamation
        Exit Sub
    End If
    
    'the Mid function format is (text, start position, number of characters)
    If Mid(EmployeeNumberString, 1, 1) = "R" Then
        'change upper case first character to lower case
        EmployeeNumberString = "r" & Mid(EmployeeNumberString, 2, 6)
        'replace the Employee Number with the lower case version of itself
        Field_EmployeeNumber.Value = EmployeeNumberString
    End If

    
    
    Range("Data!B11").Value = Field_FirstName.Value
    Range("Data!B12").Value = Field_LastName.Value
    Range("Data!B13").Value = Field_EmployeeNumber.Value
    Range("Data!B14").Value = Format(dateTime, "mm/dd/yyyy hh:mm:ss am/pm")
    
    Unload Me
    Form_WelcomeInstructions1.Show
    
End Sub
