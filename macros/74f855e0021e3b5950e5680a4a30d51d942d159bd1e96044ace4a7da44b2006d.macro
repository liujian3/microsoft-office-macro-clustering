'    Sheets("Scores").Field_ClaimScore8.Value = ""
'    Sheets("Scores").Field_ClaimScore9.Value = ""
'    Sheets("Scores").Field_ClaimScore10.Value = ""
    
    Sheets("Scores").Range("B13:B14").ClearContents
    Sheets("Scores").Range("H1:H500").ClearContents
    
    Application.ScreenUpdating = True
    
    MsgBox "All Done." & vbLf & vbLf & "Scores have been reset.", vbInformation, "Touchpoint"
    
End Sub



