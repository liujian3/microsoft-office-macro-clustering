Private Sub Button_SaveToDesktop_Click()

    ActiveWorkbook.SaveAs _
    Filename:=CreateObject("WScript.Shell").specialfolders("Desktop") & "\" & _
    Sheets("Data").Range("B11").Value & _
    Sheets("Data").Range("B12").Value & "-" & _
    Sheets("Data").Range("B13").Value & "-" & _
    Replace(Sheets("Data").Range("B85").Value, " ", "") & _
    Format(Sheets("Data").Range("B93").Value, "mmddyy-hhnnss") & _
    FileExtStr, FileFormat:=52
    
    MsgBox "The simulation file has been saved to your desktop as:" & Chr(13) & Chr(13) & _
            Sheets("Data").Range("B11").Value & _
            Sheets("Data").Range("B12").Value & "-" & _
            Sheets("Data").Range("B13").Value & "-" & _
            Replace(Sheets("Data").Range("B6").Value, " ", "") & _
            Format(Sheets("Data").Range("B14").Value, "mmddyy-hhnnss") & ".xslm" & vbLf, vbInformation, "Save Info"
    
  
End Sub



