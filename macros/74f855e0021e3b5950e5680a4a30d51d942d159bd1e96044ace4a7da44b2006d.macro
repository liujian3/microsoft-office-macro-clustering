Private Sub Workbook_Open()
    'running this code upon open each time to force all sheets to be
    're-locked with the user interface only.
    'This is required by Excel to allow the user interface only to work
    'each time the file is opened.
    
    If Sheets("Data").Range("B329").Value = "Locked" Then
        Dim ws As Worksheet
            For Each ws In Worksheets
            ws.Protect password:="Pa$$word", UserInterFaceOnly:=True
        Next ws
    End If
End Sub


