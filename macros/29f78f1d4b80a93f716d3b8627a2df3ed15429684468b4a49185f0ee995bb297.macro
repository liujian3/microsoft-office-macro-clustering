Private Sub Absenden_Click()

If KritikText = "" Then

MsgBox "Textbox ist leer.", vbOKOnly, "Verbesserung"

KritikText.SetFocus

Else

Dim MyMessage As Object, MyOutApp As Object
    Dim Qe As Integer
    Dim AWS As String

    'Outlook Object erstellen
    Set MyOutApp = CreateObject("Outlook.Application")
    'Outlook Nachricht erstellen
    Set MyMessage = MyOutApp.CreateItem(0)
    With MyMessage
        'Empf|fffd|nger
        .To = "w.fuchs@motion06.at"
        'Betreff
        .Subject = "F|fffd|rdererliste Kritik"

        'Hier wird eine HTML Mail erstellt
        .HTMLBody = KritikText
        'Hier wird die Mail gleich in den Postausgang gelegt und gesendet
        .Send
    End With
    'Variablen leeren
    Set MyOutApp = Nothing
    Set MyMessage = Nothing
    
MsgBox "Danke!", vbOKOnly, "Verbesserung"

End
    
End If

End Sub


