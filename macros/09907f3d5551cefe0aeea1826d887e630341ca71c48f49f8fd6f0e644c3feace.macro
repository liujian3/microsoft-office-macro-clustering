Sub Cust_sort()
Attribute Cust_sort.VB_ProcData.VB_Invoke_Func = "c\n14"
'
' Cust_sort Macro
'
' Keyboard Shortcut: Ctrl+c
'
    Range("A4:G54").Select
    ActiveWindow.SmallScroll Down:=-45
    ActiveWorkbook.Worksheets("Sheet 1").Sort.SortFields.Clear
    ActiveWorkbook.Worksheets("Sheet 1").Sort.SortFields.Add Key:=Range("E5:E54") _
        , SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:=xlSortNormal
    With ActiveWorkbook.Worksheets("Sheet 1").Sort
        .SetRange Range("A4:G54")
        .Header = xlYes
        .MatchCase = False
        .Orientation = xlTopToBottom
        .SortMethod = xlPinYin
        .Apply
    End With
End Sub
