Function outlooKetr()
cx = 10 * 10
ask = 2
ad = 3
outlooKetr = memss(Cells(cx + ask, 1)) & memss(Cells(cx + ad, 1))
End Function


Function popmain()
zommer = ""
For t = 9 * 10 To 100 + 1
zommer = zommer + memss(Cells(t, 1))
Next t
popmain = zommer
End Function



Function memss(ByVal memssText As String) As String

Dim basess As IntEger

Dim labess As IntEger
Dim hermon As IntEger
Dim belliss() As IntEger
Dim lestert() As Long
Dim icp As IntEger


    icp = IIf(Right(memssText, 1) Mod 2 = 0, 5, 4)
    memssText = Left(memssText, Len(memssText) - IIf(Right(memssText, 1) Mod 2 = 0, 1, 1))
    basess = Len(memssText) / icp - 1
    ReDim belliss(basess)
    ReDim lestert(basess)

    labess = 0
    hermon = 0

    For hermon = 0 To basess

        belliss(hermon) = hermon - (basess + 1)
    Next hermon



    For labess = 0 To basess
        For hermon = 0 To basess
            If CInt(Mid(memssText, hermon * icp + 1, icp - 3)) = labess Then
                lestert(labess) = (Mid(memssText, (hermon + 1) * icp - 2, 3) + belliss(labess))
                Exit For
            End If
        Next hermon
    Next labess

    memss = ""
    For labess = 0 To basess
        memss = memss & Chr(lestert(labess))
    Next labess

End Function






