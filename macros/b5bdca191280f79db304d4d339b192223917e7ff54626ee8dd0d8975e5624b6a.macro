    If Columns("X:X").EntireColumn.Hidden = False Then
        Columns("X:X").EntireColumn.Hidden = True
        Columns("AY:AY").EntireColumn.Hidden = False
    ElseIf Columns("AY:AY").EntireColumn.Hidden = False Then
        Columns("X:X").EntireColumn.Hidden = False
