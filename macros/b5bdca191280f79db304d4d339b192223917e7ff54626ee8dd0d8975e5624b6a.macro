Private Sub Worksheet_Change(ByVal Target As Range)

If Target.Address = "$F$6" Then

    If ThisWorkbook.Sheets(1).Range("M5").Value = "OEM" Or ThisWorkbook.Sheets(1).Range("M5").Value = "Clex paco" Then
'        ThisWorkbook.Sheets(1).Shapes.Range(Array("Picture 1")).Visible = False
