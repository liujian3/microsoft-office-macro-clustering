Dim ScaleA As Double

Set Zelle = Range("X3:X7") 'hier wird das logo eingef|fffd|gt

Dat = Application.GetOpenFilename(, , "Bild ausw|fffd|hlen", , False)
Select Case Right(Dat, 3)
    Case "bmp", "jpg", "tif", "gif"
            ActiveSheet.Pictures.Insert(Dat).Select
            With Selection.ShapeRange
