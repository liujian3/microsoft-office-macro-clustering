varFilename = Application.GetSaveAsFilename( _
InitialFileName:="Konfigurationsformular_T|fffd|rdr|fffd|cker_T|fffd|rbeschlag_" & Format(Now, "DD_MM_YYYY_HHMM") & ".pdf", _
FileFilter:="PDF (*.pdf), *.pdf", _
Title:="Konfigurationsformular als PDF speichern")

If varFilename <> False Then
ThisWorkbook.ExportAsFixedFormat _
Type:=xlTypePDF, _
Filename:=varFilename
End If

End Sub

Sub per_Mail_senden()
    Dim objApp As Object, objMailItem As Object
    Dim sDatei As String
    sDatei = Environ("TEMP") & "\" & "Konfigurationsformular_T|fffd|rdr|fffd|cker_T|fffd|rbeschlag_" & Format(Now, "DD_MM_YYYY_HHMM") & ".pdf"
   
    ActiveWorkbook.ExportAsFixedFormat Type:=xlTypePDF, Filename:=sDatei, Quality:=xlQualityStandard, _
        IncludeDocProperties:=False, IgnorePrintAreas:=False, OpenAfterPublish:=False
   
    On Error GoTo Errorhandler
    Set objApp = CreateObject("Outlook.Application")
    Set objMailItem = objApp.CreateItem(0)
    With objMailItem
        .Attachments.Add sDatei
        .To = "vertrieb@uundz.de"
        .Subject = "Konfigurationsformular_T|fffd|rdr|fffd|cker_T|fffd|rbeschlag_" & Format(Now, "DD_MM_YYYY_HHMM")
        .Display
    End With
Aufr|fffd|umen:
    Set objMailItem = Nothing
    Set objApp = Nothing
    Exit Sub
   
Errorhandler:
    On Error GoTo 0
    MsgBox "Fehler beim Erstellen der Email.", vbCritical, "Fehler"
    GoTo Aufr|fffd|umen
End Sub


