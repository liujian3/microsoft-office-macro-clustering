Public Sub Sub_CopyData(sheet As Worksheet, currR As Long, currC As Long, StartRow As Long)
    Dim msg As Long, i As Long
    Dim numInsert As String

    Dim topCel As Range, bottomCel As Range, sourceRange As Range, targetRange As Range
    Dim NumOfRows As Long

    Set topCel = sheet.Range("A" & StartRow)
    Set bottomCel = sheet.Range("A65536").End(xlUp)
    If topCel.row > bottomCel.row Then
        End
    End If

    Set sourceRange = Range(topCel, bottomCel)
    NumOfRows = sourceRange.Rows.Count

    If (currR > NumOfRows + StartRow - 1) Or (currR < StartRow) Or (currC = enSHT_INDIVIDUAL_CL.e|fffd|A|fffd||fffd|) Then
        Call RaiseError(302)
        Exit Sub
    End If

    msg = MsgBox("|fffd|I|fffd||fffd||fffd||fffd||fffd||a0bd||fffd|Z|fffd||fffd||fffd||312|l " & Cells(currR, currC) & " |fffd||11f02||311|S|fffd||102||303|Z|fffd||fffd||fffd||254||fffd||fffd|f|fffd|i|fffd|R|fffd|s|fffd|[|fffd|j|fffd||fffd||fffd||702||fffd||fffd|B", vbYesNo + vbQuestion, ThisWorkbook.Name)
    If msg = vbYes Then
        For i = StartRow To NumOfRows + (StartRow - 1)
            Cells(i, currC) = Cells(currR, currC)
        Next i
    End If
End Sub

'|fffd|s|fffd||312||1c9||fffd||fffd||fffd||fffd|C|fffd||fffd|
Private Sub AddRowMain()

    On Error GoTo ErrRtn
    
    Dim msg As Long
    Dim numInsert As String
    Dim currentRow As Long

    Dim topCel As Range, bottomCel As Range, sourceRange As Range, targetRange As Range
    Dim i As Long, j As Long, NumOfRows As Long

    Dim baseRow As Long
    baseRow = enSHT_INDIVIDUAL_RW.e|fffd|f|fffd|[|fffd|^|fffd||fffd|
    
    Set topCel = shtScreenIndividual.Range("A" & CStr(baseRow))
    Set bottomCel = shtScreenIndividual.Range("A65536").End(xlUp)

    If topCel.row > bottomCel.row Then
        End
    End If
    
    Set sourceRange = shtScreenIndividual.Range(topCel, bottomCel)
    NumOfRows = sourceRange.Rows.Count

    currentRow = NumOfRows + (baseRow - 1)
    shtScreenIndividual.Range("A" & currentRow).Select
    numInsert = InputBox("|fffd||fffd||fffd||742||30d|s|fffd||fffd||fffd|F" & NumOfRows & vbCrLf & "|fffd||1c9||fffd||fffd||fffd||fffd||fffd|s|fffd||fffd||fffd||fffd||fffd||fffd||342||fffd||fffd||102||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|B", "|fffd|s|fffd||312||1c9||fffd|")
    numInsert = StrConv(numInsert, vbNarrow) '|fffd|S|fffd|p|fffd||fffd||fffd||fffd||fffd|p
    If numInsert <> "" Then
        If IsNumeric(numInsert) = False Then
            Call RaiseError(303)
            Exit Sub
        End If
        If CLng(numInsert) <= 0 Then
            Call RaiseError(304)
            Exit Sub
        End If
        If Val(bottomCel.Value) + Val(numInsert) > MAX_COUNT Then
            Call RaiseError(305, CStr(MAX_COUNT))
            Exit Sub
        End If
        msg = MsgBox("|fffd|\|fffd||316||fffd||fffd||fffd||fffd||24e|w|fffd||fffd|s|fffd||fffd||fffd||fffd||1c9||fffd||fffd||fffd||fffd||702||fffd||fffd|B|fffd||fffd||b0b5||fffd||fffd||fffd||142||fffd||fffd||fffd||fffd|H", vbYesNo + vbQuestion, ThisWorkbook.Name)
        If msg = vbYes Then
            Call AddRow(currentRow, numInsert)
            If currentRow = baseRow Then
                shtScreenIndividual.Range("A" & CStr(baseRow)).Value = 1
                shtScreenIndividual.Range("A" & CStr(baseRow + 1)).Value = 2
                shtScreenIndividual.Range("A" & CStr(baseRow) & ":A" & CStr(baseRow + 1)).Select
                If baseRow + 1 = currentRow + CLng(numInsert) Then
                    '|fffd||14f|I|fffd|s|fffd||30f||a347||fffd||fffd|AutoFill|fffd||fffd||fffd||202||fffd|
                Else
                    Selection.AutoFill destination:=Range("A" & CStr(baseRow) & ":A" & currentRow + CLng(numInsert)), Type:=xlFillDefault
                End If
            Else
                shtScreenIndividual.Range("A" & currentRow - 1 & ":A" & currentRow).Select
                Selection.AutoFill destination:=Range("A" & currentRow - 1 & ":A" & currentRow + CLng(numInsert)), Type:=xlFillDefault
            End If
            shtScreenIndividual.Range("E" & currentRow).Select
        End If
    End If
   
    Exit Sub
    
ErrRtn:
    MsgBox (GOTO_ERROR_MSG & vbCrLf & Err.Number & Err.Description)

End Sub

'|fffd|s|fffd||312||1c9||fffd|
Private Sub AddRow(currentRow As Long, numInsert As String)

    shtScreenIndividual.Rows(currentRow).Copy
    shtScreenIndividual.Rows(currentRow + 1 & ":" & currentRow + CLng(numInsert)).Insert Shift:=xlDown
    shtScreenIndividual.Rows(currentRow + 1 & ":" & currentRow + CLng(numInsert)).ClearContents
    
End Sub

