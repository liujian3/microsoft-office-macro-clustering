            msg = MsgBox("|fffd|I|fffd||fffd||fffd||fffd||fffd||fffd||102||fffd||fffd||fffd|Z|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||292|u|fffd||241|A " & numInsert & "|fffd|s|fffd|}|fffd||fffd||fffd||fffd||fffd||702||fffd||fffd|B" & vbCrLf & "|fffd||fffd||b0b5||fffd||fffd||fffd||142||fffd||fffd||fffd||fffd|H", vbYesNo + vbQuestion, ThisWorkbook.Name)
