Public Function CheckData(pValue As String, pCompanyType As String, pDisplayScreen As String, _
                          pCode As String, pItemName As String, pInputSegment As String, _
                          Optional pReason As String = "") As Boolean

    Dim param As String
    Dim Target As String
    Dim i As Long
    Dim k As Long
    Dim dat As String
    Dim tmp As String
    Dim min As Long
    Dim max As Long
    Dim list As Variant
    
    CheckData = False

    param = pCompanyType & pDisplayScreen & pCode & pItemName & pInputSegment

    i = enSHT_ITEMLIST_RW.e|fffd|f|fffd|[|fffd|^|fffd||fffd|
    Do While shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||696||fffd|).Value <> ""
        Target = ""
        If pCompanyType <> "" Then Target = Target & shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||40b||656a|).Value
        If pDisplayScreen <> "" Then Target = Target & shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd|\|fffd||fffd||fffd||fffd||fffd|).Value
        If pCode <> "" Then Target = Target & shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd|R|fffd|[|fffd|h).Value
        If pItemName <> "" Then Target = Target & shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||696||fffd|).Value
        If pInputSegment <> "" Then Target = Target & shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||34b||656a|).Value
    
        tmp = shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||696||fffd|).Value
        If Trim$(shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd|R|fffd|[|fffd|h).Value) <> "" Then
            tmp = tmp & "(" & shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd|R|fffd|[|fffd|h).Value & ")"
        End If
                
        If param = Target Then
            '|fffd|K|fffd|{|fffd||fffd||fffd||fffd|
            If shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd|K|fffd|{|fffd||fffd||fffd||fffd|).Value = "|fffd||fffd|" Then
                If Trim$(pValue) = "" And CheckReason(pReason, i) = True Then
                    Call RaiseError(401, tmp)
                    Exit Function
                End If
            End If
            '|fffd|S|fffd|p
            If shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd|S|fffd|p|fffd||fffd||fffd|p).Value = "|fffd|S|fffd|p" Then
                If Len(pValue) * 2 <> LenB(StrConv(pValue, vbFromUnicode)) Then
                    Call RaiseError(402, tmp)
                    Exit Function
                End If
            End If
            '|fffd||fffd||fffd|p
            If shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd|S|fffd|p|fffd||fffd||fffd|p).Value = "|fffd||fffd||fffd|p" Then
                If Len(pValue) <> LenB(StrConv(pValue, vbFromUnicode)) Then
                    Call RaiseError(403, tmp)
                    Exit Function
                End If
            End If
            '|fffd|p|fffd||fffd||fffd||fffd|
'            If shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||355||fffd||fffd||fffd|).Value = "|fffd|p|fffd||fffd||fffd||fffd|" Then
'                For k = 1 To Len(pValue)
'                    dat = Mid$(pValue, k, 1)
'                    If Not (dat Like "[A-z]" = True Or IsNumeric(dat) = True) Then
'                        Call RaiseError(404, tmp)
'                        Exit Function
'                    End If
'                Next k
'            End If
            '|fffd||fffd||fffd||fffd|
            If shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||355||fffd||fffd||fffd|).Value = "|fffd||fffd||fffd||fffd|" Then
                If Trim$(pValue) <> "" Then
                    If IsNumeric(pValue) = False Then
                        Call RaiseError(405, tmp)
                        Exit Function
                    End If
                End If
            End If
            '|fffd||fffd||fffd||fffd|
'            If shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||355||fffd||fffd||fffd|).Value = "|fffd||fffd||fffd||fffd|" Then
'            End If
            '|fffd||151||fffd|E|fffd||14f||fffd|
            If Trim$(pValue) <> "" Then
                min = Val(shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||14f||fffd||fffd||fffd||fffd||fffd|).Value)
                max = Val(shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||151||5305||fffd||fffd|).Value)
                '|fffd||14f||fffd||fffd||fffd||fffd||fffd|
                If min > 0 Then
                    If Len(pValue) < min Then
                        tmp = shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||696||fffd|).Value & "|fffd||fffd|" & CStr(min) & "|fffd||fffd|"
                        If max > 0 And min = max Then
                            Call RaiseError(406, tmp)
                        Else
                            Call RaiseError(407, tmp)
                        End If
                        Exit Function
                    End If
                End If
                '|fffd||151||5305||fffd||fffd|
                If max > 0 Then
                    If Len(pValue) > max Then
                        tmp = shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||696||fffd|).Value & "|fffd||fffd|" & CStr(max) & "|fffd||fffd|"
                        If min > 0 And min = max Then
                            Call RaiseError(406, tmp)
                        Else
                            Call RaiseError(408, tmp)
                        End If
                        Exit Function
                    End If
                End If
            End If
            '|63d||fffd|
            If Trim$(pValue) <> "" Then
                If shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||355||fffd||fffd||fffd|).Value = "|63d||fffd|" Then
                    If CheckList(pValue, GetList(pCompanyType, pDisplayScreen, pCode, pItemName, pInputSegment)) = False Then
                        Call RaiseError(409, tmp)
                        Exit Function
                    End If
                End If
            End If
            Exit Do
        End If
        i = i + 1
    Loop
    
    CheckData = True
    
End Function

'|fffd||fffd||fffd|X|fffd|g|fffd||64fe|
Public Function GetList(pCompanyType As String, pDisplayScreen As String, _
                        pCode As String, pItemName As String, pInputSegment As String) As Variant
    Dim param As String
    Dim Target As String
    Dim i As Long
    Dim tmp As String
    
    Set GetList = Nothing
    
    param = pCompanyType & pDisplayScreen & pCode & pItemName & pInputSegment

    tmp = ""
    i = enSHT_ITEMLIST_RW.e|fffd|f|fffd|[|fffd|^|fffd||fffd|
    Do While shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||696||fffd|).Value <> ""
        Target = ""
        If pCompanyType <> "" Then Target = Target & shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||40b||656a|).Value
        If pDisplayScreen <> "" Then Target = Target & shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd|\|fffd||fffd||fffd||fffd||fffd|).Value
        If pCode <> "" Then Target = Target & shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd|R|fffd|[|fffd|h).Value
        If pItemName <> "" Then Target = Target & shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||696||fffd|).Value
        If pInputSegment <> "" Then Target = Target & shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||34b||656a|).Value
    
        If param = Target Then
            If shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd||fffd||40b||656a|).Value = "-" Then
                tmp = shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd|\|fffd||fffd||fffd||fffd||fffd|e).Value
            Else
                If tmp = "" Then
                    tmp = shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd|\|fffd||fffd||fffd||fffd||fffd|e).Value
                Else
                    tmp = tmp & "," & shtDataItemList.Cells(i, enSHT_ITEMLIST_CL.e|fffd|\|fffd||fffd||fffd||fffd||fffd|e).Value
                End If
            End If
        End If
        i = i + 1
    Loop
    
    GetList = Split(tmp, ",")
    
End Function

