            shtReportSummary.Cells(iRow, enSHT_SUMMARY_CL.e|fffd||fffd||fffd||fffd|_|fffd||fffd||fffd|v|fffd||6cc||fffd||fffd||fffd|).Value = arrSum(idxReason, idxUsageModel)
