'
'|fffd||fffd||fffd|X|fffd|g|fffd|`|fffd|F|fffd|b|fffd|N
Private Function CheckList(pValue As String, list As Variant) As Boolean

    Dim i As Long
    
    CheckList = True
    
    If Trim$(pValue) = "" Then Exit Function
    
    For i = LBound(list) To UBound(list)
        If pValue = list(i) Then
            Exit Function
        End If
    Next i
    
    CheckList = False
    
End Function

'|fffd||fffd||fffd||fffd|/|fffd||fffd||fffd||503|`|fffd|F|fffd|b|fffd|N
Private Function CheckReason(pReason As String, iRow As Long) As Boolean

    Dim i As Long
    
    CheckReason = True
    
    Select Case Trim$(shtDataItemList.Cells(iRow, enSHT_ITEMLIST_CL.e|fffd||fffd||fffd||34b||656a|).Value)
        Case SHT_ITEMLIST_|fffd||fffd||fffd||34b||656a|_|fffd||fffd|
            If pReason = REASON_|fffd||fffd||fffd||fffd| Then
                Exit Function
            End If
        Case SHT_ITEMLIST_|fffd||fffd||fffd||34b||656a|_|fffd||fffd|
            If pReason = REASON_|fffd||fffd||fffd||fffd| Then
                Exit Function
            End If
        Case Else
            Exit Function
    End Select
    
    CheckReason = False
    
End Function




