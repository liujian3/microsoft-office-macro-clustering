    folder = shtScreenMenu.Range(SHT_MENU_RG_|fffd|C|fffd||fffd||fffd|X|fffd|g|fffd|[|fffd||fffd||fffd|t|fffd|H|fffd||fffd||fffd|__|fffd||fffd||fffd||34d||fffd||fffd||fffd|).Value

