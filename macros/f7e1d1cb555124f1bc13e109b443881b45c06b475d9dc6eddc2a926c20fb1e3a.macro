Sub SorteerEnverberg()
Attribute SorteerEnverberg.VB_ProcData.VB_Invoke_Func = "q\n14"
'
' SorteerEnverberg Macro
'
' Sneltoets: Ctrl+q
'
    ActiveWorkbook.Worksheets("Verkoopprijslijn PRIJS 1 NW BTW").AutoFilter.Sort. _
        SortFields.Clear
    ActiveWorkbook.Worksheets("Verkoopprijslijn PRIJS 1 NW BTW").AutoFilter.Sort. _
        SortFields.Add Key:=Range("E4"), SortOn:=xlSortOnValues, Order:= _
        xlDescending, DataOption:=xlSortNormal
    With ActiveWorkbook.Worksheets("Verkoopprijslijn PRIJS 1 NW BTW").AutoFilter. _
        Sort
        .Header = xlYes
        .MatchCase = False
        .Orientation = xlTopToBottom
        .SortMethod = xlPinYin
        .Apply
    End With
    Sheets("Wincash Input prijs").Select
    ActiveWindow.SelectedSheets.Visible = False
    Sheets("Wincash Input aantal").Select
    ActiveWindow.SelectedSheets.Visible = False
    Sheets("Groep").Select
    ActiveWindow.SelectedSheets.Visible = False
    Sheets("Productbron").Select
    ActiveWindow.SelectedSheets.Visible = False
End Sub
