    Exit Sub
  End If
  
End Sub

Sub Grenzwert(leer$, i, fehler, Max, Min)
Dim Wert As Double
On Error GoTo errorhandler
  fehler = False
  If InStr(leer$, " ") Then
    leer$ = Replace(leer$, " ", "")
  End If
  If leer$ = "" Then
    If i = 114 Then
      MsgBox "Textfeld: " & Chr(34) & Feldbezeichnung$(i) & Chr(34) & " nicht ausgef|fffd|llt!", 48, "Elektronisches Vertragsmuster Wartung 2018" '& i '& i s. o.
      Fehlertext$ = Fehlertext$ + "Textfeld: " & Feldbezeichnung$(i) & " nicht ausgef|fffd|llt!" & vbCrLf
    Else
      MsgBox "Textfeld: " & Chr(34) & Feldbezeichnung$(i) & Chr(34) & " nicht ausgef|fffd|llt!" & _
      Chr(13) & "Er wurde durch den gesetzlichen Mindestlohn ersetzt." & msg3 & Chr(13), 48, "Elektronisches Vertragsmuster Wartung 2018" '& i '& i s. o.
      Fehlertext$ = Fehlertext$ + "Textfeld: " & Feldbezeichnung$(i) & " nicht ausgef|fffd|llt!" & vbCrLf
    End If
    fehler = True
    ok = False
    Exit Sub
  End If
  Wert = CDbl(leer$)
  If Wert > Max Or Wert < Min Then
    If i = 103 Then
      MsgBox "Im Textfeld: " & Chr(34) & Feldbezeichnung$(i) & Chr(34) & " wurde ein unzul|fffd|ssiger Wert eingesetzt." & _
      Chr(13) & "Die Eingabe wurde gel|fffd|scht" & msg3 & Chr(13), 48, "Elektronisches Vertragsmuster Wartung 2018"
    Else
      MsgBox "Textfeld: " & Chr(34) & Feldbezeichnung$(i) & Chr(34) & " wurde ein unzul|fffd|ssiger Wert eingesetzt!" & _
      Chr(13) & "Er wurde durch den gesetzlichen Mindestlohn ersetzt." & msg3 & Chr(13), 48, "Elektronisches Vertragsmuster Wartung 2018"
    End If
    fehler = True
    ok = False
    Fehlertext$ = Fehlertext$ + "Textfeld: " & Feldbezeichnung$(i) & " enthielt einen unzul|fffd|ssigen Wert!" & vbCrLf
    Exit Sub
  End If
Exit Sub
errorhandler:
  MsgBox "Textfeld: " & Chr(34) & Feldbezeichnung$(i) & Chr(34) & Chr(13) & " enthielt einen unzul|fffd|ssigen Eintrag!" & _
  Chr(13), 48, "Elektronisches Vertragsmuster Wartung 2018 "
  fehler = True
  ok = False
  Fehlertext$ = Fehlertext$ + "Textfeld: " & Feldbezeichnung$(i) & " enthielt einen unzul|fffd|ssigen Eintrag!" & vbCrLf
  Exit Sub
  
End Sub

