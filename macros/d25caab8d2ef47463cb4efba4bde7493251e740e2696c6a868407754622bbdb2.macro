Attribute VB_Name = "trucking_TR_cd"
Public Function trucking_TR_btn()

    On Error GoTo 0

    'Definitionen aktivieren
    define.define_sheets

    'Abfrage in Schattentabelle ob Unterbuttons Bahn eingeblendet ja/nein
    If tblhome.Cells(zeile_a, 3).Value = 0 Then

        'Einblenden der Unterbuttons Bahn und Ausblenden Trucking
        tblhome.Shapes("btntrucking" & a).Visible = True
        tblhome.Shapes("btnbahn" & a).Visible = False
        tblhome.Shapes("btnpreisnk" & a).Visible = True
        tblhome.Shapes("btnpreisortDE" & a).Visible = True
        tblhome.Shapes("btnpreisortAT" & a).Visible = True
        tblhome.Shapes("btnleer" & a).Visible = True
  
         'In Schattentabelle festhalten, dass Unterbuttons nun eingeblendet sind
        tblhome.Cells(zeile_a, 3).Value = 1

    Else

        'Da bereits eingeblendet. Deshalb Unterbuttons wieder ausblenden und
        'in den Urzustand zur|fffd|ck gehen.
        tblhome.Shapes("btntrucking" & a).Visible = True
        tblhome.Shapes("btnbahn" & a).Visible = True
        tblhome.Shapes("btnpreisnk" & a).Visible = False
        tblhome.Shapes("btnpreisortDE" & a).Visible = False
        tblhome.Shapes("btnpreisortAT" & a).Visible = False
        tblhome.Shapes("btnleer" & a).Visible = False
  
          'In Schattentabelle festhalten, dass Unterbuttons nun ausgeblendet sind
        tblhome.Cells(zeile_a, 3).Value = 0

    End If

    'Variable f|fffd|r die Zeilenausblendevariante wird vergeben
    verwVar = 0

End Function
