Attribute VB_Name = "bahn_BA_cd"
Public Function btnbahnclick()

On Error GoTo 0

    'Definitionen aktivieren
    define.define_sheets

    'Abfrage in Schattentabelle ob Unterbuttons Bahn eingeblendet ja/nein
    If tblhome.Cells(zeile_a, 4).Value = 0 Then

        'Einblenden der Unterbuttons Bahn und Ausblenden Trucking
        tblhome.Shapes("btntrucking" & a).Visible = False
        tblhome.Shapes("btnbahn" & a).Visible = True
        tblhome.Shapes("btnBAleistnk" & a).Visible = True
        tblhome.Shapes("btnBApreisortDE" & a).Visible = True
        tblhome.Shapes("btnBApreisortAT" & a).Visible = True
   
        'In Schattentabelle festhalten, dass Unterbuttons nun eingeblendet sind
        tblhome.Cells(zeile_a, 4).Value = 1

    Else
    
        'Da bereits eingeblendet. Deshalb Unterbuttons wieder ausblenden und
        'in den Urzustand zur|fffd|ck gehen.
        tblhome.Shapes("btntrucking" & a).Visible = True
        tblhome.Shapes("btnbahn" & a).Visible = True
        tblhome.Shapes("btnBAleistnk" & a).Visible = False
        tblhome.Shapes("btnBApreisortDE" & a).Visible = False
        tblhome.Shapes("btnBApreisortAT" & a).Visible = False
        
        'In Schattentabelle festhalten, dass Unterbuttons nun ausgeblendet sind
        tblhome.Cells(zeile_a, 4).Value = 0

    End If

    'Variable f|fffd|r Zeilenausblenden-Variante wird vergeben
    verwVar = 1

End Function
