Attribute VB_Name = "preisortsuche_cd"
Public Function btnsuche_click()

    On Error GoTo 0

    'Definition der Variablen
    define.define_sheets

    Application.ScreenUpdating = False

        'Blattschutz l|fffd|sen, damit Filter funktioniert
        preisortsucheunprotect_cd.tbl_preisort_unprotect

        'Filter zur|fffd|cksetzen, damit bei Neueingabe wieder alles in Betracht gezogen wird
        wsVar_preisort.Range("$C$10:$D$10000").AutoFilter Field:=1
        wsVar_preisort.Range("$C$10:$D$10000").AutoFilter Field:=2

        'Pr|fffd|fen ob nur Ort eingegeben wurde. Bei leeren Zahlenfilter PLZ funktioniert der Filter sonst nicht
        If filtVar_plz.Value = "" Then

            'Falls PLZ frei, dann nur |fffd|ber Ort filtern
            wsVar_preisort.Range("$C$10:$D$10000").AutoFilter Field:=2, _
            Criteria1:=filtVar_ort.Value
        
        Else

            'Pr|fffd|fen ob nur PLZ eingegeben wurde. Bei leeren Ortsfilter funktioniert der Filter sonst nicht
            If filtVar_ort.Value = "" Then

            'Falls Ort frei, dann nur |fffd|ber PLZ filter, analog PLZ
            wsVar_preisort.Range("$C$10:$D$10000").AutoFilter Field:=1, _
            Criteria1:=filtVar_plz.Value
            
                Else
                    
                    'Im Falle beide sind ausgew|fffd|hlt, |fffd|ber beide Filtern. Zuerst die PLZ
                    wsVar_preisort.Range("$C$10:$D$10000").AutoFilter Field:=1, _
                    Criteria1:=filtVar_plz.Value
        
                    wsVar_preisort.Range("$C$10:$D$10000").AutoFilter Field:=2, _
                    Criteria1:=filtVar_ort.Value
            End If

        End If

        'Blattschutz wieder aktivieren
        preisortsucheprotect_cd.tbl_preisort_protect

    Application.ScreenUpdating = True

End Function
Attribute VB_Name = "preisortsucheprotect_cd"
Public Function tbl_preisort_protect()

    'Blattschutz aktivieren
    wsVar_preisort.Protect Password:="mariok", DrawingObjects:=True, Contents:=True, Scenarios:=True, UserInterfaceOnly:=False, _
        AllowFormattingCells:=False, AllowFormattingColumns:=False, AllowFormattingRows:=False, AllowInsertingColumns:=False, _
        AllowInsertingHyperlinks:=False, AllowDeletingColumns:=False, AllowDeletingRows:=False, AllowSorting:=False, _
        AllowFiltering:=False, AllowUsingPivotTables:=False
        
        ActiveSheet.EnableSelection = xlNoSelection

End Function
