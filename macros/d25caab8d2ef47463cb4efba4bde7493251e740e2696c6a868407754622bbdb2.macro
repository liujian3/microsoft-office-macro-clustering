Attribute VB_Name = "preisort_cd"
Public Function btnpreisort_click()

    On Error GoTo 0

    Application.ScreenUpdating = False

        'Definition der Variablen
        define.define_sheets

        'Einblenden des entsprechenden Reiters. Variable wird |fffd|ber den Klick auf Button
        'zugewiesen.
        wsVar_preisort.Visible = xlSheetVisible
        wsVar_preisort.Activate
        tblhome.Visible = xlSheetVeryHidden
        wsVar_preisnk.Visible = xlSheetVeryHidden

        'Bereinigt Textfelder in Suchmaske
        filtVar_ort.Value = ""
        filtVar_plz.Value = ""

        'Blattschutz deaktivieren
        preisortsucheunprotect_cd.tbl_preisort_unprotect
    
            'Unterscheidet ob man von Bahn oder TR die Ansicht ben|fffd|tigt und gestaltet entsprechend.
            If verwVar = 0 Then
        
                'Blattschutz deaktivieren
                preisortsucheunprotect_cd.tbl_preisort_unprotect
    
                '|fffd|berschrift setzen
                wsVar_preisort.Cells(3, 3).Value = us_ortsuche_TR & us_land
        
                'Spalten Bahnpreise ausblenden
                wsVar_preisort.Columns("G:P").Hidden = True
                wsVar_preisort.Columns("F:F").Hidden = False
        
                'Zeilenh|fffd|he anpassen
                wsVar_preisort.Rows("8:8").RowHeight = 15.75
        
                'Blattschutz aktivieren
                preisortsucheprotect_cd.tbl_preisort_protect
             
                'Richtigen Verweis-Button einblenden
                wsVar_preisort.Shapes("btnpreisnk_verw").Visible = True
                wsVar_preisort.Shapes("btnleistnk_verw").Visible = False
            
            Else
        
                'Blattschutz deaktivieren
                preisortsucheunprotect_cd.tbl_preisort_unprotect
    
                '|fffd|berschrift setzen
                wsVar_preisort.Cells(3, 3).Value = us_ortsuche_BA & us_land
        
                'Spalten Bahnpreise ausblenden
                wsVar_preisort.Columns("G:P").Hidden = False
                wsVar_preisort.Columns("F:F").Hidden = True
        
                'Zeilenh|fffd|he anpassen
                wsVar_preisort.Rows("8:8").RowHeight = 52.5
        
                'Blattschutz aktivieren
                preisortsucheprotect_cd.tbl_preisort_protect
                
                'Richtigen Verweis-Button einblenden
                wsVar_preisort.Shapes("btnpreisnk_verw").Visible = False
                wsVar_preisort.Shapes("btnleistnk_verw").Visible = True
  
            End If

    Application.ScreenUpdating = True

End Function
