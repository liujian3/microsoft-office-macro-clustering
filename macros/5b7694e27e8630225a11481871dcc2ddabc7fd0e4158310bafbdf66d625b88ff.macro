Public Function |fffd|ropWord(s As Variant) As String
    If Len(s) = 1 Then
        |fffd|ropWord = s
    Else
        ss$ = UCase(Left$(s, 1)) & ".": k = InStr(s, "-")
        If k > 0 Then ss$ = ss$ & "-" & Mid$(s, k + 1, 1) & "."
        |fffd|ropWord = ss$
    End If
End Function

