    Dim doc As Object, ecount As Long, bm As Object, myStoryRange As Object, i&, oFirstCellRange As Object, bmText$, options As Dictionary
    pi.Line3 = "|fffd||fffd||fffd||fffd||fffd||fffd|: " & Dir(TemplateFilename$, vbNormal)

