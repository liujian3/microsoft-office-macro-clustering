                        pi.Line3 = "|fffd||fffd||fffd||fffd||fffd| |fffd|" & bm.Name & "|fffd|, |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd| " & rc - i + 1 & " |fffd||fffd| " & rc&
