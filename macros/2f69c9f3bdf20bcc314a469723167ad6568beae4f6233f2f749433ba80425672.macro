Private Sub CommandButton_Summe_Arbeitsgaenge_Click()
        
    If TextBox_Preis_SpaltenPU.Value = "" Then TextBox_Preis_SpaltenPU.Value = 0
    If TextBox_Preis_SpaltenPE.Value = "" Then TextBox_Preis_SpaltenPE.Value = 0
    If TextBox_Preis_SchneidenRechteck.Value = "" Then TextBox_Preis_SchneidenRechteck.Value = 0
    If TextBox_Preis_SchneidenKontur.Value = "" Then TextBox_Preis_SchneidenKontur.Value = 0
    If TextBox_Preis_Stanzen.Value = "" Then TextBox_Preis_Stanzen.Value = 0
    If TextBox_Preis_Ausbrechen.Value = "" Then TextBox_Preis_Ausbrechen.Value = 0
    If TextBox_Preis_Fr|fffd|sen.Value = "" Then TextBox_Preis_Fr|fffd|sen.Value = 0
    If TextBox_Preis_VerklebenF.Value = "" Then TextBox_Preis_VerklebenF.Value = 0
    If TextBox_Preis_VerklebenP.Value = "" Then TextBox_Preis_VerklebenP.Value = 0
    If TextBox_Preis_VerschweissenF.Value = "" Then TextBox_Preis_VerschweissenF.Value = 0
    If TextBox_Preis_VerschweissenP.Value = "" Then TextBox_Preis_VerschweissenP.Value = 0
    If TextBox_Preis_Schr|fffd|gschneiden.Value = "" Then TextBox_Preis_Schr|fffd|gschneiden.Value = 0
    If TextBox_sonstigesProStueck.Value = "" Then TextBox_sonstigesProStueck.Value = 0
    If TextBox_sonstigesPauschal.Value = "" Then TextBox_sonstigesPauschal.Value = 0
       
    TextBox_gesamtpreis_Arbeitsgaenge.Value = CDbl(TextBox_Preis_SpaltenPU.Value) + CDbl(TextBox_Preis_SpaltenPE.Value) _
    + CDbl(TextBox_Preis_SchneidenRechteck.Value) + CDbl(TextBox_Preis_SchneidenKontur.Value) _
    + CDbl(TextBox_Preis_Stanzen.Value) + CDbl(TextBox_Preis_Ausbrechen.Value) + CDbl(TextBox_Preis_Fr|fffd|sen.Value) _
    + CDbl(TextBox_Preis_VerklebenF.Value) + CDbl(TextBox_Preis_VerklebenP.Value) + CDbl(TextBox_Preis_VerschweissenF.Value) _
    + CDbl(TextBox_Preis_VerschweissenP.Value) + CDbl(TextBox_Preis_Schr|fffd|gschneiden.Value) _
    + CDbl(TextBox_sonstigesProStueck.Value) + CDbl(TextBox_sonstigesPauschal.Value)
    
    TextBox_gesamtpreis_Arbeitsgaenge1000.Value = CDbl(TextBox_Preis_SpaltenPU1000.Value) + CDbl(TextBox_Preis_SpaltenPE1000.Value) _
    + CDbl(TextBox_Preis_SchneidenRechteck1000.Value) + CDbl(TextBox_Preis_SchneidenKontur1000.Value) _
    + CDbl(TextBox_Preis_Stanzen1000.Value) + CDbl(TextBox_Preis_Ausbrechen1000.Value) + CDbl(TextBox_Preis_Fr|fffd|sen1000.Value) _
    + CDbl(TextBox_Preis_VerklebenF1000.Value) + CDbl(TextBox_Preis_VerklebenP1000.Value) + CDbl(TextBox_Preis_VerschweissenF1000.Value) _
    + CDbl(TextBox_Preis_VerschweissenP1000.Value) + CDbl(TextBox_Preis_Schr|fffd|gschneiden1000.Value) _
    + CDbl(TextBox_sonstigesProStueck1000.Value) + CDbl(TextBox_sonstigesPauschal1000.Value)
    
End Sub

