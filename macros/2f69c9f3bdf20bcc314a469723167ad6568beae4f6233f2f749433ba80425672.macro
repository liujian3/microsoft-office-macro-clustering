Private Sub CommandButton_RabattAbziehen_Click()
    If TextBox_Summe_MaterialpreisGesamt.Value = "" Then
        MsgBox "Bitte dr|fffd|cken Sie zuerst den Summen-Button!"
    Else:
        If TextBox_Rabatt_Material.Value = "" Then TextBox_Rabatt_Material.Value = 0
        If TextBox_Rabatt_Bearbeitung.Value = "" Then TextBox_Rabatt_Bearbeitung.Value = 0
        TextBox_MaterialpreisGesamt_abzglRabatt.Value = CDbl(TextBox_Summe_MaterialpreisGesamt.Value) * (1 - (CDbl(TextBox_Rabatt_Material.Value) / 100))
        TextBox_Materialpreis1000_abzglRabatt.Value = CDbl(TextBox_Summe_Materialpreis1000.Value) * (1 - (CDbl(TextBox_Rabatt_Material.Value) / 100))
        TextBox_BearbeitungspreisGesamt_abzglRabatt.Value = CDbl(TextBox_Summe_BearbeitungspreisGesamt.Value) * (1 - (CDbl(TextBox_Rabatt_Bearbeitung.Value) / 100))
        TextBox_Bearbeitungspreis1000_abzglRabatt.Value = CDbl(TextBox_Summe_Bearbeitungspreis1000.Value) * (1 - (CDbl(TextBox_Rabatt_Bearbeitung.Value) / 100))
    End If
End Sub


Private Sub CommandButton_Berechne_Endpreis_Click()
    If TextBox_Summe_MaterialpreisGesamt.Value = "" Then
        MsgBox "Bitte dr|fffd|cken Sie zuerst den Summen-Button!"
    Else:
        If TextBox_Rabatt_Material.Value = "" Then
            TextBox_Rabatt_Material.Value = 0
            TextBox_MaterialpreisGesamt_abzglRabatt.Value = CDbl(TextBox_Summe_MaterialpreisGesamt.Value) * (1 - (CDbl(TextBox_Rabatt_Material.Value) / 100))
            TextBox_Materialpreis1000_abzglRabatt.Value = CDbl(TextBox_Summe_Materialpreis1000.Value) * (1 - (CDbl(TextBox_Rabatt_Material.Value) / 100))
        End If
        If TextBox_Rabatt_Bearbeitung.Value = "" Then
            TextBox_Rabatt_Bearbeitung.Value = 0
            TextBox_BearbeitungspreisGesamt_abzglRabatt.Value = CDbl(TextBox_Summe_BearbeitungspreisGesamt.Value) * (1 - (CDbl(TextBox_Rabatt_Bearbeitung.Value) / 100))
            TextBox_Bearbeitungspreis1000_abzglRabatt.Value = CDbl(TextBox_Summe_Bearbeitungspreis1000.Value) * (1 - (CDbl(TextBox_Rabatt_Bearbeitung.Value) / 100))
        End If
        TextBox_Gesamtpreis_Angebot.Value = CDbl(TextBox_MaterialpreisGesamt_abzglRabatt.Value) + CDbl(TextBox_BearbeitungspreisGesamt_abzglRabatt.Value)
        TextBox_Gesamtpreis_Angebot1000.Value = CDbl(TextBox_Materialpreis1000_abzglRabatt.Value) + CDbl(TextBox_Bearbeitungspreis1000_abzglRabatt.Value)
    End If
End Sub

