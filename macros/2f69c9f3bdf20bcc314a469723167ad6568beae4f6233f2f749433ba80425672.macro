                gesamtpreis_Material = CDbl(TextBox_verwendeteBloecke.Value) * preisProBlock + preisExtraBlock
            End If
        End If
    ElseIf Kalkulationsdetails.TextBox_resthoehemehrbloecke.Value >= TextBox_abfallhoehe.Value And TextBox_verwendeteBloecke.Value > 1 Then 'mehr als 1 Block und rest exklusive Preis
        If bool_ekm3 = True Then
            preisProBlock = Kalkulationsdetails.TextBox_plattenProBlock.Value * plattenVolumen * verechnungspreis
            If extraBlock = True Then
                If Kalkulationsdetails.TextBox_restextrablock.Value < TextBox_abfallhoehe.Value Then
                    preisExtraBlock = preisProBlock
                Else:
                    preisExtraBlock = Kalkulationsdetails.TextBox_plattenextraBlock.Value * plattenVolumen * verechnungspreis
                End If
            End If
            If extraBlock = True Then
                gesamtpreis_Material = (CDbl(TextBox_verwendeteBloecke.Value) - 1) * preisProBlock + preisExtraBlock
            Else:
                gesamtpreis_Material = CDbl(TextBox_verwendeteBloecke.Value) * preisProBlock + preisExtraBlock
            End If
        ElseIf bool_vkm3 = True Then
            preisProBlock = Kalkulationsdetails.TextBox_plattenProBlock.Value * plattenVolumen * verechnungspreis
            If extraBlock = True Then
                If Kalkulationsdetails.TextBox_restextrablock.Value < TextBox_abfallhoehe.Value Then
                    preisExtraBlock = preisProBlock
                Else:
                    preisExtraBlock = Kalkulationsdetails.TextBox_plattenextraBlock.Value * plattenVolumen * verechnungspreis
                End If
            End If
            If extraBlock = True Then
                gesamtpreis_Material = (CDbl(TextBox_verwendeteBloecke.Value) - 1) * preisProBlock + preisExtraBlock
            Else:
                gesamtpreis_Material = CDbl(TextBox_verwendeteBloecke.Value) * preisProBlock + preisExtraBlock
            End If
        End If
    End If
'--------------------------------------------------------------------------------------------------------
'Quadratmeter-------------------------------------------------

plattenflaeche = TextBox_Laenge.Value / 1000 * ComboBox_Breite.Value / 1000 'in m|fffd|
