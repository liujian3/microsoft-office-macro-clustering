Private Sub CommandButton_|fffd|bernehmen_Fr|fffd|sen_Click()
    If TextBox_Preis_Fr|fffd|senGesamt.Value = "" Then
        MsgBox "Bitte bilden sie erst die Summe mit einem Klick auf den Summen-Button"
    Else:
        Eingabefenster.TextBox_Preis_Fr|fffd|sen.Value = TextBox_Preis_Fr|fffd|senGesamt.Value
        Eingabefenster.TextBox_Preis_Fr|fffd|sen1000.Value = TextBox_Preis_Fr|fffd|senGesamttau.Value
        Unload Me
    End If
End Sub


