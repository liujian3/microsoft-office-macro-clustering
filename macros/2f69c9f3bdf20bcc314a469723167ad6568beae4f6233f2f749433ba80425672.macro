Private Sub CommandButton_Berechnen_Ausbrechen_Click()
    Dim check As Integer
    check = 1
    If TextBox_PreisProAusbruch.Value = "" Then check = check + 1
    If TextBox_Ausbrueche.Value = "" Then check = check + 1
    If TextBox_Lagen.Value = "" Then check = check + 1
    
    If check = 1 Then
        Ausbrechen.TextBox_Preis_Ausbrechen.Value = (Ausbrechen.TextBox_PreisProAusbruch.Value * Ausbrechen.TextBox_Ausbrueche.Value) _
        / Ausbrechen.TextBox_Lagen.Value * TextBox_AnzahlKundenvorgabe.Value
        Ausbrechen.TextBox_Preis_Ausbrechentau.Value = Ausbrechen.TextBox_Preis_Ausbrechen.Value / TextBox_AnzahlKundenvorgabe.Value * 1000
    End If
End Sub


