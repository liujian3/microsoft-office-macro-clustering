Private Sub CommandButton_Berechnen_SchneidenKontur1_Click()
    Dim bool_inputCorrect As Boolean
    bool_inputCorrect = True
    If TextBox_Umfangzuschnitt1.Value = "" Then TextBox_Umfangzuschnitt1.Value = 0
    If TextBox_anzAussparungen1.Value = "" Then TextBox_anzAussparungen1.Value = 0
    If TextBox_LaengederAussparung1.Value = "" Then TextBox_LaengederAussparung1.Value = 0
    If TextBox_BreitederAussparung1.Value = "" Then TextBox_BreitederAussparung1.Value = 0
    If TextBox_Vorschub1.Value = "" Then TextBox_Vorschub1.Value = 1
    If TextBox_PreisProSchneidstunde1.Value = "" Then
        bool_inputCorrect = False
        MsgBox ("PreisProSchneidstunde muss definiert sein!")
    End If
    If TextBox_Lagen1.Value = "" Then TextBox_Lagen1.Value = 1
   
   
    If bool_inputCorrect = True Then
        TextBox_Preis_SchneidenKontur1.Value = (CDbl(TextBox_Umfangzuschnitt1.Value) + _
            CDbl(TextBox_anzAussparungen1.Value) * (CDbl(TextBox_LaengederAussparung1.Value) * 2 + CDbl(TextBox_BreitederAussparung1.Value) * 2)) / CDbl(TextBox_Vorschub1.Value) _
            / 60 * CDbl(TextBox_PreisProSchneidstunde1.Value) / 60 / CDbl(TextBox_Lagen1.Value)
          TextBox_Preis_SchneidenKontur1.Value = CDec(TextBox_Preis_SchneidenKontur1.Value) * TextBox_anzKundevorgabe.Value
          TextBox_Preis_SchneidenKontur1tau.Value = CDec(CDbl(TextBox_Preis_SchneidenKontur1.Value)) / CDbl(TextBox_anzKundevorgabe.Value) * 1000
    End If
End Sub
