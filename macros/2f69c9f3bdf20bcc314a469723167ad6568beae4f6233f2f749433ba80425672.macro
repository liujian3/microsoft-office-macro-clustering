Private Sub CommandButton_Beenden_Click()
    'Kalkulationen auf Excel Sheet |fffd|bertragen !!!!!
    'Saubere Form mit allen wichtigen Daten
    
    '------------------------------------------------------------------------
    'Erste freie Zeile ausfindeig machen
    Dim last As Integer
    last = Worksheets("Erstellte Angebote").Cells(Rows.count, 1).End(xlUp).Row + 1
    '|fffd|BERTRAGUNG in Excel
    ''Material |fffd|bertragung
    Cells(last, 1).Value = last '-1
    Cells(last, 2).Value = ComboBox_Material.Value
    Cells(last, 3).Value = TextBox_Gesamtpreis.Value
    ''Ma|fffd|e |fffd|bertragung
    Cells(last, 4).Value = TextBox_Laenge.Value + "x" + ComboBox_Breite.Value + "x" + ComboBox_Hoehe.Value
    Cells(last, 5).Value = TextBox_AnzahlKundenvorgabe.Value
    Cells(last, 6).Value = TextBox_Materialpreis.Value
    Cells(last, 7).Value = TextBox_verwendeteBloecke.Value
    Cells(last, 8).Value = Kalkulationsdetails.TextBox_verwPlatten.Value

    Cells(last, 10).Value = TextBox_gesamtpreis_Arbeitsgaenge.Value
    
    Unload Me
End Sub


