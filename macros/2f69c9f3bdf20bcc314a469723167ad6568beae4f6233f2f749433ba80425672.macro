    TextBox_Preis_Fr|fffd|senGesamttau.Value = CDbl(TextBox_Preis_Fr|fffd|sen1tau.Value) + CDbl(TextBox_Preis_Fr|fffd|sen2tau.Value) + CDbl(TextBox_Preis_Fr|fffd|sen3tau.Value) + CDbl(TextBox_Preis_Fr|fffd|sen4tau.Value) + CDbl(TextBox_Preis_Fr|fffd|sen5tau.Value) _
    + CDbl(TextBox_Preis_Fr|fffd|sen6tau.Value) + CDbl(TextBox_Preis_Fr|fffd|sen7tau.Value) + CDbl(TextBox_Preis_Fr|fffd|sen8tau.Value) + CDbl(TextBox_Preis_Fr|fffd|sen9tau.Value) + CDbl(TextBox_Preis_Fr|fffd|sen10tau.Value)
    
