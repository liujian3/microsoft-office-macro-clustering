Private Sub CommandButton_Berechnen_Fr|fffd|sen1_Click()
    Dim check As Integer
    check = 1
    If TextBox_TiefederAusnehmung1.Value = "" Then check = check + 1
    If TextBox_Fr|fffd|stiefe1.Value = "" Then check = check + 1
    If TextBox_Ausnehmungen1.Value = "" Then check = check + 1
    If TextBox_LaengederAusnehmung1.Value = "" Then check = check + 1
    If TextBox_BreitederAusnehmung1.Value = "" Then check = check + 1
    If TextBox_Vorschub1.Value = "" Then check = check + 1
    If TextBox_FraseDurchmesser1.Value = "" Then check = check + 1
    If TextBox_|fffd|berlappung1.Value = "" Then check = check + 1
    If TextBox_PreisProFraesstunde1.Value = "" Then check = check + 1
    
    If check = 1 Then
    Dim tmp As Double
    Dim aufGesMin As Double
    Dim konturGesMin As Double
    tmp = WorksheetFunction.RoundUp(CDbl(TextBox_TiefederAusnehmung1.Value) / CDbl(TextBox_Fr|fffd|stiefe1.Value), 0)
    konturGesMin = (TextBox_Ausnehmungen1.Value * (TextBox_LaengederAusnehmung1.Value * 2 + TextBox_BreitederAusnehmung1.Value * 2)) * tmp _
                / CDbl(TextBox_Vorschub1.Value) / 60
    aufGesMin = (TextBox_BreitederAusnehmung1.Value * TextBox_LaengederAusnehmung1.Value) / (TextBox_FraseDurchmesser1.Value * ((100 - CDbl(TextBox_|fffd|berlappung1.Value) / 100) _
                * TextBox_Vorschub1.Value) * tmp * TextBox_Ausnehmungen1.Value) / 60
    TextBox_Preis_Fr|fffd|sen1.Value = (konturGesMin + aufGesMin) * (TextBox_PreisProFraesstunde1.Value / 60) * TextBox_anzKundevorgabe.Value
    TextBox_Preis_Fr|fffd|sen1tau.Value = TextBox_Preis_Fr|fffd|sen1.Value / TextBox_anzKundevorgabe.Value * 1000
    End If
End Sub
