i = 3
Do While i < table_length + 3
    If IsEmpty(Worksheets("DATA").Cells(i, 2).Value) = False Then
        ComboBox_Material.AddItem (Worksheets("DATA").Cells(i, 2).Value)
        
        '.RowSource = "DATA!B3:B" & table_length
        '.Style = fmStyleDropDownList
        '.ListRows = 10
    End If
    i = i + 1
Loop

End Sub

