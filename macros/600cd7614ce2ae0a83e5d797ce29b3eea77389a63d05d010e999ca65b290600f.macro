Sub ProperCase()
Dim cell As Range
For Each cell In Selection
If Not IsError(cell.Value) Then cell.Value = WorksheetFunction.Proper(cell.Value)
Next
End Sub

Sub UpperCase()
Dim cell As Range
For Each cell In Selection
If Not IsError(cell.Value) Then cell.Value = UCase(cell.Value)
Next
End Sub

Sub LowerCase()
Dim cell As Range
For Each cell In Selection
If Not IsError(cell.Value) Then cell.Value = LCase(cell.Value)
Next
End Sub


