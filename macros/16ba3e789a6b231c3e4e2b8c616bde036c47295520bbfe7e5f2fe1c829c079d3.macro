Sub PdcSistemi_Click()
Attribute PdcSistemi_Click.VB_ProcData.VB_Invoke_Func = " \n14"
' Elimina i filtri eventualmente selezionati sulle colonne
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=1
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=2
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=3
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=4
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=5
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=6
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=7
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=8
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=9
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=10
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=11
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=12
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=13
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=14
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=15
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=16
    ActiveSheet.Range("$A$37:$Q$2173").AutoFilter Field:=17
'  Copia la prima riga delle colonne con formule sino al termine delle celle utilizzate per lavoro
  Range("J38:T38").Select
    Selection.AutoFill Destination:=Range("J38:T2173"), Type:=xlFillDefault
    Range("J38:T2173").Select
'  Creazione del File per esportazione dati in Sistemi
Sheets("Bil SISTEMI").Select
    Cells.Select
    Selection.Copy
'  Con la variabile "w1" identifico il percorso corrente
    w1 = Application.ActiveWorkbook.Path
    Workbooks.Open Filename:= _
        w1 & "\File Sistemi.xlsx"
    Cells.Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    Range("A1").Select
    Application.CutCopyMode = False
    ActiveWorkbook.Save
    ActiveWindow.Close
    Sheets("Pdc").Select
    Range("E6").Select
    MsgBox ("Fine elaborazione")
End Sub
