Sub ExpandNamedRange(rangeName As String)
    Dim n As Name
    Set n = ActiveWorkbook.Names(rangeName)
    
    StartEdit
    
    ' Pocitam s tym, ze posledny riadok je hidenuty.
    ' Najprv ho ukazem a potom znova schovam.
    ' Keby som ho neukazal, tak Insert prida riadok pred predposledny a nie pred posledny riadok.
    ' Posledny riadok je schovany, aby novopridavany riadok nebral formatovanie zo sumarneho/ukoncovacieho riadku.
    '
    ' 2007-05-18 - FIX: Insert robeny na EntireRow, inac toneposuvalo bunky aj s formatovanim korektne.
    '
    n.RefersToRange.Rows(n.RefersToRange.Rows.Count).EntireRow.Hidden = False
    n.RefersToRange.Rows(n.RefersToRange.Rows.Count).EntireRow.Insert
    n.RefersToRange.Rows(n.RefersToRange.Rows.Count).EntireRow.Hidden = True
    
    ' Musim este nastavit border na spodku, lebo to nejako blbne :-/
    Dim r As Range
    Set r = n.RefersToRange.Rows(n.RefersToRange.Rows.Count - 1)
    r.Borders(xlEdgeTop).LineStyle = xlSolid
    r.Borders(xlEdgeTop).Weight = xlThin
    
    FinishEdit
End Sub
Sub ExpandNamedRangeMaxCount(rangeName As String, maxCount As Integer)
    Dim n As Name
    Set n = ActiveWorkbook.Names(rangeName)
    
    StartEdit
    
    ' Pocitam s tym, ze posledny riadok je hidenuty.
    ' Najprv ho ukazem a potom znova schovam.
    ' Keby som ho neukazal, tak Insert prida riadok pred predposledny a nie pred posledny riadok.
    ' Posledny riadok je schovany, aby novopridavany riadok nebral formatovanie zo sumarneho/ukoncovacieho riadku.
    '
    ' 2007-05-18 - FIX: Insert robeny na EntireRow, inac toneposuvalo bunky aj s formatovanim korektne.
    '
    
    If n.RefersToRange.Rows.Count >= maxCount Then Exit Sub
    
    n.RefersToRange.Rows(n.RefersToRange.Rows.Count).EntireRow.Hidden = False
    n.RefersToRange.Rows(n.RefersToRange.Rows.Count).EntireRow.Insert
    n.RefersToRange.Rows(n.RefersToRange.Rows.Count).EntireRow.Hidden = True
    
    ' Musim este nastavit border na spodku, lebo to nejako blbne :-/
    Dim r As Range
    Set r = n.RefersToRange.Rows(n.RefersToRange.Rows.Count - 1)
    r.Borders(xlEdgeTop).LineStyle = xlSolid
    r.Borders(xlEdgeTop).Weight = xlThin
    
    FinishEdit
End Sub

' Pocita sa s mazanim posledneho riadku generickeho range-u tabuliek.
Sub DeleteLastRowFromRange(rangeName As String)
    Dim n As Name
    Set n = ActiveWorkbook.Names(rangeName)
    
    ' Ak ma range menej ako 3 riadky, tak uz nemazem, lebo by som stratil range
    If n.RefersToRange.Rows.Count < 3 Then Exit Sub
    
    StartEdit
    
    n.RefersToRange.Rows(n.RefersToRange.Rows.Count - 1).EntireRow.Delete
    
    FinishEdit
End Sub


