Sub UnprotectAll()
    Dim dialogPass As String
    dialogPass = InputBox("Zadajte heslo:", "Odomknutie formul|fffd|ra pre import", "")
    If dialogPass = Password Then
        ' Najprv unprotectnem workbook
        ThisWorkbook.Unprotect Password
        ' Unprotectnem aj vsetky worksheety
        Dim s As Worksheet
        For Each s In ThisWorkbook.Sheets
            s.Unprotect Password
    Next
    Else
        MsgBox "Neplatn|fffd| heslo pre odomknutie formul|fffd|ra.", vbCritical Or vbOKOnly, "Chyba!"
    End If
    
End Sub


