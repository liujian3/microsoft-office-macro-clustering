Sub StartEditSingle(s As Worksheet)
    Application.ScreenUpdating = False
    
    ' Najprv unprotectnem workbook
    ThisWorkbook.Unprotect Password
    ' Unprotectnem aj vsetky worksheety
    s.Unprotect Password
End Sub

Sub StartEdit()
    Application.ScreenUpdating = False
    
    ' Najprv unprotectnem workbook
    ThisWorkbook.Unprotect Password
    ' Unprotectnem aj vsetky worksheety
    Dim s As Worksheet
    For Each s In ThisWorkbook.Sheets
        s.Unprotect Password
    Next
End Sub

Sub FinishEdit()
    ' Na koniec vsetko uzamknem
    ' Unprotectnem aj vsetky worksheety
    Dim s As Worksheet
    For Each s In ThisWorkbook.Sheets
        s.Protect Password
    Next
    ' A tiez workbook
    ThisWorkbook.Protect Password
    
    Application.ScreenUpdating = True
End Sub

