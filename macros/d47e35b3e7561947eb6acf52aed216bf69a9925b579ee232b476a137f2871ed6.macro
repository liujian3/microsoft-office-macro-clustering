    StartEditSingle Sheet27
    
    For Each Row In Range(ActiveWorkbook.Names("Tab1_o3_Plan_Mena"))
      Row.Cells.Locked = False
      Row.Value = "=ObchodyInfo!F2"
      Row.Locked = True
      counter = counter + 1
    Next Row
   
    FinishEdit
