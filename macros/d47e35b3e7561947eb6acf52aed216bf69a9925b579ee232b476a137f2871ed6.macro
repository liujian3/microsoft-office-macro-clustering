Private Sub Workbook_Open()
On Error GoTo ErrorHandler
    
    ' V pripade prveho loadovania nacitam zoznam obchodov
    If ThisWorkbook.Names("form_initialized").RefersToRange.Cells(1, 1).Value = "0" Then
        ' Musim spustit inicializaciu viditelnosti jednotlivych tabuliek.
        Application.ScreenUpdating = False
        SetTablesVisibility

        StartEdit
        ' Nastavim, ze som uz generoval zoznam obchodov
                        
        ThisWorkbook.Names("form_initialized").RefersToRange.Cells(1, 1).Value = "1"
        
        FinishEdit
        
        CopyData
        
        StartEdit
        
        NastavEuro
        
        'SetOkec
        
        FinishEdit
        Application.ScreenUpdating = True
        
    End If
    
    Exit Sub
    
ErrorHandler:
    MsgBox "Nie je mo|fffd|n|fffd| nainicializova|fffd| potrebn|fffd| |fffd|daje pr|fffd|loh.", vbCritical Or vbOKOnly, "Chyba!"
End Sub

