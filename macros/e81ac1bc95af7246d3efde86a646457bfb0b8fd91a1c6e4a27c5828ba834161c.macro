                If fs_row_parts(0) = "BC" Then home_second = fs_row_parts(1)
                If fs_row_parts(0) = "BD" Then away_second = fs_row_parts(1)
                If fs_row_parts(0) = "PX" Then home_id2 = fs_row_parts(1)
            Next j
            If fs_index_value = id Then home_name = ""
        End If
        If tour_name = "" Or home_name = "" Then
        Else
            Set objRegExp = CreateObject("VBScript.RegExp")
            objRegExp.Pattern = "\s\(...\)"
            home_name = objRegExp.Replace(home_name, "")
            away_name = objRegExp.Replace(away_name, "")
            Sheets("|fffd||fffd||fffd||fffd|").Range("A" & a).Value = tour_name
            Sheets("|fffd||fffd||fffd||fffd|").Range("B" & a).Value = date_match
            Sheets("|fffd||fffd||fffd||fffd|").Range("C" & a).Value = home_name
            Sheets("|fffd||fffd||fffd||fffd|").Range("D" & a).Value = away_name
            Sheets("|fffd||fffd||fffd||fffd|").Range("E" & a).Value = home_first
            Sheets("|fffd||fffd||fffd||fffd|").Range("F" & a).Value = away_first
            Sheets("|fffd||fffd||fffd||fffd|").Range("G" & a).Value = home_second
            Sheets("|fffd||fffd||fffd||fffd|").Range("H" & a).Value = away_second
            If tour_id = Sheets("|fffd||fffd||fffd||fffd||fffd||fffd||fffd|").Range("C1").Value Then
