Attribute VB_Name = "Module1"
Sub Auto_Open()
Dim workSht As Worksheet

For Each workSht In Sheets
    If workSht.Visible Then
      workSht.Select
      Rows("2:2").Select
      With Selection
        .HorizontalAlignment = xlGeneral
        .VerticalAlignment = xlBottom
        .WrapText = True
        .Orientation = 0
        .AddIndent = False
        .ShrinkToFit = False
        .MergeCells = False
      End With
      Columns("A:AE").Select
      Columns("A:AE").EntireColumn.AutoFit
      Range("A1").Select
    End If
Next

Sheets(1).Select

End Sub
