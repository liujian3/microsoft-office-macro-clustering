Private Sub Worksheet_Change(ByVal Target As Range)

If Target.Address = Range("PC_ASIA_REMITT_EM").Address Then
    
    
Dim EMAIL_PC_ASIA
EMAIL_PC_ASIA = 10
    
If Range("PC_ASIA_REMITT_EM").Value = "" Then EMAIL_PC_ASIA = 11

      
If EMAIL_PC_ASIA = 10 Then Call EMAIL_PC_ASIA_NONE

Exit Sub
End If


If Target.Address = Range("PC_ASIA_SUSQ").Address Then

Dim ONLINE_ASIA_PC
ONLINE_ASIA_PC = 10

    
If Range("PC_ASIA_SUSQ").Value = "Please select|fffd|" Then ONLINE_ASIA_PC = 11
If Range("PC_ASIA_SUSQ").Value = "No" Then ONLINE_ASIA_PC = 12


If ONLINE_ASIA_PC = 10 Then Call ONLINE_ASIA_PC_YES
If ONLINE_ASIA_PC = 11 Then Call ONLINE_ASIA_PC_NONE
If ONLINE_ASIA_PC = 12 Then Call ONLINE_ASIA_PC_NO


Exit Sub
End If



If Target.Address = Range("PC_ASIA_TERMS").Address Then
    
    Dim TERMS_BELOW_PC_ASIA
    TERMS_BELOW_PC_ASIA = 10
    
    'If Range("PC_ASIA_TERMS").Value = "30 Days DOI" Then TERMS_BELOW_PC_ASIA = 11
    'If Range("PC_ASIA_TERMS").Value = "Please select|fffd|" Then TERMS_BELOW_PC_ASIA = 11
    'If Range("PC_ASIA_TERMS").Value = "60 Days" Then TERMS_BELOW_PC_ASIA = 11
    
    
      
    If TERMS_BELOW_PC_ASIA = 11 Then Call TERMS_BELOW_NO_PC_ASIA
    If TERMS_BELOW_PC_ASIA = 10 Then Call TERMS_BELOW_YES_PC_ASIA
        Exit Sub
    End If


    If Target.Address = Range("PC_ASIA_OU").Address Then
 
    Dim MYOU
    MYOU = 10
 
    If Range("PC_ASIA_OU").Value = "MY Project Management and Construction (MYPMC)" Then MYOU = 11
    If Range("PC_ASIA_OU").Value = "MY Investment Management (MYIM)" Then MYOU = 11
    If Range("PC_ASIA_OU").Value = "MY Development (MYDEV)" Then MYOU = 11
    If Range("PC_ASIA_OU").Value = "Please select|fffd|" Then MYOU = 12
    
    If MYOU = 12 Then Call MYOU_NO_SIMPLE_NONE_PC
    If MYOU = 11 Then Call MYOU_YES_PC
    If MYOU = 10 Then Call MYOU_NO_PC
        Exit Sub
        
    End If
    
         If Target.Address = Range("PC_ASIA_TYPEPO").Address Then
    
    Dim SIMPLE
    SIMPLE = 10
    If Range("PC_ASIA_TYPEPO").Value = "Simple Purchase Order - PSA" Then SIMPLE = 12
    If Range("PC_ASIA_TYPEPO").Value = "Complex Purchase Order - Progress Claim" Then SIMPLE = 11
    
      
    If SIMPLE = 11 Then Call SIMPLE_YES
    If SIMPLE = 10 Then Call SIMPLE_NO
    If SIMPLE = 12 Then Call PSA_ASIA
        Exit Sub
    End If
     If Target.Address = Range("PC_ASIA_METHODOLOGY").Address Then
    
    Dim PAY_METH
    PAY_METH = 10
    
    If Range("PC_ASIA_METHODOLOGY").Value = "Cheque - Sent to Supplier" Then PAY_METH = 11
    If Range("PC_ASIA_METHODOLOGY").Value = "Cheque - Returned to Lend Lease contact (Payalone)" Then PAY_METH = 11
    If Range("PC_ASIA_METHODOLOGY").Value = "Manual" Then PAY_METH = 11
      
    If PAY_METH = 11 Then Call PAY_METH_UNHIDE_ASIA_PC
    If PAY_METH = 10 Then Call PAY_METH_HIDE_ASIA_PC
        Exit Sub
    End If

    If Target.Address = Range("PC_ASIA_PO_SUPP").Address Then
    
    Dim PO_SUPP
   PO_SUPP = 10
    
    If Range("PC_ASIA_PO_SUPP").Value = "Yes - Purchase Order Supplier (default)" Then PO_SUPP = 11
    If Range("PC_ASIA_PO_SUPP").Value = "Please select|fffd|" Then PO_SUPP = 11
    
    If PO_SUPP = 10 Then Call PO_SUPP_UNHIDE_ASIA_PC
    If PO_SUPP = 11 Then Call PO_SUPP_HIDE_ASIA_PC

        Exit Sub
    End If
    
    
    


If Target.Address = Range("PC_ASIA_EXIBANDET").Address Then
    
Dim BANK_DETAILS
BANK_DETAILS = 10
    
If Range("PC_ASIA_EXIBANDET").Value = "Yes" Then BANK_DETAILS = 11
If Range("PC_ASIA_EXIBANDET").Value = "Please select|fffd|" Then BANK_DETAILS = 10
If Range("PC_ASIA_EXIBANDET").Value = "No" Then BANK_DETAILS = 12
    
If BANK_DETAILS = 10 Then Call BANK_DETAILS_HIDE_ASIA_PC
If BANK_DETAILS = 11 Then Call BANK_DETAILS_UNHIDE_ASIA_PC
If BANK_DETAILS = 12 Then Call BANK_DETAILS_UNHIDE_NONE_ASIA_PC


Exit Sub
End If

If Target.Address = Range("PC_ASIA_REQTYPE").Address Then

Dim REQUEST_TYPE_PC_ASIA
REQUEST_TYPE_PC_ASIA = 10
    
If Range("PC_ASIA_REQTYPE").Value = "Please select|fffd|" Then REQUEST_TYPE_PC_ASIA = 10
If Range("PC_ASIA_REQTYPE").Value = "New Site for Existing Supplier" Then REQUEST_TYPE_PC_ASIA = 10
If Range("PC_ASIA_REQTYPE").Value = "New Supplier" Then REQUEST_TYPE_PC_ASIA = 11
If Range("PC_ASIA_REQTYPE").Value = "New Site/Update Address for an Existing Supplier" Then REQUEST_TYPE_PC_ASIA = 12
If Range("PC_ASIA_REQTYPE").Value = "Change of Banking Details" Then REQUEST_TYPE_PC_ASIA = 13
If Range("PC_ASIA_REQTYPE").Value = "Change Supplier Name" Then REQUEST_TYPE_PC_ASIA = 14
If Range("PC_ASIA_REQTYPE").Value = "Change of Payment Terms" Then REQUEST_TYPE_PC_ASIA = 15
