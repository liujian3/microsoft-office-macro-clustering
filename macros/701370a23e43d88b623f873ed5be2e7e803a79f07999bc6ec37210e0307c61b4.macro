Private Sub Worksheet_Change(ByVal Target As Range)

If Target.Address = "$C$57" Then
    
    Dim TERMS_BELOW_STD_UK
    TERMS_BELOW_STD_UK = 10
    
    If Range("C57").Value = "30 Days (Default)" Then TERMS_BELOW_STD_UK = 11
    If Range("C57").Value = "Please select|fffd|" Then TERMS_BELOW_STD_UK = 11
    If Range("C57").Value = "40 Days" Then TERMS_BELOW_STD_UK = 11
    If Range("C57").Value = "45 Days" Then TERMS_BELOW_STD_UK = 11
    
    
      
    If TERMS_BELOW_STD_UK = 11 Then Call TERMS_BELOW_NO_STD_UK
    If TERMS_BELOW_STD_UK = 10 Then Call TERMS_BELOW_YES_STD_UK
        Exit Sub
    End If
    
