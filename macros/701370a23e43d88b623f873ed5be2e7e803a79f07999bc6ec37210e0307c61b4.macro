    If Range("STD_ASIA_OU").Value = "MY Development (MYDEV)" Then MYOU = 11
    If Range("STD_ASIA_OU").Value = "Please select|fffd|" Then MYOU = 12
    
    If MYOU = 12 Then Call MYOU_NO_SIMPLE_NONE
    If MYOU = 11 Then Call MYOU_YES_SIMPLE
    If MYOU = 10 Then Call MYOU_NO_SIMPLE
        Exit Sub
    End If
    
     If Target.Address = Range("STD_ASIA_METHODOLOGY").Address Then
    
    Dim PAY_METH
    PAY_METH = 10
    
