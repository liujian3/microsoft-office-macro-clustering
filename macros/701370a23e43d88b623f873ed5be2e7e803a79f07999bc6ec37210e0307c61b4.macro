Sub ResizeRelocateCmts()
On Error Resume Next
Dim r As Range
For Each r In ActiveSheet.UsedRange.SpecialCells(xlCellTypeComments)
s = r.Comment.Text
r.ClearComments
r.AddComment
With r.Comment
.Text Text:=s
.Shape.TextFrame.AutoSize = True
.Visible = True 'False
End With
Next
End Sub

