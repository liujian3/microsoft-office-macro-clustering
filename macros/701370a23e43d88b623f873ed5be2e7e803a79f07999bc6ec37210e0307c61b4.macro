Private Sub Worksheet_Change(ByVal Target As Range)

If Target.Address = Range("EI_ASIA_OU").Address Then
 
Dim MYOU
MYOU = 10
 
If Range("EI_ASIA_OU").Value = "MY Project Management and Construction (MYPMC)" Then MYOU = 11
If Range("EI_ASIA_OU").Value = "MY Investment Management (MYIM)" Then MYOU = 11
If Range("EI_ASIA_OU").Value = "MY Development (MYDEV)" Then MYOU = 11
If Range("EI_ASIA_OU").Value = "Please select|fffd|" Then MYOU = 12
    
If MYOU = 12 Then Call MYOU_NO_SIMPLE_NONE_EI
If MYOU = 11 Then Call MYOU_YES_SIMPLE_EI
If MYOU = 10 Then Call MYOU_NO_SIMPLE_EI
        Exit Sub
    End If
    

If Target.Address = Range("EI_ASIA_SUPUSES").Address Then

Dim ONLINE_ASIA_EI
ONLINE_ASIA_EI = 10

    
If Range("EI_ASIA_SUPUSES").Value = "Please select|fffd|" Then ONLINE_ASIA_EI = 11
If Range("EI_ASIA_SUPUSES").Value = "No" Then ONLINE_ASIA_EI = 12


If ONLINE_ASIA_EI = 10 Then Call ONLINE_ASIA_EI_YES
If ONLINE_ASIA_EI = 11 Then Call ONLINE_ASIA_EI_NONE
If ONLINE_ASIA_EI = 12 Then Call ONLINE_ASIA_EI_NO


Exit Sub
End If


If Target.Address = Range("EI_ASIA_TERMS").Address Then
    
    Dim TERMS_BELOW_EI_ASIA
    TERMS_BELOW_EI_ASIA = 10
    
    'If Range("EI_ASIA_TERMS").Value = "30 Days DOI" Then TERMS_BELOW_EI_ASIA = 11
    'If Range("EI_ASIA_TERMS").Value = "Please select|fffd|" Then TERMS_BELOW_EI_ASIA = 11
    'If Range("EI_ASIA_TERMS").Value = "60 Days" Then TERMS_BELOW_EI_ASIA = 11
    
    
      
    If TERMS_BELOW_EI_ASIA = 11 Then Call TERMS_BELOW_NO_EI_ASIA
    If TERMS_BELOW_EI_ASIA = 10 Then Call TERMS_BELOW_YES_EI_ASIA
        Exit Sub
    End If

    
   If Target.Address = Range("EI_ASIA_METHODOLOGY").Address Then
    
    Dim PAY_METH
    PAY_METH = 10
    
    If Range("EI_ASIA_METHODOLOGY").Value = "Cheque - Sent to Supplier" Then PAY_METH = 11
    If Range("EI_ASIA_METHODOLOGY").Value = "Cheque - Returned to Lend Lease contact (Payalone)" Then PAY_METH = 11
    If Range("EI_ASIA_METHODOLOGY").Value = "Manual" Then PAY_METH = 11
      
    If PAY_METH = 11 Then Call PAY_METH_UNHIDE_ASIA_EI
    If PAY_METH = 10 Then Call PAY_METH_HIDE_ASIA_EI
        Exit Sub
    End If
    
If Target.Address = Range("EI_ASIA_EXISTBANKQ").Address Then
    
Dim BANK_DETAILS
BANK_DETAILS = 10
    
If Range("EI_ASIA_EXISTBANKQ").Value = "Yes" Then BANK_DETAILS = 11
If Range("EI_ASIA_EXISTBANKQ").Value = "Please select|fffd|" Then BANK_DETAILS = 10
If Range("EI_ASIA_EXISTBANKQ").Value = "No" Then BANK_DETAILS = 12
    
If BANK_DETAILS = 10 Then Call BANK_DETAILS_HIDE_ASIA_EI
If BANK_DETAILS = 11 Then Call BANK_DETAILS_UNHIDE_ASIA_EI
If BANK_DETAILS = 12 Then Call BANK_DETAILS_UNHIDE_NONE_ASIA_EI


Exit Sub
End If

If Target.Address = Range("EI_ASIA_REQTYPE").Address Then

Dim REQUEST_TYPE_EI_ASIA
REQUEST_TYPE_EI_ASIA = 10
    
If Range("EI_ASIA_REQTYPE").Value = "Please select|fffd|" Then REQUEST_TYPE_EI_ASIA = 10
If Range("EI_ASIA_REQTYPE").Value = "New Site for Existing Supplier" Then REQUEST_TYPE_EI_ASIA = 10
If Range("EI_ASIA_REQTYPE").Value = "New Supplier" Then REQUEST_TYPE_EI_ASIA = 11
If Range("EI_ASIA_REQTYPE").Value = "New Site/Update Address for an Existing Supplier" Then REQUEST_TYPE_EI_ASIA = 12
If Range("EI_ASIA_REQTYPE").Value = "Change of Banking Details" Then REQUEST_TYPE_EI_ASIA = 13
If Range("EI_ASIA_REQTYPE").Value = "Change Supplier Name" Then REQUEST_TYPE_EI_ASIA = 14
If Range("EI_ASIA_REQTYPE").Value = "Change of Payment Terms" Then REQUEST_TYPE_EI_ASIA = 15
If Range("EI_ASIA_REQTYPE").Value = "Change Remittance Email" Then REQUEST_TYPE_EI_ASIA = 16
