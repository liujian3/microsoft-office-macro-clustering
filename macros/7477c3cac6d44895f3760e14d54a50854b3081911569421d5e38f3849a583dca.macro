Sub Remarks()
Attribute Remarks.VB_Description = "Macro recorded 12/18/98 by Ryan Kucharski"
Attribute Remarks.VB_ProcData.VB_Invoke_Func = "r\n14"
'
' Remarks Macro
' Macro recorded 12/18/98 by Ryan Kucharski
'
' Keyboard Shortcut: Ctrl+r
'
    Selection.Merge True
    With Selection
        .HorizontalAlignment = xlLeft
        .VerticalAlignment = xlCenter
        .Orientation = 0
        .IndentLevel = 0
    End With
    With Selection.Font
        .Name = "Arial"
        .FontStyle = "Bold"
        .Size = 8
        .ColorIndex = 1
    End With
    With Selection.Borders(xlEdgeBottom)
        .LineStyle = xlContinuous
        .Weight = xlHairline
        .ColorIndex = xlAutomatic
    End With
    With Selection.Borders(xlInsideHorizontal)
        .LineStyle = xlContinuous
        .Weight = xlHairline
        .ColorIndex = xlAutomatic
    End With
    Selection.Locked = False
End Sub
Sub Format_Input_Multiple_Rows()
Attribute Format_Input_Multiple_Rows.VB_Description = "Macro recorded 12/18/98 by Ryan Kucharski"
Attribute Format_Input_Multiple_Rows.VB_ProcData.VB_Invoke_Func = "w\n14"
'
' Format_Input_Cells Macro
' Macro recorded 12/18/98 by Ryan Kucharski
'
' Keyboard Shortcut: Ctrl+w
'
    Selection.Merge True
    With Selection
        .HorizontalAlignment = xlCenter
        .VerticalAlignment = xlCenter
        .WrapText = False
        .Orientation = 0
        .ShrinkToFit = False
    End With
    With Selection.Font
        .Name = "Arial"
        .FontStyle = "Bold"
        .Size = 8
        .ColorIndex = 1
    End With
    With Selection.Borders(xlEdgeBottom)
        .LineStyle = xlContinuous
        .Weight = xlHairline
        .ColorIndex = xlAutomatic
    End With
    With Selection.Borders(xlInsideHorizontal)
        .LineStyle = xlContinuous
        .Weight = xlHairline
        .ColorIndex = xlAutomatic
    End With
    Selection.Locked = False
    Selection.FormulaHidden = False
End Sub
Sub Format_Single_Input_Cell()
Attribute Format_Single_Input_Cell.VB_Description = "Macro recorded 12/18/98 by Ryan Kucharski"
Attribute Format_Single_Input_Cell.VB_ProcData.VB_Invoke_Func = "e\n14"
'
' Format_Single_Input_Cell Macro
' Macro recorded 12/18/98 by Ryan Kucharski
'
' Keyboard Shortcut: Ctrl+e
'
    Selection.Merge True
    With Selection
        .HorizontalAlignment = xlCenter
        .VerticalAlignment = xlCenter
        .Orientation = 0
        .MergeCells = True
    End With
    With Selection.Font
        .Name = "Arial"
        .FontStyle = "Bold"
        .Size = 8
        .ColorIndex = 1
    End With
    With Selection.Borders(xlEdgeBottom)
        .LineStyle = xlContinuous
        .Weight = xlHairline
        .ColorIndex = xlAutomatic
    End With
    Selection.Locked = False
End Sub
Sub Format_Normal()
Attribute Format_Normal.VB_Description = "Macro recorded 12/18/98 by Ryan Kucharski"
Attribute Format_Normal.VB_ProcData.VB_Invoke_Func = "n\n14"
'
' Format_Normal Macro
' Macro recorded 12/18/98 by Ryan Kucharski
'
' Keyboard Shortcut: Ctrl+n
'
    Selection.UnMerge
    With Selection
        .HorizontalAlignment = xlGeneral
        .VerticalAlignment = xlCenter
        .WrapText = False
        .Orientation = 0
        .ShrinkToFit = False
        .MergeCells = False
    End With
    With Selection.Font
        .Name = "Arial"
        .FontStyle = "Regular"
        .Size = 8
        .Strikethrough = False
        .Superscript = False
        .Subscript = False
        .OutlineFont = False
        .Shadow = False
        .Underline = xlUnderlineStyleNone
    End With
    Selection.Borders(xlDiagonalDown).LineStyle = xlNone
    Selection.Borders(xlDiagonalUp).LineStyle = xlNone
    Selection.Borders(xlEdgeLeft).LineStyle = xlNone
    Selection.Borders(xlEdgeTop).LineStyle = xlNone
    Selection.Borders(xlEdgeBottom).LineStyle = xlNone
    Selection.Borders(xlEdgeRight).LineStyle = xlNone
    Selection.Borders(xlInsideVertical).LineStyle = xlNone
    Selection.Borders(xlInsideHorizontal).LineStyle = xlNone
    Selection.Locked = True
    Selection.FormulaHidden = False
End Sub
