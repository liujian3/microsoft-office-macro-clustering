For Each LB In frmcal.Controls
    If TypeName(LB) = "Label" Then
        LabelCount1 = LabelCount1 + 1
        If LabelCount1 > 6 And LabelCount1 < 49 Then
            ReDim Preserve cLabel(1 To LabelCount1)
            Set cLabel(LabelCount1).Label = LB
