Attribute VB_Name = "Modul11"

Option Explicit
Option Private Module
Public cLabel() As New clsLabel
Public aktDat As Date

Public Function ErsterKW(KW As Integer, Jhr As Integer) As Double
Dim Erstertag As Double
If Month(aktDat) = 1 And KW > 51 Then Jhr = Jhr - 1
Erstertag = DateSerial(Jhr, 1, 1)
Do Until DatePart("WW", Erstertag, 2, 2) = 2
    Erstertag = Erstertag + 1
Loop
ErsterKW = DateAdd("WW", KW - 2, Erstertag)
End Function

Private Function KWoche(Datum As Date)
Dim t As Long
t = DateSerial(Year(Datum + (8 - Weekday(Datum)) Mod 7 - 3), 1, 1)
KWoche = ((Datum - t - 3 + (Weekday(t) + 1) Mod 7)) \ 7 + 1
End Function

Sub F|fffd|llen()
Dim jCounter As Integer, KWZ|fffd|hler As Integer
Dim Tagz|fffd|hler As Date

frmcal.Anzeige.Caption = Format(aktDat, "mmmm yyyy")
Tagz|fffd|hler = ErsterKW(KWoche(DateSerial(Year(aktDat), Month(aktDat), 1)), Year(aktDat))
KWZ|fffd|hler = 1
For jCounter = 1 To 6
    frmcal.Controls("Label" & jCounter).Caption = KWoche(DateSerial(Year(aktDat), Month(aktDat), KWZ|fffd|hler))
    KWZ|fffd|hler = KWZ|fffd|hler + 7
Next jCounter
For jCounter = 7 To 48
    With frmcal.Controls("Label" & jCounter)
        .Tag = Tagz|fffd|hler
        .Caption = Format(Tagz|fffd|hler, "d")
        .ForeColor = IIf(Month(Tagz|fffd|hler) <> Month(aktDat), &HC0C0C0, IIf(Weekday(Tagz|fffd|hler, 2) > 5, &HFF&, &H0&))
        .BackColor = IIf(Tagz|fffd|hler = Date, &HFFFF&, &HFFFFFF)
    End With
    Tagz|fffd|hler = Tagz|fffd|hler + 1
Next jCounter
frmcal.Label57.Caption = Date
End Sub
Sub Kalender()
frmcal.Show
End Sub
Sub Uhrzeit()
Call Zeitauswahl
End Sub
