Private Sub Label_Click()

If Label.Tag < 25 Then
    Call Reset
   strstd = Label.Caption
   Label.BackColor = &H80FF80
End If
If Label.Tag > 24 And Label.Tag < 85 Then
    Call Reset2
    If strstd = "" Then
        MsgBox "Zuerst Stunde w|fffd|hlen"
    Else
        strmin = Label.Caption
        Label.BackColor = &H80FF80
        ActiveCell.Value = strstd & ":" & strmin
        Unload frmtime
   End If
End If
If Label.Tag = 85 Then
    ActiveCell.Value = Format(Time, "hh:mm")
    Unload frmtime
End If
End Sub
