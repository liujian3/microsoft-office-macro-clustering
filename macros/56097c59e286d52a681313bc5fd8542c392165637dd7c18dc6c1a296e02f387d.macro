Sub Hold_Witness_Point()
If ActiveDocument.FormFields("Hold_Point").CheckBox.Value = True Then
ActiveDocument.FormFields("Witness_Point").CheckBox.Value = False
End If
If ActiveDocument.FormFields("Hold_Point").CheckBox.Value = False Then
ActiveDocument.FormFields("Witness_Point").CheckBox.Value = True
End If
End Sub
Sub Reply()
If ActiveDocument.FormFields("Reply_Requested_Yes").CheckBox.Value = True Then
ActiveDocument.FormFields("Reply_Requested_No").CheckBox.Value = False
End If
If ActiveDocument.FormFields("Reply_Requested_Yes").CheckBox.Value = False Then
ActiveDocument.FormFields("Reply_Requested_No").CheckBox.Value = True
ActiveDocument.Select Bookmarks( FormFields("Reply_Requested_No").Result .CheckBox.Value = True
End If
End Sub


