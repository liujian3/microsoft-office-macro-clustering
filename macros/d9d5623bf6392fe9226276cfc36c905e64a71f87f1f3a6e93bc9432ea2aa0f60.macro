Sub legnagyobb_ertek()

Dim legnagyobb As Integer
legnagyobb = Cells(1, 1)


For i = 1 To 50
    If Cells(i, 1) > legnagyobb Then
      legnagyobb = Cells(i, 1)
    Else
        legnagyobb = Cells(1, 1)
            i = i + 1
    End If

    

Next
MsgBox "A legnagyobb |fffd|rt|fffd|k: " & legnagyobb
End Sub


