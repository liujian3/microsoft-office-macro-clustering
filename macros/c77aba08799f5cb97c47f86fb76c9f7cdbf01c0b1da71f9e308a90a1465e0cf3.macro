Sub SheetKiller(suffix As String)
    Dim oldcalcul
    oldcalcul = Application.Calculation
    
    Application.Calculation = xlCalculationManual
    Dim s As Worksheet, t As String
    Dim i As Long, K As Long
    K = Sheets.Count

    For i = K To 1 Step -1
        t = Sheets(i).Name
        If Right(t, 2) = suffix Then
            Application.DisplayAlerts = False
                Sheets(i).Delete
            Application.DisplayAlerts = True
        End If
    Next i
    
    Application.Calculation = oldcalcul
End Sub

Sub KillSheetsPetiteCommune()
    Dim oldcalcul
    oldcalcul = Application.Calculation
    
    Application.Calculation = xlCalculationManual
    Dim s As Worksheet, t As String
    Dim i As Long, K As Long
    K = Sheets.Count

    For i = K To 1 Step -1
        t = Sheets(i).CodeName
        Debug.Print t
        If t = "F_AUTOFINGENERAL" Or t = "F_AUTOFINEAU" Or t = "F_AUTOFINEAUXUSEES" Or t = "F_AUTOFINDECHETS" _
                Or t = "F_RESULTATGENERAL" Or t = "F_RESULTATEAU" Or t = "F_RESULTATEAUUSEES" Or t = "F_RESULTATDECHETS" _
                Or t = "F_FINANCEMENTSPECRESUME" Then
           Application.DisplayAlerts = False
           Sheets(i).Delete
           Application.DisplayAlerts = True
        End If
    Next i
    
    Application.Calculation = oldcalcul
End Sub

