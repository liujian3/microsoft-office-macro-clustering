Sub SestavDatum(radek, strana, DateCas)     'sestavi datum_cas z bunky

den1 = Day(CDate(Sheets(strana).Cells(radek, 2).value))
mesic1 = Month(CDate(Sheets(strana).Cells(radek, 2).value))
rok1 = Year(CDate(Sheets(strana).Cells(radek, 2).value))

DATUM = rok1 & "-" & den1 & "-" & mesic1 & " "

CAS = "00" & ":" & "00" & ":" & "00"

DateCas = Chr(39) & DATUM & CAS & Chr(39)

End Sub


Sub SestavDatum2(ZadanyRok, ZadanyMesic, DateCasOD, DateCasDO)    'sestavi datum_cas OD a DO

date_start = DateSerial(ZadanyRok, ZadanyMesic, 1)  '1.den v mesici
date_end = DateAdd("m", 1, date_start)
date_end = DateAdd("d", -1, date_end)               'posledni den v mesici

rok1 = CStr(Year(date_start))
mesic1 = "0" & CStr(Month(date_start))
den2 = CStr(Day(date_end))

'    DATUM_zacatek = rok1 & "-" & mesic1 & "-" & "01" & " "
'    DATUM_konec = rok1 & "-" & mesic1 & "-" & den2 & " "
    
    DATUM_zacatek = rok1 & "-" & "01" & "-" & mesic1 & " "      'pouziva se yyyy-dd-mm
    DATUM_konec = rok1 & "-" & den2 & "-" & mesic1 & " "
    
    CAS_uni = "00" & ":" & "00" & ":" & "00"
    
    DateCasOD = Chr(39) & DATUM_zacatek & CAS_uni & Chr(39)
    DateCasDO = Chr(39) & DATUM_konec & CAS_uni & Chr(39)

End Sub


    


