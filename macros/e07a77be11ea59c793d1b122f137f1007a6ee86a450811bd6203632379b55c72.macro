Public Function vikend(DATUM, radek, strana)

Dim LWeekday As Integer

LWeekday = Weekday(DATUM, vbMonday)

If LWeekday = 6 Then
    For i = 2 To 12
     Sheets(strana).Cells(radek, i).Interior.ColorIndex = 17
    Next
End If

If LWeekday = 7 Then
    For i = 2 To 12
     Sheets(strana).Cells(radek, i).Interior.ColorIndex = 38
    Next
End If


End Function
