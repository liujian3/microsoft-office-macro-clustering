    If par >= 51 And par <= 60 Then
        StrIdx = 50
        'strana = CStr(Sheets(List_par).Cells(1, 4 + StrIdx).Value)
    End If

    Select Case (Sheets(List_par).Cells(10, 3 + par).value)
        Case "Pr|fffd|m|fffd|r"
