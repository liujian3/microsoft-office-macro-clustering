                    PoleHodnotMin(ActDen - 1) = vysledek              'Indexovani poli od nuly
                    vysledek = 1000000
                End If
            Next
            If Count > Count_max2 And ActDen < 30 Then
                PoleHodnotMin(ActDen) = vysledek
            End If
            
            For Count = 0 To 30
                If PoleHodnotMax(Count) <> -1 And PoleHodnotMin(Count) <> -1 Then
                    PoleHodnotMesic(Count) = Abs(PoleHodnotMax(Count) - PoleHodnotMin(Count))
                End If
            Next
            
    End Select
        
        PoleHodnotMesicStat = PoleHodnotMesic
        If (ROK = CInt(Year(Date))) And (MESIC = CInt(Month(Date))) Then    ' osetreni aby statistika nepocitala posledni den
          PoleHodnotMesicStat(CInt(Day(Date)) - 1) = -1
        End If
        
        StatStruct = stat(ROK, MESIC, PoleHodnotMesicStat)                  ' fce spocita statistiky
        StatStruct.Pole = PoleHodnotMesic
        GoTo KONEC

ERROR:
StatStruct.Pole = PoleHodnotMesic                                       'naplnim strukturu
DotazSQLfceArray = StatStruct                                           'vratim strukturu

Exit Function
