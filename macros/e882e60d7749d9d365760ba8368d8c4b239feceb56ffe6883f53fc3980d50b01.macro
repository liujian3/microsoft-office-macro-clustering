Attribute VB_Name = "Module1"
Sub Lookup()
With WorksheetFunction
    On Error GoTo Error_handler:
    Range("B17").Value = .VLookup(Range("B3"), Sheet2.Range("B4:L48"), 9, False)
    Exit Sub
Error_handler:
MsgBox ("Number not found")
Range("B17").Value = 0
End With

End Sub

Sub CreateAccrual()
With WorksheetFunction
If Range("B36").Value < 0 Then
'Use Sundry Creditor
Sheets("SD").Visible = False
Sheets("SC").Visible = True
Sheets("SC").Activate
Else
'Use Sundry Debtor
Sheets("SC").Visible = False
Sheets("SD").Visible = True
Sheets("SD").Activate
End If
End With
End Sub
