Attribute VB_Name = "Module1"
Option Explicit

Function MODERNISIERUNG(GND As Integer, Alter As Integer, Punkte As Integer)
    ' a, b unc c berechnen
    Dim a As Single, b As Single, c As Single
    Dim RND As Integer 'Restnutzungsdauer
    Dim RNDnachFormel 'Zwischenergebnis
    Dim RelAlter As Single ' Relatives Alter
    Dim RA As Integer ' Vergleichsgr|fffd||fffd|e f|fffd|r das relative Alter
    
    If Alter > GND Then Alter = GND
    RND = GND - Alter
    RelAlter = (Alter / GND) * 100
    If RND < 0 Then RND = 0
      
    Select Case Punkte
        Case Is <= 1
            a = 0.0125: b = 2.625: c = 152.5: RA = 60
        Case 2 To 4
            a = 0.0073: b = 1.577: c = 111.33: RA = 40
        Case 5 To 8
            a = 0.005: b = 1.1: c = 100: RA = 20
        Case 9 To 17
            a = 0.0033: b = 0.735: c = 95.28: RA = 15
        Case Is >= 18
            a = 0.002: b = 0.44: c = 94.2: RA = 10
    End Select
    
    RNDnachFormel = a * (100 / GND) * (Alter * Alter) - b * Alter + c * (GND / 100)
     
    If RelAlter < RA Or RNDnachFormel < 0 Or RNDnachFormel <= RND Then
        MODERNISIERUNG = RND ' wenn das relative Alter nicht erreicht wird, gilt die normale RND
    Else
        MODERNISIERUNG = Int(Round(RNDnachFormel, 0))
    End If
End Function

