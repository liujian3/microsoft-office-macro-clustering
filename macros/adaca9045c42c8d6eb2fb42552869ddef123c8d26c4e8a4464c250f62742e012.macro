Public Sub Zeilenhoehen_anpassen(Position As Integer, Optional ByVal zus|fffd|tzliche_Zellenh|fffd|he As Integer = 0, _
                                    Optional ByVal datenfeldname_h|fffd|hen_multiplikator As Integer = 1, _
                                    Optional ByVal datenfeldname_h|fffd|hen_multiplikator_eng As Integer = 1)
    With Fragebogen
        .Rows(Position & ":" & Position).RowHeight = 2
        .Rows(Position + 1 & ":" & Position + 1).RowHeight = 12.75 * datenfeldname_h|fffd|hen_multiplikator
        If Not ws_Verwaltung.Cells(Sprache_PFB, 2).Value = "Deutsch_Englisch" Then
            .Rows(Position + 2 & ":" & Position + 2).RowHeight = 0
        Else
            .Rows(Position + 2 & ":" & Position + 2).RowHeight = 12.75 * datenfeldname_h|fffd|hen_multiplikator_eng
        End If
        .Rows(Position + 3 & ":" & Position + 3).RowHeight = 2
        .Rows(Position + 4 & ":" & Position + 4).RowHeight = 3
        .Rows(Position + 5 & ":" & Position + 5).RowHeight = 12.75 + zus|fffd|tzliche_Zellenh|fffd|he
        .Rows(Position + 6 & ":" & Position + 6).RowHeight = 3
        .Rows(Position + 7 & ":" & Position + 7).RowHeight = 2
        .Rows(Position + 8 & ":" & Position + 8).RowHeight = 5.5
    End With
End Sub

