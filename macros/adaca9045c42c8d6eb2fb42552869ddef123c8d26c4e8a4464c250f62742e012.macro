'Aktuell nicht genutzt
Private Function Datenfeld_Arbeitgeber_Anlegen(name As String, name_eng As String, hoehe_vielfaches_von_Standardfeldhoehe As Integer, pos As Integer, _
                                                Optional ByVal datenfeldname_hoehen_multiplikator As Integer = 1, _
                                                Optional ByVal datenfeldname_hoehen_multiplikator_eng As Integer = 0)
    Dim rng, rng2, rng3, rng4 As String
    
    With Worksheets("Druckansicht")
        'Die Zeilenh|fffd|hen festlegen
        .Rows(pos & ":" & pos).RowHeight = 2
        .Rows(pos + 1 & ":" & pos + 1).RowHeight = 12.75 * datenfeldname_hoehen_multiplikator
        .Rows(pos + 2 & ":" & pos + 2).RowHeight = 12.75 * datenfeldname_hoehen_multiplikator_eng
        .Rows(pos + 3 & ":" & pos + 3).RowHeight = 2
        .Rows(pos + 4 & ":" & pos + 4).RowHeight = 3
        'die Standardfeldh|fffd|he ist 37.5
        'das Feld mit der Bezeichnung ist 12,75
        .Rows(pos + 5 & ":" & pos + 5).RowHeight = (37.5 * hoehe_vielfaches_von_Standardfeldhoehe) - 2 - 12.75 - 2 - 3 - 3 - 2 'Die anderen Werte sind Leer
        .Rows(pos + 6 & ":" & pos + 6).RowHeight = 3
        .Rows(pos + 7 & ":" & pos + 7).RowHeight = 2
        'Den Rahmen erzeugen
        With .Range("C" & pos & ":AW" & pos + 7)
            With .Interior
                .ColorIndex = 2
                .Pattern = xlSolid
            End With
            With .Borders(xlEdgeLeft)
                .LineStyle = xlContinuous
                .Weight = xlThick
                .ColorIndex = 1
            End With
            With .Borders(xlEdgeTop)
                .LineStyle = xlContinuous
                .Weight = xlThin
                .ColorIndex = 1
            End With
        End With
        'Den Namen des Feldes in die linke obere Ecke und der hypotetischen Eingabefelder
        rng = "D" & pos + 1 & ":AV" & pos + 1
        rng2 = "D" & pos + 2 & ":AV" & pos + 2
        rng3 = "D" & pos + 4 & ":AV" & pos + 6
        rng4 = "AB" & pos + 4 & ":AV" & pos + 6
        'Die deutsche Bezeichnug mit richtiger Formatierung einf|fffd|gen
        With .Range(rng)
            .HorizontalAlignment = xlLeft
            .VerticalAlignment = xlCenter
            .WrapText = True
            .MergeCells = True
            .Value = name 'Name des Feldes
            With .Font
                .name = "Verdana"
                .Size = 8
                .Bold = False
                .Italic = False
                .ColorIndex = 1
            End With
        End With
        'Die englische Bezeichnung mit richtiger Formatierung einf|fffd|gen
        With .Range(rng2)
            .HorizontalAlignment = xlLeft
            .VerticalAlignment = xlCenter
            .WrapText = True
            .MergeCells = True
            .Value = name_eng 'englischer Name des Feldes
            With .Font
                .name = "Verdana"
                .Size = 8
                .Bold = False
                .Italic = True
                .ColorIndex = 1
            End With
        End With
        'Zellen der Eingabefelder werden verbunden um bei der Erkennung der letzten genutzten Zeile keine Fehler zu versuchen
        .Range(rng3).MergeCells = True
        .Range(rng4).MergeCells = True
        'Eine Zeile Abstand zwischen zwei Feldern
        .Rows(pos + 8 & ":" & pos + 8).RowHeight = 5.5
        'Falls ein Datenfeldname mehrzeilig sein sollte wird diese Zeilenh|fffd|he verdoppelt
    End With
    'Die pos-Variable um die genutzte Zeilenzahl erh|fffd|hen und zur|fffd|ckgeben
    Datenfeld_Arbeitgeber_Anlegen = pos + 9
End Function

