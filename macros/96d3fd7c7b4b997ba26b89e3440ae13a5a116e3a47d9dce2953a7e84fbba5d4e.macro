Public Sub ProtegerFeuilles()

  Dim xlSheet As Excel.Worksheet

  For Each xlSheet In Sheets
    xlSheet.Protect "MAXIM"
  Next

End Sub

Public Sub LibererFeuilles()

  Dim xlSheet As Excel.Worksheet

  For Each xlSheet In Sheets
    xlSheet.Unprotect "MAXIM"
  Next

End Sub
