Sub CheckForDisplay()
    Dim r As Integer
    Dim ValueToFind
    Dim found
    found = False
    ValueToFind = "DISPLAY"
    For r = 1 To 2510    ' Revise the 500 to include all of your values
        If ThisWorkbook.Sheets("DSL - Supplier").Cells(r, 1).Value = ValueToFind Then
            On Error Resume Next
            ThisWorkbook.Sheets("DSL - Supplier").Range("S_ARTICLE_DISPLAY").EntireColumn.Hidden = False
            found = True
            On Error GoTo 0
            Exit Sub
        End If
    Next r
    If found = False Then
        On Error Resume Next
        Range("S_ARTICLE_DISPLAY").EntireColumn.Hidden = True
        On Error GoTo 0
    End If
End Sub

