        If Not IsNumeric(ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column) Or ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column) = 0 Or ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column) > 99999999999999#) Then
            error = True
            If firstGTIN = True And changedcell.column = i Then
                MsgBox ("GTIN should have a numeric value (other than 0) and contain max 14 numbers")
                firstGTIN = False
            End If
            ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column).Value = ""
            ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column).NumberFormat = "@"
        Else
            Call checkGTIN(changedcell)
        End If
    Next
    
    ThisWorkbook.Application.EnableEvents = True
    ThisWorkbook.Application.ScreenUpdating = True
End Sub

Sub checkGTIN(changedcell)
    Dim lenval
    Dim factor
    Dim Sum
    Dim Index
    Dim Findstring
    
    lenval = Len(ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column)) - 1
    factor = 3
    Sum = 0
    
    For Index = lenval To 1 Step -1
        Sum = Sum + (CInt(Mid(ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column).Value, Index, 1)) * factor)
        factor = 4 - factor
    Next
    calculatecheckdigit = ((1000 - Sum) Mod 10)
    If ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column).Value <> "" Then
        If (Left(ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column).Value, Len(ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column)) - 1) & calculatecheckdigit <> ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column).Value) Then
            error = True
            If firstGTIN = True And changedcell.column = i Then
                MsgBox ("GTIN is not a GS1 GTIN")
                firstGTIN = False
            End If
            ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column).Value = ""
        ElseIf changedcell.row > 1 Then
            Findstring = ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column).Value
            If Trim(Findstring) <> "" Then
                With Sheets("GTIN").Range("C2:AC" & changedcell.row - 1)
                    Set Rng = .Find(What:=Findstring, _
                    After:=.Cells(.Cells.Count), _
                    LookIn:=xlValues, _
                    lookat:=xlWhole, _
                    SearchOrder:=xlByRows, _
                    MatchCase:=False)
                    
                    If Not Rng Is Nothing Then
                        error = True
                        ThisWorkbook.Sheets("GTIN").Cells(changedcell.row, changedcell.column).Value = ""
                        If firstGTIN = True Then
                            MsgBox ("GTIN is already used in this file")
                            firstGTIN = False
                        End If
                    End If
                End With
            End If
        End If
    End If
End Sub

