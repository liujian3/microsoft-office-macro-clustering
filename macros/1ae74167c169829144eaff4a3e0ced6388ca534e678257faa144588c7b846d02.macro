Public Function MMGDispZoeken(nummer) As String
    Dim LR As Long, x As Long
    Dim dispo As String
    LR = Sheets("Dispo-Fiege").Range("A" & Rows.Count).End(xlUp).row
    For x = 2 To LR
        dispo = Sheets("Dispo-Fiege").Range("B" & x).Value
        If CStr(nummer) = Sheets("Dispo-Fiege").Range("A" & x) Then
            MMGDispZoeken = Sheets("Dispo-Fiege").Range("B" & x).Value
            Exit Function
        End If
        
    Next x
End Function

