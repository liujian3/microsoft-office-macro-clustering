Sub checkDecimalArticle(changedcell)
    If ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_DECIMAL_ARTICLE").column).Value = "x" Or ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_DECIMAL_ARTICLE").column).Value = "X" Then
        If ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_BUNDLE_CONTENT").column).Value <> "1" And ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_BUNDLE_CONTENT").column).Value <> "" Then
            MsgBox ("Decimal articles do not have different bundles, only one row with bundle content 1 is required for decimal articles")
            ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_BUNDLE_CONTENT").column).Value = ""
        Else
            ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_BUNDLE_TYPE").column) = "KG - KILOGRAM"
            ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_SELLING_BUNDLE").column) = "X"
            ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_BUYING_BUNDLE").column) = "X"
            ThisWorkbook.Sheets("DSL - Catman").Cells(changedcell.row + 4, ThisWorkbook.Sheets("DSL - Catman").Range("C_CONTENT_UNIT").column).Value = "1 - net weight"
            ThisWorkbook.Sheets("DSL - Catman").Cells(changedcell.row + 4, ThisWorkbook.Sheets("DSL - Catman").Range("C_REFERENCE_VALUE").column).Value = "KG"
            ThisWorkbook.Sheets("DSL - Catman").Cells(changedcell.row + 4, ThisWorkbook.Sheets("DSL - Catman").Range("C_CONTENT").column).Value = "1"
            ThisWorkbook.Sheets("DSL - Catman").Cells(changedcell.row + 4, ThisWorkbook.Sheets("DSL - Catman").Range("C_BASE_FACTOR").column).Value = "1"
        End If
        ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_DECIMAL_ARTICLE").column).Value = "X"
    ElseIf ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_DECIMAL_ARTICLE").column).Value = "" And ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, ThisWorkbook.Sheets("DSL - Supplier").Range("S_NET_VOLUME").column).Value <> "" Then
        ThisWorkbook.Sheets("DSL - Catman").Cells(changedcell.row + 4, ThisWorkbook.Sheets("DSL - Catman").Range("C_CONTENT_UNIT").column).Value = "2 - net volume"
        ThisWorkbook.Sheets("DSL - Catman").Cells(changedcell.row + 4, ThisWorkbook.Sheets("DSL - Catman").Range("C_CONTENT").column).Value = ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, ThisWorkbook.Sheets("DSL - Supplier").Range("S_NET_VOLUME").column).Value / 1000
        ThisWorkbook.Sheets("DSL - Catman").Cells(changedcell.row + 4, ThisWorkbook.Sheets("DSL - Catman").Range("C_BASE_FACTOR").column).Value = "1"
                                
    ElseIf ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_DECIMAL_ARTICLE").column).Value <> "X" And ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_DECIMAL_ARTICLE").column).Value <> "" Then
        error = True
        ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_DECIMAL_ARTICLE").column).Value = ""
        If firstSellingBundle = True And changedcell.column = changedcell.column Then
            MsgBox ("Decimal article should be either ""X"" or empty")
            firstSellingBundle = False
        End If
    End If
    
    
    
End Sub
