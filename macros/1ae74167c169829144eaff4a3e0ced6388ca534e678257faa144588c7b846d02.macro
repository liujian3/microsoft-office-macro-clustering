Sub checkGTIN(changedcell, i)
    Dim lenval
    Dim factor
    Dim Sum
    Dim Index
    Dim Findstring
    
    lenval = Len(ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_GTIN").column)) - 1
    factor = 3
    Sum = 0
    
    On Error Resume Next
    For Index = lenval To 1 Step -1
        Sum = Sum + (CInt(Mid(ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_GTIN").column).Value, Index, 1)) * factor)
        factor = 4 - factor
    Next
    On Error GoTo 0
    calculatecheckdigit = ((1000 - Sum) Mod 10)
    If (Left(ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_GTIN").column).Value, Len(ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_GTIN").column)) - 1) & calculatecheckdigit <> ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_GTIN").column).Value) Then
        error = True
        If firstGTIN = True And changedcell.column = i Then
            MsgBox ("GTIN is not a GS1 GTIN")
            firstGTIN = False
        End If
        ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_GTIN").column).Value = ""
        On Error Resume Next
            ThisWorkbook.Sheets("DSL - Supplier").Cells(changedcell.row, Range("S_GTIN").column).Interior.Color = 5296274
