    ThisWorkbook.Application.ScreenUpdating = False
    ThisWorkbook.Application.EnableEvents = False
    
    'find subcategories from categories
    Dim c, datarow
    ThisWorkbook.Sheets("Categories").Unprotect Password:="Categories"
