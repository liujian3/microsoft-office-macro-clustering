Function LevName(levNr As String) As String
    Dim gevonden
    Dim rij
    Dim levnaam
    gevonden = False
    rij = 3
    levnaam = ""
    levpad = "X:\Data\Makro Communication\FLOW REGULIER ASSORTIMENT\SupplierProducer Master(1004).xlsx"
    
    If Dir(levpad) <> "" Then
        Set levMap = Workbooks.Open(levpad)
        Do Until gevonden = True Or levMap.Sheets(1).Range("A" & rij).Value = ""
            If levMap.Sheets(1).Range("A" & rij).Value = levNr Then
                levnaam = levMap.Sheets(1).Range("B" & rij).Value
                gevonden = True
            End If
            rij = rij + 1
        Loop
        levMap.Close False
    End If
    LevName = levnaam
End Function


