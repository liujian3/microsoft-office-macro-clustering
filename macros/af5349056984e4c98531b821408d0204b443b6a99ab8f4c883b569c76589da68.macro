Sub DropDown317_Change()

Dim ccc As Integer
Sheets("Calc for Power Cable").Unprotect password:="darby"
ccc = Range("e8")

Select Case ccc

Case 1

Range("b9:c9").Select
Selection.Locked = False
Selection.Interior.ColorIndex = xlNone


Case 2, 3

Range("b9") = Null
Range("c9") = 1
Range("b9:c9").Select
Selection.Locked = True
With Selection.Interior
    .ColorIndex = 15
    .Pattern = xlSolid
    .PatternColorIndex = xlAutomatic
End With

Range("k8").Select
    ActiveCell.Formula = "=VLOOKUP(VALUE('Conductor Aux'!$Y$2),'Conductor Aux'!$AF$2:$AU$90,12,FALSE)"
    

    
    Range("k9").Formula = "=IF(B9>0,INDEX(Conductor!$L$36:$L$60,C9,1),0)"
    
    
    
    Range("L8").Formula = "=VLOOKUP(VALUE('Conductor Aux'!$Y$2),'Conductor Aux'!$AF$2:$AU$90,10,FALSE)/1000"
    
    
    
    Range("L9").Formula = "=IF(B9>0,INDEX(Conductor!$M$36:$M$60,C9,1),0)"
    
Range("b18") = Sheets("Conduit").Range("V59")
End Select
Sheets("Calc for Power Cable").Protect password:="darby"
End Sub
