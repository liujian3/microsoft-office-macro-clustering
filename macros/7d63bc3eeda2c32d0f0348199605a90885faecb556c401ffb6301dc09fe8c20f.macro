Attribute VB_Name = "Emailsustainability"
Sub DateiOutlook()
  If ActiveWorkbook.Saved = False Then
  
  savebeforedeu.Show
   
  'MsgBox "Bevor Sie die E-mail versenden, speichern Sie zuerst die Datei!"
  
  Exit Sub
  End If
  On Error GoTo err
  
  
  Dim olApp As Object
  Dim inhaltzelle As String
  Dim inhaltzelle2 As String
  'Signatur:
  Dim oldBody As String

  
  Set olApp = CreateObject("Outlook.Application")
  inhaltzelle = Worksheets("Deckblatt").Range("D5")
  inhaltzelle2 = Worksheets("Deckblatt").Range("D6")
  Leadauditor = Worksheets("Deckblatt").Range("J54")
  
  With olApp.CreateItem(0)
        'Empf|fffd|nger
        .Recipients.Add "Supplier-Sustainability-Evaluation@daimler.com"
        'Betreff
        .Subject = "Meldung des aktuellen Sustainability Status des Lieferanten: " & inhaltzelle & " " & "Standort " & inhaltzelle2
        'Nachricht
        .Body = "Sehr geehrte Damen und Herren," & Chr(13) & Chr(13) & _
                "anbei erhalten Sie den aktuellen Sustainability Status des o.g. Lieferanten." & Chr(13) & Chr(13) & _
                "Detaillierte Informationen entnehmen Sie bitte dem Reiter 'Corporate Social Responsibility' aus dem Anhang." & Chr(13) & Chr(13) & _
                "Mit freundlichen Gr|fffd||fffd|en" & Chr(13) & Chr(13) & Leadauditor

         .Attachments.Add (ActiveWorkbook.FullName)
        .Display

    End With
        
        Set olApp = Nothing
    Exit Sub
    
err:
   MsgBox "Die e-mail ist nicht erstellt!", vbInformation
   Exit Sub

End Sub

