Private Sub Worksheet_Change(ByVal Target As Range)
     If Intersect(Target, Range("C11:C41,G11:G41")) Is Nothing Then Exit Sub
     If Target.Count > 1 Then Exit Sub 'Bearbeiten mehrerer Zeilen wird abgefangen
     If Target = "" Then
         Cells(Target.Row, 8).ClearContents
         Else:
         Cells(Target.Row, 8).Value = Date
     End If
 End Sub
