Attribute VB_Name = "Module1"
Sub Reset_valve()
'
' Reset_valve Macro
'
' Keyboard Shortcut: Ctrl+i
'
Dim i As Integer
    Dim Valve As Range
    Set Valve = Range("w10:w60")
    
    i = 9
    
    Do While Valve(i - 8) <> 0
        Cells(i + 1, 9) = "=R[0]C[11]"
        
        i = i + 1
    Loop
End Sub


Sub calc_pipe()
