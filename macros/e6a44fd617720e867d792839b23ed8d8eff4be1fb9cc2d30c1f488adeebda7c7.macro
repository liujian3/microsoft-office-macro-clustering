Attribute calc_pipe.VB_ProcData.VB_Invoke_Func = " \n14"
'
' calc_pipe Macro
' Macro recorded 14-04-2004 by CK
'

'
    Range("z10:z31").Select
    Selection.Copy
    Range("F10").Select
    ActiveSheet.Shapes("Drop Down 15").Select
    Range("F10").Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    Range("L6").Select
    Application.CutCopyMode = False
End Sub
