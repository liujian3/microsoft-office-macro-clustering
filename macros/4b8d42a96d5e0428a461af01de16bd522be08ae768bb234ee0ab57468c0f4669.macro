Private Sub Worksheet_Activate()
Application.ScreenUpdating = False
ActiveSheet.Unprotect ("nnwhcb")
For i = 1 To 110
    If Cells(6 + i, 1).Value = 0 Then
        Cells(6 + i, 1).EntireRow.Hidden = True
    Else
        Cells(6 + i, 1).EntireRow.Hidden = False
    End If
Next i
ActiveSheet.Protect ("nnwhcb")
Application.ScreenUpdating = True
End Sub
