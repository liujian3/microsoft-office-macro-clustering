Sub drukowanie1()

    If Worksheets("N").Range("error") > 0 Then
        MsgBox "Wprowad|fffd| poprawne dane"
    Else
        'drukowanie pierwszej strony
        Sheets("B").Select
        Range("A1:F69").Select
        Selection.PrintOut Copies:=1, Collate:=True
    End If
    
End Sub

Sub generuj_PDF()
    Dim Filenamestr As String
    Dim Dirnamestr As String

    If Worksheets("N").Range("error") > 0 Then
        MsgBox "Wprowad|fffd| poprawne dane"
    Else
        If Dir(Environ("commonprogramfiles") & "\Microsoft Shared\OFFICE" _
             & Format(Val(Application.Version), "00") & "\EXP_PDF.DLL") <> "" Then
            'nazwa pliku pdf (plik tworzy si|fffd| w katalogu z excelem)
            Filenamestr = Application.ActiveWorkbook.Path & "\kalkulacja_dokupienie.pdf"
            'ustalam obszar wydruku (na wszelki wypadek)
            Sheets("B").PageSetup.PrintArea = "$A$1:$F$69"
            'generuj|fffd| pdf
            Sheets("B").Select
            Sheets("B").Activate
            ActiveSheet.ExportAsFixedFormat Type:=xlTypePDF, Filename:= _
                Filenamestr _
                , Quality:=xlQualityStandard, IncludeDocProperties:=True, IgnorePrintAreas _
                :=False, OpenAfterPublish:=True
        Else
            'nazwa pliku pdf (plik tworzy si|fffd| w katalogu z excelem)
            Filenamestr = Application.ActiveWorkbook.Path & "\kalkulacja_dokupienie.pdf"
            Dirnamestr = Application.ActiveWorkbook.Path
            'ustalam obszar wydruku (na wszelki wypadek)
            Sheets("B").PageSetup.PrintArea = "$A$1:$F$69"
            Sheets("B").Select
            ChDir _
                Dirnamestr
            ActiveSheet.ExportAsFixedFormat Type:=xlTypePDF, Filename:= _
                Filenamestr _
                , Quality:=xlQualityStandard, IncludeDocProperties:=True, IgnorePrintAreas _
                :=False, OpenAfterPublish:=True
        End If
    End If
End Sub

