Attribute VB_Name = "Module1"
Sub Make_Site_Detail_sheets()
HowMany = 10
Dim NewSheetName As String
For x = 2 To HowMany
    'if already exists, delete it.
    NewSheetName = "Site " & x
    If SheetExists(NewSheetName) Then Sheets(NewSheetName).Delete
    
    Sheets("Site 1").Copy Before:=Sheets("Total Sites")
    ActiveSheet.Name = NewSheetName
    Range("H1") = x
    
Next x

End Sub

Sub LockAllSheets()
HowMany = 10
Dim NewSheetName As String
For x = 1 To HowMany
    'if already exists, delete it.
    NewSheetName = "Site " & x
    Sheets(NewSheetName).Protect DrawingObjects:=True, Contents:=True, Scenarios:=True
Next x
    Sheets("Main Page").Protect DrawingObjects:=True, Contents:=True, Scenarios:=True
    Sheets("Total Sites").Protect DrawingObjects:=True, Contents:=True, Scenarios:=True
End Sub


Function SheetExists(shtName As String, Optional wb As Workbook) As Boolean
    Dim sht As Worksheet

    If wb Is Nothing Then Set wb = ThisWorkbook
    On Error Resume Next
    Set sht = wb.Sheets(shtName)
    On Error GoTo 0
    SheetExists = Not sht Is Nothing
End Function
