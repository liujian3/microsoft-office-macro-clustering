Public Sub Worksheet_Change(ByVal Target As Range)                                  'Makro starten bei |fffd|nderung des Zelleninhaltes

'------------------ Standort |fffd|ndern

If Target.Address = ("$AA$11") Then

   If [AA11] <> "" Then Call SG_auslesen
    
End If

End Sub

Public Sub SG_auslesen()

Dim I As Integer
Dim SG As String

For I = 3 To 14                                                         'Schleife um die zum Netzanschluss zugeh|fffd|rige SG auszulesen.

    If Tabelle4.Cells(I, 11) = Tabelle1.Cells(11, 27) Then              'Abgleich Plattform
        
        If Tabelle4.Cells(I, 14) = "N" Then                             'Auslesen SG WEST / NORD
            
            Tabelle4.Cells(3, 3) = "N"
            Exit For
            
        Else:
        
            Tabelle4.Cells(3, 3) = "W"
            Exit For
            
        End If
        
    I = I + 1
    
    End If

Next

End Sub

