Private Sub TEXTO_Change()

NumeroDatos = Hoja6.Range("B" & Rows.Count).End(xlUp).Row
''MsgBox NumeroDatos
Hoja6.AutoFilterMode = False
Me.LISTA = Clear
Me.LISTA.RowSource = Clear
y = 0

For fila = 2 To NumeroDatos
descrip = Hoja2.Cells(fila, 2).value

   If UCase(descrip) Like "*" & UCase(Me.TEXTO.value) & "*" Then
    Me.LISTA.AddItem
    Me.LISTA.List(y, 0) = Hoja6.Cells(fila, 2).value
    Me.LISTA.List(y, 1) = Hoja6.Cells(fila, 3).value
    Me.LISTA.List(y, 2) = Hoja6.Cells(fila, 4).value
    Me.LISTA.List(y, 3) = Hoja6.Cells(fila, 5).value
    Me.LISTA.List(y, 4) = Hoja6.Cells(fila, 6).value
    Me.LISTA.List(y, 5) = Hoja6.Cells(fila, 7).value
    y = y + 1
    End If
Next




End Sub

