Option Explicit

Const EXPAND_SYMBOL = "[+]"
Const COLLAPSE_SYMBOL = "[-]"

Private Sub Workbook_SheetFollowHyperlink(ByVal Sh As Object, ByVal target As Hyperlink)
    On Error GoTo err_1
    If (target.Range.Value = EXPAND_SYMBOL) Or (target.Range.Value = COLLAPSE_SYMBOL) Then
        ExpandOrCollapse target.Range
    End If
err_1:
End Sub



