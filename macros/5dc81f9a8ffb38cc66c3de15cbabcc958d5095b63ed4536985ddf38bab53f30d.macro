Attribute VB_Name = "Expand_Contract_module"
Option Explicit

Const INDEX_SHEET_NAME = "Index"
Const EXPAND_SYMBOL = "[+]"
Const COLLAPSE_SYMBOL = "[-]"
Const TOP_INDEX_ROW = 5
' ************ Following has to be same as constants defined in MakeStandalone.xls **************
Const TOPIC_EXPAND_COL = 2
Const TOPIC_COL = TOPIC_EXPAND_COL + 1
Const SECTION_EXPAND_COL = TOPIC_COL + 1
Const SECTION_COL = SECTION_EXPAND_COL + 1
Const ANSWER_STATUS_COL = SECTION_COL + 1
Const QUESTION_COL = ANSWER_STATUS_COL + 1
Const PWD_1 = "0OoilLIbh"
Const PWD_2 = "871345"
' ******************************* End of constants that have to be the same *********************
Private Sub ProtectSheet(ws As Worksheet)
    Dim pwd: GetPWD pwd
    ws.Protect pwd
    ThisWorkbook.Protect pwd
End Sub
Private Sub UnprotectSheet(ws As Worksheet)
    Dim pwd: GetPWD pwd
    ThisWorkbook.Unprotect pwd
    ws.Unprotect pwd
End Sub

Private Sub GetPWD(ByRef pwd)
    pwd = PWD_1 & PWD_2 & PWD_2 & PWD_1
End Sub
        
Sub ExpandOrCollapse(target As Range)
    Application.ScreenUpdating = False
    Worksheets(INDEX_SHEET_NAME).Activate
    UnprotectSheet ActiveSheet
    If target.Column = TOPIC_EXPAND_COL Then
        ExpandOrCollapseTopic target
    Else
        ExpandOrCollapseSection target
    End If
    ProtectSheet ActiveSheet
    Application.ScreenUpdating = True
End Sub

Private Sub ExpandOrCollapseSection(target As Range)
    Dim sectionRange As Range
    Set sectionRange = GetSectionRange(target)
    If target.Value = COLLAPSE_SYMBOL Then
        sectionRange.Hidden = True
        target.Value = EXPAND_SYMBOL
    Else
        sectionRange.Hidden = False
        target.Value = COLLAPSE_SYMBOL
    End If
End Sub

Private Sub ExpandOrCollapseTopic(target As Range)
    Dim topicRange As Range
    Set topicRange = GetTopicRange(target)
    If target.Value = COLLAPSE_SYMBOL Then
        topicRange.Hidden = True
        target.Value = EXPAND_SYMBOL
    Else
        topicRange.Hidden = False
        target.Value = COLLAPSE_SYMBOL
        SetSectionStates topicRange
    End If
End Sub

'Inputs:  r is list of rows for the current topic
' Iterate through all the sections in this topic and set their expanded or contracted states
Private Sub SetSectionStates(r As Range)
    Dim sectionRange As Range
    Set r = Application.Intersect(r, Columns(SECTION_EXPAND_COL))
    Dim k
    For k = 1 To r.Rows.Count
        Dim rr As Range
        Set rr = r.Cells(k, 1)
        If rr.Value = EXPAND_SYMBOL Then
            Set sectionRange = GetSectionRange(rr)
            sectionRange.Hidden = True
        End If
    Next
End Sub

Private Function GetSectionRange(target As Range)
    Dim r As Range
    Set r = Worksheets(INDEX_SHEET_NAME).Cells(target.Row + 1, ANSWER_STATUS_COL)
    While Len(r) > 0
        Set r = r.Offset(1)
    Wend
    Set GetSectionRange = Range(Rows(target.Row + 1), Rows(r.Row - 1))
End Function

Private Function GetTopicRange(target As Range)
    Dim r As Range
    Set r = Worksheets(INDEX_SHEET_NAME).Cells(target.Row + 1, TOPIC_COL)
    Set r = r.End(xlDown)
    If r.Row > Range("zz_TOC_last_row").Row Then
        Set r = Range("zz_TOC_last_row")
    End If
    Set GetTopicRange = Range(Rows(target.Row + 1), Rows(r.Row - 1))
End Function
