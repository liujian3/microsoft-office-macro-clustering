Private Declare Function GetACP& Lib "kernel32" ()

Public Sub InitWizardName(fDummy As Boolean)
    Dim iResult As Integer
    Dim lCodePage As Long
    
    On Error GoTo KeyNotFound
    ' What language are we using?
    iResult = Application.LanguageSettings.LanguageID(msoLanguageIDInstall)
    
    Select Case iResult
        Case 1033, 4105 ' US or Canadian English
            iWizardMode = wzRESUME
        Case Else
            iWizardMode = wzCV
    End Select

AssignName:
    If iWizardMode = wzRESUME Then
        lCodePage = GetACP
        If lCodePage = 1252 Then
            ' Resume Wizard Name and FileNames with Accent
            strWizShortName = "Curr|fffd|culo"
            strWizLongName = "Asistente para curr|fffd|culos"
        Else
            ' Resume Wizard Name and FileNames without Accent
            strWizShortName = "Curr|fffd|culo"
            strWizLongName = "Asistente para curr|fffd|culos"
        End If
    Else
        ' CV Wizard Name and FileNames
        strWizShortName = "Curr|fffd|culo"
        strWizLongName = "Asistente para curr|fffd|culos"
    End If
    strWizName = strWizLongName
    
    Exit Sub
    
KeyNotFound:
    iWizardMode = wzCV
    Resume AssignName

End Sub


