   Case 9

     Word = "|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|"

  End Select

  str = Word

  Word = ""

  c = c - digital * 100

  If c > 19 Then

   digital = Int(c / 10)

   Select Case digital

