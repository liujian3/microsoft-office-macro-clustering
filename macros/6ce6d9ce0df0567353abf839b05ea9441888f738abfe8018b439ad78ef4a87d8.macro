Sub BlattschutzAUS()
'Entfernt den Blattschutz aus allen Bl|fffd|ttern, falls anderes Passwort als benz33 muss dies unten angepasst werden
Dim Passwort

Set dat = Application.FileDialog(msoFileDialogFilePicker)
Passwort = InputBox("Bitte Passwort eingeben" & Chr(13) & _
     "", "Passwort")

  Dim i As Integer
    For i = 1 To Sheets.Count
    Worksheets(i).Activate
    ActiveSheet.Unprotect Password:=Passwort
  
  Next i
  
 

End Sub

Sub BlattschutzEIN()
' Aktiviert den Blattschutz in jedem Arbeitsblatt und sch|fffd|tzt auch das komplette Workbook
'Sollte der Blattschutz in Verbindung mit dem Makro Reiterfarbe |fffd|ndern verwendet, muss hier der Schutz des gesamten Workbooks entfernt werden.
Dim Passwort
Dim Passwort2

Sprungmarke:
Set dat = Application.FileDialog(msoFileDialogFilePicker)
Passwort = InputBox("Bitte Passwort eingeben" & Chr(13) & _
     "", "Passwort")
Passwort2 = InputBox("Bitte Passwort best|fffd|tigen" & Chr(13) & _
     "", "Passwort")
If Passwort = Passwort2 Then
  Dim i As Integer
    For i = 1 To Sheets.Count
     Worksheets(i).Activate
    If ActiveSheet.ProtectContents = True Then
MsgBox "Blattschutz bereits vorhanden"
Else
    ActiveSheet.EnableSelection = xlNoRestrictions
    ActiveSheet.Protect Password:=Passwort, AllowFiltering:=True
End If
      
  Next i
Else
MsgBox "Passw|fffd|rter stimmen nicht |fffd|berein"
GoTo Sprungmarke
End If

End Sub





Private Sub Workbook_SheetChange(ByVal Sh As Object, ByVal Target As Range)
'F|fffd|rbt den Reiter Rot wenn in Zelle A1 keine 1 steht, Zelle und Wert k|fffd|nnen unten angepasst werden
Dim i%
On Error GoTo Fehler
    For i = 1 To Worksheets.Count

If Sheets(i).Range("A1") = "1" Or Sheets(i).Range("A1") = "" Then
    Sheets(i).Tab.ColorIndex = 2
                    Else
    Sheets(i).Tab.ColorIndex = 3
Fehler:
Sheets(i).Tab.ColorIndex = 3
End If
Next i


End Sub








