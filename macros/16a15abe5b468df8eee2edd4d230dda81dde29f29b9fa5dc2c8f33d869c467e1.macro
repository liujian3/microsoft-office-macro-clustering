Attribute VB_Name = "Module2"
Option Private Module
Private Const p As String = "72,71,86,89,89,66,80,88"

Sub PrepBook(bk As Workbook)
    Dim ary() As String
    Dim pwd As String
    Dim s
    
    pwd = ""
    ary = Split(p, ",")
    For Each s In ary
        pwd = pwd + rot(Chr(CInt(s)))
    Next
    
    bk.Unprotect pwd
End Sub

Sub FinishBook(bk As Workbook)
    Dim ary() As String
    Dim pwd As String
    Dim s
    
    pwd = ""
    ary = Split(p, ",")
    For Each s In ary
        pwd = pwd + rot(Chr(CInt(s)))
    Next
    
    bk.Protect pwd
End Sub

Sub PrepSheet(sh As Worksheet)
    Dim ary() As String
    Dim pwd As String
    Dim s
    
    pwd = ""
    ary = Split(p, ",")
    For Each s In ary
        pwd = pwd + rot(Chr(CInt(s)))
    Next
    
    sh.Unprotect pwd
End Sub

Sub FinishSheet(sh As Worksheet)
    Dim ary() As String
    Dim pwd As String
    Dim s
    
    pwd = ""
    ary = Split(p, ",")
    For Each s In ary
        pwd = pwd + rot(Chr(CInt(s)))
    Next
    
    sh.Protect pwd
End Sub

Private Function rot(c As String) As String
    Dim i As Integer
    i = Asc(c)
    If i >= 65 And i <= 90 Then
        i = ((i - 52) Mod 26) + 65
    ElseIf i >= 97 And i <= 122 Then
        i = ((i - 84) Mod 26) + 97
    End If
    rot = Chr(i)
End Function

