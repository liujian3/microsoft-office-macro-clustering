Public Sub UpdateLightingCF()
    Dim wsForm4 As Worksheet
    Dim result As VbMsgBoxResult

    Set wsForm4 = ThisWorkbook.Sheets(FORM4)
    
    'Only check boxes that have beeen selected
    If (wsForm4.Range("AC2").Value = True) Then
        UpdateLighting
        Exit Sub
    End If
    
    Dim rQuantity As String
    If (wsForm4.Range("AC4").Value = True) Then
        rQuantity = "M22:M24,M26:M30"
    Else
        rQuantity = "M22:M24,M26:M30,M35"
    End If
    
    'Check if quantity was entered, if not, exit function
    If (Application.Sum(wsForm4.Range(rQuantity)) = 0) Then
        UpdateLighting
        Exit Sub
    End If
    
    'User attempting to hide rows with non-empty quantities
    Dim currCell
    result = MsgBox("You have entered a Quantity for 'Compact Fluorescent Lamps/Fixtures'." & vbCrLf & "Would you like to remove the Quantities entered?", vbYesNo + vbExclamation, "")
    If result = vbYes Then
        Application.ScreenUpdating = False
        For Each currCell In wsForm4.Range(rQuantity)
            If currCell.Value > 0 Then currCell.ClearContents
        Next
        Application.ScreenUpdating = True
        UpdateLighting
    Else
        wsForm4.Range("AC2").Value = True
    End If
End Sub

