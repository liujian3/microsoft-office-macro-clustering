Public Sub UpdateLighting()
    Dim startRow As Integer, endRow As Integer, Index As Integer
    Dim bIsLightingSelected As Boolean, visible As Boolean
    Dim currSheet As Worksheet
    
    'Flag to know when at least one of the lighting sections has been selected.
    bIsLightingSelected = False

    Application.ScreenUpdating = False
    PrepSheet ThisWorkbook.Sheets(FORM4)
    
    Set currSheet = ThisWorkbook.Sheets(FORM4)
    For Index = 2 To 8
        startRow = currSheet.Range("AA" & Index).Value
        endRow = currSheet.Range("AB" & Index).Value
        visible = currSheet.Range("AC" & Index).Value
        
        SetSectionVisible currSheet, startRow, endRow, visible
        bIsLightingSelected = bIsLightingSelected Or visible
    Next Index
    
    'Add a single CF line from 'Display and Accent Ligting' when CF is selected
    Dim bDisplaySICF As Boolean
    bDisplaySICF = currSheet.Range("AC2").Value Or currSheet.Range("AC4").Value
    SetSectionVisible currSheet, 31, 31, bDisplaySICF
    SetSectionVisible currSheet, 35, 35, bDisplaySICF
    
    'Update the lighting header and total rows
    SetSectionVisible ThisWorkbook.Sheets(FORM4), 20, 20, bIsLightingSelected
    SetSectionVisible ThisWorkbook.Sheets(FORM4), 85, 85, bIsLightingSelected
    
    Dim iOffset As Integer
    iOffset = ThisWorkbook.Sheets(FORM4).Range("TC_Start").Row - 1
    
    'Update the lighting glossary
    SetSectionVisible ThisWorkbook.Sheets(FORM4), iOffset - 2, iOffset - 2, bIsLightingSelected
    
    'Update T&C Lighting header
    Index = 2
    startRow = currSheet.Range("AD" & Index).Value
    endRow = currSheet.Range("AE" & Index).Value
    SetSectionVisible currSheet, iOffset + startRow, iOffset + endRow, bIsLightingSelected
    
    'Update T&C Lighting footer
    SetSectionVisible ThisWorkbook.Sheets(FORM4), iOffset + 153, iOffset + 162, bIsLightingSelected
    
    FinishSheet ThisWorkbook.Sheets(FORM4)
    Application.ScreenUpdating = True
End Sub

