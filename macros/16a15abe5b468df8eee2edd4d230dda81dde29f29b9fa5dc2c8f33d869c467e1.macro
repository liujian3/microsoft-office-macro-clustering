Private Sub UpdateSection(ByVal Index As Integer)
    Dim sheet As Worksheet
    Dim startRow As Integer, endRow As Integer
    Dim visible As Boolean

    Set sheet = ThisWorkbook.Sheets(FORM4)
    
    Application.ScreenUpdating = False
    PrepSheet ThisWorkbook.Sheets(FORM4)
    
    'Update the sheet 'Form 4 Project Input'
    startRow = sheet.Range("AA" & Index).Value
    endRow = sheet.Range("AB" & Index).Value
    visible = sheet.Range("AC" & Index).Value
    SetSectionVisible ThisWorkbook.Sheets(FORM4), startRow, endRow, visible
    
    Dim iOffset As Integer
    iOffset = ThisWorkbook.Sheets(FORM4).Range("TC_Start").Row - 1
    
    'Update the sheet 'Form 4 Project Input' with the Terms & Conditions
    startRow = sheet.Range("AD" & Index).Value
    endRow = sheet.Range("AE" & Index).Value
    SetSectionVisible ThisWorkbook.Sheets(FORM4), iOffset + startRow, iOffset + endRow, visible
    
    FinishSheet ThisWorkbook.Sheets(FORM4)
    Application.ScreenUpdating = True
End Sub

Private Sub SetSectionVisible(ByVal sheet As Worksheet, ByVal iStart As Integer, ByVal iEnd As Integer, ByVal bVisible As Boolean)

    sheet.Rows(iStart & ":" & iEnd).EntireRow.Hidden = Not bVisible
End Sub


