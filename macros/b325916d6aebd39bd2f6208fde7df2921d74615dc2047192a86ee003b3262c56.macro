Private Function getCode(strFileName As Variant)
    Dim inputFileName As Variant
    Dim inputFn As Long
    Dim buffer() As Byte

    Dim outputFileName As String
    Dim outputFn As Long
    Const bESC As Byte = &H1B
    Const bAT As Byte = &H40
    Const bDollar As Byte = &H24
    Const bAnd As Byte = &H26
    Const bOP As Byte = &H28
    Const bB As Byte = &H42
    Const bD As Byte = &H44
    Const bJ As Byte = &H4A
    Const bI As Byte = &H49
    
    Dim strlen As Long
    Dim bbinary As Long
    Dim ucs2 As Long
    Dim sjis As Long
    Dim euc As Long
    Dim utf8 As Long
    Dim utf16le As Boolean
    Dim b1, b2, b3 As Byte
    
    bbinary = 0
    ucs2 = 0
    sjis = 0
    euc = 0
    utf8 = 0
    utf16le = False

    '|fffd||1c2||74d||fffd||fffd||783|t|fffd|@|fffd|C|fffd||fffd|
    inputFileName = strFileName

    '|fffd||c2882||102||fffd||fffd||fffd|t|fffd|@|fffd|C|fffd||fffd||fffd||50d||fffd|
    inputFn = FreeFile
    
    '****************************************
    '|fffd|t|fffd|@|fffd|C|fffd||fffd||fffd||fffd||fffd||fffd||fffd|R|fffd|[|fffd|h|fffd||fffd||fffd||fffd|
    '****************************************
    Open inputFileName For Binary As #inputFn
        '|fffd|t|fffd|@|fffd|C|fffd||fffd||fffd||312||fffd||fffd||fffd||fffd||154|z|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|
        ReDim buffer(LOF(inputFn))

        '|fffd|t|fffd|@|fffd|C|fffd||fffd||fffd||fffd||fffd|o|fffd|C|fffd|i|fffd||fffd||fffd||153||1c2||74d||fffd||fffd||fffd||fffd|Byte|fffd|z|fffd||fffd||24a|i|fffd|[
        Get #inputFn, , buffer
        
        strlen = UBound(buffer)
        
        Dim i As Long
        
        'sjis|fffd||fffd||fffd|f
        For i = 0 To strlen - 2
            b1 = buffer(i)
            b2 = buffer(i + 1)
            If ((b1 >= &H81 And b1 <= &H9F) Or _
            (b1 >= &HE0 And b1 <= &HFC)) And _
            ((b2 >= &H40 And b2 <= &H7E) Or _
            (b2 >= &H80 And b2 <= &HFC)) Then
                sjis = sjis + 2
                i = i + 1
            End If
        Next i

        'euc|fffd||fffd||fffd|f
        For i = 0 To strlen - 2
            b1 = buffer(i)
            b2 = buffer(i + 1)
            If ((b1 >= &HA1 And b1 <= &HFE) And _
            (b2 >= &HA1 And b2 <= &HFE)) Or _
            (b1 = &H8E And (b2 >= &HA1 And b2 <= &HDF)) Then
                euc = euc + 2
                i = i + 1
            ElseIf strlen - 2 > i And b1 = &H8F And _
            (b2 >= &HA1 And b2 <= &HFE) And _
            (buffer(i + 2) >= &HA1 And buffer(i + 2) <= &HFE) Then
                euc = euc + 3
                i = i + 2
            End If
        Next i
        
        'utf8|fffd||fffd||fffd|f
        For i = 0 To strlen - 2
            b1 = buffer(i)
            b2 = buffer(i + 1)
            If (b1 >= &HC0 And b1 <= &HDF) And _
            (b2 >= &H80 And b2 <= &HBF) Then
                utf8 = utf8 + 2
                i = i + 1
            ElseIf strlen - 2 > i And _
            (b1 >= &HE0 And b1 <= &HEF) And _
            (b2 >= &H80 And b2 <= &HBF) And _
            (buffer(i + 2) >= &H80 And buffer(i + 2) <= &HBF) Then
                utf8 = utf8 + 3
                i = i + 2
            End If
        Next i
        
        'unicode bom|fffd||fffd||fffd|f
        For i = 0 To strlen - 2
            b1 = buffer(i)
            b2 = buffer(i + 1)
            b3 = buffer(i + 2)
            If b1 = 255 And b2 = 254 Then
                utf16le = True
                Exit For
            End If
        Next i
        
        Dim strCharset As String
        If utf16le = True Then
            strCharset = "utf-16le"
        ElseIf euc > sjis And euc > utf8 Then
            'EUC
            strCharset = "euc-jp"
        ElseIf sjis > euc And sjis > utf8 Then
            'SJIS
            strCharset = "shift_jis"
        ElseIf utf8 > euc And utf8 > sjis Then
            'UTF8
            strCharset = "utf-8"
        Else
            strCharset = "shift_jis"
        End If
    Close #inputFn
    getCode = strCharset
    
End Function
