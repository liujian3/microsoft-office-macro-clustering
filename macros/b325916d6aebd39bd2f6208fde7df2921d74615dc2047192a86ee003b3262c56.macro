Public Function row_delete(lag_CD)
    Dim col As Integer
    Dim xWk As Variant
    
    Dim endrow As Long          ' |fffd|e|fffd|[|fffd|u|fffd||fffd||fffd|T|fffd|C|fffd|Y(|fffd|s)
    Dim endcol As Integer       ' |fffd|e|fffd|[|fffd|u|fffd||fffd||fffd|T|fffd|C|fffd|Y(|fffd||fffd|)
    
    Dim myRow As Long   ' |fffd|I|fffd||fffd||fffd|Z|fffd||fffd||fffd||30d|s|fffd||50d||fffd|
    
    Dim ro As Long      ' |fffd|J|fffd|E|fffd||fffd||fffd|^
    Dim chkRow As Long  ' |fffd|J|fffd|E|fffd||fffd||fffd|^
    Dim recode As Long  ' |fffd|J|fffd|E|fffd||fffd||fffd|^
    
    Dim myNo As Long    ' |fffd||d3dc||fffd||fffd||fffd|R|fffd|[|fffd|h|fffd||30d||fffd||fffd||fffd|
    
    ActiveWorkbook.Unprotect Password:="scc"
    Worksheets(Target_menu).Unprotect Password:="scc"
    Worksheets(Target_info).Unprotect Password:="scc"
    Worksheets(Target_check).Unprotect Password:="scc"
    
    With Worksheets(Target_menu)
        endcol = .Cells(10, 2).End(xlDown).Row
        xWk = .Range(.Cells(11, 2), .Cells(endcol + 10, 2))
        '###|fffd|e|fffd||fffd||fffd||68f||fffd||fffd||74d|s|fffd|m|fffd|F###
        For col = 11 To endcol Step 1
            Call Sheet2.setColumns(col - 10, xWk(col - 10, 1))
        Next col
    End With
    
    With Worksheets(Target_info)
        endrow = .Cells(2, 1).End(xlDown).Row
        If endrow = 65536 Then
            endrow = 3
        End If
        
        If (Selection.Row > 2) And (Selection.Row < endrow + 1) Then
            
            myRow = Selection.Row
            
            If .Cells(myRow, No).Interior.ColorIndex = 4 Then
                
                myNo = .Cells(myRow, No).Value
                
                ' ###|fffd||fffd||fffd|[|fffd||d3dc||fffd|s|fffd||90||752||fffd|f|fffd|[|fffd|^|fffd||fffd||fffd|s|fffd||1c9||fffd||fffd|s|fffd||fffd|### 2013/01/07 |fffd||fffd||fffd||352||1c9||fffd|
                k = 4
                ExtensionDay = 0
                Do Until ExtensionDay <> 0
                    If .Cells(myRow, G_00004) = Sheets("Extension").Cells(k, 4) Then
                        If Sheets("Extension").Cells(k, 6) = "1" Then
                            If lag_CD = 1 Then
                                MsgBox "|fffd|I|fffd||fffd||fffd||fffd||fffd||fffd||fffd|f|fffd|[|fffd|^|fffd||355||fffd||fffd|[|fffd||d3dc||fffd||142||fffd||fffd||702||fffd||fffd||fffd|B"
                            ElseIf lag_CD = 2 Then
                                MsgBox "You can not delete the Split Dlvy."
                            ElseIf lag_CD = 3 Then
                                showUserForm1 (Worksheets("Translation").Range("D251").Value)
                            End If
                            ActiveWorkbook.Protect Structure:=True, Windows:=False, Password:="scc"
                            Worksheets(Target_menu).Protect Password:="scc"
                            Worksheets(Target_info).Protect Password:="scc", UserInterfaceOnly:=True, Contents:=True, AllowFormattingColumns:=True, AllowFormattingRows:=True
                            Worksheets(Target_check).Protect Password:="scc", Contents:=True, AllowFormattingColumns:=True, AllowFormattingRows:=True
                            Exit Function
                        End If
                    Else
                        If Sheets("Extension").Cells(k, 4) = "" Then
                            ExtensionDay = 10000
                        End If
                    End If
                k = k + 1
                Loop
                
                ' ###|fffd|s|fffd||d3dc|###
                .Rows(myRow).Select
                Selection.Delete Shift:=xlUp
                
                ' ###|fffd||fffd||fffd||502||30f|C|fffd||fffd|###
                For ro = 3 To endrow - 1 Step 1
                    If .Cells(ro, No).Value > myNo Then
                       .Cells(ro, No).Value = .Cells(ro, No).Value - 1
                    End If
                Next
                
                ' ###|fffd||53683|`|fffd|F|fffd|b|fffd|N|fffd||fffd||fffd||fffd|(No)|fffd||30f|C|fffd||fffd|###
                With Worksheets(Target_check)
                    chkRow = 3
                    Do Until .Cells(chkRow, 1).Value = ""
                        If .Cells(chkRow, 1).Value = myNo Then
                            .Rows(chkRow).Delete Shift:=xlUp
                            chkRow = chkRow - 1
                        Else
                            If .Cells(chkRow, 1).Value > myNo Then
                                .Cells(chkRow, 1).Value = .Cells(chkRow, 1).Value - 1
                            End If
                        End If
                        chkRow = chkRow + 1
                    Loop
                End With
                
                ' |fffd||fffd||fffd|[No|fffd||30f|C|fffd||fffd|
                recode = 0
                Do Until (.Cells(myRow + recode, G_S0045).Value = "1") Or (.Cells(myRow + recode, G_S0045).Value = "")
                    .Cells(myRow + recode, G_S0045).Value = .Cells(myRow + recode, G_S0045).Value - 1
                    recode = recode + 1
                Loop
            Else
                If lag_CD = 1 Then
                    MsgBox "|fffd|I|fffd||fffd||fffd||fffd||fffd||a0bd||fffd|s|fffd||34d||d3dc||fffd||142||fffd||fffd||702||fffd||fffd||fffd|B|fffd||d3dc||fffd||94|\|fffd||202||302||355||fffd||fffd|[|fffd||fffd||fffd||a0bd||fffd|s|fffd||142||fffd||fffd||fffd|u|fffd||fffd||fffd||501|v|fffd||fffd||fffd||fffd||fffd||390|F|fffd||252||fffd||fffd|F|fffd||fffd||fffd||a0bd||fffd|s|fffd||302||742||142||fffd||fffd|B", , ""
                ElseIf lag_CD = 2 Then
                    MsgBox "You can not delete the Split Dlvy. Only added Split Dlvy. line can be deleted.", , ""
                ElseIf lag_CD = 3 Then
                    showUserForm1 (Worksheets("Translation").Range("D244").Value)
                End If
            End If
            .Cells(myRow - 1, 2).Select
            ActiveWorkbook.Protect Structure:=True, Windows:=False, Password:="scc"
            Worksheets(Target_menu).Protect Password:="scc"
            Worksheets(Target_info).Protect Password:="scc", UserInterfaceOnly:=True, Contents:=True, AllowFormattingColumns:=True, AllowFormattingRows:=True
            Worksheets(Target_check).Protect Password:="scc", Contents:=True, AllowFormattingColumns:=True, AllowFormattingRows:=True
        End If
    End With
End Function

