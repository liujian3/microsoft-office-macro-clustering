Attribute VB_Name = "Module1"
Sub make_ready2sent()
'
' make_ready2sent Macro
' Make all fixed and remove tabs for sending
' Remove "Nordic Guide", "UK Guide", "Europe Guide"
' "How To" needs to be removed manual

'
    Sheets("SSD").Select
    Cells.Select
    Application.CutCopyMode = False
    Selection.Copy
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    Sheets("UFD").Select
    Cells.Select
    Application.CutCopyMode = False
    Selection.Copy
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    Sheets("DACH Guide").Select
    Cells.Select
    Application.CutCopyMode = False
    Selection.Copy
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    Sheets("WSP").Select
    Cells.Select
    Application.CutCopyMode = False
    Selection.Copy
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    Sheets(Array("Nordic Guide", "UK Guide", "Europe Guide")).Select
    Sheets("Europe Guide").Activate
    Application.CutCopyMode = False
    ActiveWindow.SelectedSheets.Delete
    Sheets("SSD").Select
    Range("A1").Select
End Sub


