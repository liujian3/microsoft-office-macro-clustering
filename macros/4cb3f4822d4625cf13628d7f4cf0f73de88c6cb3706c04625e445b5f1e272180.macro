    Worksheets("betalone").Range("a" & myLastRowSheet_2 & ":ev" & myLastRowSheet_2).Value = _
        Worksheets("statscopy").Range("a" & myLastRowSheet_1 & ":ev" & myLastRowSheet_1).Value

    'Worksheets("betalone").Cells(myLastRowSheet_2, "ex").Value = Date
    
    End If

