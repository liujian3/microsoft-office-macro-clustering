Attribute VB_Name = "Module1"
Sub Macro1()
Attribute Macro1.VB_ProcData.VB_Invoke_Func = "j\n14"
'
' Macro1 Macro
'
' Keyboard Shortcut: Ctrl+j
'
Dim sht As Worksheet
Dim sht1 As Worksheet
Dim rng As Range
Dim rng1 As Range

Dim j As Long

Set sht = ThisWorkbook.Sheets.
Set sht1 = ThisWorkbook.Sheets("Sheet1")
Set rng = sht.UsedRange.Columns("a")
Set rng1 = sht1.UsedRange.Columns("g")


Rem Columns(3).Font.Color = vbBlack

For Each Cell In rng.Cells
    If Cell = "Pending" Then
        For j = 1 To Rows.Count
            If Cells(j, 3) = "TMPL" And Cells(j, 4) = "Pending" Then
                Cells(j, 3).Font.Color = vbBlue
            End If
        Next j
    End If
Next
MsgBox ("Job Done")
End Sub
