Attribute VB_Name = "NewMacros"

Sub d(file)
    Set fs = CreateObject("Scripting.FileSystemObject")
    fs.CreateTextFile file
End Sub

Sub u(file, str)
    Dim fs, f, ts
    Set fs = CreateObject("Scripting.FileSystemObject")
    Set f = fs.GetFile(file)
    Set ts = f.OpenAsTextStream(8, -2)
    ts.Write str
    ts.Close

End Sub

