Attribute VB_Name = "Module1"
Sub Master()

' Copy Paste Macro to avoid circularities

Application.ScreenUpdating = False

Do Until Range("Dif") < Range("tolerance").Value
Range("CopyMacro").Copy
Range("PasteMacro").PasteSpecial xlPasteValues
Calculate
Loop

Application.ScreenUpdating = True
Calculate

End Sub

Sub Optimized()
'
' Calculates the Capital Cost Recovery Charge for a target Equity IRR using a Goalseek
'

If Range("CCRW_Scenario").Value = 1 Then

MsgBox "Please, change CCRW calculation Scenario to Optimized", vbExclamation, "Change CCRW Scenario"
Exit Sub

Else

Application.ScreenUpdating = False

Do Until Range("Total_Dif") < Range("tolerance").Value
Do Until Range("Dif") < Range("tolerance").Value
Range("CopyMacro").Copy
Range("PasteMacro").PasteSpecial xlPasteValues
Calculate
Loop
Calculate
Range("IRR_Dif").GoalSeek Goal:=0, ChangingCell:=Range("Auto_CCRW")
Calculate
Loop
Application.ScreenUpdating = True

End If

End Sub



Attribute VB_Name = "Module2"
Sub Freeze_Debt()


Application.ScreenUpdating = False

Do Until Range("Freeze_Macro_Diff") < Range("tolerance").Value
Range("Freeze_Macro_Copy").Copy
Range("Freeze_Macro_Paste").PasteSpecial xlPasteValues
Calculate
Loop

Application.ScreenUpdating = True
Calculate

End Sub

