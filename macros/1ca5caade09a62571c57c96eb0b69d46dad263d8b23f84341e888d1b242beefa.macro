'Zaokr|fffd|glenie rezystancji do 5 miejsc po przecinku.
   r = Round(R_zmierzona, 5)


'Precyzyjne wyliczenie temperatury SPRT  na podstawie zmierzonej rezystancji "R_zmierzona".
    T_nizsza = i
    T_wyzsza = i + 1
    R_nizsza = SPRT(i)
    R_wyzsza = SPRT(i + 1)
    Skok_T = (T_wyzsza - T_nizsza) / 100000
    Skok_R = (R_wyzsza - R_nizsza) / 100000
                                    
    For i = 0 To 100000
       If R_nizsza + Skok_R * i > r Then
       i = i - 1
       T_wyliczona = Skok_T * i + T_nizsza
    Exit For
       End If
    Next
    'Rezystancja "R_zmierzona" czujnika "SPRT" odpowiada temperaturze "T_wyliczona".
    SPRTx = T_wyliczona
End If

End Function




















