    Skok_R = (R_wyzsza - R_nizsza) / 100000

    For i = 0 To 100000
       If T_nizsza + Skok_T * i > T Then
       i = i - 1
       R_wyliczona = Skok_R * i + R_nizsza
    Exit For
       End If
    Next
