Public Function DATTO(ByVal Dat As String)
Dim Rok, Mc, Dz
  Rok = Right(Dat, 4)
  If Len(Dat) < 8 Then
  Mc = Mid(Dat, 2, 2)
  Dz = "0" & Left(Dat, 1)
  Else
  Mc = Mid(Dat, 3, 2)
  Dz = Left(Dat, 2)
  End If
DATTO = DateSerial(Rok, Mc, Dz)
End Function

