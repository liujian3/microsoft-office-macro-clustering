    Dim fin As Integer
    Dim inicioCol As String
    Dim finCol As String
    ReDim zerrenda(0) As String
    inicio = Sheets("Data").Range("FilaInicioArticulosLR").Value
    fin = Sheets("Data").Range("FilaFinArticulosLR").Value
