Sub ordenaDropDown()
    For Each sh In Sheets("BoM").Shapes
        If Left(sh.Name, 9) = "Drop Down" Then
                sh.ZOrder msoSendToFront
        End If
    Next
End Sub

Sub ordenaCheckBoxShapes()
    Sheets("BoM").Unprotect
    For Each sh In Sheets("BoM").Shapes
        If sh.Name = "CheckBox1" Then
            chpos = sh.ZOrderPosition + 1
        End If
    Next
    n = Sheets("BoM").Shapes.Count - chpos
    k = 0
    Do While n <> 0
        newn = 0
        For i = chpos + 1 To Sheets("BoM").Shapes.Count - k
            If Left(Sheets("BoM").Shapes(i - 1).Name, 8) <> "CheckBox" Then
                Sheets("BoM").Shapes(i - 1).ZOrder msoBringToFront
                k = k + 1
                newn = 1
                Exit For
            Else
                If Left(Sheets("BoM").Shapes(i - 1).Name, 8) = "CheckBox" And Left(Sheets("BoM").Shapes(i).Name, 8) = "CheckBox" Then
                    If CInt(Right(Sheets("BoM").Shapes(i - 1).Name, Len(Sheets("BoM").Shapes(i - 1).Name) - 8)) > CInt(Right(Sheets("BoM").Shapes(i).Name, Len(Sheets("BoM").Shapes(i).Name) - 8)) Then
                        Sheets("BoM").Shapes(i - 1).ZOrder msoBringForward
                        newn = i
                    End If
                End If
            End If
        Next
        n = newn
    Loop
    ThisWorkbook.Worksheets("BoM").Protect AllowFormattingCells:=True, AllowFormattingColumns:=True, AllowFormattingRows:=True, AllowInsertingRows:=True, AllowDeletingRows:=True, UserInterfaceOnly:=True
    ThisWorkbook.Worksheets("BoM").EnableOutlining = True
End Sub

Sub renombraCheckBoxShapes()
    Sheets("BoM").Unprotect
    ordenaCheckBoxShapes
    For Each sh In Sheets("BoM").Shapes
        If sh.Name = "CheckBox1" Then
            chpos = sh.ZOrderPosition + 1
        End If
    Next
    For i = chpos To Sheets("BoM").Shapes.Count
        If Left(Sheets("BoM").Shapes(i - 1).Name, 8) <> "CheckBox" Then
            Sheets("BoM").Shapes(i - 1).ZOrder msoBringToFront
            k = k + 1
        End If
    Next
    counter = 2
    For i = chpos To Sheets("BoM").Shapes.Count - k
        If Left(Sheets("BoM").Shapes(i).Name, 8) = "CheckBox" Then
           Worksheets("BoM").Shapes(i).Name = "CheckBox" & counter
           counter = counter + 1
        End If
    Next
    ThisWorkbook.Worksheets("BoM").Protect AllowFormattingCells:=True, AllowFormattingColumns:=True, AllowFormattingRows:=True, AllowInsertingRows:=True, AllowDeletingRows:=True, UserInterfaceOnly:=True
    ThisWorkbook.Worksheets("BoM").EnableOutlining = True
End Sub
