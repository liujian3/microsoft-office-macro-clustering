Sub eguneratu(ByVal hoja As String)
    Application.ScreenUpdating = False
    Application.EnableEvents = False
    taulaFormatua hoja
    colExtra = ConvertToLetter(ConvertToNumber(Sheets("Data").Range("NumColumFinListaLR").Value) + 2)
    coltags = ConvertToLetter(ConvertToNumber(Sheets("Data").Range("NumColumInicioListaLR").Value) - 2)
    colInicio = Sheets("Data").Range("NumColumInicioListaLR").Value
    colFin = Sheets("Data").Range("NumColumFinListaLR").Value
    filaFSum = Application.Match("Tag_Servicio", Sheets("BoM").Range(coltags & ":" & coltags), 0) + 1
    sumafechas = WorksheetFunction.CountA(Sheets("BoM").Range(colInicio & filaFSum & ":" & colFin & filaFSum))
    inicio = Sheets("Data").Range("FilaInicioArticulosLR").Value
    fin = Sheets("Data").Range("FilaFinArticulosLR").Value
    If (Application.IfError(WorksheetFunction.Sum(Sheets("BoM").Range(colExtra & inicio & ":" & colExtra & fin)), 0) <> 0) Then
        MsgBox Sheets("Languages").Range("Msg31_importarCantExtra"), vbExclamation
    End If
    If (Sheets("Data").Range("tipo_estudio").Value = "estudioArgentina") Then
        EguneratuArgentina (hoja)
    Else
        If (Sheets("Data").Range("id_Grafsystem").Value = "X") Then
            Modo = Sheets("Data").Range("Control_ModoOperacionGraf").Value
        Else
            Modo = Sheets("Data").Range("Control_ModoOperacion").Value
        End If
        If (Modo = 0) Then
            If sumafechas > 0 Then
                EguneratuZutabekaFetxekin (hoja)
            Else
                EguneratuZutabeka (hoja)
            End If
        Else
            MsgBox Sheets("Languages").Range("Msg32_importarTotales"), vbExclamation
            eguneratuTotalak (hoja)
        End If
    End If
    Application.ScreenUpdating = True
    Application.EnableEvents = True
End Sub


