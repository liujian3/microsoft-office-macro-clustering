Sub creaLinea()
    Dim stock As String, sobra As String, falta As String, colInicio As String, extra As String
    colInicio = Sheets("Data").Range("NumColumInicioListaLR").Value
    colNotas = ConvertToLetter(ConvertToNumber(colInicio) - 1)
    extra = ConvertToLetter(ConvertToNumber(Sheets("Data").Range("ColStock").Value) - 1)
    stock = Sheets("Data").Range("ColStock").Value
    sobra = ConvertToLetter(ConvertToNumber(stock) + 1)
    falta = ConvertToLetter(ConvertToNumber(stock) + 2)
    stockAzkena = Sheets("Data").Range("UltColUsado_Stock").Value
    If (Sheets("Data").Range("TieneStock") = "X") Then
        AzkenZut = stockAzkena
    Else
        AzkenZut = falta
    End If
    sarpuntua = CInt(txt_Desde.Value)
    zenbat = CInt(txt_hasta.Value)
    DeleteAllCheckBoxes
    Sheets("BoM").Rows(sarpuntua & ":" & sarpuntua).Select
    ActiveCell.EntireRow.Resize(zenbat).Insert
    Sheets("BoM").Range("C" & sarpuntua + zenbat & ":" & AzkenZut & sarpuntua + zenbat).Select
    Selection.Copy
    Sheets("BoM").Range("C" & sarpuntua & ":" & AzkenZut & sarpuntua + zenbat - 1).Select
    ActiveSheet.Paste
    Sheets("BoM").Range("C" & sarpuntua & ":C" & sarpuntua + zenbat - 1 & ",D" & sarpuntua & ":D" & sarpuntua + zenbat - 1 & ",F" & sarpuntua & ":F" & sarpuntua + zenbat - 1 & ",G" & sarpuntua & ":G" & sarpuntua + zenbat - 1 & ",I" & sarpuntua & ":I" & sarpuntua + zenbat - 1 & ",L" & sarpuntua & ":L" & sarpuntua + zenbat - 1 & ",M" & sarpuntua & ":M" & sarpuntua + zenbat - 1 & "," & colNotas & sarpuntua & ":" & extra & sarpuntua + zenbat - 1).Select
    Application.CutCopyMode = False
    Selection.ClearContents
    Selection.ClearComments
    'Faktoriak 1.00 ipini sartutako linean beti
    Sheets("BoM").Range("H" & sarpuntua & ":H" & sarpuntua + zenbat - 1).Value = 1#
    Sheets("BoM").Range("J" & sarpuntua & ":J" & sarpuntua + zenbat - 1).Value = 1#
    Sheets("BoM").Range("A1").Select
    Sheets("Data").Range("CheckBoxGenerados") = ""
    checkBoxGen
End Sub

