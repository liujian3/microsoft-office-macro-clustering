        .lbl_filaFechaStock.Caption = Sheets("Languages").Range("ImportarStockForm_lblFilaFecha").Value
    End With
    If (Sheets("Data").Range("TieneStock").Value = "X") Then
        Me.combo_Columnas.Enabled = True
        Dim primCol, ultCol As String
        primCol = Sheets("Data").Range("colPrimStock").Value
        ultCol = Sheets("Data").Range("ultColUsado_Stock").Value
        primColNumber = ConvertToNumber(primCol)
        ultColNumber = ConvertToNumber(ultCol)
        For i = primColNumber To ultColNumber
            Me.combo_Columnas.AddItem ConvertToLetter(i)
        Next
    Else 'ez da stockik bidali Repconetik
        Me.combo_Columnas.Value = Sheets("Data").Range("colStock").Value
        Me.combo_Columnas.Enabled = False
    End If
