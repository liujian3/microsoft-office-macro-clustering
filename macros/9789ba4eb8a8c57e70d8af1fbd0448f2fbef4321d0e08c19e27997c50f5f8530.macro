Sub eliminaLineasVacias()
    Dim inicio, fin As Integer
    inicio = ThisWorkbook.Sheets("Data").Range("FilaInicioArticulosLR").Value
    fin = ThisWorkbook.Sheets("Data").Range("FilaFinArticulosLR").Value
    colCant = "P"
    DeleteAllCheckBoxes
    Dim i As Integer
    i = inicio
    Do While i <= fin
        If Sheets("BoM").Range(colCant & i).Value = 0 Then
            eliminalineaConcretaSinCheckbox i
            fin = fin - 1
        Else
            i = i + 1
        End If
    Loop
    Sheets("Data").Range("CheckBoxGenerados") = ""
    checkBoxGen
End Sub

Sub eliminaPreciosCero()
    Dim inicio, fin As Integer
    inicio = ThisWorkbook.Sheets("Data").Range("FilaInicioArticulosLR").Value
    fin = ThisWorkbook.Sheets("Data").Range("FilaFinArticulosLR").Value
    colPrecio = "Q"
    DeleteAllCheckBoxes
    Dim i As Integer
    i = inicio
    Do While i <= fin
        If Sheets("BoM").Range(colPrecio & i).Value = 0 Then
            eliminalineaConcretaSinCheckbox i
            fin = fin - 1
        Else
            i = i + 1
        End If
    Loop
    Sheets("Data").Range("CheckBoxGenerados") = ""
    checkBoxGen
End Sub



