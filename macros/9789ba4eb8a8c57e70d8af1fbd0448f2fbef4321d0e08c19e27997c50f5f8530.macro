Function CopyPasteHeader(ByVal originSheet As String, ByVal targetSheet As String, ByVal non As Integer) As Integer
    'logoa ekarri
    Dim Logo As Shape
    Dim tempImageFile As String
    Set Logo = Sheets(originSheet).Shapes("LogoUlma")
    tempImageFile = Environ("temp") & "\image.jpg"
    Save_Object_As_Picture Logo, tempImageFile
    With Sheets(targetSheet).PageSetup
        .RightHeaderPicture.Filename = tempImageFile
        .RightHeader = "&G"
        .HeaderMargin = 50
    End With
    'goiburua ekarri
'        'header
    Sheets(targetSheet).Range("A" & non) = Sheets(originSheet).Range("D2").Value
    Sheets(targetSheet).Range("B" & non & ":G" & non).Merge
    Sheets(targetSheet).Range("B" & non) = separadorCodNom(Sheets(originSheet).Range("ID_Data_Cliente").Value, "//", False)
    Sheets(targetSheet).Range("A" & non + 1) = Sheets(originSheet).Range("N2").Value
    Sheets(targetSheet).Range("B" & non + 1 & ":G" & non + 1).Merge
    Sheets(targetSheet).Range("B" & non + 1) = Sheets(originSheet).Range("ID_Data_OfertaSolicitud").Value
    Sheets(targetSheet).Range("A" & non + 2) = Sheets(originSheet).Range("I3").Value
    Sheets(targetSheet).Range("B" & non + 2 & ":G" & non + 2).Merge
    Sheets(targetSheet).Range("B" & non + 2) = Sheets(originSheet).Range("ID_Data_ObraGet").Value & ", " & Sheets(originSheet).Range("ID_Data_PoblacionObra").Value
    Sheets(targetSheet).Range("A" & non + 3) = Sheets(originSheet).Range("N4").Value
    Sheets(targetSheet).Range("B" & non + 3 & ":G" & non + 3).Merge
    Sheets(targetSheet).Range("B" & non + 3) = Sheets(originSheet).Range("ID_Data_Contacto").Value
    
    Sheets(targetSheet).Range("A" & non & ":A" & non + 3).Font.Size = 8
    Sheets(targetSheet).Range("B" & non & ":B" & non + 3).Font.Size = 11
    
        
    With Sheets(targetSheet).Range("A" & non & ":G" & non + 3).Borders(xlEdgeBottom)
        .LineStyle = xlContinuous
        .Weight = xlThin
        .ColorIndex = 0
    End With
    With Sheets(targetSheet).Range("A" & non & ":G" & non + 3).Borders(xlEdgeLeft)
        .LineStyle = xlContinuous
        .Weight = xlThin
        .ColorIndex = 0
    End With
    With Sheets(targetSheet).Range("A" & non & ":G" & non + 3).Borders(xlEdgeRight)
        .LineStyle = xlContinuous
        .Weight = xlThin
        .ColorIndex = 0
    End With
    With Sheets(targetSheet).Range("A" & non & ":G" & non + 3).Borders(xlEdgeTop)
        .LineStyle = xlContinuous
        .Weight = xlThin
        .ColorIndex = 0
    End With
    With Sheets(targetSheet).Range("A" & non & ":G" & non + 3).Borders(xlInsideHorizontal)
        .LineStyle = xlContinuous
        .Weight = xlThin
    End With
    'Sheets(targetSheet).Range("A" & non & ":G" & non + 3).Font.Size = 8
    Sheets(targetSheet).Range("A" & non & ":G" & non + 3).Font.Bold = True
    Sheets(targetSheet).Range("A" & non & ":G" & non + 3).Interior.Color = RGB(255, 231, 155)
    CopyPasteHeader = non + 5
End Function



