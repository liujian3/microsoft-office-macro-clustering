    ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
    
    'Orria zabaltzean babesa kendu errorea eman ez dezan hau exekutatzean
    Sheets("BoM").Unprotect
    'Change atalera pasa ez dadin hau exekutatzena
    cambioModoPorCheckBox = "X"
    'Eventak gelditu hau exekutatzeko
    Application.EnableEvents = False
    'Ez eguneratu pantaila azkarrago exekutatu dadin dena
    Application.ScreenUpdating = False
    Dim chpos As Integer
    'Checkbox orokorra bada
    If (chName = "CheckBox1") Then
    voyACambiarAnclajes = "X"
        'checkBox-a True-ra pasa bada
        If (Sheets("BoM").Shapes(chName).OLEFormat.Object.Object.Value = True) Then
        'lerro bakoitzeko
        For i = Range("FilaInicioArticulosLR") To Range("FilaFinArticulosLR")
            'Modo zutabeko zeldak babestu
            If (Sheets("BoM").Range("L" & i).Locked = False) Then
                Sheets("BoM").Range("L" & i).Locked = True
            End If
            'lerroko checkbox-a aktibatu
            checkBox_NoClick = "X"
            Sheets("BoM").Shapes(loturikoCheckBoxa(i)).OLEFormat.Object.Object.Value = True
            checkBox_NoClick = ""
        Next
        'Modo_seleccionado ipini modu orokorra jarrai dezan
        Sheets("BoM").Range("O" & Range("FilaInicioArticulosLR") & ":O" & Range("FilaFinArticulosLR")).Formula = Sheets("Data").Range("Modo_Seleccionado_Formula").Value
        Else 'False bada, lerro bakoitzeko
            For i = Range("FilaInicioArticulosLR") To Range("FilaFinArticulosLR")
            'Ipini duen balioari dagokion modua
            Sheets("BoM").Range("O" & i).Formula = Application.VLookup(Sheets("BoM").Range("O" & i).Value, Sheets("Data").Range("F56:H59"), 3, 0)
            'checkBox-a desaktibatu
            checkBox_NoClick = "X"
            Sheets("BoM").Shapes(loturikoCheckBoxa(i)).OLEFormat.Object.Object.Value = False
            checkBox_NoClick = ""
            'Modo zutabeko zelden babesa kendu
            If (Sheets("BoM").Range("O" & i).Locked = True) Then
                Sheets("BoM").Range("O" & i).Locked = False
            End If
            Next
        End If
    voyACambiarAnclajes = ""
    Else 'lerroko checkbox aldaketa
    If (voyACambiarAnclajes = "") Then
        'checkBox-a true bada, modo seleccionado eta babesu modo zelda
        If Sheets("BoM").Shapes(chName).OLEFormat.Object.Object.Value = True Then
            Sheets("BoM").Cells(Sheets("BoM").Shapes(chName).TopLeftCell.row, Sheets("BoM").Shapes(chName).TopLeftCell.column + 1).Formula = Sheets("Data").Range("Modo_Seleccionado_Formula").Value
            If Sheets("BoM").Cells(Sheets("BoM").Shapes(chName).TopLeftCell.row, Sheets("BoM").Shapes(chName).TopLeftCell.column + 1).Locked = False Then
            Sheets("BoM").Cells(Sheets("BoM").Shapes(chName).TopLeftCell.row, Sheets("BoM").Shapes(chName).TopLeftCell.column + 1).Locked = True
            End If
        Else 'False bada, modo seleccionadok duen balioaren modua ipini eta babesa kendu modo zeldari
            Sheets("BoM").Cells(Sheets("BoM").Shapes(chName).TopLeftCell.row, Sheets("BoM").Shapes(chName).TopLeftCell.column + 1).Formula = Application.VLookup(Sheets("Data").Range("G55").Value, Sheets("Data").Range("G56:H59"), 2, 0)
            If Sheets("BoM").Cells(Sheets("BoM").Shapes(chName).TopLeftCell.row, Sheets("BoM").Shapes(chName).TopLeftCell.column + 1).Locked = True Then
                Sheets("BoM").Cells(Sheets("BoM").Shapes(chName).TopLeftCell.row, Sheets("BoM").Shapes(chName).TopLeftCell.column + 1).Locked = False
            End If
        End If
    Else
    GoTo fin:
    End If
    End If
        ThisWorkbook.Worksheets("BoM").Protect AllowFormattingCells:=True, AllowFormattingColumns:=True, AllowFormattingRows:=True, AllowInsertingRows:=True, AllowDeletingRows:=True, UserInterfaceOnly:=True
    ThisWorkbook.Worksheets("BoM").EnableOutlining = True
    cambioModoPorCheckBox = ""
fin:
'Leheneratu balioak
Application.EnableEvents = True
Application.ScreenUpdating = True
End Sub

Function loturikoCheckBoxa(ByVal i As Integer) As String
    Dim balioa As Variant
    For Each obj In Sheets("BoM").OLEObjects
        On Error Resume Next
        balioa = Evaluate("=ROW(" & obj.LinkedCell & ")")
        If Not balioa Is Nothing Then
            If balioa(1) = i Then
                erantzuna = obj.Name
            End If
        End If
    Next
    loturikoCheckBoxa = erantzuna
End Function
