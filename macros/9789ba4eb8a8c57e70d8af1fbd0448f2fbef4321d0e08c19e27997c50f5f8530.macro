Function createTotals(ByVal printOrria As String, ByVal inicioP As Integer, ByVal non As Integer) As Integer
    Dim TotalsLabel, TotalsFormula, FormulaF, FormulaT, str, RangeNameValue As String
    Dim TotalLehena As Integer
    TotalLehena = non + 1
    TotalsLabel = "A"
    TotalsFormula = "G"
    For i = 1 To 10
        FormulaT = "=IFERROR(INDIRECT(CONCATENATE(""T_"",Pregunta_" & i & ")),"""")"
        FormulaF = "=IFERROR(Eval(INDIRECT(CONCATENATE(""F_"",Pregunta_" & i & "))),"""")"

        Sheets(printOrria).Range(TotalsLabel & TotalLehena + i).Formula = FormulaT
        Sheets(printOrria).Range(TotalsFormula & TotalLehena + i).Formula = FormulaF
        Sheets(printOrria).Range(TotalsLabel & TotalLehena + i).Value = Sheets(printOrria).Range(TotalsLabel & TotalLehena + i).Value
        Sheets(printOrria).Range(TotalsFormula & TotalLehena + i).Value = Sheets(printOrria).Range(TotalsFormula & TotalLehena + i).Value
      
        
    Next
    Sheets(printOrria).Range("A" & TotalLehena & ":G" & TotalLehena + 10).Font.Size = 8
    Sheets(printOrria).Range("A" & TotalLehena & ":G" & TotalLehena + 10).Font.Name = "Arial"
    
    'formatuak
    With Sheets(printOrria).Range("A" & TotalLehena & ":G" & TotalLehena).Borders(xlEdgeTop)
        .LineStyle = xlContinuous
        .Weight = xlThin
        .ColorIndex = 0
    End With
    With Sheets(printOrria).Range("A" & TotalLehena + 10 & ":G" & TotalLehena + 10).Borders(xlEdgeBottom)
        .LineStyle = xlContinuous
        .Weight = xlThin
        .ColorIndex = 0
    End With
    With Sheets(printOrria).Range("A" & TotalLehena & ":G" & TotalLehena + 10).Borders(xlInsideHorizontal)
        .LineStyle = xlDot
        .Weight = xlThin
        .ColorIndex = 0
    End With
    Sheets(printOrria).Range("F" & TotalLehena & ":G" & TotalLehena + 10).NumberFormat = "#,##0.00"
    Sheets(printOrria).Range("F" & TotalLehena & ":G" & TotalLehena + 10).Font.Bold = False
    createTotals = non + 12
End Function

Function CreateTotalsEspana(ByVal printOrria As String, ByVal non As Integer) As Integer
'izena
'kantitatea
'unitatea
'diasenObra
hasiera = non
i = non
    Do While i < 1000
        'Sartu lerroa
        If Sheets(printOrria).Range("H" & i).Value = "ZH" Then
            Sheets(printOrria).Rows(non).Insert
            i = i + 1
            Sheets(printOrria).Range("A" & non).Value = Sheets(printOrria).Range("A" & i).Value
            Sheets(printOrria).Range("F" & non).Value = separadorCodNom(Sheets(printOrria).Range("D" & i).Value, "=", False) & " d|fffd|as"
            Sheets(printOrria).Range("C" & non).Value = separadorCodNom(Sheets(printOrria).Range("F" & i).Value, ":", False)
            
        End If
        If Sheets(printOrria).Range("H" & i).Value = "S" Then
            Sheets(printOrria).Range("E" & non).Value = Sheets(printOrria).Range("G" & i).Value
            Sheets(printOrria).Range("G" & non).Value = Sheets(printOrria).Range("E" & non).Value * separadorCodNom(Sheets(printOrria).Range("F" & non).Value, " ", True)
            Sheets(printOrria).Range("E" & i).Copy
            Sheets(printOrria).Range("A" & non & ":G" & non).PasteSpecial xlFormats
            Sheets(printOrria).Range("A" & non & ":A" & non).HorizontalAlignment = xlLeft
            Sheets(printOrria).Range("B" & non & ":G" & non).HorizontalAlignment = xlRight
            non = non + 1
        ElseIf Sheets(printOrria).Range("H" & i).Value = "V" Then
            'Sheets(printOrria).Range("E" & non).Value = Sheets(printOrria).Range("G" & i).Value
            Sheets(printOrria).Range("G" & non).Value = Sheets(printOrria).Range("G" & i).Value
            Sheets(printOrria).Range("F" & non).Value = "" 'kendu "dias"
            Sheets(printOrria).Range("E" & i).Copy
            Sheets(printOrria).Range("A" & non & ":G" & non).PasteSpecial xlFormats
            Sheets(printOrria).Range("A" & non & ":A" & non).HorizontalAlignment = xlLeft
            Sheets(printOrria).Range("B" & non & ":G" & non).HorizontalAlignment = xlRight
            non = non + 1
        End If
         i = i + 1
    Loop
    'Totala
    Sheets(printOrria).Range("F" & non).Value = "Total:"
    Sheets(printOrria).Range("G" & non).Formula = "=SUM(G" & hasiera & ":G" & non - 1 & ")"
    Sheets(printOrria).Range("A" & non - 1 & ":G" & non - 1).Copy
    Sheets(printOrria).Range("F" & non & ":G" & non).PasteSpecial xlFormats
