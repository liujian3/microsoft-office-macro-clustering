Sub cambioEnAlgunaHoja(ByVal sh As Object, ByVal Target As Range)
If (Sheets("Data").Range("CargaInicialFinalizada").Value <> "X") Then
        Application.EnableEvents = False
    End If
    If ActiveWorkbook.ActiveSheet.Name = "BoM" And Sheets("Data").Range("CargaInicialFinalizada") = "X" And voyACambiarCondicion = "" Then
        Dim filaInicio As Integer
        filaInicio = Sheets("Data").Range("FilaInicioArticulosLR")
        Dim filaFin As Integer
        filaFin = Sheets("Data").Range("FilaFinArticulosLR")
        If Target.column = Sheets("Data").Range("NumColumnaCondicion") And Target.row >= filaInicio And Target.row <= filaFin Then
            Call CambiarUnaCondicion(Target.row)
        End If
    End If
    If ActiveWorkbook.ActiveSheet.Name = "BoM" And Sheets("Data").Range("CargaInicialFinalizada") = "X" And voyACambiarModo = "" And cambioModoPorCheckBox = "" Then
        Dim filaInicioModo As Integer
        filaInicioModo = Sheets("Data").Range("FilaInicioArticulosLR")
        Dim filaFinModo As Integer
        filaFinModo = Sheets("Data").Range("FilaFinArticulosLR")
        If Target.column = Sheets("Data").Range("NumColumnaModo") And Target.row >= filaInicioModo And Target.row <= filaFinModo Then
            Call CambiarUnModo(Target.row)
        End If
    End If
    If ActiveWorkbook.ActiveSheet.Name = "BoM" And Sheets("Data").Range("CargaInicialFinalizada") = "X" And Target.column = Sheets("Data").Range("NumColumnaIDSeleccionado") And Target.row = Sheets("Data").Range("FilaInicioArticulosLR") - 2 And voyACambiarTipoLista = "" Then
        Call CambiarTipoLista
    End If
    If ActiveWorkbook.ActiveSheet.Name = "BoM" And Sheets("Data").Range("CargaInicialFinalizada") = "X" And ((Target.column = 3 Or (Target.column >= ConvertToNumber(Sheets("Data").Range("NumColumInicioListaLR")) And Target.column <= ConvertToNumber(Sheets("Data").Range("NumColumFinListaLR")) + 2)) Or Target.column = Sheets("Data").Range("NumColumnaIDSeleccionado")) And Sheets("Data").Range("tipo_estudio").Value = "estudioUSA" And actualizandoPalet <> "X" Then
        kalkulatuPaletE1
        kalkulatuRack
    End If
    If ActiveWorkbook.ActiveSheet.Name = "BoM" And Sheets("Data").Range("CargaInicialFinalizada") = "X" And ((Target.column >= ConvertToNumber(Sheets("Data").Range("NumColumInicioListaLR")) And Target.column <= ConvertToNumber(Sheets("Data").Range("NumColumFinListaLR"))) Or Target.column = Sheets("Data").Range("NumColumnaIDSeleccionado") Or Target.column = 3) And Sheets("Data").Range("tipo_estudio").Value = "estudioUSA" And actualizandoPaquetesUSA <> "X" Then
        kalkulatuExtra
    End If
    If (Target.Address = Sheets("Data").Range("id_inicializar").Address And Sheets("Data").Range("id_inicializar").Value = "X") Then
        inicializar
    End If
    If ActiveWorkbook.ActiveSheet.Name = "BoM" And Sheets("Data").Range("CargaInicialFinalizada") = "X" And voyACambiarSuministro = "" And Sheets("Data").Range("ID_Grafsystem").Value = "X" Then
        Dim filaInicioSum As Integer
        filaInicioSum = Sheets("Data").Range("FilaInicioArticulosLR")
        Dim filaFinSum As Integer
        filaFinSum = Sheets("Data").Range("FilaFinArticulosLR")
        If Target.column = (Sheets("Data").Range("NumColumnaModo")) And Target.row >= filaInicioSum And Target.row <= filaFinSum And (Sheets("Data").Range("ID_Grafsystem").Value = "X") Then
            Call CambiarUnModoSumGraf(Target.row)
        End If
    End If
    If ((sh.Name = "BoM" And Target.column >= ConvertToNumber(Sheets("Data").Range("NumColumInicioListaLR")) And Target.column <= ConvertToNumber(Sheets("Data").Range("NumColumFinListaLR")) And Target.row = Sheets("Data").Range("FilaUnidadLista").Value) Or (Target.column = (ConvertToNumber(Sheets("Data").Range("NumColumInicioListaLR").Value) - 1) And Target.row = Sheets("Data").Range("FilaBaremoPrecioOfertado").Value - 1)) Then
       If (Sheets("Data").Range("tipo_estudio").Value = "estudioEspana") Then
            formatosBaremoTotalesEspana Target.row, Target.column
       End If
    formatoPrecioAlqDia Target.row, Target.column
    End If
    If (sh.Name = "BoM" And Sheets("BoM").OLEObjects("combo_stock").Object.Value = "Calculated" And Target.row = Sheets("Data").Range("FilaInicioArticulosLR") - 2 And Target.column >= ConvertToNumber(Sheets("Data").Range("colPrimStock")) And Target.column <= ConvertToNumber(Sheets("Data").Range("colUltStock"))) Then
        calculatedZutabeakKoloreztatu Target.column
    End If
    If (sh.Name = "BoM" And Target.row = Sheets("Data").Range("FilaInicioArticulosLR") - 3 And Target.column >= ConvertToNumber(Sheets("Data").Range("colPrimStock")) And Target.column <= ConvertToNumber(Sheets("Data").Range("colUltStock")) And Sheets("Data").Range("TieneStock") = "X") Then
        beteComboStock
    End If
    If (sh.Name = "BoM" And Target.column = 3 And Target.row >= Sheets("Data").Range("FilaInicioArticulosLR") And Target.row <= Sheets("Data").Range("FilaFinArticulosLR") And Worksheets("BoM").OLEObjects("cmd_modoXML").Object.Caption = "Online" And Sheets("Data").Range("CargaInicialFinalizada") = "X") Then
        lehena = Target.Rows(1).row
        azkena = Target.Rows.Count + lehena - 1
        If azkena <= Sheets("Data").Range("FilaFinArticulosLR") Then
            For i = lehena To azkena
                rellenaDatosXML (i)
                familiaEzezagunaLerroa (i)
                If Sheets("Data").Range("TieneStock") = "X" Then
                    tagUpdateNeed (Target.row)
                End If
            Next
        End If
    End If
    If (sh.Name = "BoM" And Target.column = 3 And Target.row >= Sheets("Data").Range("FilaInicioArticulosLR") And Target.row <= Sheets("Data").Range("FilaFinArticulosLR") And Sheets("Data").Range("CargaInicialFinalizada") = "X") Then
        lehena = Target.Rows(1).row
        azkena = Target.Rows.Count + lehena - 1
        If azkena <= Sheets("Data").Range("FilaFinArticulosLR") Then
            For i = lehena To azkena
                PROzesatuBakarra (i)
            Next
        End If
    End If
End Sub


