Sub EguneratuArgentina(ByVal hoja As String)
 'Lortu BOM orriko beharrezko datuak
    Dim zutHasiBom, zutAmaBom As String
    Dim lerroHasiBom, lerroAmaBom, lerroBider As Integer
    lerroHasiBom = Sheets("Data").Range("FilaInicioArticulosLR").Value
    lerroAmaBom = Sheets("Data").Range("FilaFinArticulosLR").Value
    zutHasiBom = Sheets("Data").Range("NumColumInicioListaLR").Value
    zutAmaBom = Sheets("Data").Range("NumColumFinListaLR").Value
    lerroBider = Sheets("Data").Range("FilaInicioArticulosLR").Value - 2
    
    BorrarDatos hoja
    
    'goiburua
    
    Dim cliente, codCliente, nomCliente, comercial, codComercial, nomComercial As String
    cliente = Application.Trim(Sheets("BoM").Range("ID_Data_Cliente").Value)
    codCliente = Left(cliente, Application.IfError(Application.Find("//", cliente), 1) - 1)
    nomCliente = Right(cliente, Len(cliente) - (Application.IfError(Application.Find("//", cliente), Len(cliente) - 1) + 1))
    comercial = Application.Trim(Sheets("BoM").Range("ID_Data_CodNomComercial").Value)
    codComercial = Left(comercial, Application.IfError(Application.Find("//", comercial), 1) - 1)
    nomComercial = Right(comercial, Len(comercial) - (Application.IfError(Application.Find("//", comercial), Len(comercial) - 1) + 1))
    If nomCliente = "" And codCliente = "" Then
        nomCliente = cliente
        codCliente = cliente
    End If
    If nomComercial = "" And codComercial = "" Then
        nomComercial = comercial
        codComercial = comercial
    End If
    Sheets(hoja).Range("B6").Value = codCliente 'nombre cliente
    Sheets(hoja).Range("D6").Value = nomCliente 'nombre cliente
    Sheets(hoja).Range("B7").Value = codComercial 'Cod Comercial
    Sheets(hoja).Range("D7").Value = nomComercial 'Nom Comercial
    Sheets(hoja).Range("B9").Value = Sheets("BoM").Range("ID_DATA_CodContrato").Value 'contrato
    
    Dim ofertaSolicitud, oferta, solicitud As String
    Dim pos As Integer
    ofertaSolicitud = Application.Trim(Sheets("BoM").Range("ID_Data_OfertaSolicitud").Value)
    pos = Application.IfError(Application.Find("//", ofertaSolicitud), -1)
    If (pos > -1) Then
        oferta = ""
        solicitud = Right(ofertaSolicitud, Len(ofertaSolicitud) - (Application.IfError(Application.Find("//", ofertaSolicitud), Len(ofertaSolicitud) - 1) + 1))
    Else
        oferta = Application.Trim(Sheets("BoM").Range("ID_Data_OfertaSolicitud").Value)
    End If
    If oferta <> "" Then
        Sheets(hoja).Range("B8").Value = oferta 'ofertaSolicitud
    Else
        Sheets(hoja).Range("B8").Value = ""
    End If
    
    
    Dim filaServicio As Integer
    Dim codigoBom, MaterialBom, CondicionBom, CantidadesBom, PreciosBom, FechaSumBom, columnaCheck As String
    
    codigoBom = "C" 'columna
    MaterialBom = "D" ' columna
    CondicionBom = "E" ' columna
    CantidadesBom = "P" 'columna
    PreciosVentaBoM = "I" 'columna
    PreciosAlquilerBom = "G" 'columna
    FechaSumBom = "J6" 'celda
    columnaCheck = ConvertToLetter(ConvertToNumber(Sheets("Data").Range("NumColumInicioListaLR").Value) - 2)
    filaServicio = Application.IfError(Application.Match("Tag_Servicio", Sheets("BoM").Range(columnaCheck & ":" & columnaCheck), 0), 0)
    
    'Baan orriko beharrezko aldagaiak definitu
    Dim lerroHasiBaan, unekoLerroa, kantitatea As Integer
    Dim CodigoBaan, DescripcionBaan, MaterialBaan, CondicionBaan, CalidadBaan, CantidadesBaan, PreciosBaan, ServicioBaan, FechaSumBaan As String
    
    CodigoBaan = "A" 'columna
    CondicionBaan = "C" 'columna
    DescripcionBaan = "B" 'columna
    CalidadBaan = "C" 'columna
    CantidadesBaan = "G" 'columna
    PreciosVentaBaan = "D" 'columna
    PreciosAlquilerBaan = "E" 'columna
    ServicioBaan = "F" 'columna
    FechaSumBaan = "G13" 'celda
    
    'eguneratu datu denak
    lerroHasBaan = 14 'lerroa
    unekoLerroa = lerroHasBaan
        For i = lerroHasiBom To lerroAmaBom
        kantitatea = 0
        For j = ConvertToNumber(zutHasiBom) To ConvertToNumber(zutAmaBom)
            unekoZut = ConvertToLetter(j)
            biderkatzailea = Sheets("BoM").Range(unekoZut & lerroBider).Value
            kontrolBalioa = Sheets("BoM").Range(unekoZut & i).Value * biderkatzailea
            unekoBalioa = Sheets("BoM").Range(unekoZut & i).Value
            portzentaia = Sheets("BoM").Cells(Sheets("Data").Range("FilaFinArticulosLR").Value + 26, j).Value
            If Sheets("BoM").Range(CondicionBom & i).Formula = "=A1Alquiler" Or Sheets("BoM").Range(CondicionBom & i).Formula = "=A4Montaje" Then
                prezioaArg = prezioaKalkulatuArg(Sheets("BoM").Range(PreciosAlquilerBom & i).Value, portzentaia)
            Else
                prezioArg = Sheets("BoM").Range(PreciosVentaBaan & i).Value
            End If
            If kontrolBalioa <> 0 Then
                zerbitzua = Sheets("BoM").Range(unekoZut & filaServicio).Value
                kodea = Sheets("BoM").Range(codigoBom & i).Value
                deskripzioa = Sheets("BoM").Range(MaterialBom & i).Value
                If (CargarDatosBaan.chk_Venta) Then
                    PrezioSalm = Sheets("BoM").Range(PreciosVentaBoM & i).Value 'Precios Venta
                Else
                    PrezioSalm = 0
                End If
                If (CargarDatosBaan.chk_Alquiler) Then
                    PrezioAlok = prezioaKalkulatuArg(Sheets("BoM").Range(PreciosAlquilerBom & i).Value, portzentaia) 'Precios Alquiler
                Else
                    PrezioAlok = 0
                End If
                If ((Sheets("BoM").Range(CondicionBom & i).Formula = "=A1Alquiler" And hoja = "Baan_Rental") Or (Sheets("BoM").Range(CondicionBom & i).Formula = "=A4Montaje" And hoja = "Baan_Rental") Or (Sheets("BoM").Range(CondicionBom & i).Formula = "=A2Venta" And hoja = "Baan_Sale") Or (Sheets("BoM").Range(CondicionBom & i).Formula = "=A3Especial" And hoja = "Baan_Sale") Or (Sheets("BoM").Range(CondicionBom & i).Formula = "=A5VentaTongada" And hoja = "Baan_Sale")) Then
                    'badago Baan orrian?
                    badago = bilatuLerroa(hoja, kodea, PrezioSalm, PrezioAlok, zerbitzua)
                    If badago > 0 Then
                        baanzerbitzua = Sheets(hoja).Range(ServicioBaan & badago).Value
                        If zerbitzua = baanzerbitzua Then 'eguneratu kantitatea
                            If (CargarDatosBaan.chk_Alquiler) Then
                                If hoja = "Baan_Rental" Then
                                    If prezioaArg = Sheets(hoja).Range(PreciosAlquilerBaan & badago).Value Then
                                        idaztekoBal = Sheets(hoja).Range(CantidadesBaan & badago).Value + (unekoBalioa * biderkatzailea)
                                        Sheets(hoja).Range(CantidadesBaan & badago).Value = idaztekoBal
                                    Else 'prezio ezberdina duenez, sartu berria
                                        GoTo berria
                                    End If
                                Else
                                    Sheets(hoja).Range(CantidadesBaan & badago).Value = 0
                                    idaztekoBal = Sheets(hoja).Range(CantidadesBaan & badago).Value + (unekoBalioa * biderkatzailea)
                                    Sheets(hoja).Range(CantidadesBaan & badago).Value = idaztekoBal
                                End If
                          Else
                            Sheets(hoja).Range(CantidadesBaan & badago).Value = Sheets(hoja).Range(CantidadesBaan & badago).Value + (unekoBalioa * biderkatzailea)
                        End If
                    Else 'sartu berria
berria:
                            Sheets(hoja).Range(CodigoBaan & unekoLerroa).Value = kodea 'Codigo
                            Sheets(hoja).Range(DescripcionBaan & unekoLerroa).Value = deskripzioa 'Descripcion
                            Sheets(hoja).Range(CantidadesBaan & unekoLerroa).Value = unekoBalioa * biderkatzailea  'Cantidades
                            Sheets(hoja).Range(FechaSumBaan).Value = Sheets("BoM").Range("ID_Data_FechaINSum").Value 'Fecha Suministro
                            If (CargarDatosBaan.chk_Venta) Then
                                Sheets(hoja).Range(PreciosVentaBaan & unekoLerroa).Value = Sheets("BoM").Range(PreciosVentaBoM & i).Value 'Precios Venta
                            Else
                                Sheets(hoja).Range(PreciosVentaBaan & unekoLerroa).Value = 0
                            End If
                            If hoja = "Baan_Rental" Then 'Rental bada idatzi alkilerreko prezioak
                                If (CargarDatosBaan.chk_Alquiler) Then
                                    Sheets(hoja).Range(PreciosAlquilerBaan & unekoLerroa).Value = prezioaArg 'Precios Alquiler
                                    Sheets(hoja).Range("BQ" & unekoLerroa).Value = portzentaia
                                Else
                                    Sheets(hoja).Range(PreciosAlquilerBaan & unekoLerroa).Value = 0
                                End If
                            Else
                                Sheets(hoja).Range(PreciosAlquilerBaan & unekoLerroa).Value = 0
                            End If
                            Sheets(hoja).Range(ServicioBaan & unekoLerroa).Value = zerbitzua
                            unekoLerroa = unekoLerroa + 1
                        End If
                    Else
                            Sheets(hoja).Range(CodigoBaan & unekoLerroa).Value = kodea 'Codigo
                            Sheets(hoja).Range(DescripcionBaan & unekoLerroa).Value = deskripzioa 'Descripcion
                            Sheets(hoja).Range(CantidadesBaan & unekoLerroa).Value = unekoBalioa * biderkatzailea  'Cantidades
                            Sheets(hoja).Range(FechaSumBaan).Value = Sheets("BoM").Range("ID_Data_FechaINSum").Value 'Fecha Suministro
                            If (CargarDatosBaan.chk_Venta) Then
                                Sheets(hoja).Range(PreciosVentaBaan & unekoLerroa).Value = Sheets("BoM").Range(PreciosVentaBoM & i).Value 'Precios Venta
                            Else
                                Sheets(hoja).Range(PreciosVentaBaan & unekoLerroa).Value = 0
                            End If
                            If hoja = "Baan_Rental" Then 'Rental bada idatzi alkilerreko prezioak
                                If (CargarDatosBaan.chk_Alquiler) Then
                                    Sheets(hoja).Range(PreciosAlquilerBaan & unekoLerroa).Value = prezioaArg  'Precios Alquiler
                                    Sheets(hoja).Range("BQ" & unekoLerroa).Value = portzentaia
                                Else
                                    Sheets(hoja).Range(PreciosAlquilerBaan & unekoLerroa).Value = 0
                                End If
                            Else
                                Sheets(hoja).Range(PreciosAlquilerBaan & unekoLerroa).Value = 0
                            End If
                            Sheets(hoja).Range(ServicioBaan & unekoLerroa).Value = zerbitzua
                            unekoLerroa = unekoLerroa + 1
                    End If
                End If
            End If
        Next
    Next
    TaulaKoloreak (hoja)
End Sub

Function prezioaKalkulatuArg(ByVal prezioa As Double, ByVal portzentaia As Double) As Double
    If portzentaia <> 0 Then
        prezioaTotala = prezioa + (prezioa * portzentaia)
    Else
        prezioaTotala = prezioa
    End If
    prezioaKalkulatuArg = prezioaTotala / Sheets("Baan_Rental").Range("ID_Baan_Cotizacion").Value / Sheets("Baan_Rental").Range("ID_Baan_Dias").Value
End Function

Sub eliminaCotizacionArgentina()
    egoera = Application.ScreenUpdating
    Application.ScreenUpdating = False
    egoera2 = Application.EnableEvents
    Application.EnableEvents = False
    Sheets("Baan_Rental").Range("O6:O7").Value = ""
    Sheets("Baan_Rental").Range("Q6:Q7").Formula = ""
    Sheets("Baan_Sale").Range("O6:O7").Value = ""
    Sheets("Baan_Sale").Range("Q6:Q7").Formula = ""
    Application.ScreenUpdating = egoera
    Application.EnableEvents = egoera2
End Sub

