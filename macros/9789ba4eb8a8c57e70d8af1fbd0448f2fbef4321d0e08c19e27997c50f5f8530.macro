                    Sheets("BoM").Range(Col & linea).Font.Color = vbRed
                Else
                    If kont = 1 Then
                        totalTemp = totalTemp + kantitateak
                    Else
                        totalTemp = totalTemp + kantitateak(Application.Match(balioa, zerrenda, False))
                    End If
                End If
            End If
        Next
        total = total + (totalTemp / zatitzailea)
