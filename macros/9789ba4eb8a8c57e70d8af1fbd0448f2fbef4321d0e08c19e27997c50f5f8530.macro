Sub eliminaColumna()
    Sheets("BoM").Range(txt_Desde.Value & ":" & txt_Desde.Value).EntireColumn.Delete
    colInicio = Sheets("Data").Range("NumColumInicioListaLR").Value
    colelim = ConvertToNumber(txt_Desde.Value)
    numCol = colelim - ConvertToNumber(colInicio) + 1
    'eliminalinea = colelim - ConvertToNumber(colinicio) + 16 + 1 '16.lerroan hasten dira laburpenak eta lerro bateko gapa
    
    ''
    posLineaCol = getLineaResumen(numCol)
    cantLineaCol = getLineasEliminarResumen(numCol)
    i = 1
    Do While i <= cantLineaCol
        Sheets("BoM").Rows(posLineaCol).EntireRow.Delete
        i = i + 1
    Loop
    ''
    metaTablaResumenLista
    'eguneratu azken stock zutabea stock-a baldin badu
    Sheets("Data").Range("UltColUsado_Stock").Value = ConvertToLetter(ConvertToNumber(Sheets("Data").Range("UltColUsado_Stock").Value) - 1)
End Sub

'-------------------
'SORTU ZUTABEA
'-------------------
Sub creaColumna()
    Dim stock, sobra, falta, colInicio, sarpuntuaLetra, norainoletra As String
    Dim inicio, fin, zenbat As Integer
    Dim rConstants As Range
    colInicio = Sheets("Data").Range("NumColumInicioListaLR").Value
    colFin = Sheets("Data").Range("NumColumFinListaLR").Value
    sarpuntua = ConvertToNumber(colFin)
    sarpuntuaLetra = colFin
    norainoletra = ConvertToLetter(sarpuntua)
    inicio = ThisWorkbook.Sheets("Data").Range("FilaInicioArticulosLR").Value
    fin = ThisWorkbook.Sheets("Data").Range("FilaFinArticulosLR").Value
    Sheets("BoM").Columns(sarpuntua).Select
    ActiveCell.EntireColumn.Offset(, 1).Resize(, 1).Insert
    Sheets("BoM").Range(norainoletra & inicio - 3 & ":" & norainoletra & fin + 1000).Select
    Selection.Copy
    kopiama = ConvertToLetter(ConvertToNumber(norainoletra) + 1)
    Sheets("BoM").Range(kopiama & inicio - 3 & ":" & kopiama & fin + 1000).Select
    ActiveSheet.Paste
    Sheets("BoM").Range(kopiama & inicio - 3 & ":" & kopiama & inicio - 3).Locked = False
    Set rConstants = Sheets("BoM").Range(kopiama & inicio - 3 & ":" & kopiama & fin + 1000).SpecialCells(xlCellTypeConstants)
    rConstants.ClearContents
    rConstants.ClearComments
    Sheets("BoM").Range(kopiama & inicio - 2 & ":" & kopiama & inicio - 2) = 1  'biderkatzailea
    Application.CutCopyMode = False
    ''
    Sheets("BoM").Range(inicio - 6 & ":" & inicio - 6).Select
    ActiveCell.EntireRow.Offset(1).Resize(1).Insert
    inicio = ThisWorkbook.Sheets("Data").Range("FilaInicioArticulosLR").Value
    colFin = Sheets("Data").Range("NumColumFinListaLR").Value
    Sheets("BoM").Range(inicio - 7 & ":" & inicio - 7).Select
    Selection.Copy
    Sheets("BoM").Range(inicio - 6 & ":" & inicio - 6).Select
    ActiveSheet.Paste
    Set rConstants = Sheets("BoM").Range(inicio - 6 & ":" & inicio - 6).SpecialCells(xlCellTypeConstants)
    rConstants.ClearContents
    rConstants.ClearComments
    Sheets("BoM").Range(colFin & inicio - 1).Locked = False
    Sheets("BoM").Range(colFin & inicio - 1).Value = "CN" & Sheets("Data").Range("NumColumCreado").Value
    Sheets("BoM").Range("C" & inicio - 6).Value = "=" & colFin & inicio - 1
    Sheets("BoM").Range("D" & inicio - 6).Value = "NEW UNIT " & Sheets("Data").Range("NumColumCreado").Value
    Sheets("BoM").Range("E" & inicio - 6).Value = 1
    Sheets("BoM").Range(colFin & inicio - 3).Locked = False
    Sheets("BoM").Range(colFin & inicio - 3).AddComment "Columna editable / Editable Column"
    Sheets("BoM").Range(colFin & inicio - 3).Comment.Visible = False
    Sheets("BoM").Range(colFin & inicio - 3).Value = "NEW LIST " & Sheets("Data").Range("NumColumCreado").Value
    Sheets("BoM").Range("G" & inicio - 6).Value = "=" & colFin & inicio - 3
    Sheets("BoM").Range("M" & inicio - 6).Value = 1
    
    Sheets("Data").Range("NumColumCreado").Value = Sheets("Data").Range("NumColumCreado").Value + 1
    Sheets("BoM").Range("A1").Select
    ''
    metaTablaResumenLista
    'eguneratu azken stock zutabea stock-a baldin badu
    Sheets("Data").Range("UltColUsado_Stock").Value = ConvertToLetter(ConvertToNumber(Sheets("Data").Range("UltColUsado_Stock").Value) + 1)
End Sub


