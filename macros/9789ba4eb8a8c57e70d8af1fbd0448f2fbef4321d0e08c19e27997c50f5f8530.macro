    'Baremo
    LehenZut = Sheets("Data").Range("NumColumInicioListaLR").Value
    checkZut = ConvertToLetter(ConvertToNumber(LehenZut) - 1)
    non = inicioP
    'idatzi goiburua
    Select Case modosuministro
        Case "=A2Venta"
            modosumTexto = "EN VENTA"
        Case "=A3Especial"
            modosumTexto = "ESPECIALES"
        Case "=A5VentaTongada"
            modosumTexto = "VENTA por TONGADA"
        Case Else
            modosumTexto = ""
        End Select
    
    cantModoSum = 0
    For i = inicio To fin
        If modosuministro = Sheets(iturburua).Range("E" & i).Formula Then
            cantModoSum = Sheets(iturburua).Range("P" & i).Value
        End If
    Next
    If cantModoSum = 0 Then
        GoTo fin
    End If
    IdatziZutabeTituluaVenta iturburua, printOrria, unekoZut, inicio, non, modosumTexto
    non = non + 3
    IdatziZutabeHeaderVenta iturburua, printOrria, unekoZut, inicio, non
    non = non + 1
    zerrenHasi = non
    For i = inicio To fin
        'hartu lerro bakoitzeko datuak
        'kodea
        kodea = Sheets(iturburua).Range("C" & i).Value
        'deskripzioa
        deskripzioa = Sheets(iturburua).Range("D" & i).Value
        'modoSum
        modoSum = Sheets(iturburua).Range("E" & i).Formula
