Public Sub checkBoxGen()
    If (Sheets("Data").Range("id_Grafsystem") <> "X") Then
        egoera = Application.ScreenUpdating
        Application.ScreenUpdating = False
        egoera2 = Application.EnableEvents
        Application.EnableEvents = False
        If (Sheets("Data").Range("CheckBoxGenerados") = "X") Then Exit Sub
        Sheets("BoM").Unprotect
        cambioModoPorCheckBox = "X"
        Dim code As String
        Dim difCHToLinea, inicio As Integer
        inicio = Sheets("Data").Range("FilaInicioArticulosLR")
        difCHToLinea = Range("FilaInicioArticulosLR") - 12
        'sortu CheckBox-ak
        For i = Range("FilaInicioArticulosLR") To Range("FilaFinArticulosLR")
            If Application.Version = "12.0" Then
                Application.ScreenUpdating = False
                Set oleObj = ActiveSheet.OLEObjects.Add(ClassType:="Forms.CheckBox.1", Link:=False, _
                DisplayAsIcon:=False, Left:=Range("N" & i).Left + 11, _
                Top:=Range("N" & i).Top + 5#, Width:=9.75, Height:=11.25)
            Else
                Application.ScreenUpdating = False
                Set oleObj = ActiveSheet.OLEObjects.Add(ClassType:="Forms.CheckBox.1", Link:=False, _
                DisplayAsIcon:=False, Left:=Range("N" & i).Left + 11, _
                Top:=Range("N" & i).Top + 2.5, Width:=9.75, Height:=11.25)
            End If
            oleObj.LinkedCell = "N" & i
            If (Sheets("BoM").Range("O" & i).Formula = "=Modo_Seleccionado") Then
                With oleObj.Object
                    .Caption = ""
