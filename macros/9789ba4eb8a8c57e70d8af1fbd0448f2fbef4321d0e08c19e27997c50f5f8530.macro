Private Sub cmd_OK_Click()
    CargarDatosBaan.Hide
    Modo = Sheets("BoM").Range("ID_Seleccionado").Formula
     If (Modo <> "=ID_SumaTotalColumnas" And Sheets("Data").Range("id_Grafsystem").Value <> "X" And Sheets("Data").Range("tipo_estudio").Value <> "estudioEspana") Then
        MsgBox "La operaci|fffd|n no es suma, no se pueden importar datos"
        GoTo fin
