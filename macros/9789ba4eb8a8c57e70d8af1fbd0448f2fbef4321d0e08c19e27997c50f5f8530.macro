Function fillBodyEspana(ByVal iturburua As String, ByVal printOrria As String, ByVal inicio As Integer, ByVal fin As Integer, ByVal inicioC As String, ByVal finC As String, ByVal inicioP As Integer) As Integer
    'Lortu datuak
    'Baremo
    LehenZut = Sheets("Data").Range("NumColumInicioListaLR").Value
    checkZut = ConvertToLetter(ConvertToNumber(LehenZut) - 1)
    If (Sheets("Data").Range("tipo_estudio").Value = "estudioEspana") Then
        BaremoLerro = Sheets("Data").Range("FilaBaremoPrecioOfertado").Value
        DiasEnObraLerro = BaremoLerro + 1
        If (Application.IfError(Application.Match(ThisWorkbook.Names("ID_Baremo").RefersToRange(1, 1), Sheets(iturburua).Range(checkZut & ":" & checkZut), 0), 0) <> BaremoLerro) Then
            BaremoLerro = BaremoLerro - 1
        End If
    Else
        BaremoLerro = Application.IfError(Application.Match(ThisWorkbook.Names("ID_Porcentaje").RefersToRange(1, 1), Sheets(iturburua).Range(checkZut & ":" & checkZut), 0), 0)
    End If
    'aldatu letra tamaina goiburua ez den beste guztiarentzat
    Sheets(printOrria).Range("A" & inicioP & ":G500").Font.Size = 8
    Sheets(printOrria).Range("A" & inicioP & ":G500").Font.Bold = True
    'zutabeka kopiatu
    non = inicioP
    inicioCInt = ConvertToNumber(inicioC)
    finCInt = ConvertToNumber(finC)
    For j = inicioCInt To finCInt
        'kantitatea eta unitatea
        unekoZut = ConvertToLetter(j)
        'biderkatzailea
        biderkatzailea = Sheets(iturburua).Range(unekoZut & inicio - 2)
        danak = 0 'kontrolatu kantitatea, 0 bada zutabeko, ez idatzi ezer
        For z = inicio To fin
            If (Sheets(iturburua).Range("E" & z).Formula = "=A1Alquiler" Or Sheets(iturburua).Range("E" & z).Formula = "=A4Montaje") Then
                danak = danak + Sheets(iturburua).Range(unekoZut & z).Value * biderkatzailea
            End If
        Next
        If danak <> 0 Then
            unitatea = Sheets(iturburua).Range(unekoZut & Sheets("Data").Range("FilaUnidadLista"))
            kantitatea = Sheets(iturburua).Range(unekoZut & Sheets("Data").Range("FilaUnidadLista") + 1)
            'baremoa
            baremoa = Sheets(iturburua).Range(unekoZut & BaremoLerro)
            kantBider = kantitatea * biderkatzailea
            'idatzi goiburua
            IdatziZutabeTitulua iturburua, printOrria, unekoZut, inicio, non, kantBider, unitatea, biderkatzailea, CInt(Sheets("BoM").Range(unekoZut & DiasEnObraLerro).Value)
            non = non + 3
            IdatziZutabeHeader iturburua, printOrria, unekoZut, inicio, non, kantBider, unitatea
            non = non + 1
            zerrenHasi = non
            For i = inicio To fin
                'idatziTaula baremoa kontutan hartuz
                'hartu lerro bakoitzeko datuak
                'kodea
                kodea = Sheets(iturburua).Range("C" & i).Value
                'deskripzioa
                deskripzioa = Sheets(iturburua).Range("D" & i).Value
                'modoSum
                modoSum = Sheets(iturburua).Range("E" & i).Formula
                'pisua
                pisua = Sheets(iturburua).Range("L" & i).Value
                'area
                area = Sheets(iturburua).Range("M" & i).Value
                 'prezioa
                prezioa = Sheets(iturburua).Range("K" & i).Value
                'kantitatea
                kopurua = Sheets(iturburua).Range(unekoZut & i).Value * biderkatzailea
               
                'alk bada kalkulatu baremoaz prezioa
                If (modoSum = "=A1Alquiler" Or modoSum = "=A4Montaje") Then
    
                     If (baremoa <> 0 Or baremoa <> "") Then
                        If (Sheets("Data").Range("tipo_estudio").Value = "estudioEspana") Then
                            If baremoa = "" Then
                                prezioEgun = 0
                            Else
                                prezioEgun = prezioa * baremoa / 1000
                            End If
                        Else
                            prezioEgun = prezioa - (prezioa * baremoa)
                            
                        End If
                    Else
                        prezioEgun = 0
                    End If
    
                If kopurua > 0 Then
                    'idatzi datuak lerrorako
                    Sheets(printOrria).Range("A" & non).HorizontalAlignment = xlLeft
                    Sheets(printOrria).Range("A" & non) = kodea
                    Sheets(printOrria).Range("B" & non) = deskripzioa
                    Sheets(printOrria).Range("C" & non) = prezioa
                    Sheets(printOrria).Range("C" & non).NumberFormat = "#,##0.00 |fffd|"
                    Sheets(printOrria).Range("D" & non) = prezioEgun
                    Sheets(printOrria).Range("D" & non).NumberFormat = "#,##0.0000 |fffd|"
                    Sheets(printOrria).Range("E" & non) = pisua
                    Sheets(printOrria).Range("F" & non) = area
                    Sheets(printOrria).Range("G" & non) = kopurua
                
                    non = non + 1
                End If
                End If
            Next
            zerrenAma = non - 1
            idatziSubtotala iturburua, printOrria, non + 1, zerrenHasi, zerrenAma, kantBider, unitatea, unekoZut, inicio
            non = non + 5
        End If
    Next
    non = non - 4 'azken zerrenda eta gero ez utzi 5 zeldako saltoa.
    'autofit
    Sheets(printOrria).Range("A1:ZZ999999").Columns.AutoFit
    fillBodyEspana = non
End Function

Sub idatziSubtotala(ByVal iturburua As String, ByVal printOrria As String, ByVal non As Integer, ByVal zerrenHasi As Integer, ByVal zerrenAma As Integer, ByVal kantitatea As Double, ByVal unitatea As String, ByVal zutabea As String, ByVal inicio As Integer)
    'kalkulatu subtotala
    'D alkiler, G kantidad
    'Subtotal = Application.SumProduct("D" & zerrenHasi & ":D" & zerrenAma, "G" & zerrenHasi & ":G" & zerrenAma)
    Dim Subtotal As String
    Subtotal = "=SUMPRODUCT(D" & zerrenHasi & ":D" & zerrenAma & ",G" & zerrenHasi & ":G" & zerrenAma & ")"
    'formatua
    Sheets(printOrria).Range("A" & non & ":G" & non).Interior.Color = RGB(255, 231, 155)
    Sheets(printOrria).Range("A" & non & ":G" & non).Font.Bold = True
