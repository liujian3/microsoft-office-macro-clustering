Function saveAsXLSXtemp() As String
    Application.DisplayAlerts = False
    Dim xlObj As Workbook
    Set xlObj = Application.Workbooks.Add
    
    If (ThisWorkbook.Sheets("BoM").Range("ID_Data_CodObra").Value <> "" And ThisWorkbook.Sheets("BoM").Range("ID_Data_OfertaSolicitud").Value <> "") Then
        excelizena = ThisWorkbook.Sheets("BoM").Range("ID_Data_CodObra").Value & "_" & ThisWorkbook.Sheets("BoM").Range("ID_Data_OfertaSolicitud").Value
    Else
        excelizena = CreateObject("scripting.filesystemobject").getbasename(ThisWorkbook.Name)
    End If
    fitxategia = Environ("TEMP") & "\" & excelizena & ".xlsx"
    'fitxategia = "c:\ulma" & "\" & excelizena & ".xlsx"
    If Not fitxategia = False Then
    ThisWorkbook.Sheets(ThisWorkbook.ActiveSheet.Name).Copy Before:=xlObj.Sheets(1)
    'ezabatu izen guztiak
        On Error Resume Next
        For Each MyName In xlObj.Names
                xlObj.Names(MyName.Name).Delete
        Next
        'Ezabatu link guztiak
        ExternalLinksArray = xlObj.LinkSources(Type:=xlLinkTypeExcelLinks)
        If IsEmpty(ExternalLinksArray) = False Then
            For x = 1 To UBound(ExternalLinksArray)
                xlObj.BreakLink Name:=ExternalLinksArray(x), Type:=xlLinkTypeExcelLinks
            Next x
        End If
        'xlObj.Windows(1).Visible = False
        xlObj.ActiveSheet.Buttons.Delete
        
        xlObj.SaveAs Filename:=fitxategia, FileFormat:=xlWorkbookDefault, CreateBackup:=False, AccessMode:=xlExclusive, ConflictResolution:=xlLocalSessionChanges
        
        xlObj.Close
        saveAsXLSXtemp = fitxategia
    Else
        saveAsXLSXtemp = ""
    End If
    Application.DisplayAlerts = True
End Function

