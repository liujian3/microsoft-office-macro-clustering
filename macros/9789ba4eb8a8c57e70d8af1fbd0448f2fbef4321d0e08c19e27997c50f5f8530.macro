Sub formatosBaremoTotalesEspana(ByVal row As Integer, ByVal column As Integer)
        If (row = Sheets("Data").Range("FilaUnidadLista").Value) Then
            formato = "#,##0.0000" & " """ & "/ " & Sheets("BoM").Cells(row, column).Value & """"
            non = Application.IfError(Application.Match(ThisWorkbook.Names("ID_PrecioOfertaAlqDiaUnidad").RefersToRange(1, 1), Sheets("BoM").Range(ConvertToLetter(ConvertToNumber(Sheets("Data").Range("NumColumInicioListaLR")) - 1) & ":" & ConvertToLetter(ConvertToNumber(Sheets("Data").Range("NumColumInicioListaLR")) - 1)), 0), 0)
            Sheets("BoM").Cells(non, column).NumberFormat = formato
        Else
            columnaCheck = ConvertToLetter(ConvertToNumber(Sheets("Data").Range("NumColumInicioListaLR").Value) - 1)
            hasi = ConvertToNumber(Sheets("Data").Range("NumColumInicioListaLR").Value)
            ama = ConvertToNumber(Sheets("Data").Range("NumColumFinListaLR").Value)
            
            If (Sheets("BoM").Range(columnaCheck & row).Value = ThisWorkbook.Names("ID_Baremo").RefersToRange(1, 1)) Then
                For i = hasi To ama
                    formato1 = "#,##0.0000" & " """ & "/ " & Sheets("BoM").Cells(Sheets("Data").Range("FilaUnidadLista").Value, i) & """"
                    formato2 = "0.00|fffd|"
                
                    Sheets("BoM").Cells(row + 1, i).NumberFormat = formato1
                    Sheets("BoM").Cells(row, i).NumberFormat = formato2
                    Sheets("BoM").Cells(row, i).Value = ""
                Next
            Else
                For i = hasi To ama
                    formato1 = "0.00|fffd|"
                    formato2 = "#,##0.0000" & " """ & "/ " & Sheets("BoM").Cells(Sheets("Data").Range("FilaUnidadLista").Value, i) & """"
                
                    Sheets("BoM").Cells(row, i).NumberFormat = formato2
                    Sheets("BoM").Cells(row, i).Value = ""
                    Sheets("BoM").Cells(row + 1, i).NumberFormat = formato1
                Next
            End If
        End If
End Sub


