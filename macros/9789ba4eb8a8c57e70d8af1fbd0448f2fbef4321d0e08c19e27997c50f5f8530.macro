    totalPesos = Sheets("BoM").Range(columnaTotales & fin + 6).Value
    Dim totalPrecios As Double
    totalPrecios = Sheets("BoM").Range(columnaTotales & fin + 5).Value
    If Not mensaje Then
        cantidades = totalCantidades
        pesos = totalPesos
        precios = totalPrecios
    Else
        cantidades = 0
        pesos = 0
        precios = 0
    End If
    If inicioCol <> finCol Then
        creaColumna ("JOINED COLUMN")
        inicio = Sheets("Data").Range("FilaInicioArticulosLR").Value
        fin = Sheets("Data").Range("FilaFinArticulosLR").Value
        finCol = ConvertToNumber(Sheets("Data").Range("NumColumFinListaLR").Value)
        For i = inicio To fin
            batura = 0
            For j = inicioCol To finCol - 1
                unekoZut = ConvertToLetter(j)
                batura = batura + Sheets("BoM").Range(unekoZut & i).Value * Sheets("BoM").Range(unekoZut & inicio - 2).Value
            Next
            Sheets("BoM").Range(ConvertToLetter(finCol) & i).Value = batura
        Next
        For h = inicioCol To finCol - 1
            zutabea = ConvertToLetter(inicioCol)
            Mzz_Columnas.eliminaColumna (zutabea)
        Next
    End If
    inicio = Sheets("Data").Range("FilaInicioArticulosLR").Value
    fin = Sheets("Data").Range("FilaFinArticulosLR").Value
    Dim totalCantidadesFin As Double
    totalCantidadesFin = Application.Sum(Sheets("BoM").Range(columnaCantidades & inicio & ":" & columnaCantidades & fin))
    Dim totalPesosFin As Double
    totalPesosFin = Sheets("BoM").Range(columnaTotales & fin + 6).Value
    Dim totalPreciosFin As Double
    totalPreciosFin = Sheets("BoM").Range(columnaTotales & fin + 5).Value
    Application.ScreenUpdating = egoera1
    Application.EnableEvents = egoera2
    Application.DisplayAlerts = egoera3
    If mensaje Then
        If (Application.Round(totalCantidades, 4) <> Application.Round(totalCantidadesFin, 4) Or Application.Round(totalPesos, 4) <> Application.Round(totalPesosFin, 4) Or Application.Round(totalPrecios, 4) <> Application.Round(totalPreciosFin, 4)) Then
            MsgBox "Total quantity, price or weight before Join are different to Totals after Join (before/after): " & vbNewLine & "Quantities:     " & totalCantidades & "  /  " & totalCantidadesFin & vbNewLine & "Weights:        " & totalPesos & "  /  " & totalPesosFin & vbNewLine & "Prices:            " & totalPrecios & "  /  " & totalPreciosFin, vbCritical
        End If
    End If
    ThisWorkbook.Worksheets("BoM").Protect AllowFormattingCells:=True, AllowFormattingColumns:=True, AllowFormattingRows:=True, AllowInsertingRows:=True, AllowDeletingRows:=True, UserInterfaceOnly:=True
    ThisWorkbook.Worksheets("BoM").EnableOutlining = True
