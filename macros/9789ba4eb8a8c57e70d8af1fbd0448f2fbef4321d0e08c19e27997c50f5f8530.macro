    Sheets(printOrria).Range("E" & non).Value = "-------------------------------->"
    Sheets(printOrria).Range("E" & non).HorizontalAlignment = xlRight
    Sheets(printOrria).Range("F" & non).Value = "Subtotal: ..........."
    Sheets(printOrria).Range("G" & non).Value = Subtotal
    Sheets(printOrria).Cells(non, 7).NumberFormat = "#,##0.00 |fffd|"
    Sheets(printOrria).Cells(non, 7).Font.Size = 10
    With Sheets(printOrria).Range("A" & non & ":G" & non).Borders(xlEdgeBottom)
        .LineStyle = xlContinuous
        .Weight = xlThin
        .ColorIndex = 0
    End With
    With Sheets(printOrria).Range("A" & non & ":G" & non).Borders(xlEdgeTop)
        .LineStyle = xlContinuous
        .Weight = xlThin
        .ColorIndex = 0
    End With
    With Sheets(printOrria).Range("A" & non & ":G" & non).Borders(xlEdgeLeft)
        .LineStyle = xlContinuous
        .Weight = xlThin
        .ColorIndex = 0
    End With
    With Sheets(printOrria).Range("A" & non & ":G" & non).Borders(xlEdgeRight)
        .LineStyle = xlContinuous
        .Weight = xlThin
        .ColorIndex = 0
    End With
    'utzi marka
    Sheets(printOrria).Range("H" & non).Value = "S"
    Sheets(printOrria).Range("H" & non).Font.Color = RGB(255, 255, 255)
End Sub


