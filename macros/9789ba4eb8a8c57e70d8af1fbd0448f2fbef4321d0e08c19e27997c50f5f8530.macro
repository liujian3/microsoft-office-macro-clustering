Sub eliminaLineasNegativas(ByVal hoja As String)
    Dim inicio, fin, j, i As Integer
    Dim deleted As Boolean
    inicio = 14
    fin = 2000
    i = inicio
    Do While i <= fin
        deleted = False
        j = 7
        Do While j < 22 And deleted = False
            If Not (Len(Trim(Sheets(hoja).Cells(i, j).Value)) = 0) Then
                If CInt(Sheets(hoja).Cells(i, j).Value) <= 0 Then
                    Sheets(hoja).Rows(i).EntireRow.Delete
                    deleted = True
                End If
            End If
            j = j + 2
        Loop
        If deleted = False Then
            i = i + 1
        End If
    Loop
End Sub

Sub eliminaValoresNegativos(ByVal hoja As String)
    Application.ScreenUpdating = False
    Application.EnableEvents = False
    Dim inicio, fin, j, i As Integer
    Dim denakHutsak As Boolean
    inicio = 14
    fin = 2000
    i = inicio
    Do While i <= fin
        denakHutsak = True
        j = 7
        Do While j < 22
            If Not (Len(Trim(Sheets(hoja).Cells(i, j).Value)) = 0) Then
                If CDbl(Sheets(hoja).Cells(i, j).Value) <= 0 Then
                    Sheets(hoja).Cells(i, j).Value = ""
                    Sheets(hoja).Cells(i, j).Font.Color = vbBlack
                    Sheets(hoja).Cells(i, j).Interior.Color = vbWhite
                Else
                    denakHutsak = False
                End If
            End If
            j = j + 2
        Loop
        If denakHutsak = True And Sheets(hoja).Range("A" & i).Value <> "" Then
            Sheets(hoja).Rows(i).EntireRow.Delete
        Else
            i = i + 1
        End If
        
