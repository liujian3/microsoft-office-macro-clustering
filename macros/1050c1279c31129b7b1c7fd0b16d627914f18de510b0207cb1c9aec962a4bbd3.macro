Private Sub cmdSenden_Click()
' ab 2007

Dim objOL As Object
Dim strTempPath As String
Dim strFileNameNoExtension As String
Dim strPDFPath As String
Dim objMail As Object
Dim strNN As String
Dim strPNR As String

'AG T-Systems

If Me.opt1 = False And Me.opt11 = False Then
MsgBox " W|fffd|hlen Sie bitte, ob Arbeitnehmer bei der T-Systems war: ja oder nein", vbCritical + vbOKOnly, "Wichtig!"
Exit Sub
End If

If Me.txtVorname = "" Or Me.txtNachname = "" Or Me.txtPNR = "" Then
MsgBox "|fffd|berpr|fffd|fen Sie bitte Ihre Angaben: Vorname, Nachname, Personalnummer - d|fffd|rfen nicht leer sein!", vbCritical + vbOKOnly, "wichtig"
Exit Sub
End If
 
 strNN = Me.txtNachname.Value
 strPNR = Me.txtPNR.Value
 
     If ActiveDocument.Path = "" Then
        MsgBox "Dokument muss erst gespeichert werden!", vbExclamation
        Exit Sub
    End If
    Set objOL = CreateObject("Outlook.Application")
    strTempPath = Environ("TEMP")
    
    strFileNameNoExtension = Mid(ActiveDocument.Name, 1, InStrRev(ActiveDocument.Name & strNN, ".", -1, vbTextCompare) - 1)
    strPDFPath = strTempPath & "\" & strFileNameNoExtension & "_" & strNN & ".pdf"
    
   ActiveDocument.SaveAs FileName:=strPDFPath, FileFormat:=wdFormatPDF ', _
                                   OpenAfterExport:=False, OptimizeFor:=wdExportOptimizeForPrint, Range:= _
                                   wdExportAllDocument, Item:=wdExportDocumentContent, _
                                   IncludeDocProps:=True, KeepIRM:=True, CreateBookmarks:= _
                                   wdExportCreateNoBookmarks, DocStructureTags:=True, _
                                   BitmapMissingFonts:=True, UseISO19005_1:=False 'SaveAs2 FileName:=strPDFPath, FileFormat:=wdFormatPDF
    
    Debug.Print strPDFPath
    
    Set objMail = objOL.CreateItem(0)
    With objMail
        .To = "ids.admin@fidelis-hr.de"
        .Subject = "Beurteilungsbogen" & " " & strNN & " PNR: " & strPNR
        .GetInspector.Display
        '.olOldBody = .HTMLBody
        '.Body = "Text"
        .Attachments.Add strPDFPath
        .Display
    End With
    Set objOL = Nothing
    Set objMail = Nothing
End Sub





