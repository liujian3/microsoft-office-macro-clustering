If Range("belt_length_s").Value <> "" And Range("length_divi").Value = 1 Then
longer.Value = Range("long").Value
shorter.Value = Range("short").Value
End If

If Range("length_divi").Value = 1 And Range("empty").Value = "Falsch" Then
