  Startpage.Show vbModeless

  ' Wartezeit 4 Sekunden
  Application.Wait Time:=Now + TimeSerial(0, 0, 5)
  Unload Startpage
  showform
'  MsgBox "Jetzt beginnt das Verarbeitungsprogramm"
End Sub
