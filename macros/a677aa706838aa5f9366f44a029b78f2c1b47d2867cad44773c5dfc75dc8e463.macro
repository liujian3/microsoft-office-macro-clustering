Attribute VB_Name = "basActiveX"
Option Explicit

Sub CreateColorsF()
   Dim oTxtBox As OLEObject
   Dim rng As Range
   Dim iCounter As Integer
   For Each oTxtBox In ActiveSheet.OLEObjects
      oTxtBox.Delete
   Next
   Application.ScreenUpdating = False
   For iCounter = 2 To 87
      Set rng = Cells(iCounter, 1)
      Set oTxtBox = ActiveSheet.OLEObjects.Add( _
      ClassType:="Forms.Label.1", _
      Left:=rng.Left, _
      Top:=rng.Top, _
      Width:=rng.Width, _
      Height:=rng.Height)
      With oTxtBox.Object
         .BackColor = RGB( _
            rng.Offset(0, 1).Value, _
            rng.Offset(0, 2).Value, _
            rng.Offset(0, 3).Value)
         .Caption = ""
      End With
   Next iCounter
   Application.ScreenUpdating = True
End Sub

Sub CreateColorsS()
   Dim shp As Shape
   Dim rng As Range
   Dim iCounter As Integer
   Application.ScreenUpdating = False
   For Each shp In ActiveSheet.Shapes
      If shp.Type = 1 Then
         shp.Delete
      End If
   Next
   For iCounter = 2 To 87
      Set rng = Cells(iCounter, 1)
      Set shp = ActiveSheet.Shapes.AddShape( _
         msoShapeRectangle, _
         Left:=rng.Left, _
         Top:=rng.Top, _
         Width:=rng.Width, _
         Height:=rng.Height)
      shp.Fill.ForeColor.RGB _
          = RGB(rng.Offset(0, 1).Value, _
            rng.Offset(0, 2).Value, _
            rng.Offset(0, 3).Value)
   Next iCounter
   Application.ScreenUpdating = True
End Sub


Attribute VB_Name = "basMain"
Option Explicit

Sub GetColors()
   Dim iColor As Integer
   Call ReSetTable
   For iColor = 1 To 56
      With Cells(iColor + 2, 1)
         .Value = iColor
         .Interior.ColorIndex = iColor
         Cells(iColor + 2, 2).Value = .Interior.Color
      End With
   Next iColor
End Sub

Sub SetColors()
   Dim iCounter As Integer, iColor As Integer
   Application.ScreenUpdating = False
   For iCounter = 1 To 56
      iColor = 256 / 56
      ActiveWorkbook.Colors(iCounter) = _
        RGB(iColor * iCounter, iColor * _
        iCounter, iColor * iCounter)
   Next iCounter
   Call GetColors
   Range("A1:I2").Font.ColorIndex = 56
End Sub

Sub ReSetColors()
   ActiveWorkbook.ReSetColors
   Call GetColors
   Range("A1:I2").Font.ColorIndex = 2
End Sub

Sub ReSetTable()
   With ThisWorkbook.Worksheets("Indizes").Range("A3:B58")
      .ClearContents
      .Interior.ColorIndex = xlColorIndexNone
   End With
   ThisWorkbook.Worksheets("Indizes").Range("F3:H58").FormulaR1C1 = _
     "=RIGHT(""000000""&DEC2HEX(RC[-3]),2)"
   Application.Calculate
End Sub

