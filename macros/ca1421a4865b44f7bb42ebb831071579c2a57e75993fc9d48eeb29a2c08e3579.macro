Private Sub SaveAsJPGHiResMainChart()
' created by smartcoder247 2013
On Error Resume Next


XlVersion = Application.Version

If XlVersion >= 14 Then

'===========================================
            If (MsgBox("   Add Date Stamp to JPEG?" & _
            " ", 4)) = vbYes Then
            
            ActiveSheet.Shapes.Range(Array("PredictionsDateBox")).Visible = True 'show box
            Else
            End If
     
'=================================******===================

Application.ScreenUpdating = False
Application.Run "unProtectSheet" '@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@

Worksheets.Add(After:=Worksheets(1)).Name = "SheetForPrinting"
Application.ScreenUpdating = True

Sheets("MainChart").Activate


Worksheets("MainChart").Range("O8:CE73").CopyPicture xlScreen, xlBitmap '===

Application.ScreenUpdating = False
Charts.Add
ActiveChart.Location Where:=xlLocationAsObject, Name:="SheetForPrinting"
ActiveChart.Parent.Name = "ChartForPrinting"

    ActiveSheet.ChartObjects("ChartForPrinting").Activate
    ActiveSheet.Shapes("ChartForPrinting").Height = 1607.2440944882
    ActiveSheet.Shapes("ChartForPrinting").Width = 2085.7322834646
    
ActiveSheet.ChartObjects("ChartForPrinting").Activate
ActiveChart.Paste

Dim WSHShell As Object
Dim strPath As String
Set WSHShell = CreateObject("Wscript.Shell")
strPath = WSHShell.SpecialFolders("Desktop") & "\"

ActiveChart.Export strPath & Worksheets("MainChart").Range("DA15") & ".jpg"


Application.DisplayAlerts = False
Sheets("SheetForPrinting").Delete
Application.DisplayAlerts = True

Sheets("MainChart").Activate
ActiveSheet.Shapes.Range(Array("PredictionsDateBox")).Visible = False ' box hide
Application.Run "ProtectSheetMainChart" 'xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx


JPEGPictureForm.Show
'=================================******===================


'==============

Else
MsgBox "Save Chart as a JPEG option only available for Office 2010+ at the moment"
End If

Range("U11").Select
 
End Sub

