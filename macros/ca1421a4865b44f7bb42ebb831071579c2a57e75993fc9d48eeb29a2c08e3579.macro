Private Sub LoadOneSlotData() ' button  generic
Application.ScreenUpdating = False


If (MsgBox("   Load Prediction?" & _
         " ", 1)) = vbOK Then


Dim CurrentPredictionSheet As Worksheet '--x
Set CurrentPredictionSheet = ActiveSheet '--x

CurrentPredictionSheet.Activate '******


If Range("W159") = True Then


If Range("DA28") = "Flags On (Auto)" Then
FlagStatusOnoff = "Flags On (Auto)"
Else
FlagStatusOnoff = "Flags off"
End If


If FlagStatusOnoff = "Flags On (Auto)" Then
Application.Run "FlagsOffSlotUpdate"
Else
End If

Application.Run "UnFlagsProtectSheet" '---

Range("M16").Select ' ++++++++++++++++++++++++++SERIAL +++++++++++++

Application.Run "ConvertSerial" 'generic

CurrentPredictionSheet.Activate '******

Range("R162:U226").Value = Sheets("SerialConversion").Range("B5:E69").Value

Application.Run "LoadSlotDataSUB" 'generic


CurrentPredictionSheet.Activate '******



Range("U11").Activate


If FlagStatusOnoff = "Flags On (Auto)" Then
Application.Run "FlagsOnSlotUpdate"
Else
End If

    Range("M16").Select  '
Application.Run "FormatUploadDownloadcell" '---

Else

MsgBox "Prediction serial code incorrectly entered"

End If

Application.Run "FlagsProtectSheet"


Else '--------------
End If '-------------

End Sub

