Sub FMAIL()
'
' FMAIL Macro
'
' Keyboard Shortcut: Ctrl+Shift+M
'
    Range("D6").Select
    Selection.Copy
    Range("B1:C6").Select
    Selection.PasteSpecial Paste:=xlPasteFormats, Operation:=xlNone, _
        SkipBlanks:=False, Transpose:=False
    Application.CutCopyMode = False
    With Selection.Font
        .ThemeColor = xlThemeColorDark1
        .TintAndShade = 0
    End With
   
  
    Range("AV9:BL46").Select
    Selection.Copy
    
    Range("A9").Select
    ActiveSheet.Paste
    
    Range("Aw1:Ax2").Select
    Selection.Copy
    
    Range("h1").Select
    ActiveSheet.Paste
    
    Range("A1").Select
End Sub


