Dim LV

Dim TOT

Dim RetVal

Function RiskVal(AV As String, TL As String, LV As String)

RetVal = 0

Select Case AV
        Case "M"
            RetVal = RetVal + 3
        Case "H"
            RetVal = RetVal + 4
        Case Else
            RetVal = RetVal + 0
    End Select

Select Case TL
        Case "M"
            RetVal = RetVal + 3
        Case "H"
            RetVal = RetVal + 6
        Case Else
            RetVal = RetVal + 0
End Select

Select Case LV
        Case "M"
            RetVal = RetVal + 1
        Case "H"
            RetVal = RetVal + 2
        Case Else
            RetVal = RetVal + 0
    End Select

RiskVal = RetVal

End Function





