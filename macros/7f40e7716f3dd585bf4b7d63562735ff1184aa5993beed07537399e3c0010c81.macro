Attribute VB_Name = "Module1"
Sub Chernoff()

    Dim i As Integer
    For i = Range("G2") To Range("H2")
 
    Range("D" + Trim(Str(i))).GoalSeek Goal:=Range("F2").Value, ChangingCell:=Range("A" + Trim(Str(i)))
 
Next
 
End Sub
Attribute VB_Name = "Module2"
Function AddressOfMax(rng As Range) As String
    AddressOfMax = WorksheetFunction.Index(rng, WorksheetFunction.Match(WorksheetFunction.Max(rng), rng, 0)).Address

End Function
Attribute VB_Name = "Module3"
Function RowOfMax(rng As Range) As String
    RowOfMax = Range(WorksheetFunction.Index(rng, WorksheetFunction.Match(WorksheetFunction.Max(rng), rng, 0)).Address).Row

