    Dim Pwd             As String
     
    'Pwd = InputBox("Enter your password to protect all worksheets", "LCHSA")
    Pwd = "LCHSA"
    On Error Resume Next
    For Each wSheet In Worksheets
        wSheet.Unprotect Password:=Pwd
    Next wSheet
    If Err <> 0 Then
