Sub Schaltfl|fffd|che2253_Klicken()
  Range("A1").Select
    ActiveWorkbook.Worksheets("Inhaltsverzeichnis aller KPR").Sort.SortFields.Clear
    ActiveWorkbook.Worksheets("Inhaltsverzeichnis aller KPR").Sort.SortFields.Add2 _
        Key:=Range("B2:B65000"), SortOn:=xlSortOnValues, Order:=xlAscending, _
        DataOption:=xlSortNormal
    ActiveWorkbook.Worksheets("Inhaltsverzeichnis aller KPR").Sort.SortFields.Add2 _
        Key:=Range("C2:C65000"), SortOn:=xlSortOnValues, Order:=xlAscending, _
        DataOption:=xlSortNormal
    ActiveWorkbook.Worksheets("Inhaltsverzeichnis aller KPR").Sort.SortFields.Add2 _
        Key:=Range("D2:D65000"), SortOn:=xlSortOnValues, Order:=xlAscending, _
        DataOption:=xlSortNormal
    With ActiveWorkbook.Worksheets("Inhaltsverzeichnis aller KPR").Sort
        .SetRange Range("A1:G65000")
        .Header = xlYes
        .MatchCase = False
        .Orientation = xlTopToBottom
        .SortMethod = xlPinYin
        .Apply
    End With
    Range("A1").Select
End Sub
