Sub OcultarExibControles3()
   With UserForm1EditPln
      Jj|fffd|Adic = .JurAdicParc_Chb.Value
      JAC = .JurApartir_Chb.Value   ' juros ap. da chav (true)
      JSC = .JurSimp_Chb.Value   ' Juros simples ou composto(falso)
      JsAC = .JurSimplesApartir_Chb.Value   ' Juros simples s|fffd| apartir da chave
      Op|fffd|JurAntEntrg = Princ(42, 2).Value Or (Princ(43, 2).Value And Princ(8, 2).Value)

      If nMesesEntrg = 1 And nDiasEntrg = 1 And .JurApartir_Chb.Value Then .JurApartir_Chb.Value = False
      If nMesesEntrg = 1 And nDiasEntrg = 1 And .JurSimplesApartir_Chb.Value Then .JurSimplesApartir_Chb.Value = False
      .JurTaxa_Cb.Enabled = Not Jj|fffd|Adic
      .JurTaxaCalculadora_Bt.Enabled = Not Jj|fffd|Adic
      .JurSimp_Chb.Enabled = Not Jj|fffd|Adic
      .JurSimplesApartir_Chb.Enabled = Not Jj|fffd|Adic
      .JurApartir_Chb.Enabled = Not Jj|fffd|Adic
      .JurMasCapt_Chb.Enabled = Not Jj|fffd|Adic

      .JurSimplesApartir_Chb.Visible = JSC
      .JurMasCapt_Chb.Visible = Op|fffd|JurAntEntrg And (JAC Or JsAC)

      .JurAdicParc_Chb.Caption = "Adicionar logo nas parcelas" & IIf(JSC And JAC, " ap|fffd|s " & Princ(8, 1).Value & " (Tab. Price).", " (Tab. Price).")   'Princ(46, 1).Value
      .JurApartir_Chb.Caption = IIf(JSC, "Juros Compostos a partir d", "Aplicar juros s|fffd| a partir d") & Princ(8, 1).Value
      .JurSimplesApartir_Chb.Caption = Princ(44, 1).Value

      If Op|fffd|JurAntEntrg Then
         .JurSimplesApartir_Chb.Top = .JurSimp_Chb.Top
         .JurSimplesApartir_Chb.Left = .JurSimp_Chb.Left + .JurSimp_Chb.Width + 5
         .JurApartir_Chb.Visible = True
         Else
         .JurSimplesApartir_Chb.Top = .JurApartir_Chb.Top
         .JurSimplesApartir_Chb.Left = .JurApartir_Chb.Left
         .JurApartir_Chb.Visible = Not JSC
      End If
   End With
End Sub


