Private Sub Workbook_Open()
    Dim wsh As Worksheet
    For Each wsh In Me.Worksheets
        wsh.EnableOutlining = True
        wsh.Protect UserInterfaceOnly:=True, Password:="ff"
    Next wsh
End Sub

