Attribute VB_PredeclaredId = True
Attribute VB_Exposed = True
Attribute VB_TemplateDerived = False
Attribute VB_Customizable = True
Sub ChangeAxisScales()
ActiveSheet.Unprotect Password:="(drsscg7)"
   Dim objCht As ChartObject
   For Each objCht In ActiveSheet.ChartObjects
      With objCht.Chart
         ' Value (Y) Axis
         With .Axes(xlValue)
            .MaximumScale = ActiveSheet.Range("f60").Value
            .MinimumScale = ActiveSheet.Range("f61").Value
            .MajorUnit = ActiveSheet.Range("f62").Value
         End With
      End With
   Next objCht
ActiveSheet.Protect Password:="(drsscg7)", DrawingObjects:=False
End Sub

Private Sub Worksheet_Change(ByVal Target As Range)
If Target.Address = "$J$3" Then
Call ChangeAxisScales
End If
If Target.Address = "$J$7" Then
Call ChangeAxisScales
End If
End Sub



