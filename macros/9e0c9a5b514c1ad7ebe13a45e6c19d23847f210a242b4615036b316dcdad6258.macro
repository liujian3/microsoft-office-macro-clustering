Attribute VB_Name = "Module1"
Sub refreshMWTR()
ActiveSheet.Range("A1:AL50").Select

    'MW REFRESH
    Call RefreshMEAddin1
    
    'TR REFRESH
    DoEvents
    Application.Run "EikonRefreshWorksheet", True, 120000
    DoEvents


'Insert date and time

ActiveSheet.Range("datecell").Activate
ActiveCell.Value = Now

ActiveSheet.Range("A1").Select

End Sub


Public Sub RefreshMEAddin1()
    Dim strMacroString As String
    strMacroString = "'Modelware Lite.xla'!FetchMetricsForSelectionV2"
    Application.Run (strMacroString)
End Sub

Sub refreshMWoldnew()
ActiveSheet.Range("A1:K20").Select

    'MW REFRESH
    Call RefreshMEAddin1

'Insert date and time
ActiveSheet.Range("dateMW").Activate
ActiveCell.Value = Now

ActiveSheet.Range("modeldate").Select
Selection.NumberFormat = "m/d/yyyy"

ActiveSheet.Range("A1").Select

End Sub

Sub updateDScontrols()

Worksheets("FX").Range("FX_date").Value = Now

Application.COMAddIns("PowerlinkCOMAddIn.COMAddIn").Object.RefreshWorkbook

  
End Sub

Sub Validation()
'
' Validation Macro
'
' Keyboard Shortcut: Ctrl+Shift+A
'
    With Selection.Validation
        .Delete
        .Add Type:=xlValidateList, AlertStyle:=xlValidAlertStop, Operator:= _
        xlBetween, Formula1:="='Model settings'!$B$6:$B$17"
        .IgnoreBlank = True
        .InCellDropdown = True
        .InputTitle = ""
        .ErrorTitle = ""
        .InputMessage = ""
        .ErrorMessage = ""
        .ShowInput = True
        .ShowError = True
    End With
End Sub


