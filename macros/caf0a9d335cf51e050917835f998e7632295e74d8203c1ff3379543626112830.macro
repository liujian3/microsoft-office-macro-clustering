Private Sub Worksheet_SelectionChange(ByVal Target As Range)
Dim click As String
If Target.Count > 1 Then Exit Sub
If Not Intersect(Range("B17,B18,A24:A30,A40,C40,A46,C46,A59,C59,A68,C68"), Target) Is Nothing Then 'Bereich A1:Z100 anpassen
    Cancel = True
    click = Target.Value
    Select Case click
        Case ""
            Target = "x"
        Case "x"
            Target = ""
    End Select
End If
End Sub
