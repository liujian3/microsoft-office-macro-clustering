                Quality:=xlQualityStandard, IncludeDocProperties:=True, _
                IgnorePrintAreas:=False, OpenAfterPublish:=False
    
End Sub
Sub Save_Week_daybook()
Dim Fname As String
Dim Path As String
Dim ws1 As Worksheet
Dim Border As Integer

Set ws1 = Worksheets("Sam. Sondering") 'whatever you worksheet is

    With ws1.Range("A10:A50").Borders(xlEdgeLeft)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    With ws1.Range("A10:A50").Borders(xlEdgeRight)
        .LineStyle = xlContinuous
        .ColorIndex = 0
        .TintAndShade = 0
        .Weight = xlThin
    End With
    For Border = 12 To 40
        With ws1.Range("A10" & ":" & "A" & Border).Borders(xlEdgeBottom)
             .LineStyle = xlContinuous
             .ColorIndex = 0
             .TintAndShade = 0
             .Weight = xlHairline
        End With
   Next Border
    
    ThisWorkbook.Save
    Fname = Application.InputBox("V|fffd|nligen ange vecka f|fffd|r f|fffd|ltrapport", Type:=2)
    Path = ThisWorkbook.Path
            '//Export to PDF with new File Path
            ActiveSheet.Range("A1:G76").ExportAsFixedFormat _
                Type:=xlTypePDF, _
                Filename:=Path + "\" + Range("D3") + " " + "F|fffd|ltrapport" + " " + "V." + Fname, _
                Quality:=xlQualityStandard, IncludeDocProperties:=True, _
                IgnorePrintAreas:=False, OpenAfterPublish:=False
                
    
End Sub
