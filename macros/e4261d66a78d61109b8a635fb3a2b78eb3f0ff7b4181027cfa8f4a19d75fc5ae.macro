Sub copydata()
Dim x As Integer
Dim y As Integer
Dim ws1 As Worksheet
Dim ws2 As Worksheet
Dim rng As Range
Dim T As Range
Dim A As Range
Dim H As Integer
Dim Border As Integer

r = 10 'rad f|fffd|r Sammanfattning

H = 14
Set ws1 = Worksheets("Sam. Sondering") 'whatever you worksheet is
ws1.Range("A10:G50").ClearContents

For W1 = 1 To 7

Set ws2 = Worksheets(W1) 'or whatever your worksheet is called
    Range("A12:F39").Select
    ActiveWorkbook.Worksheets(W1).Sort.SortFields.Clear
    ActiveWorkbook.Worksheets(W1).Sort.SortFields.Add Key:=Range("A12:A39") _
        , SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:=xlSortNormal
    With ActiveWorkbook.Worksheets(W1).Sort
        .SetRange Range("A12:F39")
        .Header = xlYes
        .MatchCase = False
        .Orientation = xlTopToBottom
        .SortMethod = xlPinYin
        .Apply
    End With
    
    Range("A46:E61").Select
    ActiveWorkbook.Worksheets(W1).Sort.SortFields.Clear
    ActiveWorkbook.Worksheets(W1).Sort.SortFields.Add Key:=Range("C46:C61") _
        , SortOn:=xlSortOnValues, Order:=xlDescending, DataOption:=xlSortNormal
    With ActiveWorkbook.Worksheets(W1).Sort
        .SetRange Range("A46:E61")
        .Header = xlYes
        .MatchCase = False
        .Orientation = xlTopToBottom
        .SortMethod = xlPinYin
        .Apply
    End With
    
    
    Worksheets(W1).Range("G1") = "Kontakt: P.Nilsson"
    Worksheets(W1).Range("E1") = "|fffd|F Infrastructure AB"
    Worksheets(W1).Range("R2") = "Kontakt: P.Nilsson"
    Worksheets(W1).Range("P2") = "|fffd|F Infrastructure AB"
    
    
    
x = 14 's|fffd|krad i dagbok

    Do Until ws2.Range("A" & x) = "" 'This will loop until column A is empty, set the column to whatever you want
                            'but it cannot have blanks in it, or it will stop looping. Choose a column that is
                            'always going to have data in it.

        If Not ws2.Range("B" & x).Value = "" Then 'This checks your column H to make sure it's not empty
                                            'If empty, it goes on to the next line, if not it copies the data.
                                            'This column should be something that will have something in it if
                                            'there is a product ordered for Item 1
                                            'i.e. don't choose column J if it will have blanks where there is
    
            ws1.Range("A" & r).Value = ws2.Range("A" & x).Value ''ID                                            'actually an item ordered
            r = r + 1 ''Advances r when there is a matching case
            x = x + 1
    
        Else
        x = x + 1 'Advances only x (to check the next line) when there is not a matching case,
                'i.e. your output line stays on the next line down from where it last wrote data
                'while x advances
        End If
    Loop

    If x > H Then
    H = x
    Else
    x = x
    End If

Next W1

ws1.Range("A9" & ":" & "A" & r).RemoveDuplicates Columns:=1, Header:=xlYes
    Range("A9" & ":" & "A" & r).Select
    ActiveWorkbook.ActiveSheet.Sort.SortFields.Clear
    ActiveWorkbook.ActiveSheet.Sort.SortFields.Add Key:=Range("A9" & ":" & "A" & r) _
        , SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:=xlSortNormal
    With ActiveWorkbook.ActiveSheet.Sort
        .SetRange Range("A9" & ":" & "A" & r)
        .Header = xlYes
        .MatchCase = False
        .Orientation = xlTopToBottom
        .SortMethod = xlPinYin
        .Apply
    End With
    
For w2 = 1 To 7
    Set ws2 = Worksheets(w2)
    Set rng = ws1.Range("A10:A50")
    x = 14
    
    If Not ws2.Range("B" & x).Value = "" Then
        For y = 14 To H
            
            If Not ws2.Range("B" & y).Value = "" And ws2.Range("A" & y).Value = "" Then
             MsgBox "Inlagd metod saknar Borrh|fffd|ls-ID se blad " & ws2.Name & " och rad " & y
             Else
                For Each T In rng
                    If T.Value = ws2.Range("A" & y).Value Then

                    ws1.Range("H" & T.Row).End(xlToLeft).Offset(0, 1).Value = ws2.Range("B" & y).Value
                    End If
                
                Next T
             
             End If
        Next y
   
    End If
Next w2

    Range("B53:F68").Select
    ActiveWorkbook.Worksheets(8).Sort.SortFields.Clear
    ActiveWorkbook.Worksheets(8).Sort.SortFields.Add Key:=Range("D53:D68") _
        , SortOn:=xlSortOnValues, Order:=xlDescending, DataOption:=xlSortNormal
    With ActiveWorkbook.Worksheets(8).Sort
        .SetRange Range("B53:F68")
        .Header = xlYes
        .MatchCase = False
        .Orientation = xlTopToBottom
        .SortMethod = xlPinYin
        .Apply
    End With
    
    Worksheets(9).Range("F2") = "|fffd|F Infrastructure AB"
    Worksheets(8).Range("F1") = "Kontakt: P.Nilsson"
    Worksheets(8).Range("E1") = "|fffd|F Infrastructure AB"
    Range("A1").Select
 
End Sub
