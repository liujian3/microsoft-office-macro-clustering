Sub otosfeladat()

'els|fffd| sor megkeres|fffd|se (ez csak az alap a t|fffd|nyleges feladathoz)
elsosor = 1
Do While Cells(elsosor, 1) <> ""
elsosor = elsosor + 1
Loop
'MsgBox "Az els|fffd| sor: " & elsosor

'utols|fffd| sor megkeres|fffd|se
utolsosor = 1
Do While Cells(utolsosor, 2) <> ""
utolsosor = utolsosor + 1
Loop
utolsosor = utolsosor - 1
MsgBox "Az utols|fffd| sor: " & utolsosor

'utols|fffd| oszlop megkeres|fffd|se
utolsooszlop = 2
Do While Cells(1, utolsooszlop) <> ""
utolsooszlop = utolsooszlop + 1
Loop
utolsooszlop = utolsooszlop - 1
MsgBox "Az utols|fffd| oszlop: " & utolsooszlop

'K|fffd|SZ AZ 5/A
 
Dim szam As Long
Dim datum As Date
Dim szoveg As String

elsosor = 1
For i = elsosor To utolsosor
For j = 2 To utolsooszlop
Select Case Cells(sor, oszlop) = IsNumeric
szam = szam + 1

Case Cells(sor, oszlop) = IsDate
datum = datum + 1

'kiirat|fffd|s
Cells(utolsosor + 1, oszlop) = "szoveg"
Cells(utolsosor + 1, oszlop) = "Datum"
Cells(utolsosor + 1, oszlop) = "Szam"
End Sub
