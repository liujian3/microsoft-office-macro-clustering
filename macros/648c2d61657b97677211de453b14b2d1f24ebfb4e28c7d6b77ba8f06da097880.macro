Sub gircik()
For i = Range("N65536").End(3).Row To 1 Step -1
    Range("O" & i).Select
    Application.SendKeys "{F2}"
    Application.SendKeys "{ENTER}"
    Next i
    Range("O1").Select
    Application.SendKeys "{NUMLOCK}%s"
    
End Sub
