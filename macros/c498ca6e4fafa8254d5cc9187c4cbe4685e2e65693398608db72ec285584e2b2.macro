    For i = 65 To 66
     For j = 65 To 66
      For k = 65 To 66
       For l = 65 To 66
        For m = 65 To 66
         For n = 65 To 66
          For o = 65 To 66
           For p = 65 To 66
            For q = 65 To 66
             For r = 65 To 66
              For s = 65 To 66
               For t = 32 To 66
    
    ActiveSheet.Unprotect Chr(i) & Chr(j) & Chr(k) & Chr(l) & Chr(m) & Chr(n) & Chr(o) & Chr(p) & Chr(q) & Chr(r) & Chr(s) & Chr(t)
    
                Next t
               Next s
              Next r
             Next q
            Next p
           Next o
          Next n
         Next m
        Next l
       Next k
      Next j
     Next i
     MsgBox "Der Blattschutz wurde aufgehoben."
End Sub

