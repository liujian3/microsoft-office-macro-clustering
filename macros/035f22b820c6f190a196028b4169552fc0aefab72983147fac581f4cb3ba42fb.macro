Public Sub ShowHiddenRows()
    Dim i As Long
    
    With Range(CStr(strCurRazdel))
        For i = 1 To .Rows.Count
            .Rows(i).EntireRow.Hidden = CBool(.Cells(i, 6) = 0 Or .Cells(i, 6) = "")
        Next
    End With
