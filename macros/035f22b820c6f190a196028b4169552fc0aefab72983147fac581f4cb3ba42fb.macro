Public Sub GetDataToSheet()
    Dim strQuery As String
    Dim i As Integer
    Dim j As Integer
    Dim strCurNumOrg As String
    Dim vAry() As Variant
    
    Application.ScreenUpdating = False

    '|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd| |fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|
    strCurNumOrg = Left(Range("ListORG"), 5)
    If Len(Trim(strCurNumOrg)) = 0 Then
        MsgBox "|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd|!", vbOKOnly + vbCritical, "|fffd||fffd||fffd||fffd||fffd||fffd|"
        Range("ListORG").Select
        Exit Sub
    End If
    
    '|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|
    iCountSelectSum = Range("DataCountSelectSum")
    iDelimiterSum = Range("DataDelimiterSum ")
    iEISum = Range("DataEISum ")
    
    SetPeriods      '|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd|, |fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd| |fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd|
    
    With Range("QueryData")
        
        For i = 1 To .Rows.Count
            '|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd|
            strQuery = .Cells(i, 8)
            If strQuery <> "" Then
                strQuery = Replace(strQuery, "<CountSelectSum>", iCountSelectSum)
                strQuery = Replace(strQuery, "<DelimiterSum>", iDelimiterSum)
                strQuery = Replace(strQuery, "<EISum>", iEISum)
                
                strQuery = Replace(strQuery, "<CodeBP>", strCurNumOrg)
                
                strQuery = Replace(strQuery, "<PrevPeriod>", sPrevPeriod)
                strQuery = Replace(strQuery, "<PrevPeriodAPPG>", sPrevPeriodAPPG)
                strQuery = Replace(strQuery, "<CurPeriodAPPG>", sCurPeriodAPPG)
                strQuery = Replace(strQuery, "<Period>", sCurPeriod)
                
                '|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd|
                vAry = GetParamsToQuery(.Cells(i, 6))
                If IsArray(vAry) Then
                    For j = LBound(vAry, 1) To UBound(vAry, 1)
                        strQuery = Replace(strQuery, vAry(j, 3), vAry(j, 2), 1, , vbTextCompare)
                    Next
                End If
                strQuery = Replace(strQuery, "<OtherProps>", .Cells(i, 5))
                strQuery = Replace(strQuery, "<CodeForm>", FormatForms(.Cells(i, 4)))
                
                strCurRazdel = .Cells(i, 7)
                Application.ScreenUpdating = False
                GetDataFromSvod strQuery
'                Application.ScreenUpdating = False
'                UnProtectXlt
'                Application.ScreenUpdating = False
'                ShowHiddenRows
            End If
        Next
    End With
    UnProtectXlt
    Application.ScreenUpdating = True
    Beep
End Sub

Public Function FormatForms(ByVal FormNames As String) As String
    FormatForms = Replace(Replace(FormNames, " ", ""), ",", "','")
End Function

Public Function GetParamsToQuery(ByVal StringParams As String) As Variant
    Dim vAr() As Variant
    Dim vTemp As Variant
    Dim strTemp As String
    Dim i As Integer
    Dim iCount As Integer
    
    On Error GoTo errHandler
    
    vTemp = Split(StringParams, ";", , vbTextCompare)
    If IsArray(vAr) Then
        ReDim vAr(0 To (UBound(vTemp)), 3)
        For i = 0 To UBound(vAr, 1)
            iCount = InStr(1, vTemp(i), ":")
            If iCount > 0 Then
                vAr(i, 1) = Left(vTemp(i), iCount - 1)
                strTemp = Mid(vTemp(i), iCount + 1)
                iCount = InStr(1, strTemp, "|")
                If iCount > 0 Then
                    vAr(i, 2) = Trim(Left(strTemp, iCount - 1))
                    vAr(i, 3) = "<" & Trim(Mid(strTemp, iCount + 1)) & ">"
                Else
                    MsgBox "|fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|: " & vbCrLf & StringParams & vbCrLf & "|fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd|.", vbOKOnly + vbCritical, "|fffd||fffd||fffd||fffd||fffd||fffd|"
                End If
                
            Else
                MsgBox "|fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|: " & vbCrLf & StringParams & vbCrLf & "|fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd|.", vbOKOnly + vbCritical, "|fffd||fffd||fffd||fffd||fffd||fffd|"
            End If
        Next
    End If
    GetParamsToQuery = vAr
    Exit Function
    
errHandler:
    MsgBox "|fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|: " & vbCrLf & StringParams & vbCrLf & "|fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd|. |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd|, |fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd||fffd||fffd| |fffd||fffd||fffd||fffd||fffd||fffd|.", vbOKOnly + vbCritical, "|fffd||fffd||fffd||fffd||fffd||fffd|"
End Function

