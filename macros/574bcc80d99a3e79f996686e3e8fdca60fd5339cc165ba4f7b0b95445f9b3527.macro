Sub RefreshData()
Attribute RefreshData.VB_Description = "Refreshes the data in the pivot table driving data for Change Program Support segmentation"
Attribute RefreshData.VB_ProcData.VB_Invoke_Func = " \n14"
'
' RefreshData Macro
' Refreshes the data in the pivot table driving data for Change Program Support segmentation
'

'
    Sheets("Change Program Tracking").Select
    ActiveWindow.ScrollColumn = 12
    ActiveWindow.ScrollColumn = 11
    ActiveWindow.ScrollColumn = 6
    ActiveWindow.ScrollColumn = 1
    Range("A116").Select
    ActiveSheet.PivotTables("PivotTable1").PivotCache.Refresh
    Sheets("Dashboard").Select
End Sub
