    Range("B3").Value = "='" & SelSheet & "'!$K$54"
    Range("C3").Value = "='" & SelSheet & "'!$K$55"
    Range("D3").Value = "='" & SelSheet & "'!$K$56"
    
    If Sheets(SelSheet).Name = "H&S English" Then
        Range("D6").Value = "=IF('" & SelSheet & "'!$H$55=1,""Pass"",IF('" & SelSheet & "'!$H$55=2,""Refer to EHS"",""No Answer""))"
