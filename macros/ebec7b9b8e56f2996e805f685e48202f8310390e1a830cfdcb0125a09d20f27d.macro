'        'ElseIf Target.Address = "$G$17" And Range("G17").Value = "Yes" Then
'            'Rows("18:18").EntireRow.Hidden = False
'        Else
        
        If Target.Address = "$D$18" And Range("D18").Value = "No" Then
            Rows("19:19").EntireRow.Hidden = False
            Rows("20:20").EntireRow.Hidden = True
        ElseIf Target.Address = "$D$18" And Range("D18").Value = "Yes" Then
            Rows("20:20").EntireRow.Hidden = False
            Rows("19:19").EntireRow.Hidden = True
        ElseIf Target.Address = "$D$18" And Range("D18").Value = "Please Select from Drop Down Menu" Then
            Rows("20:20").EntireRow.Hidden = True
            Rows("19:19").EntireRow.Hidden = True
        ElseIf Target.Address = "$C$20" And Range("C20").Value = "Please Select from Drop Down Menu" Then
            Range("E20:G20").MergeCells = False
            Range("E20:G20").MergeCells = True
            
            With Range("E20:G20").Interior
                .ThemeColor = xlThemeColorAccent1
                .TintAndShade = 0.799981688894314
            End With
            
            Range("F20:G20").FormatConditions.Delete
            Rows("21:21").EntireRow.Hidden = True
            Range("E20:G20").Locked = True
            Application.EnableEvents = False
            Range("F20:G20").Value = ""
            Application.EnableEvents = True
        ElseIf Target.Address = "$C$20" And Range("C20").Value = "Client Directed" Then
            
            Range("E20:G20").Locked = True
            Range("E20:G20").MergeCells = True
            Rows("21:21").EntireRow.Hidden = False
            Range("F20:G20").FormatConditions.Delete
            Application.EnableEvents = False
            Range("F20:G20").Value = ""
            Application.EnableEvents = True
        ElseIf Target.Address = "$C$20" And Range("C20").Value = "Other" Then
            Range("E20:G20").MergeCells = False
            With Range("E20").Interior
                .Pattern = xlSolid
                .PatternColorIndex = xlAutomatic
                .ThemeColor = xlThemeColorLight2
                .TintAndShade = 0.599993896298105
                .PatternTintAndShade = 0
            End With
            Range("F20:G20").MergeCells = True
            Rows("21:21").EntireRow.Hidden = False
            Range("F20:G20").Locked = False
            With Range("F20:G20").Interior
                .Pattern = xlNone
                .TintAndShade = 0
                .PatternTintAndShade = 0
            End With
            With Range("F20:G20")
                .FormatConditions.Add Type:=xlExpression, Formula1:="=LEN(TRIM(C20:D20))=0"
                .FormatConditions(.FormatConditions.Count).SetFirstPriority
                With .FormatConditions(1).Interior
                    .PatternColorIndex = xlAutomatic
                    .ThemeColor = xlThemeColorAccent1
                    .TintAndShade = 0.799981688894314
                End With
                .FormatConditions(1).StopIfTrue = False
            
                With .Borders(xlEdgeLeft)
                    .LineStyle = xlContinuous
                    .ColorIndex = 0
                    .Weight = xlThin
                End With
            End With
        ElseIf Target.Address = "$C$20" And Range("C20").Value <> "Please Select from Drop Down Menu" Then
            
            Range("E20:G20").Locked = True
            Range("E20:G20").MergeCells = True
            Rows("21:21").EntireRow.Hidden = False
            Range("F20:G20").FormatConditions.Delete
            Application.EnableEvents = False
            Range("F20:G20").Value = ""
            Application.EnableEvents = True
