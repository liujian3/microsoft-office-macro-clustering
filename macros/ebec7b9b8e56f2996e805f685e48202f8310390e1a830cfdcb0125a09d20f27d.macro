Sub PopUpMsgBoxAnualSpend(CelVal)
    Select Case CelVal
        Case "< USD $1M": Debug.Print "y" 'PrepareMsg "non additional attachment is required"
        Case "USD $1M - USD $5M": PrepareMsg "Indirect Supplier Assesment"
        Case "USD $5M - USD $15M": PrepareMsg "Indirect Supplier Assesment"
        Case "> USD $15M": PrepareMsg "Indirect Supplier Assesment" & vbNewLine & vbNewLine & "Financial Assesment"
    Case Else
        'MsgBox "please try one more time."
    End Select
End Sub

