Sub SelectMandatoryCellsWhichAreEmpty(MyArray As Variant)

    Dim SingleCell As Variant
    Dim CellsToSelect As String
    
    With Sheet3
        
        For Each SingleCell In MyArray
            If .Range(SingleCell).Value = "" Or .Range(SingleCell).Value = "Please Select from Drop Down Menu" Then
                CellsToSelect = CellsToSelect & "," & SingleCell
            End If
        Next SingleCell
        
        CellsToSelect = Mid(CellsToSelect, 2)
        
        Application.EnableEvents = False
        .Range("E78").Value = "Please Select from Drop Down Menu"
        Application.EnableEvents = True
        
        .Range(CellsToSelect).Select
        
        MsgBox "Please note that supplier's details are incomplete!" & vbNewLine & "Please complete all mandatory details in cells:" & vbNewLine & vbNewLine & CellsToSelect, vbInformation, Title:="Mandatory Fields"
        
    End With
    
    ListOfApprovalsCanBeDisplayed = False
    
End Sub

