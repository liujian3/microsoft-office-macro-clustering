Private Sub Worksheet_Change(ByVal Target As Range)

    On Error GoTo TheEnd
    
    Dim RegionFlag As Boolean
    RegionFlag = False
    
    ActiveSheet.Unprotect Password:=SheetLock
    
    If Target.Address = "$C$50" Then
        Call PopUpMsgBoxSupplierType(Range("C50").Value)
    End If
    
    If Target.Address = "$C$53" Then
        Call PopUpMsgBoxAnualSpend(Range("C53").Value)
    End If
    
    If Target.Address = "$C$59" Then
        Call PopUpMsgBoxPaymentTerms(Range("C59").Value)
    End If
    
    If Target.Address = "$C$61" Then
        Call PopUpMsgBoxPOnonPO(Range("C61").Value)
    End If
    
    If Target.Address = "$F$71" Then
        Call PopUpMsgBoxConflict(Range("F71").Value)
    End If
    
    If Target.Address = "$F$72" Then
        Call PopUpMsgBoxConflict(Range("F72").Value)
    End If
    
    If Target.Address = "$G$71" Then
        Call PopUpMsgBoxConflict(Range("G71").Value)
    End If
    
    If Target.Address = "$G$72" Then
        Call PopUpMsgBoxConflict(Range("G72").Value)
    End If
    
    ListOfApprovalsCanBeDisplayed = True
    
    If Target.Address = "$E$78" Then
        If Sheet3.Range("D4").Value = "Add a New Supplier" Then 'call mandatory fields only for adding requests
            Call PopUpMsgBoxMandatoryFields(Range("E78").Value)
            If ListOfApprovalsCanBeDisplayed = True Then
                'show list of required approvals
                Call ShowListAtTheEnd
            End If
        End If
    End If
    
    If Target.Address = "$D$4" And Range("D4").Value = "Please Select from Drop Down Menu" Then
        Rows("5:91").EntireRow.Hidden = True
    ElseIf Target.Address = "$D$4" And Range("D4").Value = "Add a New Supplier" Then
        
        Rows("18:18").EntireRow.Hidden = False
        'Rows("13:13").EntireRow.Hidden = False 'pk row 13 should be deleted
        Rows("6:7").EntireRow.Hidden = True
        Rows("23:25").EntireRow.Hidden = True
        Rows("28:28").EntireRow.Hidden = True
    ElseIf Target.Address = "$D$4" And Range("D4").Value = "Change an Existing Supplier Record" Then
        Rows("6:7").EntireRow.Hidden = False
        Rows("23:24").EntireRow.Hidden = False
        Rows("28:28").EntireRow.Hidden = False
'        Rows("13:13").EntireRow.Hidden = True
        Rows("18:18").EntireRow.Hidden = True ' hide row 18 if D4 is defauls
        
    ElseIf Range("D4").Value = "Add a New Supplier" Then
    
