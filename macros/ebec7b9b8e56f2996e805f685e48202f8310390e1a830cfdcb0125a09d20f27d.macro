            Rows("22:22").EntireRow.Hidden = False
            Rows("40:47").EntireRow.Hidden = True
        ElseIf Target.Address = "$D$21" And Range("D21").Value = "Yes" Then
            Rows("22:22").EntireRow.Hidden = True
            Rows("23:24").EntireRow.Hidden = False
            Rows("26:29").EntireRow.Hidden = False
            Rows("40:47").EntireRow.Hidden = False
        ElseIf Target.Address = "$D$21" And Range("D21").Value = "Please Select from Drop Down Menu" Then
            Rows("22:29").EntireRow.Hidden = True
            Rows("40:47").EntireRow.Hidden = True
        ElseIf Target.Address = "$C$23" Then
            If Range("C23").Value = "Please Select from Drop Down Menu" Then
                Exit Sub
            Else
                If Range("E78").Value = "Yes" Then
         
                    Rows("79:91").EntireRow.Hidden = True
                    Rows("80:83").EntireRow.Hidden = False
                    Rows("88:91").EntireRow.Hidden = False
                    Range("C80").Select
                    
                    If Range("F23").Value = "APAC" Then
                        Rows("85:85").EntireRow.Hidden = False
                    ElseIf Range("F23").Value = "EMEA" Then
                        Rows("84:84").EntireRow.Hidden = False
                    ElseIf Range("F23").Value = "LATAM" Then
                        Rows("86:86").EntireRow.Hidden = False
                    ElseIf Range("F23").Value = "North America" Then
                        Rows("87:87").EntireRow.Hidden = False
                    End If
                End If
            End If
            
            If Range("C24").Value = "Oracle 11i US" Then
                ActiveWorkbook.Names("Commodity_Des1").RefersToR1C1 = "='Data Sheet'!R3C57:R471C57"
            Else
                ActiveWorkbook.Names("Commodity_Des1").RefersToR1C1 = "='Data Sheet'!R3C16:R625C16"
            End If
            
        
        ElseIf Target.Address = "$C$28" And Range("C28").Value = "Please Select from Drop Down Menu" Then
            Rows("30:39").EntireRow.Hidden = True
            Rows("48:50").EntireRow.Hidden = True
        ElseIf Target.Address = "$C$28" And Range("C28").Value = "United Kingdom" Or Target.Address = "$C$28" And Range("C28").Value = "Ireland" Then
            Rows("30:39").EntireRow.Hidden = True
            Rows("48:49").EntireRow.Hidden = True
            Rows("30:31").EntireRow.Hidden = False
            Rows("50:50").EntireRow.Hidden = False
        ElseIf Target.Address = "$C$28" And Range("C28").Value = "Argentina" Then
