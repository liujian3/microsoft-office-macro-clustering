Function HideFinalRows()
    With ActiveSheet
        If .CheckBox1.Value = False And .CheckBox2.Value = False And .CheckBox3.Value = False And .CheckBox4.Value = False And .CheckBox5.Value = False And .CheckBox6.Value = False And .CheckBox7.Value = False And .CheckBox8.Value = False And .CheckBox9.Value = False And .CheckBox10.Value = False Then
            Rows("75:91").EntireRow.Hidden = True
        End If
    End With
    ActiveSheet.Protect Password:=SheetLock, DrawingObjects:=True, Contents:=True, Scenarios:=True
End Function

