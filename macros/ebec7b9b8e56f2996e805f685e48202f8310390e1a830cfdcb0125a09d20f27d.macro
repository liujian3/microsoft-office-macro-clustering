Attribute VB_Name = "ListOfApprovals"
Option Explicit

Global ListOfApprovalsCanBeDisplayed As Boolean

Sub ShowListAtTheEnd()
    'check conditions and show correct list with approvals needed
    
    Const CDPT As String = "Customer Dictated Payment Terms"
    Dim MyColl As String
    Dim MyRow As Long
    Dim ValueC50 As String
    Dim ValueC61 As String
    Dim ValueC59 As String
    Dim sh As Shape
    Dim s As Shape
    Dim sExist As Boolean
    Dim clLeft As Double
    Dim clTop As Double
    Dim cl As Range
    
    If Sheet3.Range("D4").Value <> "Add a New Supplier" Then
        Exit Sub 'list of approvals should be visible only for adding new supplier
    End If

    'assign values to variables
    With Sheet3
        ValueC50 = CStr(.Range("C50").Value)
        ValueC61 = CStr(.Range("C61").Value)
        ValueC59 = CStr(.Range("C59").Value)
    End With
    
    'exit sub if no value have been chosen from Drop Down List
    If ValueC61 = "Please Select from Drop Down Menu" Then
        MsgBox "Please choose if supplier ia Non-PO in Cell C61!", vbExclamation
        
        'return previous value
        Application.EnableEvents = False
        Sheet3.Range("E78").Value = "Please Select from Drop Down Menu"
        Application.EnableEvents = True
        Sheet3.Range("C61").Select
        Exit Sub
    End If
    
    If ValueC59 = "Please Select from Drop Down Menu" Then
        MsgBox "Please choose Supplier Payment Terms in Cell C59", vbExclamation
        
        'return previous value
        Application.EnableEvents = False
        Sheet3.Range("E78").Value = "Please Select from Drop Down Menu"
        Application.EnableEvents = True
        
        Sheet3.Range("C59").Select
        
        Exit Sub
    End If
    
    If ValueC50 = "Please Select from Drop Down Menu" Then
        MsgBox "Please choose Supplier Type in Cell C50", vbExclamation
        
        'return previous value
        Application.EnableEvents = False
        Sheet3.Range("E78").Value = "Please Select from Drop Down Menu"
        Application.EnableEvents = True
        
        Sheet3.Range("C50").Select
        
        Exit Sub
    End If

    'Check correct column in sheet "Data Sheet" based on below conditions
    
    If ValueC61 = "No" And ValueC59 <> CDPT Then ' column BR or BT
        MyColl = "BR"
    ElseIf ValueC61 = "Yes" And ValueC59 <> CDPT Then
        MyColl = "BS"
    ElseIf ValueC61 = "No" And ValueC59 = CDPT Then
        MyColl = "BT"
    ElseIf ValueC61 = "Yes" And ValueC59 = CDPT Then
        MyColl = "BU"
    Else
        MsgBox "Please check Cell C59 and Cell C61 because macro is not able to match it's values", vbExclamation
        Exit Sub
    End If
    
    'Check correct Row
    Select Case ValueC50
        Case "Inter-company": MyRow = 3
        Case "Indirect": MyRow = 4
        Case "Sub-contractor/Service": MyRow = 5
        Case "Freight": MyRow = 6
        Case "Government Agency/Municipality": MyRow = 7
        Case "Utility": MyRow = 8
        Case "Union/Court Ordered payments": MyRow = 9
        Case "Charity": MyRow = 10
        Case "Sponsorship": MyRow = 11
        Case "Financial/Leasing/Insurance": MyRow = 12
        Case "Business Consultant/ Professional Fees": MyRow = 13
        Case "Landlord": MyRow = 14
        Case "Please Select from Drop Down Menu": MsgBox "Please select Supplier Type in cell C50", vbExclamation
            Exit Sub
    Case Else:
        MsgBox "Please make sure that cell C50 contain correct value!", vbExclamation
        MyRow = 1
    End Select
    
    'Paste value to main sheet
    With Sheet3
        Application.EnableEvents = False
        .Unprotect SheetLock
        
        .Range("C94").Value = "Please find below list of approvals needed:"
        .Range("C94").Font.Bold = True
        
        Sheet6.Range(MyColl & MyRow).Copy .Range("C96")
        .Range("C96").WrapText = True
        
        'insert button to refresh approvals list
        sExist = False
        For Each s In .Shapes
            If s.Name = "RunMacro" Then
                sExist = True
                Exit For
            End If
        Next s
        
        If sExist = False Then ' do not double insert button on refresh
            Set cl = .Range("C98")
            clLeft = cl.Left
            clTop = cl.Top
            
            Set sh = .Shapes.AddFormControl(xlButtonControl, clLeft, clTop, 150, 25)
            sh.Name = "RunMacro"
            sh.Select
            Selection.Characters.Text = "Refresh List"
            'apply formating to button
            With Selection.Characters(Start:=1, Length:=12).Font
                .Name = "Calibri"
                .FontStyle = "Bold"
                .Size = 14
            End With
            
            sh.OnAction = "ShowListAtTheEnd"
            
            .Range("C94").Select
        End If
        
        '.Protect SheetLock
        Application.EnableEvents = True
    End With
    

End Sub
