'Sortiert wird die aktive Tabelle in ihrer momentanen L|fffd|nge und mit der angege-
'benen Spaltenzahl nach den beiden ebenfalls angebenen Spalten als Schl|fffd|ssel
Sub Sortieren(Spaltenzahl, Hauptschl|fffd|ssel, Nebenschl|fffd|ssel1, Nebenschl|fffd|ssel2)
    z1 = ActiveCell.Row
    z2 = Columns(1).Find("").Row
    Range(Cells(2, 2), Cells(z2 - 1, Spaltenzahl)).Select
    ws = ActiveSheet.Name
    
    ActiveWorkbook.Worksheets(ws).sort.SortFields.Clear
    ActiveWorkbook.Worksheets(ws).sort.SortFields.Add Key:=Range(Cells(2, Hauptschl|fffd|ssel), Cells(z2 - 1, Hauptschl|fffd|ssel)) _
        , SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:=xlSortNormal
    ActiveWorkbook.Worksheets(ws).sort.SortFields.Add Key:=Range(Cells(2, Nebenschl|fffd|ssel1), Cells(z2 - 1, Nebenschl|fffd|ssel1)) _
        , SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:=xlSortNormal
    ActiveWorkbook.Worksheets(ws).sort.SortFields.Add Key:=Range(Cells(2, Nebenschl|fffd|ssel2), Cells(z2 - 1, Nebenschl|fffd|ssel2)) _
        , SortOn:=xlSortOnValues, Order:=xlAscending, DataOption:=xlSortNormal
    With ActiveWorkbook.Worksheets(ws).sort
        .SetRange Range("A1:z11000")
        .Header = xlYes
        .MatchCase = False
        .Orientation = xlTopToBottom
        .SortMethod = xlPinYin
        .Apply
    End With
    
    Range("A2").Select
  

  
    
End Sub




