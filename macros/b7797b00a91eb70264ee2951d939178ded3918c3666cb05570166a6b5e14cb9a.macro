    SendKeys "{ENTER}"
    
    Application.ScreenUpdating = True

End Sub

Sub InputKey_backspace()

    Dim strSheetName As String

    strSheetName = ActiveSheet.Name

    Application.ScreenUpdating = False

    If myBuffNo > 1 Then

        myBuffNo = myBuffNo - 1
        myKeyBuff(myBuffNo) = ""
        Worksheets(strSheetName).OLEObjects("TextBox1").Object.Value = Val(myKeyBuff(0) & myKeyBuff(1) & myKeyBuff(2) & myKeyBuff(3) & myKeyBuff(4) & myKeyBuff(5) & myKeyBuff(6) & myKeyBuff(7) & myKeyBuff(8) & myKeyBuff(9) & myKeyBuff(10) & myKeyBuff(11) & myKeyBuff(12) & myKeyBuff(13) & myKeyBuff(14) & myKeyBuff(15))
        ActiveCell.Value = Worksheets(strSheetName).OLEObjects("TextBox1").Object.Value

    ElseIf myBuffNo = 1 Then

        myBuffNo = myBuffNo - 1
        myKeyBuff(myBuffNo) = ""
        Worksheets(strSheetName).OLEObjects("TextBox1").Object.Value = ""
        ActiveCell.Value = Worksheets(strSheetName).OLEObjects("TextBox1").Object.Value

    Else
        
        ActiveCell.ClearContents
        
        Worksheets(strSheetName).OLEObjects("TextBox1").Object.Value = ""
    
    End If
    
    Application.ScreenUpdating = True

End Sub

Sub Sheet_Clear()

    Dim strSheetName As String
    Dim rc As Integer

    strSheetName = ActiveSheet.Name

    rc = MsgBox("|fffd|V|fffd|[|fffd|g|fffd||fffd||fffd|N|fffd||fffd||fffd|A|fffd||fffd||fffd||702||fffd||fffd||fffd|?                           ", vbYesNo + vbQuestion, "Microsoft Office Drink Management Template 2013")

    If rc = vbYes Then

        Worksheets(strSheetName).Range("C16:I46") = ""
        Worksheets(strSheetName).Range("B49") = 1

        rc = MsgBox("|fffd|V|fffd|[|fffd|g|fffd||fffd||fffd|N|fffd||fffd||fffd|A|fffd||fffd||fffd||702||fffd||fffd||fffd||fffd|B                           ", vbOKOnly + vbInformation, "Microsoft Office Drink Management Template 2013")

    Else

        rc = MsgBox("|fffd|V|fffd|[|fffd|g|fffd||303|N|fffd||fffd||fffd|A|fffd||fffd||fffd|L|fffd||fffd||fffd||fffd||fffd|Z|fffd||fffd||fffd||fffd||fffd||702||fffd||fffd||fffd||fffd|B        ", vbOKOnly + vbInformation, "Microsoft Office Drink Management Template 2013")

    End If

End Sub

