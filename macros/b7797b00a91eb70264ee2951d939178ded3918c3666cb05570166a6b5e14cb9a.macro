    Application.ScreenUpdating = False

    i = myKeyBuff(0) & myKeyBuff(1) & myKeyBuff(2) & myKeyBuff(3) & myKeyBuff(4) & myKeyBuff(5) & myKeyBuff(6) & myKeyBuff(7) & myKeyBuff(8) & myKeyBuff(9) & myKeyBuff(10) & myKeyBuff(11) & myKeyBuff(12) & myKeyBuff(13) & myKeyBuff(14) & myKeyBuff(15)
    
    If InStr(i, ".") Then
    
