Attribute VB_Control = "StartMonitor, 1, 0, MSForms, CommandButton"
Attribute VB_Control = "EndMonitor, 2, 1, MSForms, CommandButton"
Const CELLS_TO_PROCESS = "F4:BC46"

Private Sub EndMonitor_Click()
    Dim rng As RANGE, cell As RANGE
    Set rng = RANGE(CELLS_TO_PROCESS)
    For Each cell In rng
        If Len(cell.Formula) > 0 And Left(cell.Formula, 1) = "=" Then
            cell.Value = Right(cell.Formula, Len(cell.Formula) - 1)
        End If
    Next cell
End Sub

Private Sub StartMonitor_Click()
    Dim rng As RANGE, cell As RANGE
    Set rng = RANGE(CELLS_TO_PROCESS)
    For Each cell In rng
        If Len(cell.Formula) > 0 And Left(cell.Formula, 1) <> "=" Then
            cell.Formula = "=" & cell.text
        End If
    Next cell
End Sub
