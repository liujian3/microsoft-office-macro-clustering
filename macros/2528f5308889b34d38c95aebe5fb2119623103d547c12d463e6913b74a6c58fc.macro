Option Explicit    'Tranferido de FormPageSetpMgr e generalizado para ser chamado pela fun|fffd||fffd|o fPageSetpMgrCopy() sem carregar form
'Page Tab
Public bOrientation As Boolean
Public bScaling As Boolean
Public bPaperSize As Boolean
Public bPrintQuality As Boolean
Public bFirstPageNumber As Boolean

'Margins Tab
Public bMagins As Boolean
Public bCenterOnPage As Boolean

'Header/Footer Tab
Public bLH As Boolean
Public bCH As Boolean
Public bRH As Boolean
Public bLF As Boolean
Public bCF As Boolean
Public bRF As Boolean

'Sheet Tab
Public bPrintArea As Boolean
Public bPrintTitles As Boolean
Public bPrintOpts As Boolean
Public bPageOrder As Boolean

Public StoreArr(19 + 12 * 18 + 9)

Function StoreSettings() As Boolean
    With StoreSettingsSh.PageSetup
        'Page Tab
        If bOrientation Then
            StoreArr(0) = .Orientation
        End If
        If bScaling Then
            StoreArr(1) = .Zoom
            StoreArr(2) = .FitToPagesWide
            StoreArr(3) = .FitToPagesTall
        End If
        If bPaperSize Then
            StoreArr(4) = .PaperSize
        End If
        If bPrintQuality Then
            StoreArr(5) = .PrintQuality
        End If
        If bFirstPageNumber Then
            StoreArr(6) = .FirstPageNumber
        End If


        'Margins Tab
        If bMagins Then
            StoreArr(7) = .LeftMargin
            StoreArr(8) = .RightMargin
            StoreArr(9) = .TopMargin
            StoreArr(10) = .BottomMargin
            StoreArr(11) = .HeaderMargin
            StoreArr(12) = .FooterMargin
        End If
        If bCenterOnPage Then
            StoreArr(13) = .CenterHorizontally
            StoreArr(14) = .CenterVertically
        End If

        'Header/Footer Tab
        If dHostAppVer >= 11 + 1 Then
            StoreArr(15) = .OddAndEvenPagesHeaderFooter
            StoreArr(16) = .DifferentFirstPageHeaderFooter
            StoreArr(17) = .ScaleWithDocHeaderFooter
            StoreArr(18) = .AlignMarginsHeaderFooter
        End If

        'If StoreSettingsSh change, get new picture file names or titles if paths are lost,if need
        If (bLH Or bCH Or bRH Or bLF Or bCF Or bRF) And PicFileOrigNames.sSheet <> StoreSettingsSh.Name & " - " & StoreSettingsSh.Parent.FullName Then UpdatePicFileOrigNames

        If bLH Then If GetPictSettings(.LeftHeader, PicFileOrigNames.sLH, "LH", .LeftHeaderPicture, 19) = False Then Exit Function
        If bCH Then If GetPictSettings(.CenterHeader, PicFileOrigNames.sCH, "CH", .CenterHeaderPicture, 19 + 12) = False Then Exit Function
        If bRH Then If GetPictSettings(.RightHeader, PicFileOrigNames.sRH, "RH", .RightHeaderPicture, 19 + 12 * 2) = False Then Exit Function
        If bLF Then If GetPictSettings(.LeftFooter, PicFileOrigNames.sLF, "LF", .LeftFooterPicture, 19 + 12 * 3) = False Then Exit Function
        If bCF Then If GetPictSettings(.CenterFooter, PicFileOrigNames.sCF, "CF", .CenterFooterPicture, 19 + 12 * 4) = False Then Exit Function
        If bRF Then If GetPictSettings(.RightFooter, PicFileOrigNames.sRF, "RF", .RightFooterPicture, 19 + 12 * 5) = False Then Exit Function

        If dHostAppVer >= 11 + 1 Then
            If bLH Then If GetPictSettings(.FirstPage.LeftHeader.Text, PicFileOrigNames.sLHFIRST, "LHFIRST", .FirstPage.LeftHeader.Picture, 19 + 12 * 6) = False Then Exit Function
            If bCH Then If GetPictSettings(.FirstPage.CenterHeader.Text, PicFileOrigNames.sCHFIRST, "CHFIRST", .FirstPage.CenterHeader.Picture, 19 + 12 * 7) = False Then Exit Function
            If bRH Then If GetPictSettings(.FirstPage.RightHeader.Text, PicFileOrigNames.sRHFIRST, "RHFIRST", .FirstPage.RightHeader.Picture, 19 + 12 * 8) = False Then Exit Function
            If bLF Then If GetPictSettings(.FirstPage.LeftFooter.Text, PicFileOrigNames.sLFFIRST, "LFFIRST", .FirstPage.LeftFooter.Picture, 19 + 12 * 9) = False Then Exit Function
            If bCF Then If GetPictSettings(.FirstPage.CenterFooter.Text, PicFileOrigNames.sCFFIRST, "CFFIRST", .FirstPage.CenterFooter.Picture, 19 + 12 * 10) = False Then Exit Function
            If bRF Then If GetPictSettings(.FirstPage.RightFooter.Text, PicFileOrigNames.sRFFIRST, "RFFIRST", .FirstPage.RightFooter.Picture, 19 + 12 * 11) = False Then Exit Function

            If bLH Then If GetPictSettings(.EvenPage.LeftHeader.Text, PicFileOrigNames.sLHEVEN, "LHEVEN", .EvenPage.LeftHeader.Picture, 19 + 12 * 12) = False Then Exit Function
            If bCH Then If GetPictSettings(.EvenPage.CenterHeader.Text, PicFileOrigNames.sCHEVEN, "CHEVEN", .EvenPage.CenterHeader.Picture, 19 + 12 * 13) = False Then Exit Function
            If bRH Then If GetPictSettings(.EvenPage.RightHeader.Text, PicFileOrigNames.sRHEVEN, "RHEVEN", .EvenPage.RightHeader.Picture, 19 + 12 * 14) = False Then Exit Function
            If bLF Then If GetPictSettings(.EvenPage.LeftFooter.Text, PicFileOrigNames.sLFEVEN, "LFEVEN", .EvenPage.LeftFooter.Picture, 19 + 12 * 15) = False Then Exit Function
            If bCF Then If GetPictSettings(.EvenPage.CenterFooter.Text, PicFileOrigNames.sCFEVEN, "CFEVEN", .EvenPage.CenterFooter.Picture, 19 + 12 * 16) = False Then Exit Function
            If bRF Then If GetPictSettings(.EvenPage.RightFooter.Text, PicFileOrigNames.sRFEVEN, "RFEVEN", .EvenPage.RightFooter.Picture, 19 + 12 * 17) = False Then Exit Function
        End If

        'Sheet Tab
        If bPrintArea Then StoreArr(19 + 12 * 18) = .PrintArea
        If bPrintTitles Then
            StoreArr(19 + 12 * 18 + 1) = .PrintTitleRows
            StoreArr(19 + 12 * 18 + 2) = .PrintTitleColumns
        End If
        If bPrintOpts Then
            StoreArr(19 + 12 * 18 + 3) = .PrintGridlines
            StoreArr(19 + 12 * 18 + 4) = .BlackAndWhite
            StoreArr(19 + 12 * 18 + 5) = .Draft
            StoreArr(19 + 12 * 18 + 6) = .PrintHeadings
            StoreArr(19 + 12 * 18 + 7) = .PrintComments
            StoreArr(19 + 12 * 18 + 8) = .PrintErrors
        End If
        If bPageOrder Then StoreArr(19 + 12 * 18 + 9) = .Order

    End With
    StoreSettings = True
End Function

Function GetPictSettings(sHdFtText As String, sPicFileOrigNames As String, sPicPosInfo As String, objPosPicture As Object, lStoreArrPos As Long) As Boolean
'sHdFtText .LeftHeader
'sPicFileOrigNames PicFileOrigNames.sLH
    StoreArr(lStoreArrPos) = sHdFtText
    If InStr(1, sHdFtText, "&G") > 0 Then
        If Len(sPicFileOrigNames) > 4 And VBA.FileSystem.Dir(sPicFileOrigNames) = "" Then RestPicsToFiles
        If InStr(1, sPicFileOrigNames, "\") = 0 Then    'sPicFileOrigNames = "" 'xpms Era um bug, pois picture existente apenas com title este era retornado e n|fffd|o fullname file quando falhava restauro como acontece em macrosheet
            If sPicPosInfo = "LH" Then Msg1 = IIf(IsPortg, "P|fffd|gina Padr|fffd|o - Cabe|fffd|alho - Esquerda", "Default Page - Header - Left")
            If sPicPosInfo = "CH" Then Msg1 = IIf(IsPortg, "P|fffd|gina Padr|fffd|o - Cabe|fffd|alho - Centro", "Default Page - Header - Center")
            If sPicPosInfo = "RH" Then Msg1 = IIf(IsPortg, "P|fffd|gina Padr|fffd|o - Cabe|fffd|alho - Direita", "Default Page - Header - Right")
            If sPicPosInfo = "LF" Then Msg1 = IIf(IsPortg, "P|fffd|gina Padr|fffd|o - Rodap|fffd| - Esquerda", "Default Page - Footer - Left")
            If sPicPosInfo = "CF" Then Msg1 = IIf(IsPortg, "P|fffd|gina Padr|fffd|o - Rodap|fffd| - Centro", "Default Page - Footer - Center")
            If sPicPosInfo = "RF" Then Msg1 = IIf(IsPortg, "P|fffd|gina Padr|fffd|o - Rodap|fffd| - Direita", "Default Page - Footer - Right")
            If sPicPosInfo = "LHFIRST" Then Msg1 = IIf(IsPortg, "Primeira P|fffd|gina - Cabe|fffd|alho - Esquerda", "First Page - Header - Left")
            If sPicPosInfo = "CHFIRST" Then Msg1 = IIf(IsPortg, "Primeira P|fffd|gina - Cabe|fffd|alho - Centro", "First Page - Header - Center")
            If sPicPosInfo = "RHFIRST" Then Msg1 = IIf(IsPortg, "Primeira P|fffd|gina - Cabe|fffd|alho - Direita", "First Page - Header - Right")
            If sPicPosInfo = "LFFIRST" Then Msg1 = IIf(IsPortg, "Primeira P|fffd|gina - Rodap|fffd| - Esquerda", "First Page - Footer - Left")
            If sPicPosInfo = "CFFIRST" Then Msg1 = IIf(IsPortg, "Primeira P|fffd|gina - Rodap|fffd| - Centro", "First Page - Footer - Center")
            If sPicPosInfo = "RFFIRST" Then Msg1 = IIf(IsPortg, "Primeira P|fffd|gina - Rodap|fffd| - Direita", "First Page - Footer - Right")
            If sPicPosInfo = "LHEVEN" Then Msg1 = IIf(IsPortg, "P|fffd|gina Par - Cabe|fffd|alho - Esquerda", "Even Page - Header - Left")
            If sPicPosInfo = "CHEVEN" Then Msg1 = IIf(IsPortg, "P|fffd|gina Par - Cabe|fffd|alho - Centro", "Even Page - Header - Center")
            If sPicPosInfo = "RHEVEN" Then Msg1 = IIf(IsPortg, "P|fffd|gina Par - Cabe|fffd|alho - Direita", "Even Page - Header - Right")
            If sPicPosInfo = "LFEVEN" Then Msg1 = IIf(IsPortg, "P|fffd|gina Par - Rodap|fffd| - Esquerda", "Even Page - Footer - Left")
            If sPicPosInfo = "CFEVEN" Then Msg1 = IIf(IsPortg, "P|fffd|gina Par - Rodap|fffd| - Centro", "Even Page - Footer - Center")
            If sPicPosInfo = "RFEVEN" Then Msg1 = IIf(IsPortg, "P|fffd|gina Par - Rodap|fffd| - Direita", "Even Page - Footer - Right")
            Msg1 = IIf(IsPortg, "O caminho do arquivo da figura a copiar de ''" & Msg1 & "'' foi perdido e n|fffd|o foi poss|fffd|vel restaurar.", _
                    "The path of the picture file to copy from ''" & Msg1 & "'' was lost and wasn't possible to restore.")
            Msg2 = sAddInCaptByApp & " - " & IIf(IsPortg, "Falha!", "Fail!")
            Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & 3 & ")"
            If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2    'O alert ou n|fffd|o de erro de fPageSetpMgrCopy ser|fffd| feito aqui tamb|fffd|m por isso o 3
            Exit Function
        End If

        With objPosPicture
            StoreArr(lStoreArrPos + 1) = sPicFileOrigNames  '.Filename

            StoreArr(lStoreArrPos + 2) = .CropBottom
            StoreArr(lStoreArrPos + 3) = .CropLeft
            StoreArr(lStoreArrPos + 4) = .CropRight
            StoreArr(lStoreArrPos + 5) = .CropTop
            StoreArr(lStoreArrPos + 6) = .Height
            StoreArr(lStoreArrPos + 7) = .Width
            StoreArr(lStoreArrPos + 8) = .LockAspectRatio

            StoreArr(lStoreArrPos + 9) = .Brightness
            StoreArr(lStoreArrPos + 10) = .Contrast
            StoreArr(lStoreArrPos + 11) = .ColorType
        End With
    End If
    GetPictSettings = True
End Function

Sub SetStoreSettings(DestSh As Worksheet)
    With DestSh.PageSetup
        'Page Tab
        If bOrientation Then
            .Orientation = StoreArr(0)
        End If
        If bScaling Then
            .Zoom = StoreArr(1)
            .FitToPagesWide = StoreArr(2)
            .FitToPagesTall = StoreArr(3)
        End If
        If bPaperSize Then
            On Error Resume Next: .PaperSize = StoreArr(4): On Error GoTo 0    'xpms Um tamanho pode ser estocado mas n|fffd|o ser aceito para definir, por exemplo, 287.
        End If
        If bPrintQuality Then
            .PrintQuality = StoreArr(5)
        End If
        If bFirstPageNumber Then
            .FirstPageNumber = StoreArr(6)
        End If

        'Margins Tab
        If bMagins Then
            .LeftMargin = StoreArr(7)
            .RightMargin = StoreArr(8)
            .TopMargin = StoreArr(9)
            .BottomMargin = StoreArr(10)
            .HeaderMargin = StoreArr(11)
            .FooterMargin = StoreArr(12)
        End If
        If bCenterOnPage Then
            .CenterHorizontally = StoreArr(13)
            .CenterVertically = StoreArr(14)
        End If

        'Header/Footer Tab
        If dHostAppVer >= 11 + 1 Then
            .OddAndEvenPagesHeaderFooter = StoreArr(15)
            .DifferentFirstPageHeaderFooter = StoreArr(16)
            .ScaleWithDocHeaderFooter = StoreArr(17)
            .AlignMarginsHeaderFooter = StoreArr(18)
        End If

        Dim HdFtText As String
        If bLH Then SetPictSettings HdFtText, .LeftHeaderPicture, 19: .LeftHeader = HdFtText
        If bCH Then SetPictSettings HdFtText, .CenterHeaderPicture, 19 + 12: .CenterHeader = HdFtText
        If bRH Then SetPictSettings HdFtText, .RightHeaderPicture, 19 + 12 * 2: .RightHeader = HdFtText
        If bLF Then SetPictSettings HdFtText, .LeftFooterPicture, 19 + 12 * 3: .LeftFooter = HdFtText
        If bCF Then SetPictSettings HdFtText, .CenterFooterPicture, 19 + 12 * 4: .CenterFooter = HdFtText
        If bRF Then SetPictSettings HdFtText, .RightFooterPicture, 19 + 12 * 5: .RightFooter = HdFtText

        If dHostAppVer >= 11 + 1 Then
            If bLH Then SetPictSettings HdFtText, .FirstPage.LeftHeader.Picture, 19 + 12 * 6: .FirstPage.LeftHeader.Text = HdFtText
            If bCH Then SetPictSettings HdFtText, .FirstPage.CenterHeader.Picture, 19 + 12 * 7: .FirstPage.CenterHeader.Text = HdFtText
            If bRH Then SetPictSettings HdFtText, .FirstPage.RightHeader.Picture, 19 + 12 * 8: .FirstPage.RightHeader.Text = HdFtText
            If bLF Then SetPictSettings HdFtText, .FirstPage.LeftFooter.Picture, 19 + 12 * 9: .FirstPage.LeftFooter.Text = HdFtText
            If bCF Then SetPictSettings HdFtText, .FirstPage.CenterFooter.Picture, 19 + 12 * 10: .FirstPage.CenterFooter.Text = HdFtText
            If bRF Then SetPictSettings HdFtText, .FirstPage.RightFooter.Picture, 19 + 12 * 11: .FirstPage.RightFooter.Text = HdFtText

            If bLH Then SetPictSettings HdFtText, .EvenPage.LeftHeader.Picture, 19 + 12 * 12: .EvenPage.LeftHeader.Text = HdFtText
            If bCH Then SetPictSettings HdFtText, .EvenPage.CenterHeader.Picture, 19 + 12 * 13: .EvenPage.CenterHeader.Text = HdFtText
            If bRH Then SetPictSettings HdFtText, .EvenPage.RightHeader.Picture, 19 + 12 * 14: .EvenPage.RightHeader.Text = HdFtText
            If bLF Then SetPictSettings HdFtText, .EvenPage.LeftFooter.Picture, 19 + 12 * 15: .EvenPage.LeftFooter.Text = HdFtText
            If bCF Then SetPictSettings HdFtText, .EvenPage.CenterFooter.Picture, 19 + 12 * 16: .EvenPage.CenterFooter.Text = HdFtText
            If bRF Then SetPictSettings HdFtText, .EvenPage.RightFooter.Picture, 19 + 12 * 17: .EvenPage.RightFooter.Text = HdFtText
        End If

        'Sheet Tab
        If bPrintArea Then .PrintArea = StoreArr(19 + 12 * 18)
        If bPrintTitles Then
            .PrintTitleRows = StoreArr(19 + 12 * 18 + 1)
            .PrintTitleColumns = StoreArr(19 + 12 * 18 + 2)
        End If
        If bPrintOpts Then
            .PrintGridlines = StoreArr(19 + 12 * 18 + 3)
            .BlackAndWhite = StoreArr(19 + 12 * 18 + 4)
            .Draft = StoreArr(19 + 12 * 18 + 5)
            .PrintHeadings = StoreArr(19 + 12 * 18 + 6)
            .PrintComments = StoreArr(19 + 12 * 18 + 7)
            .PrintErrors = StoreArr(19 + 12 * 18 + 8)
        End If
        If bPageOrder Then .Order = StoreArr(19 + 12 * 18 + 9)

    End With

    Dim lCellCount As Double
    If dHostAppVer < 11 + 1 Then lCellCount = DestSh.Cells.Count _
        Else lCellCount = DestSh.Cells.CountLarge
    If appHostApp.WorksheetFunction.CountBlank(DestSh.Cells) = lCellCount _
            And DestSh.Shapes.Count = 0 And DestSh.ProtectContents = False Then DestSh.Range("A1").Value = " "
End Sub

