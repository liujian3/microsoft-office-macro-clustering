Private Sub UpdateWorkbookTo_Cb()
    vEnableEvents = False
    If Copy_Bt.Enabled Then Copy_Bt.SetFocus
    Do While WorkbookTo_Cb.ListCount > 0
        WorkbookTo_Cb.RemoveItem (0)
    Loop
    WorkbookTo_Cb.ListIndex = -1
    SheetTo_Cb.ListIndex = -1

    LastWbCount = 0
    For Each Wb In appHostApp.Workbooks
        If Wb.Windows(1).Visible Then
            WorkbookTo_Cb.AddItem (Wb.Name)
            LastWbCount = LastWbCount + 1
        End If
    Next

    vEnableEvents = True
    If LastWbCount = 1 Then
        If TargetWb.Windows(1).Visible Then
            WorkbookTo_Cb.Value = TargetWb.Name
        Else
            appHostApp.ActiveWindow.ActivateNext: ActWbWinAppSDIBaseForm Form_hWnd1
            WorkbookTo_Cb.Value = appHostApp.ActiveWorkbook.Name
        End If
    ElseIf LastWbCount > 1 Then
        WorkbookTo_Cb.AddItem (IIf(IsPortg, "[Todas]", "[All]"))
        WorkbookTo_Cb.Value = IIf(IsPortg, "[Todas]", "[All]")
    End If
    If WorkbookTo_Cb.Value = "" Then
        WorkbookTo_Cb.AddItem (IIf(IsPortg, "[Nenhuma]", "[None]"))
        WorkbookTo_Cb.Value = IIf(IsPortg, "[Nenhuma]", "[None]")
    End If
End Sub

