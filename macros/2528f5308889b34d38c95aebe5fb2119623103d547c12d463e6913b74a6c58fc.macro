Sub MapPictUpdate()         'Update picture map
    With appHostApp.ActiveWindow.ActiveSheet.PageSetup
        MapPictDP_Fr.ControlTipText = IIf(IsPortg, "Mapa das entradas de figuras em P|fffd|gina Padr|fffd|o", "Picture entry map in Default Page")
        Dim DiffFirstPage_ChB_Value As Boolean
        Dim DiffEvenPage_ChB_Value As Boolean
        If dHostAppVer >= 11 + 1 Then DiffFirstPage_ChB_Value = .DifferentFirstPageHeaderFooter: DiffEvenPage_ChB_Value = .OddAndEvenPagesHeaderFooter
        MapPictFP_Fr.ControlTipText = IIf(IsPortg, "Mapa das entradas de figuras em Primeira P|fffd|gina", "Picture entry map in First Page") & IIf(DiffFirstPage_ChB_Value, "", IIf(IsPortg, " (Diferente n|fffd|o configurado)", " (Different is not set)") & IIf(dHostAppVer >= 11 + 1, "", ". Excel 2007-10..."))
        MapPictEP_Fr.ControlTipText = IIf(IsPortg, "Mapa das entradas de figuras em P|fffd|gina Par", "Picture entry map in Even Page") & IIf(DiffEvenPage_ChB_Value, "", IIf(IsPortg, " (Diferente n|fffd|o configurado)", " (Different is not set)") & IIf(dHostAppVer >= 11 + 1, "", ". Excel 2007-10..."))

        'Reset position controls
        Dim i As Long
        For i = 1 To 18
            Me.Controls("MapPict" & i & "_Lb").BackColor = Me.BackColor
            Me.Controls("MapPict" & i & "_Lb").BorderStyle = fmBorderStyleNone
            Me.Controls("MapPict" & i & "_Lb").SpecialEffect = fmSpecialEffectEtched
            Me.Controls("MapPict" & i & "_Lb").Tag = IIf(IsPortg, "Nenhuma figura definida ainda", "No picture set yet")
