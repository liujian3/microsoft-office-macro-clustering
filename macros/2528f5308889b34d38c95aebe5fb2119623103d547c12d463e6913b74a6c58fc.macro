Private Sub Close_Lb_Click()
    If Left(WorkbookTo_Cb.Value, 1) = "[" Then
        If IsPortg Then
            Msg1 = "Fechar todas as pastas" & IIf(Reset_OpB.Value, "", ", exceto a pasta fonte de c|fffd|pia") & ". OK?"
            Msg2 = sAddInCaptByApp & " - Fechar"
        Else
            Msg1 = "Close all workbooks" & IIf(Reset_OpB.Value, "", ", except the copy source workbook") & ". OK?"
            Msg2 = sAddInCaptByApp & " - Close"
        End If
        If MyMsgBox(Msg1, 2, Msg2) = 2 Then Exit Sub
        For Each Wb In appHostApp.Workbooks
            If Wb.Windows(1).Visible And Wb.Name <> TargetWb.Name Then
                Wb.Close
            End If
        Next
    Else
        If WorkbookTo_Cb.Value = TargetWb.Name Then
            MyMsgBox IIf(IsPortg, "A pasta fonte de c|fffd|pia n|fffd|o pode ser fechada!", "The copy source workbook cannot be closed!"), 1, sAddInCaptByApp & " - " & IIf(IsPortg, "Fechar", "Close")
            Exit Sub
        End If
        If MyMsgBox(IIf(IsPortg, "Fechar a pasta ''" & WorkbookTo_Cb.Value & "''. OK? ", "Close the workbook ''" & WorkbookTo_Cb.Value & "''. OK?"), 2, sAddInCaptByApp & " - " & IIf(IsPortg, "Fechar", "Close")) = 2 Then Exit Sub
        appHostApp.Workbooks(WorkbookTo_Cb.Value).Close
    End If

    UpdateWorkbookFrom_Cb
    UpdateWorkbookTo_Cb
    LastWbCount = appHostApp.Workbooks.Count
End Sub

