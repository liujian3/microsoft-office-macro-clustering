Function fPageSetpMgrCopy(SourceSheet As Variant, DestSheet As Variant, _
        Optional Orientation As Boolean, Optional Scaling As Boolean, Optional PaperSize As Boolean, Optional PrintQuality As Boolean, Optional FirstPageNumber As Boolean, _
        Optional Magins As Boolean, Optional CenterOnPage As Boolean, _
        Optional LeftHeaderPict As Boolean, Optional CenterHeaderPict As Boolean, Optional RightHeaderPict As Boolean, _
        Optional LeftFooterPict As Boolean, Optional CenterFooterPict As Boolean, Optional RightFooterPict As Boolean, _
        Optional PrintArea As Boolean, Optional PrintTitles As Boolean, Optional PrintOpts As Boolean, Optional PageOrder As Boolean) As Long

    If appHostApp.DisplayAlerts Then MustBeRegToCont Else AutentReg   'O alerta feito em MustBeRegToCont, al|fffd|m de suficiente ainda o usu|fffd|rio pode registrar no v|fffd|o.
    If Not bRegtrd And Not bIsFreePass Then
        fPageSetpMgrCopy = 1001
        On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
        Exit Function
    End If

    On Error Resume Next  'xx n|fffd|o esquecer de abilitar antes de publicar
    If TypeName(SourceSheet) <> "Worksheet" Then
        fPageSetpMgrCopy = 1
        Msg1 = IIf(IsPortg, "Planilha fonte n|fffd|o foi informada ou n|fffd|o foi encontrada!", "Source sheet wasn't informed or wasn't found!")
        Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & fPageSetpMgrCopy & ")"
        If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
        On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
        Exit Function
    End If
    If TypeName(DestSheet) <> "Worksheet" Then
        fPageSetpMgrCopy = 2
        Msg1 = IIf(IsPortg, "Planilha destino n|fffd|o foi informada ou n|fffd|o foi encontrada!", "Destination sheet wasn't informed or wasn't found!")
        Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & fPageSetpMgrCopy & ")"
        If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
        On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
        Exit Function
    End If

    If DestSheet.Name = SourceSheet.Name And DestSheet.Parent.Name = SourceSheet.Parent.Name Then Exit Function    'If dest=sourc copied is

    bOrientation = Orientation
    bScaling = Scaling
    bPaperSize = PaperSize
    bPrintQuality = PrintQuality
    bFirstPageNumber = FirstPageNumber
    bMagins = Magins
    bCenterOnPage = CenterOnPage
    bLH = LeftHeaderPict
    bCH = CenterHeaderPict
    bRH = RightHeaderPict
    bLF = LeftFooterPict
    bCF = CenterFooterPict
    bRF = RightFooterPict
    bPrintArea = PrintArea
    bPrintTitles = PrintTitles
    bPrintOpts = PrintOpts
    bPageOrder = PageOrder

    If Not (bOrientation Or bScaling Or bPaperSize Or bPrintQuality Or bFirstPageNumber _
            Or bMagins Or bCenterOnPage _
            Or bLH Or bCH Or bRH Or bLF Or bCF Or bRF _
            Or bPrintArea Or bPrintTitles Or bPrintOpts Or bPageOrder) Then Exit Function    'No selection, exit in silence

    Set StoreSettingsSh = SourceSheet    'Como se fosse um argumento entre fun|fffd||fffd|es chamada por StoreSettings() a seguir
    If StoreSettings() Then
        Dim DestSheet1 As Worksheet
        Set DestSheet1 = DestSheet
        SetStoreSettings DestSheet1
    Else
        fPageSetpMgrCopy = 3
        'Alert de erro ou n|fffd|o ser|fffd| feito em StoreSettings() usando inclusive o n|fffd|mero 3
    End If
    Set StoreSettingsSh = Nothing
    On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
End Function
