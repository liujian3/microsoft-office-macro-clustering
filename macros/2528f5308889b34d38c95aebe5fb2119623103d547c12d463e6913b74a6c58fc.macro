    If vEnableEvents = False Then Exit Sub
    If WorkbookFrom_Cb.Value = "" Then Exit Sub
    appHostApp.Workbooks(WorkbookFrom_Cb.Value).Activate: ActWbWinAppSDIBaseForm Form_hWnd1
    vEnableEvents = False
    Do While SheetFrom_Cb.ListCount > 0
        SheetFrom_Cb.RemoveItem (0)
    Loop
    SheetFrom_Cb.ListIndex = -1    'Inserido no Excel 15, mas era bug antigo onde o evento em SheetFrom_Cb n|fffd|o disparava se houvesse apenas uma planilha na pasta.
    SheetFrom_Cb.Enabled = False: Copy_Bt.Enabled = False: PreviewFrom_Bt.Enabled = False: Settings_Fr.Enabled = False  'Idem bug antigo quando ativasse um wb sem Worksheet active e sim outros tipos. Alterei inserindo s|fffd| esta linha e as duas |fffd|ltimas.
    vEnableEvents = True
    Dim Sh As Object    'Worksheet xpms
    For Each Sh In appHostApp.Workbooks(WorkbookFrom_Cb.Value).Sheets
        If TypeName(Sh) = "Worksheet" Then SheetFrom_Cb.AddItem (Sh.Name)       'xpms
        If TypeName(Sh) = "Worksheet" And appHostApp.Workbooks(WorkbookFrom_Cb.Value).ActiveSheet.Name = Sh.Name Then SheetFrom_Cb.Value = Sh.Name
    Next
    SheetFrom_Cb.Enabled = SheetFrom_Cb.ListCount > 0
    If SheetFrom_Cb.ListIndex <> -1 Then Copy_Bt.Enabled = True: PreviewFrom_Bt.Enabled = True: Settings_Fr.Enabled = True
End Sub

Private Sub SheetFrom_Cb_Change()
    If vEnableEvents = False Then Exit Sub
    If SheetFrom_Cb.Value = "ResetTempSh-DefaultInNew" Then    'Em COM add-in, a ativa|fffd||fffd|o de planilha em wb oculto n|fffd|o o ativa e ainda gera erro, mas felizmente essa |fffd| identificavel
        ResetTempWb.Activate: ActWbWinAppSDIBaseForm Form_hWnd1
        Set TargetWb = ResetTempWb
        Set TargetSh = ResetTempSh
    Else
        appHostApp.Workbooks(WorkbookFrom_Cb.Value).Activate: appHostApp.ActiveWorkbook.Sheets(SheetFrom_Cb.Value).Activate    'xpms Parece que era um bug e estava faltando ativar primeiro o workbook.
        Set TargetWb = appHostApp.ActiveWorkbook: ActWbWinAppSDIBaseForm Form_hWnd1
        Set TargetSh = appHostApp.ActiveWindow.ActiveSheet
    End If
    UpdateWorkbookTo_Cb
End Sub

Private Sub WorkbookFrom_Cb_MouseMove(ByVal Button As Integer, ByVal Shift As Integer, ByVal X As Single, ByVal Y As Single)
'Necess|fffd|rio pois, embora form modal com COMAddin, o Excel aceita abrir pastas atrav|fffd|s do Explores
    If appHostApp.Workbooks.Count = LastWbCount Then Exit Sub
    UpdateWorkbookFrom_Cb
    UpdateWorkbookTo_Cb
