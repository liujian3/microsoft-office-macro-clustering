Private Sub AdjustFormCtrls()
    If Copy_OpB.Value Then
        If lBotFormPageSetpMgr = 0 Then Caption = sAddInCaptByApp & " - " & IIf(IsPortg, "Copiar Configura|fffd||fffd|es Entre Planilhas", "Copy Settings Between Sheets")
        Copy_Bt.Caption = IIf(IsPortg, "Copiar", "Copy")
        From_Fr.Visible = True: To_Fr.Left = HomePage_Lb.Left: To_Fr.Width = From_Fr.Width
        To_Fr.Caption = IIf(IsPortg, "Para: ", "To: ")
        Settings_Fr.Caption = IIf(IsPortg, "Selecione configura|fffd||fffd|es para copiar em cada guia da cx. de di|fffd|logo de config. de p|fffd|g. do Excel: ", "Select settings to copy in each tab of Excel page setup dialog box: ")
        Alert_ChB.Caption = IIf(IsPortg, "Confirmar cada c|fffd|pia", "Confirm each copy")
        ShowPageTab_Lb.Enabled = True
        ShowMarginsTab_Lb.Enabled = True
        ShowHdFtTab_Lb.Enabled = True
        ShowSheetTab_Lb.Enabled = True
        If lBotFormPageSetpMgr = 111 Then OpenExcelWatermark_Lb.Enabled = True     'Vindo do command copy no menu atualiz OpenExcelWatermark labels de acordo com a planilha ativada
    End If
    If Reset_OpB.Value Then
        If lBotFormPageSetpMgr = 0 Then Caption = sAddInCaptByApp & " - " & IIf(IsPortg, "Restaurar Configura|fffd||fffd|es", "Reset Settings")
        Copy_Bt.Caption = IIf(IsPortg, "Restaurar", "Reset")
        From_Fr.Visible = False: To_Fr.Left = From_Fr.Left: If lBotFormPageSetpMgr = 0 Then To_Fr.Width = Settings_Fr.Width
        To_Fr.Caption = IIf(IsPortg, "Restaurar: ", "Reset: ")
        Settings_Fr.Caption = IIf(IsPortg, "Selecione configura|fffd||fffd|es para restaurar em cada guia da cx de di|fffd|logo de config. de p|fffd|g. do Excel: ", "Select settings to reset in each tab of Excel page setup dialog box: ")
        Alert_ChB.Caption = IIf(IsPortg, "Confirmar cada restaurar", "Confirm each reset")
        ShowPageTab_Lb.Enabled = PreviewTo_Bt.Enabled
        ShowMarginsTab_Lb.Enabled = PreviewTo_Bt.Enabled
        ShowHdFtTab_Lb.Enabled = PreviewTo_Bt.Enabled
        ShowSheetTab_Lb.Enabled = PreviewTo_Bt.Enabled

        If lBotFormPageSetpMgr = 121 Then OpenExcelWatermark_Lb.Enabled = PreviewTo_Bt.Enabled     'Vindo do command reset no menu atualiz OpenExcelWatermark labels de acordo com a planilha ativada, se houver
    End If

    If ShowHdFtTab_Lb.Enabled Then
        MapPict_Lb.Visible = True: MapPictDP_Fr.Visible = True: MapPictFP_Fr.Visible = True: MapPictEP_Fr.Visible = True
        MapPictUpdate
    Else
        MapPict_Lb.Visible = False: MapPictDP_Fr.Visible = False: MapPictFP_Fr.Visible = False: MapPictEP_Fr.Visible = False
    End If
End Sub

Private Sub Copy_OpB_Click()
    If Not TargetSh Is Nothing Then
        If TargetSh.Name = "ResetTempSh-DefaultInNew" Then
            TargetWb.Close False    'appHostApp.ActiveWindow.ActivateNext
            Set ResetTempWb = Nothing: Set ResetTempSh = Nothing
            Set TargetWb = appHostApp.ActiveWorkbook: ActWbWinAppSDIBaseForm Form_hWnd1
            Set TargetSh = appHostApp.ActiveWindow.ActiveSheet
            If TargetSh Is Nothing Then Close_Bt_Click: Exit Sub    'All can have been closed when in reset
            UpdateWorkbookFrom_Cb
            If SheetTo_Cb.Enabled Then UpdateWorkbookTo_Cb    'Se UpdateWorkbookFrom_Cb desabilitou SheetTo_Cb |fffd| porque n|fffd|o existe outra planilha para a c|fffd|pia, assim evita repetir alert
            LastWbCount = appHostApp.Workbooks.Count
        End If
    End If

    AdjustFormCtrls
End Sub

Private Sub Reset_OpB_Click()
'The reset is simply a copy from a new temp sheet that have all default settings that can change according the language or country.
    If Reset_OpB.Value Then
        If ResetTempWb Is Nothing Or ResetTempSh Is Nothing Then    'Add if not yet
            appHostApp.ScreenUpdating = False
            Set ResetTempWb = appHostApp.Workbooks.Add(1)
            Set ResetTempSh = ResetTempWb.Sheets(1)
            ResetTempSh.Name = "ResetTempSh-DefaultInNew"
            ResetTempWb.Saved = True
            ResetTempWb.Windows(1).Visible = False
            appHostApp.ScreenUpdating = True
        End If
        Set TargetWb = ResetTempWb    'Activate to copy
        Set TargetSh = ResetTempSh
        UpdateWorkbookFrom_Cb
        UpdateWorkbookTo_Cb
        LastWbCount = appHostApp.Workbooks.Count
    End If

    AdjustFormCtrls
End Sub


Private Sub Close_Bt_Click()
    If Not ResetTempWb Is Nothing Then ResetTempWb.Close False: Set ResetTempWb = Nothing
    If bAlertAboutNewVer Then AlertAboutNewVer             'Ocorre como o padr|fffd|o, desde que n|fffd|o esteja registrado
    lBotFormPageSetpMgr = 0
    Unload Me
End Sub
