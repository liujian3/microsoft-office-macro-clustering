Private Sub Copy_Bt_Click()
    Dim CopyCount
    Dim CopyCnt As Long

    If Copy_OpB.Value Then If Not MustBeRegToCont Then Exit Sub    'Only copy action need to register

    CopyCount = CopyCountOnly
    'Hide

    'Page Tab
    bOrientation = Orientation_ChB.Value
    bScaling = Scaling_ChB.Value
    bPaperSize = PaperSize_ChB.Value
    bPrintQuality = PrintQuality_ChB.Value
    bFirstPageNumber = FirstPageNumber_Chb.Value
    'Margins Tab
    bMagins = Magins_ChB.Value
    bCenterOnPage = CenterOnPage_ChB.Value
    'Header/Footer Tab
    bLH = LH_ChB.Value
    bCH = CH_ChB.Value
    bRH = RH_ChB.Value
    bLF = LF_ChB.Value
    bCF = CF_ChB.Value
    bRF = RF_ChB.Value
    'Sheet Tab
    bPrintArea = PrintArea_ChB.Value
    bPrintTitles = PrintTitles_ChB.Value
    bPrintOpts = PrintOpts_ChB.Value
    bPageOrder = PageOrder_ChB.Value

    If Not (bOrientation Or bScaling Or bPaperSize Or bPrintQuality Or bFirstPageNumber _
            Or bMagins Or bCenterOnPage _
            Or bLH Or bCH Or bRH Or bLF Or bCF Or bRF _
            Or bPrintArea Or bPrintTitles Or bPrintOpts Or bPageOrder) Then Exit Sub    'No selection, exit in silence

    Set StoreSettingsSh = TargetSh    'Como se fosse um argumento entre fun|fffd||fffd|es chamada por StoreSettings() a seguir
    If StoreSettings() Then
        For Each Wb In appHostApp.Workbooks
            If Wb.Windows(1).Visible And _
                    (Left(WorkbookTo_Cb.Value, 1) = "[" Or WorkbookTo_Cb.Value = Wb.Name) Then
                For Each Sh In Wb.Sheets
                    If Not (Wb.Name = TargetWb.Name And Sh.Name = TargetSh.Name) And _
                            (Left(SheetTo_Cb.Value, 1) = "[" Or SheetTo_Cb.Value = Sh.Name) And TypeName(Sh) = "Worksheet" Then       'xpms
                        Wb.Activate: ActWbWinAppSDIBaseForm Form_hWnd1
                        Sh.Activate
                        CopyCnt = CopyCnt + 1
                        If Copy_OpB.Value Then appHostApp.StatusBar = IIf(IsPortg, "Copiando " & CopyCnt & " de " & CopyCount, "Copying " & CopyCnt & " of " & CopyCount) _
                            Else appHostApp.StatusBar = IIf(IsPortg, "Restaurando " & CopyCnt & " de " & CopyCount, "Reseting " & CopyCnt & " of " & CopyCount)
                        If Alert_ChB.Value Then
                            If IsPortg Then
                                If Copy_OpB.Value Then
                                    Msg1 = "Copiar para:" & Chr(10) & "   Pasta: " & Wb.Name & Chr(10) & "   Planilha: " & Sh.Name & "?"
                                    Msg2 = sAddInCaptByApp & " - Copiar Configura|fffd||fffd|es de P|fffd|gina"
                                Else
                                    Msg1 = "Restaurar:" & Chr(10) & "   Pasta: " & Wb.Name & Chr(10) & "   Planilha: " & Sh.Name & "?"
                                    Msg2 = sAddInCaptByApp & " - Restaurar Configura|fffd||fffd|es de P|fffd|gina"
                                End If
                            Else
                                If Copy_OpB.Value Then
                                    Msg1 = "Copy to:" & Chr(10) & "   Workbook: " & Wb.Name & Chr(10) & "   Sheet: " & Sh.Name & "?"
                                    Msg2 = sAddInCaptByApp & " - Copy Page Settings"
                                Else
                                    Msg1 = "Reset:" & Chr(10) & "   Workbook: " & Wb.Name & Chr(10) & "   Sheet: " & Sh.Name & "?"
                                    Msg2 = sAddInCaptByApp & " - Reset Page Settings"
                                End If
                            End If
                            If MyMsgBox(Msg1, 2, Msg2) = 1 Then SetStoreSettings Sh Else CopyCnt = CopyCnt - 1
                        Else
                            SetStoreSettings Sh
                        End If
                    End If
                Next
            End If
        Next
        appHostApp.StatusBar = False
    End If
    Set StoreSettingsSh = Nothing
    'If CopyCnt > 1 Then Unload Me Else Show 1
    If IsPortg Then
        If Copy_OpB.Value Then
            MyMsgBox IIf(CopyCnt = 0, "Nenhuma copia foi feita!", IIf(CopyCnt = 1, "Uma c|fffd|pia foi feita!", CopyCnt & " c|fffd|pias foram feitas!")), 1, Msg2
        Else
            MyMsgBox IIf(CopyCnt = 0, "Nenhuma restaura|fffd||fffd|o foi feita!", IIf(CopyCnt = 1, "Uma restaura|fffd||fffd|o foi feita!", CopyCnt & " restaura|fffd||fffd|es foram feitas!")), 1, Msg2
        End If
    Else
        If Copy_OpB.Value Then
            MyMsgBox IIf(CopyCnt = 0, "No copies were made!", IIf(CopyCnt = 1, "One copy was made!", CopyCnt & " copies were made!")), 1, Msg2
        Else
            MyMsgBox IIf(CopyCnt = 0, "No resettings were made!", IIf(CopyCnt = 1, "One resetting was made!", CopyCnt & " resettings were made!")), 1, Msg2
        End If
    End If
    If CopyCnt > 0 Then MapPictUpdate
End Sub

Private Function CopyCountOnly()
'Conta usando os mesmos loops para inserir
    Dim CopyCnt As Long
    For Each Wb In appHostApp.Workbooks
        If Wb.Windows(1).Visible And _
                (Left(WorkbookTo_Cb.Value, 1) = "[" Or WorkbookTo_Cb.Value = Wb.Name) Then
            For Each Sh In Wb.Sheets
                If Not (Wb.Name = TargetWb.Name And Sh.Name = TargetSh.Name) And _
                        (Left(SheetTo_Cb.Value, 1) = "[" Or SheetTo_Cb.Value = Sh.Name) Then
                    CopyCnt = CopyCnt + 1
                End If
            Next
        End If
    Next
    CopyCountOnly = CopyCnt
End Function

Private Sub NewSh_Lb_Click()
    Dim Sh1 As Worksheet
    On Error Resume Next
    appHostApp.Workbooks(WorkbookTo_Cb.Value).Activate
    Set Sh1 = appHostApp.Workbooks(WorkbookTo_Cb.Value).Sheets(appHostApp.Workbooks(WorkbookTo_Cb.Value).Sheets.Count)
    Set Sh1 = appHostApp.Workbooks(WorkbookTo_Cb.Value).Sheets.Add(After:=Sh1)
    If Err.Number <> 0 Then
        MyMsgBox Err.Description, 1, sAddInCaptByApp & " - " & IIf(IsPortg, "Falha!", "Fail!")
        Exit Sub
    End If

    WorkbookTo_Cb_Change
    SheetTo_Cb.Value = Sh1.Name
    If IsPortg Then
        Msg1 = "A nova planilha ''" & Sh1.Name & "'' foi criada na pasta ''" & WorkbookTo_Cb.Value & "''."
        Msg2 = sAddInCaptByApp & " - Nova planilha"
    Else
        Msg1 = "The new sheet ''" & Sh1.Name & "'' was created on the workbook ''" & WorkbookTo_Cb.Value & "''."
        Msg2 = sAddInCaptByApp & " - New sheet"
    End If
    MyMsgBox Msg1, 1, Msg2
    Copy_Bt_Click
End Sub

