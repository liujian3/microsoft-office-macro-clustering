Function fPageSetpMgrPictInsText(TargetSheet As Variant, HeaderFooterPos As String, PageType As String, _
        Text As String, _
        Optional FontName As String = "Times New Roman", Optional FontSize As Long = -1, _
        Optional FontColor As Long = -1, Optional PresetEffect As Long, Optional Transparency As Double = 50, _
        Optional RotLayout As Long = 315, Optional RefArea As Long = -1) As Long

    fPageSetpMgrPictInsText = CritTgtShHdFtPosPgTp(TargetSheet, HeaderFooterPos, PageType, "Target sheet")
    If fPageSetpMgrPictInsText <> 0 Then Exit Function

    Dim TargetShF As Worksheet
    Dim sText As String
    Dim sFont As String
    Dim lFontSize As Long
    Dim lColor As Long
    Dim lPresetEffect As Long
    Dim dTransp As Double
    Dim sRot As Single

    Set TargetShF = TargetSheet
    sText = Left(Text, 189 - 19)    'Mais de 189 gera erro
    sFont = FontName    'Poderia criticar, mas como n|fffd|o gera erro, |fffd| melhor aceitar a op|fffd||fffd|o autom|fffd|tica em silencio
    lFontSize = IIf(FontSize < 36 Or FontSize > 150, 8, FontSize)    '8 is a flag to be automatic
    lColor = IIf(FontColor < RGB(0, 0, 0) Or FontColor > RGB(255, 255, 255), RGB(192 + 18, 192 + 18, 192 + 18), FontColor)
