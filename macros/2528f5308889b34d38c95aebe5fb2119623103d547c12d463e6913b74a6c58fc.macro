        Case 4, 5, 6    'In footer. Semelhante ao header, mas como ajusta no CropBottom um 'Bottom' |fffd| que |fffd| encontrado e precisa ser convertido em Top
            IniMoveTop = (OriHeight - HrzHdftPosPict.CropBottom / TopAdjustFat) / OriHeight * (CurrRealHeight * dbZoom) + .FooterMargin * dbZoom - CurrRealHeight   '- 5.5
            IniMoveTop = PaperHeight - CurrRealHeight - IniMoveTop
        End Select

        IniLockAspectRatio = HrzHdftPosPict.LockAspectRatio
        HrzHdftPosPict.LockAspectRatio = msoFalse
        HrzHdftPosPict.CropLeft = 0
        HrzHdftPosPict.CropRight = 0
        HrzHdftPosPict.CropTop = 0
        HrzHdftPosPict.CropBottom = 0
        If SizeWidth = -1 And SizeHeight = -1 Then    'Restore original size
            HrzHdftPosPict.Width = OriWidth
            HrzHdftPosPict.Height = OriHeight
        ElseIf SizeWidth <> -1 And SizeHeight = -1 Then    'Adjust by width with aspecto lock
            HrzHdftPosPict.Width = OriWidth
            HrzHdftPosPict.Height = OriHeight
            HrzHdftPosPict.LockAspectRatio = msoTrue
            HrzHdftPosPict.Width = SizeWidth
        ElseIf SizeHeight <> -1 And SizeWidth = -1 Then    'Adjust by height with aspecto lock
            HrzHdftPosPict.Width = OriWidth
            HrzHdftPosPict.Height = OriHeight
            HrzHdftPosPict.LockAspectRatio = msoTrue
            HrzHdftPosPict.Height = SizeHeight
        ElseIf SizeWidth <> -1 And SizeHeight <> -1 Then    'Adjust by width and heigh with aspecto unlock
            HrzHdftPosPict.Width = SizeWidth
            HrzHdftPosPict.Height = SizeHeight
            HrzHdftPosPict.LockAspectRatio = IniLockAspectRatio
        End If
        HrzHdftPosPict.LockAspectRatio = IniLockAspectRatio

