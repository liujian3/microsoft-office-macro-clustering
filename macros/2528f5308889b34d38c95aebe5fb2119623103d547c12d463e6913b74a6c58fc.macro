Private Sub UpdateWorkbookFrom_Cb()
    vEnableEvents = False
    Do While WorkbookFrom_Cb.ListCount > 0
        WorkbookFrom_Cb.RemoveItem (0)
    Loop
    For Each Wb In appHostApp.Workbooks
        If Wb.Windows(1).Visible Or Wb.Sheets(1).Name = "ResetTempSh-DefaultInNew" Then
            WorkbookFrom_Cb.AddItem (Wb.Name)
            If Wb.Name = TargetWb.Name Then
                vEnableEvents = True
                WorkbookFrom_Cb.Value = Wb.Name
            End If
        End If
    Next
    vEnableEvents = True
    On Error Resume Next
    TargetWb.Activate: ActWbWinAppSDIBaseForm Form_hWnd1
    TargetSh.Activate
End Sub



