Private Sub WorkbookTo_Cb_Change()
    If vEnableEvents = False Then Exit Sub
    If WorkbookTo_Cb.Value = "" Then Exit Sub
    If Left(WorkbookTo_Cb.Value, 1) = "[" Then
        SheetTo_Cb.Enabled = False
        SheetTo_Cb.AddItem (IIf(IsPortg, "[Todas em todas]", "[All in all]"))
        SheetTo_Cb.ListIndex = SheetTo_Cb.ListCount - 1
        NewSh_Lb.Enabled = False
        TargetWb.Activate: ActWbWinAppSDIBaseForm Form_hWnd1
        TargetSh.Activate
        Exit Sub
    Else
        SheetTo_Cb.Enabled = True
        NewSh_Lb.Enabled = True
        appHostApp.Workbooks(WorkbookTo_Cb.Value).Activate
    End If

    vEnableEvents = False
    Do While SheetTo_Cb.ListCount > 0
        SheetTo_Cb.RemoveItem (0)
    Loop
    SheetTo_Cb.ListIndex = -1
    vEnableEvents = True
    For Each Sh In appHostApp.Workbooks(WorkbookTo_Cb.Value).Sheets
        If (WorkbookTo_Cb.Value = TargetWb.Name _
                And Sh.Name = TargetSh.Name) Or TypeName(Sh) <> "Worksheet" Then Else _
                SheetTo_Cb.AddItem (Sh.Name)    'xpms
    Next
    If SheetTo_Cb.ListCount > 1 Then
        SheetTo_Cb.AddItem (IIf(IsPortg, "[Todas]", "[All]"))
        SheetTo_Cb.ListIndex = SheetTo_Cb.ListCount - 1
        TargetWb.Activate: ActWbWinAppSDIBaseForm Form_hWnd1
        TargetSh.Activate
    End If
    If SheetTo_Cb.ListCount = 1 Then
        SheetTo_Cb.ListIndex = SheetTo_Cb.ListCount - 1
    End If
    If SheetTo_Cb.ListCount = 0 Then
        If Copy_OpB.Value Or (lBotFormPageSetpMgr >= 11 And lBotFormPageSetpMgr <= 16) Then
            Msg1 = IIf(IsPortg, "N|fffd|o h|fffd| outra planilha na pasta ''" & WorkbookTo_Cb.Value & "'' para destino de c|fffd|pia!", "There isn't other sheet on workbook ''" & WorkbookTo_Cb.Value & "'' to copy destination!")
            Msg2 = sAddInCaptByApp & " - " & IIf(IsPortg, "Imposs|fffd|vel Copiar!", "Cannot Copy!")
            MyMsgBox Msg1, 1, Msg2
        ElseIf Reset_OpB.Value Then           'Se n|fffd|o tem op|fffd||fffd|o ainda |fffd| melhor ficar em sil|fffd|ncio, supondo que iniciou com objetivo de abrir ou criar novos para c|fffd|pia
            Msg1 = IIf(IsPortg, "N|fffd|o h|fffd| outra planilha na pasta ''" & WorkbookTo_Cb.Value & "'' para restaurar!", "There isn't other sheet on workbook ''" & WorkbookTo_Cb.Value & "'' to reset!")
            Msg2 = sAddInCaptByApp & " - " & IIf(IsPortg, "Imposs|fffd|vel Restaurar!", "Cannot Reset!")
            MyMsgBox Msg1, 1, Msg2
        End If
        Copy_Bt.Enabled = False
        SheetTo_Cb.AddItem (IIf(IsPortg, "[Nenhuma]", "[None]"))
        SheetTo_Cb.Value = IIf(IsPortg, "[Nenhuma]", "[None]")
        SheetTo_Cb.Enabled = False
    Else
        Copy_Bt.Enabled = True
        SheetTo_Cb.Enabled = True
    End If
End Sub

Private Sub SheetTo_Cb_Change()
    If vEnableEvents = False Then Exit Sub
    If Left(SheetTo_Cb.Value, 1) <> "[" Then
        If Left(WorkbookTo_Cb.Value, 1) <> "[" And Reset_OpB.Value Then    'Ao selecionar uma |fffd|nica planilha para receber copia, ativa para mape|fffd|-la, mas isso causa confus|fffd|o maior que a |fffd|tilidade, ent|fffd|o desabilitei restrigindo apenas para Reset. Se o usu|fffd|rio quiser ver o mapa de uma planilha, que a selecione temp. como fosse copiar o use o bot|fffd|o Preview. Percebi a confus|fffd|o ao editar para o Excel 15.
            appHostApp.Workbooks(WorkbookTo_Cb.Value).Activate: ActWbWinAppSDIBaseForm Form_hWnd1
            appHostApp.ActiveWorkbook.Sheets(SheetTo_Cb.Value).Activate
        Else
            TargetWb.Activate: ActWbWinAppSDIBaseForm Form_hWnd1
            TargetSh.Activate
        End If
        PreviewTo_Bt.Enabled = True
    Else
        TargetWb.Activate: ActWbWinAppSDIBaseForm Form_hWnd1
