        Msg2 = sAddInCaptByApp & " - " & IIf(IsPortg, "Visualizar", "Preview")
        If MyMsgBox(Msg1, 2, Msg2) = 1 Then
            If lInsTextSel = 0 Then Hide
            PreviewBtClick
            If lInsTextSel <> 2 Then Show 1
        Else
            If lInsTextSel = 1 Then Show 1
        End If
    End If
