                If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
                Set TargetSh = Nothing: Set TargetWb = Nothing: ActSh.Activate
                On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
                Exit Function
            End If
            ActSh.Activate
        End If

        GetPicOrigSize HrzHdftPosPict
        GetPicCurrRealSize HrzHdftPosPict

        'Get current moves to restore after resize considerando margens de refer|fffd|ncias = 0, portanto area do papel deve considerar ao restaurar no final
        dbZoom = 1    'Mantive para as formulas ficarem semelhantes as de UpdatePos em FormSizePosition
        Select Case lEntryPointHdFt    'Lefts
        Case 1, 4    'In left. Original of version with only LH the 1
            IniMoveLeft = (OriWidth - HrzHdftPosPict.CropLeft / LeftAdjustFat) / OriWidth * (CurrRealWidth * dbZoom) + IIf(dHostAppVer >= 11 + 1 And bAnchorOnLeftMargin, IIf(.LeftMargin < 53.858, 53.858, .LeftMargin), 53.858) * dbZoom - CurrRealWidth
        Case 2, 5    'In center. O Excel centraliza levando em conta os dois crops laterais. LeftAdjustFat n|fffd|o parece necess|fffd|rio aplicar e remover
            If HrzHdftPosPict.CropLeft <= 0 And HrzHdftPosPict.CropLeft <= HrzHdftPosPict.CropRight Then
                IniMoveLeft = PaperWidth / 2 - (OriWidth + (HrzHdftPosPict.CropLeft + HrzHdftPosPict.CropRight)) * CurrRealWidth / OriWidth * dbZoom / 2
            Else
                IniMoveLeft = PaperWidth / 2 - (OriWidth - (HrzHdftPosPict.CropLeft + HrzHdftPosPict.CropRight)) * CurrRealWidth / OriWidth * dbZoom / 2
            End If
        Case 3, 6    'In right. Semelhante ao left, mas como ajusta no CropRight um 'Right' |fffd| que |fffd| encontrado e precisa ser convertido em Left
            IniMoveLeft = (OriWidth - HrzHdftPosPict.CropRight / LeftAdjustFat) / OriWidth * (CurrRealWidth * dbZoom) + IIf(dHostAppVer >= 11 + 1 And bAnchorOnLeftMargin, IIf(.RightMargin < 53.858, 53.858, .RightMargin), 53.858) * dbZoom - CurrRealWidth
            IniMoveLeft = PaperWidth - CurrRealWidth - IniMoveLeft
