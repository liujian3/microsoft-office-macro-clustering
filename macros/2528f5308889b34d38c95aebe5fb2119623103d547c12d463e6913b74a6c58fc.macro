Function fPageSetpMgrPictInsObj(TargetSheet As Variant, HeaderFooterPos As String, PageType As String, _
        Optional ShpOrRng As Variant, Optional RefArea As Long = -1, Optional Brighten As Boolean = True) As Long

    fPageSetpMgrPictInsObj = CritTgtShHdFtPosPgTp(TargetSheet, HeaderFooterPos, PageType, "Target sheet")
    If fPageSetpMgrPictInsObj <> 0 Then Exit Function

    Dim oShpOrRng As Object
    Dim sObjName As String
    If IsMissing(ShpOrRng) Then Set ShpOrRng = appHostApp.Selection
    If IsObject(ShpOrRng) Then
        Set oShpOrRng = ShpOrRng
    Else
        fPageSetpMgrPictInsObj = 3 + 2 + 1    'Must be the last of fPageSetpMgrPictInsFile + 1
        Msg1 = IIf(IsPortg, "O objeto informado n|fffd|o |fffd| v|fffd|lido!", "Object informed isn't valid!")
        Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & fPageSetpMgrPictInsObj & ")"
        If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
        On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
        Exit Function
    End If

    On Error Resume Next
    'First verif if the object can be copied, if not try its parent and parent
    Do
        Err.Clear
        oShpOrRng.CopyPicture xlScreen, xlPicture
        If Err.Number <> 0 Then
            Err.Clear
            Set oShpOrRng = oShpOrRng.Parent
            If Err.Number <> 0 Then    'If haven't parent before caming in Worsheet
                fPageSetpMgrPictInsObj = 3 + 2 + 2
                Msg1 = IIf(IsPortg, "O objeto informado n|fffd|o |fffd| um objeto de planilha v|fffd|lido!", "Object informed isn't a valid sheet object!")
                Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & fPageSetpMgrPictInsObj & ")"
                If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
                On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
                Exit Function
            End If
        End If
        If TypeName(oShpOrRng) = "Worksheet" Then     'Can't copy this object. Vou deixar silencioso pode ser que questionem
            fPageSetpMgrPictInsObj = 3 + 2 + 3
            Msg1 = IIf(IsPortg, "O object informado |fffd| um objeto de planilha v|fffd|lido, mas n|fffd|o foi poss|fffd|vel copi|fffd|-lo!", "The object informed is a valid sheet object, but it wasn't possible to copy it!")
            Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & fPageSetpMgrPictInsObj & ")"
            If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
            On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
            Exit Function
        End If
    Loop While Err.Number <> 0


    If TypeName(oShpOrRng) = "Range" Then
        sObjName = IIf(IsPortg, "Intervalo ", "Range ") & oShpOrRng.Address(False, False)
    Else
        sObjName = oShpOrRng.Name
        If sObjName = "" Then sObjName = TypeName(oShpOrRng)    'If hasn't name use type name
    End If

    On Error GoTo 0
    bTestOtherIPicCode = ShiftDown
    sObjName = MyReplace(sObjName, ":", "")    'If range

    Dim vFileName    'as String need be variable
    If RefArea = -1 Then RefArea = 1     'If missing
    RefArea = IIf(RefArea < 0 Or RefArea > 3, 1, RefArea)    'Criticize and set default

    vFileName = ClipbIpicTempFile(oShpOrRng, sObjName, True)
    If Not vFileName = False Then
        fPageSetpMgrPictInsObj = fPageSetpMgrPictInsFile(TargetSheet, HeaderFooterPos, PageType, CStr(vFileName), RefArea, Brighten)
    Else
        fPageSetpMgrPictInsObj = 3 + 2 + 4
        'Msg1 = was set in ClipbIpicTempFile function
        Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & fPageSetpMgrPictInsObj & ")"
        If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
        On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
        Exit Function
    End If
End Function

Function fPageSetpMgrPictMove(TargetSheet As Variant, HeaderFooterPos As String, PageType As String, _
        Optional MoveLeft As Double = -169093, Optional MoveTop As Double = -169093, Optional RefArea As Long = -1) As Long

    fPageSetpMgrPictMove = CritTgtShHdFtPosPgTp(TargetSheet, HeaderFooterPos, PageType, "Target sheet")
    If fPageSetpMgrPictMove <> 0 Then Exit Function

    Set TargetSh = TargetSheet
    Set TargetWb = TargetSheet.Parent
    With TargetSh.PageSetup
        Dim bHasPic As Boolean
        Dim HrzHdftPosPict As Graphic
        Dim PaperWidth As Double
        Dim PaperHeight As Double

        Select Case lDiffPage
        Case 0
            Select Case lEntryPointHdFt
            Case 1: bHasPic = InStr(1, .LeftHeader, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .LeftHeaderPicture
            Case 2: bHasPic = InStr(1, .CenterHeader, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .CenterHeaderPicture
            Case 3: bHasPic = InStr(1, .RightHeader, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .RightHeaderPicture
            Case 4: bHasPic = InStr(1, .LeftFooter, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .LeftFooterPicture
            Case 5: bHasPic = InStr(1, .CenterFooter, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .CenterFooterPicture
            Case 6: bHasPic = InStr(1, .RightFooter, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .RightFooterPicture
            End Select
        Case 1
            Select Case lEntryPointHdFt
            Case 1: bHasPic = InStr(1, .FirstPage.LeftHeader.Text, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .FirstPage.LeftHeader.Picture
            Case 2: bHasPic = InStr(1, .FirstPage.CenterHeader.Text, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .FirstPage.CenterHeader.Picture
            Case 3: bHasPic = InStr(1, .FirstPage.RightHeader.Text, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .FirstPage.RightHeader.Picture
            Case 4: bHasPic = InStr(1, .FirstPage.LeftFooter.Text, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .FirstPage.LeftFooter.Picture
            Case 5: bHasPic = InStr(1, .FirstPage.CenterFooter.Text, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .FirstPage.CenterFooter.Picture
            Case 6: bHasPic = InStr(1, .FirstPage.RightFooter.Text, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .FirstPage.RightFooter.Picture
            End Select
        Case 2
            Select Case lEntryPointHdFt
            Case 1: bHasPic = InStr(1, .EvenPage.LeftHeader.Text, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .EvenPage.LeftHeader.Picture
            Case 2: bHasPic = InStr(1, .EvenPage.CenterHeader.Text, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .EvenPage.CenterHeader.Picture
            Case 3: bHasPic = InStr(1, .EvenPage.RightHeader.Text, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .EvenPage.RightHeader.Picture
            Case 4: bHasPic = InStr(1, .EvenPage.LeftFooter.Text, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .EvenPage.LeftFooter.Picture
            Case 5: bHasPic = InStr(1, .EvenPage.CenterFooter.Text, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .EvenPage.CenterFooter.Picture
            Case 6: bHasPic = InStr(1, .EvenPage.RightFooter.Text, "&G") > 0: If bHasPic Then Set HrzHdftPosPict = .EvenPage.RightFooter.Picture
            End Select
        End Select

        If Not bHasPic Then
            fPageSetpMgrPictMove = 3 + 1
            Msg1 = IIf(IsPortg, "N|fffd|o h|fffd| figura em  ", "There is no picture in ") & PictPosSpell(lDiffPage, lEntryPointHdFt) & "!"
            Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & fPageSetpMgrPictMove & ")"
            If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
            Set TargetSh = Nothing: Set TargetWb = Nothing
            On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
            Exit Function
        End If

        'Move arg em in point the user may use InchesToPoints or CentimetersToPoints Methods. Critica de
        'Moves. Embora o m|fffd|x aceito pelo Excel para crop seja 169093 usando aqui como flag de missing vou
        'limitar a 100000 em sil|fffd|ncio, pois ainda |fffd| bem acima de um valor pr|fffd|tico em torno de 841 para A4
        If MoveLeft < -100000 And MoveLeft <> -169093 Then MoveLeft = -100000
        If MoveLeft > 100000 Then MoveLeft = 100000
        If MoveTop < -100000 And MoveTop <> -169093 Then MoveTop = -100000
        If MoveTop > 100000 Then MoveTop = 100000

        If RefArea = -1 Or RefArea = 0 Then RefArea = 1     'If missing. Nas outras fun|fffd||fffd|es RefArea=0 |fffd| um flag para n|fffd|o centralizar que aqui |fffd| feito em cada dire|fffd||fffd|o por MoveLeft e MoveTop if missing
        RefArea = IIf(RefArea < 0 Or RefArea > 3, 1, RefArea)   'Criticize and set default

        If MoveLeft = -169093 Or MoveTop = -169093 Or lEntryPointHdFt > 1 Then     'Necessita tamanho do papel para converter coord right in left and bottom in top e para centralizar
            Dim ActSh As Worksheet
            Set ActSh = appHostApp.ActiveWindow.ActiveSheet
            TargetSh.Activate    'Must ativate to fPaperSize
            If fPaperSize(False) Then
                If .Orientation = xlPortrait Then
                    PaperWidth = appHostApp.InchesToPoints(tPaperSize.dWidth)
                    PaperHeight = appHostApp.InchesToPoints(tPaperSize.dHeight)
                Else
                    PaperHeight = appHostApp.InchesToPoints(tPaperSize.dWidth)
                    PaperWidth = appHostApp.InchesToPoints(tPaperSize.dHeight)
                End If
            Else    'Se n|fffd|o conseguir o tamanho do papel, n|fffd|o d|fffd| para mover
                fPageSetpMgrPictMove = 3 + 2
                Msg1 = IIf(IsPortg, "N|fffd|o foi poss|fffd|vel obter o tamanho do papel!", "Unable to get the paper size!")
                Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & fPageSetpMgrPictMove & ")"
                If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
                Set TargetSh = Nothing: Set TargetWb = Nothing: ActSh.Activate
                On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
                Exit Function
            End If
            ActSh.Activate
        End If

        If dHostAppVer >= 11 + 1 Then    'No 2007 essas condi|fffd||fffd|es s|fffd|o necess|fffd|rias para centralizar ou posicionar
            .ScaleWithDocHeaderFooter = False    'N|fffd|o parece |fffd|til e mesmo ao ajustar no FormSizePosition vou desabilitar
            .AlignMarginsHeaderFooter = bAnchorOnLeftMargin    'N|fffd|o influi no centrar e sim ao alterar a margem direita depois, mas |fffd| configur|fffd|vel no FormPositionCenter
        End If

        RefLeftMargin = Choose(RefArea, 0, .LeftMargin, .LeftMargin)
        RefRightMargin = Choose(RefArea, 0, .RightMargin, .RightMargin)
        RefTopMargin = Choose(RefArea, 0, .HeaderMargin, .TopMargin)
        RefBottomMargin = Choose(RefArea, 0, .FooterMargin, .BottomMargin)

        GetPicOrigSize HrzHdftPosPict
        GetPicCurrRealSize HrzHdftPosPict

        If MoveLeft = -169093 Then           'Centralizar
            MoveLeft = (PaperWidth - RefLeftMargin - RefRightMargin - CurrRealWidth) / 2 + RefLeftMargin
        Else
            MoveLeft = MoveLeft + RefLeftMargin    'Adiciona a margem da |fffd|rea de ref
        End If

        HrzHdftPosPict.CropLeft = 0: dbZoom = 1    'Mantive para as formulas ficarem semelhantes as de UpdatePos em FormSizePosition
        HrzHdftPosPict.CropRight = 0
        HrzHdftPosPict.CropTop = 0
        HrzHdftPosPict.CropBottom = 0

        If MoveLeft <> 0 Then
            Select Case lEntryPointHdFt    'Lefts
            Case 1, 4    'In left. Original of version with only LH the 1
                HrzHdftPosPict.CropLeft = -(MoveLeft + HrzHdftPosPict.Width - IIf(dHostAppVer >= 11 + 1 And bAnchorOnLeftMargin, IIf(.LeftMargin < 53.858, 53.858, .LeftMargin), 53.858) * dbZoom) / (HrzHdftPosPict.Width * dbZoom) * OriWidth + OriWidth
                HrzHdftPosPict.CropLeft = Round(HrzHdftPosPict.CropLeft * LeftAdjustFat, 1)    'O arredond n|fffd|o |fffd| significativo e ainda pode encaixar em posi|fffd||fffd|o padr|fffd|o
            Case 2, 5    'In center. O Excel j|fffd| centraliza. Se centro Image1 estiver a direita empurra com com esquerdo negativo, do contr|fffd|rio com o direito. LeftAdjustFat n|fffd|o parece necess|fffd|rio aplicar e remover
                If MoveLeft + HrzHdftPosPict.Width / 2 >= PaperWidth / 2 Then
                    HrzHdftPosPict.CropLeft = -(MoveLeft + HrzHdftPosPict.Width / 2 - PaperWidth / 2) * 2 / (HrzHdftPosPict.Width * dbZoom) * OriWidth
                    HrzHdftPosPict.CropLeft = Round(HrzHdftPosPict.CropLeft * 1, 1)
                Else
                    HrzHdftPosPict.CropRight = -((PaperWidth - HrzHdftPosPict.Width - MoveLeft) + HrzHdftPosPict.Width / 2 - PaperWidth / 2) * 2 / (HrzHdftPosPict.Width * dbZoom) * OriWidth
                    HrzHdftPosPict.CropRight = Round(HrzHdftPosPict.CropRight * 1, 1)
                End If
            Case 3, 6    'In right. Semelhante ao left, mas ajusta no CropRight
                HrzHdftPosPict.CropRight = -((PaperWidth - HrzHdftPosPict.Width - MoveLeft) + HrzHdftPosPict.Width - IIf(dHostAppVer >= 11 + 1 And bAnchorOnLeftMargin, IIf(.RightMargin < 53.858, 53.858, .RightMargin), 53.858) * dbZoom) / (HrzHdftPosPict.Width * dbZoom) * OriWidth + OriWidth
                HrzHdftPosPict.CropRight = Round(HrzHdftPosPict.CropRight * LeftAdjustFat, 1)
            End Select
        End If

        If MoveTop = -169093 Then           'Centralizar
            MoveTop = (PaperHeight - RefTopMargin - RefBottomMargin - CurrRealHeight) / 2 + RefTopMargin
        Else
            MoveTop = MoveTop + RefTopMargin    'Adiciona a margem da |fffd|rea de ref
        End If
        If MoveTop <> 0 Then
            Select Case lEntryPointHdFt    'Tops
            Case 1, 2, 3    'In header. Original of version with only LH the 1
                HrzHdftPosPict.CropTop = -(MoveTop + HrzHdftPosPict.Height - .HeaderMargin * dbZoom) / (HrzHdftPosPict.Height * dbZoom) * OriHeight + OriHeight
                HrzHdftPosPict.CropTop = Round(HrzHdftPosPict.CropTop * TopAdjustFat, 1)
            Case 4, 5, 6    'In footer. Semelhante ao header, mas ajusta no CropBottom
                HrzHdftPosPict.CropBottom = -((PaperHeight - HrzHdftPosPict.Height - MoveTop) + HrzHdftPosPict.Height - .FooterMargin * dbZoom) / (HrzHdftPosPict.Height * dbZoom) * OriHeight + OriHeight
                HrzHdftPosPict.CropBottom = Round(HrzHdftPosPict.CropBottom * TopAdjustFat, 1)
            End Select
        End If
    End With
End Function


