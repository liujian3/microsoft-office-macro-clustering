Private Function CritTgtShHdFtPosPgTp(TargetSheet As Variant, HeaderFooterPos As String, PageType As String, _
        TargetSheetInMsg As String) As Long

    If appHostApp.DisplayAlerts Then MustBeRegToCont Else AutentReg   'O alerta feito em MustBeRegToCont, al|fffd|m de suficiente ainda o usu|fffd|rio pode registrar no v|fffd|o.
    If Not bRegtrd And Not bIsFreePass Then
        CritTgtShHdFtPosPgTp = 1001
        On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
        Exit Function
    End If

    On Error Resume Next  'xx n|fffd|o esquecer de abilitar antes de publicar
    If TypeName(TargetSheet) <> "Worksheet" Then
        If TargetSheetInMsg = "Target sheet" Then Msg1 = IIf(IsPortg, "Planilha alvo", TargetSheetInMsg)
        If TargetSheetInMsg = "Source sheet" Then Msg1 = IIf(IsPortg, "Planilha fonte", TargetSheetInMsg)
        CritTgtShHdFtPosPgTp = 1
        Msg1 = IIf(IsPortg, Msg1 & " n|fffd|o foi informada ou n|fffd|o foi encontrada!", Msg1 & " wasn't informed or wasn't found!")
        Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & CritTgtShHdFtPosPgTp & ")"
        If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
        On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
        Exit Function
    End If

    If Not (HeaderFooterPos = "LeftHeader" Or HeaderFooterPos = "CenterHeader" Or HeaderFooterPos = "RightHeader" _
            Or HeaderFooterPos = "LeftFooter" Or HeaderFooterPos = "CenterFooter" Or HeaderFooterPos = "RightFooter") Then
        CritTgtShHdFtPosPgTp = 2
        Msg1 = IIf(IsPortg, "A posi|fffd||fffd|o '" & HeaderFooterPos & "' de cabe|fffd|alho e rodap|fffd| informada n|fffd|o existe ou n|fffd|o |fffd| suportada por esta vers|fffd|o do Excel!", "The '" & HeaderFooterPos & "' header and footer position informed doesn't exist or isn't supported by this Excel version!")
        Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & CritTgtShHdFtPosPgTp & ")"
        If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
        On Error GoTo 0
        Exit Function
    End If
    If HeaderFooterPos = "LeftHeader" Then lEntryPointHdFt = 1
    If HeaderFooterPos = "CenterHeader" Then lEntryPointHdFt = 2
    If HeaderFooterPos = "RightHeader" Then lEntryPointHdFt = 3
    If HeaderFooterPos = "LeftFooter" Then lEntryPointHdFt = 4
    If HeaderFooterPos = "CenterFooter" Then lEntryPointHdFt = 5
    If HeaderFooterPos = "RightFooter" Then lEntryPointHdFt = 6

    If (dHostAppVer < 11 + 1 And Not PageType = "Default") Or _
            (dHostAppVer >= 11 + 1 And Not (PageType = "Default" Or PageType = "First" Or PageType = "Even")) Then
        CritTgtShHdFtPosPgTp = 3
        Msg1 = IIf(IsPortg, "O tipo '" & PageType & "' de p|fffd|gina informada n|fffd|o |fffd| suportada por esta vers|fffd|o do Excel!", "The '" & PageType & "' page type informed isn't supported by this Excel version!")
        Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & CritTgtShHdFtPosPgTp & ")"
        If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
        On Error GoTo 0
        Exit Function
    End If
    If PageType = "Default" Then lDiffPage = 0
    If PageType = "First" Then lDiffPage = 1
    If PageType = "Even" Then lDiffPage = 2
End Function


Function fPageSetpMgrPictInsFile(TargetSheet As Variant, HeaderFooterPos As String, PageType As String, _
        FileFullName As String, Optional RefArea As Long = -1, Optional Brighten As Boolean = True) As Long

    fPageSetpMgrPictInsFile = CritTgtShHdFtPosPgTp(TargetSheet, HeaderFooterPos, PageType, "Target sheet")
    If fPageSetpMgrPictInsFile <> 0 Then Exit Function

    If Not InsertPictureTempWb(FileFullName, True) Then    'Test picture on temp wb first
        fPageSetpMgrPictInsFile = 3 + 1
        'Msg1 = was set in InsertPictureTempWb function
        Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & fPageSetpMgrPictInsFile & ")"
        If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
        On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
        Exit Function
    End If

    bAnchorOnLeftMargin = False    'Nunca deixar ancorado, se quiser, que o fa|fffd|a via VBA

    Dim TargetShF As Worksheet
    Set TargetShF = TargetSheet
    If Not InsertPicture(FileFullName, TargetShF, Brighten, False, True) Then
        fPageSetpMgrPictInsFile = 3 + 2
