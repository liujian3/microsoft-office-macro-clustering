        Msg1 = IIf(IsPortg, "N|fffd|o foi poss|fffd|vel restaurar a figura em  ", "Unable to restore the picture in ") & PictPosSpell(lDiffPage, lEntryPointHdFt) & "!"
        Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & fPageSetpMgrPictGetIpic & ")"
        If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
        Set TargetSh = Nothing: Set TargetWb = Nothing
        On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
        Exit Function
    End If

    Set IPictRet = GetIPictWmfConv(sPicFileOrigNames)
    If IPictRet Is Nothing Then
        fPageSetpMgrPictGetIpic = 3 + 3
        Msg1 = IIf(IsPortg, "N|fffd|o foi poss|fffd|vel criar o objeto IPicture!", "Unable to create IPicture object!")
        Msg2 = sAddInCaptByApp & IIf(IsPortg, " - Falha! (Erro N|fffd| ", " - Fail! (Err N|fffd| ") & fPageSetpMgrPictGetIpic & ")"
        If appHostApp.DisplayAlerts Then MyMsgBox Msg1, 1, Msg2
        On Error GoTo 0    'Se n|fffd|o limpar, vai ecoar no c|fffd|digo que chamou
    End If
    'DelTempFiles    'Kill if sPicFileOrigNames was created temporally
    Set TargetSh = Nothing: Set TargetWb = Nothing
End Function
