          Exit Sub
        Else
            If LCase(TextBox1.Text) = "ak" Or LCase(TextBox1.Text) = "hi" Or LCase(TextBox1.Text) = "pr" Then
                If LCase(TextBox1.Text) = "ak" Then
                    offReg = "AK"
