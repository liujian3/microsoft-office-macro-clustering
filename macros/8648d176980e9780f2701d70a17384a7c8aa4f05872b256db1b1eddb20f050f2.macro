                        offReg = "HI"
                    Else
                        If LCase(TextBox1.Text) = "pr" Then
                            offReg = "PR"
                        End If
                    End If
                End If
            Else
                'Convert the text (String) to the data type Double
                If IsNumeric(.Text) = False Then
                    MsgBox "Invalid Department Entry" & vbCrLf & "Enter department #, AK, HI, or PR."
                    .SetFocus
                    Exit Sub
                Else
                    dNumber = CDbl(.Text)
                End If
            End If
       End If
    End With
    
    If offReg = "NO" Then
