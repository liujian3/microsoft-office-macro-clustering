Function AddToThePoints2(iIncrement2 As Integer) As Integer
    Static ipoints2 As Integer
    If iIncrement2 = -9999 Then
        ipoints2 = 0
    Else
        ipoints2 = ipoints2 + iIncrement2
    End If
    AddToThePoints2 = ipoints2
End Function

