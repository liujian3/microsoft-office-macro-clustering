Function AddToThePoints(iIncrement As Integer) As Integer
    Static ipoints As Integer
    If iIncrement = -9999 Then
        ipoints = 0
    Else
        ipoints = ipoints + iIncrement
    End If
    AddToThePoints = ipoints
End Function

